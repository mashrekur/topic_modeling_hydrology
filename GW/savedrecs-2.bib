@article{ ISI:000263600800012,
Author = {Clayton, Matthew H. and Borden, Robert C.},
Title = {{Numerical Modeling of Emulsified Oil Distribution in Heterogeneous
   Aquifers}},
Journal = {{GROUND WATER}},
Year = {{2009}},
Volume = {{47}},
Number = {{2}},
Pages = {{246-258}},
Month = {{MAR-APR}},
Abstract = {{In situ anaerobic bioremediation using edible oil emulsions will be most
   effective if the oil droplets can be brought into close contact with the
   contaminant to be treated. However, uniformly distributing oil in
   heterogeneous aquifers can be difficult. The impact of injection
   conditions on emulsion distribution in a three-dimensional heterogeneous
   aquifer is examined using MODFLOW and RT3D. Emulsion retention is
   simulated using a rate-limited Langmuir isotherm. Volume and flow
   contact efficiency are shown to be functions of mass of oil injected,
   injection fluid volume, well spacing, and injection sequence. Regression
   equations are developed relating dimensionless scaling factors to
   expected contact efficiency for area treatment and barriers. Cleanup
   time for uncontacted zones is estimated using a mobile-immobile zone
   modeling approach.}},
