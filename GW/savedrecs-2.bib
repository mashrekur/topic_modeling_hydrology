@article{ ISI:000259193300006,
Author = {Chui, Ting Fong May and Freyberg, David L.},
Title = {{Simulating a lake as a high-conductivity variably saturated porous
   medium}},
Journal = {{GROUND WATER}},
Year = {{2008}},
Volume = {{46}},
Number = {{5}},
Pages = {{688-694}},
Month = {{SEP-OCT}},
Abstract = {{One approach for simulating ground water-lake interactions is to
   incorporate the lake into the ground water Solution domain as a
   high-conductivity region. Previous studies have developed this approach
   models. This Study extends this approach to variably saturated models.
   so that ground-water water-lake interactions may be more easily
   simulated with Commonly used or public domain variably saturated codes
   that do not explicitly support Coupled lake-water balance modeling.
   General guidelines Are developed for the choices Of saturated hydraulic
   conductivity and moisture retention and relative permeability curves for
   the lake region. When applied to ail example ground water-lake system,
   model results are very similar to those from a model in which the lake
   is represeuted as a specified head boundary continuosly updated by a
   lake mass balance. The high-conductivity region approach is most
   suitable for relatively simple geometries and lakes with slower and
   smaller fluctuations when the overall flow pattern and system fluxes,
   rather than the detailed flow pattern around the intersection of the
   lake and land surfaces, are of interest.}},
