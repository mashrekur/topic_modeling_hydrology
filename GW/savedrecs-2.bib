@article{ ISI:000233323800015,
Author = {Muldoon, M and Bradbury, KR},
Title = {{Site characterization in densely fractured dolomite: Comparison of
   methods}},
Journal = {{GROUND WATER}},
Year = {{2005}},
Volume = {{43}},
