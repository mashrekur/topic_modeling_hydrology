@article{ ISI:000240289300011,
Author = {Mettier, Ralph and Kosakowski, Georg and Kolditz, Olaf},
Title = {{Influence of small-scale heterogeneities on contaminant transport in
   fractured crystalline rock}},
Journal = {{GROUND WATER}},
Year = {{2006}},
Volume = {{44}},
Number = {{5}},
Pages = {{687-696}},
Month = {{SEP-OCT}},
Abstract = {{We present a sequence of purely advective transport models that
   demonstrate the influence of small-scale geometric inhomogeneities on
   contaminant transport in fractured crystalline rock. Special weight is
   placed on the role of statistically generated variable fracture
   apertures. The fracture network geometry and the aperture distribution
   are based on information from an in situ radionuclide retardation
   experiment performed at Grimsel test site (Swiss Alps). The obtained
   breakthrough curves are fitted with the advection dispersion equation
   and continuous-time random walks (CTRW). CTRW is found to provide
   superior fits to the late-arrival tailing and is also found to show a
   good correlation with the velocity distributions obtained from the
   hydraulic models. The impact of small-scale heterogeneities, both in
   fracture geometry and aperture, on transport is shown to be
   considerable.}},
