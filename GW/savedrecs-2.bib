@article{ ISI:000247982000007,
Author = {Kamann, Patrick J. and Ritzi, Robert W. and Dominic, David F. and
   Conrad, Caleb M.},
Title = {{Porosity and permeability in sediment mixtures}},
Journal = {{GROUND WATER}},
Year = {{2007}},
Volume = {{45}},
Number = {{4}},
Pages = {{429-438}},
Month = {{JUL-AUG}},
Abstract = {{Porosity in sediments that contain a mix of coarser- and finer-grained
   components varies as a function of the porosity and volume fraction of
   each component. We considered sediment mixtures representing poorly
   sorted sands and gravely sands. We expanded an existing
   fractional-packing model for porosity to represent mixtures in which
   finer grains approach the size of the pores that would exist among the
   coarser grains alone. The model well represents the porosity measured in
   laboratory experiments in which grain sizes and volume fractions were
   systematically changed within sediment mixtures. Permeability values
   were determined for these sediment mixtures using a model based on
   grain-size statistics and the expanded fractional-packing porosity
   model. The permeability model well represents permeability measured in
   laboratory experiments using air- and water-based permeametry on the
   model sediment mixtures.}},
