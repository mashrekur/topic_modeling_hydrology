@article{ ISI:000234426000016,
Author = {Zaadnoordijk, WJ},
Title = {{Building pit dewatering: Application of transient analytic elements}},
Journal = {{GROUND WATER}},
Year = {{2006}},
Volume = {{44}},
Number = {{1}},
Pages = {{106-110}},
Month = {{JAN-FEB}},
Abstract = {{Analytic elements are well suited for the design of building pit
   dewatering. Wells and drains can be modeled accurately by analytic
   elements, both nearby to determine the pumping level and at some
   distance to verify the targeted drawdown at the building site and to
   estimate the consequences in the vicinity. The ability to shift
   locations of wells or drains easily makes the design process very
   flexible. The temporary pumping has transient effects, for which
   transient analytic elements may be used. This is illustrated using the
   free, open-source, object-oriented analytic element simulator Tim(SL)
   for the design of a building pit dewatering near a canal. Steady
   calculations are complemented with transient calculations. Finally, the
