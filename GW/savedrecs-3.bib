@article{ ISI:000222434800014,
Author = {Gotkowitz, MB and Schreiber, ME and Sim, JA},
Title = {{Effects of water use on arsenic release to well water in a confined
   aquifer}},
Journal = {{GROUND WATER}},
Year = {{2004}},
Volume = {{42}},
Number = {{4}},
Pages = {{568-575}},
Month = {{JUL-AUG}},
Abstract = {{Field-based experiments were designed to investigate the release of
   naturally occurring, low to moderate (< 50 mug/L) arsenic concentrations
   to well water in a confined sandstone aquifer in northeastern Wisconsin.
   Geologic, geochemical, and hydrogeologic data collected from a 115 m(2)
   site demonstrate that arsenic concentrations in ground water are
   heterogeneous at the scale of the field site, and that the distribution
   of arsenic in ground water correlates to solid-phase arsenic in aquifer
   materials. Arsenic concentrations in a test well varied from 1.8 to 22
   mug/L during experiments conducted under no, low, and high pumping
   rates. The quality of ground water consumed from wells under typical
   domestic water use patterns differs from that of ground water in the
   aquifer because of reactions that occur within the well. Redox
   conditions in the well can change rapidly in response to ground water
   withdrawals. The well borehole is an environment conducive to
   microbiological growth, and biogeochemical reactions also affect
   borehole chemistry. While oxidation of sulfide minerals appears to
   release arsenic to ground water in zones within the aquifer, reduction
   of arsenic-bearing iron (hydr)oxides is a likely mechanism of arsenic
