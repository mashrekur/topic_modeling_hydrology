@article{ ISI:000181382900006,
Author = {Tsai, FTC and Sun, NZ and Yeh, WWG},
Title = {{A combinatorial optimization scheme for parameter structure
   identification in ground water modeling}},
Journal = {{GROUND WATER}},
Year = {{2003}},
Volume = {{41}},
