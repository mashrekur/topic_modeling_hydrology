@article{ ISI:000172131100015,
Author = {Paulsen, RJ and Smith, CF and O'Rourke, D and Wong, TF},
Title = {{Development and evaluation of an ultrasonic ground water seepage meter}},
Journal = {{GROUND WATER}},
Year = {{2001}},
Volume = {{39}},
