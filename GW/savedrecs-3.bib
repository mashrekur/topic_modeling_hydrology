@article{ ISI:000181382900016,
Author = {Russell, TF and Heberton, CI and Konikow, LF and Hornberger, GZ},
Title = {{A finite-volume ELLAM for three-dimensional solute-transport modeling}},
Journal = {{GROUND WATER}},
Year = {{2003}},
Volume = {{41}},
Number = {{2}},
