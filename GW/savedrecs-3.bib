@article{ ISI:000178050000009,
Author = {Wright, M and Dillon, P and Pavelic, P and Peter, P and Nefiodovas, A},
Title = {{Measurement of 3-D hydraulic conductivity in aquifer cores at in situ
   effective stresses}},
Journal = {{GROUND WATER}},
Year = {{2002}},
Volume = {{40}},
Number = {{5}},
Pages = {{509-517}},
Month = {{SEP-OCT}},
Abstract = {{An innovative and nondestructive method to measure the hydraulic
   conductivity of drill core samples in horizontal and vertical directions
   within a triaxial cell has been developed. This has been applied to
   characterizing anisotropy and heterogeneity of a confined consolidated
   limestone aquifer. Most of the cores tested were isotropic, but
   hydraulic conductivity varied considerably and the core samples with
   lowest values were also the most anisotropic, Hydraulic conductivity
   decreased with increasing effective stress due to closure of
   microfractures caused by sampling for all core samples. This
   demonstrates the importance of replicating in situ effective stresses
   when measuring hydraulic conductivity of cores of deep aquifers in the
   laboratory.}},
