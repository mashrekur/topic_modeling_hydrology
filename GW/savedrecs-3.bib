@article{ ISI:000229051400010,
Author = {Manning, AH and Thiros, SA},
Title = {{H-3/He-3 age data in assessing the susceptibility of wells to
   contamination}},
Journal = {{GROUND WATER}},
Year = {{2005}},
Volume = {{43}},
Number = {{3}},
Pages = {{353-367}},
Month = {{MAY-JUN}},
Abstract = {{Regulatory agencies are becoming increasingly interested in using
   young-ground water dating techniques, such as the H-3/He-3 method, in
   assessing the susceptibility of public supply wells (PSWs) to
   contamination. However, recent studies emphasize that ground water
   samples of mixed age may be the norm, particularly from long-screened
   PSWs, and tracer-based ``apparent{''} ages can differ substantially from
   actual mean ages for mixed-age samples. We present age and contaminant
   data from PSWs in Salt Lake Valley, Utah, that demonstrate the utility
   of H-3 and He-3 measurements in evaluating well susceptibility, despite
   potential age mixing. Initial H-3 concentrations (measured H-3 +
   measured tritiogenic He-3) are compared to those expected based on the
   apparent H-3/He-3 age and the local precipitation 3H record. This
   comparison is used to determine the amount of modern water (recharged
   after \&SIM; 1950) vs. prebomb water (recharged before \&SIM; 1950)
   samples might contain. Concentrations of common contaminants were also
   measured using detection limits generally lower than those used for
   regulatory purposes. A clear correlation exists between the potential
   magnitude of the modern water fraction and both the occurrence and
   concentration of contaminants. For samples containing dominantly modern
   water based on their initial H-3 concentrations, potential discrepancies
   between apparent H-3/He-3 ages and mean ages are explored using
   synthetic samples that are random mixtures of different modern waters.
   Apparent ages can exceed mean ages by up to 13 years for these samples,
   with an exponential age distribution resulting in the greatest
   discrepancies.}},
