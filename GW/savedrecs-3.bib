@article{ ISI:000229051400014,
Author = {Clarke, DD and Meerschaert, MM and Wheatcraft, SW},
Title = {{Fractal travel time estimates for dispersive contaminants}},
Journal = {{GROUND WATER}},
Year = {{2005}},
Volume = {{43}},
Number = {{3}},
Pages = {{401-407}},
Month = {{MAY-JUN}},
Abstract = {{Alternative fractional models of contaminant transport lead to a new
   travel time formula for arbitrary concentration levels. For an evolving
   contaminant plume in a highly heterogeneous aquifer, the new formula
   predicts much earlier arrival at low concentrations. Travel times of
   contaminant fronts and plumes are often obtained from Darcy's law
   calculations using estimates of average pore velocities. These estimates
   only provide information about the travel time of the average
   concentration (or peak, for contaminant pulses). Recently, it has been
   shown that finding the travel times of arbitrary concentration levels is
   a straightforward process, and equations were developed for other
   portions of the breakthrough curve for a nonreactive contaminant. In
   this paper, we generalize those equations to include alternative
   fractional models of contaminant transport.}},
