@article{ ISI:000186412600018,
Author = {Hoaglund, JR and Pollard, D},
Title = {{Dip and anisotropy effects on flow using a vertically skewed model grid}},
Journal = {{GROUND WATER}},
Year = {{2003}},
Volume = {{41}},
Number = {{6}},
Pages = {{841-846}},
Month = {{NOV-DEC}},
Abstract = {{Darcy flow equations relating vertical and bedding-parallel flow to
   vertical and bedding-parallel gradient components are derived for a
   skewed Cartesian grid in a vertical plane, correcting for structural dip
   given the principal hydraulic conductivities in bedding-parallel and
   bedding-orthogonal directions. Incorrect-minus-correct flow error
   results are presented for ranges of structural dip (0 less than or equal
   to theta less than or equal to 90) and gradient directions (0 less than
   or equal to phi less than or equal to 360). The equations can be coded
   into ground water models (e.g., MODFLOW) that can use a skewed Cartesian
   coordinate system to simulate flow in structural terrain with deformed
   bedding planes. Models modified with these equations will require input
   arrays of strike and dip, and a solver that can handle off-diagonal
   hydraulic conductivity terms.}},
