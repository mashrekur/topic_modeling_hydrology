@article{ ISI:000207174300006,
Author = {Claxton, Amanda J. and Bates, Paul D. and Cloke, Hannah L.},
Title = {{Mixing of Hillslope, River, and Alluvial Ground Waters in Lowland
   Floodplains}},
Journal = {{GROUND WATER}},
Year = {{2003}},
Volume = {{41}},
