@article{ ISI:000172131100018,
Author = {Haitjema, H and Kelson, V and de Lange, W},
Title = {{Selecting MODFLOW cell sizes for accurate flow fields}},
Journal = {{GROUND WATER}},
Year = {{2001}},
Volume = {{39}},
