@article{ ISI:000185133900012,
Author = {Ferguson, G and Woodbury, AD and Matile, GLD},
Title = {{Estimating deep recharge rates beneath an interlobate moraine using
   temperature logs}},
Journal = {{GROUND WATER}},
Year = {{2003}},
Volume = {{41}},
Number = {{5}},
Pages = {{640-646}},
Month = {{SEP-OCT}},
Abstract = {{The Sandilands area of southeastern Manitoba contains an interlobate
   moraine that is a major ground water recharge area. Underlying the
   highly permeable sediments of the moraine are up to 100 m of till and
   the subcrop of the Winnipeg Formation, which contains a major sandstone
   aquifer. Ground water flow within the till is examined using high
   resolution temperature profiles and solutions to the differential
   equation for heat flow in porous media. These analyses indicate that
   recharge to the sandstone aquifer is occurring at a rate of
   approximately 2 X 10(-8) m/sec beneath the moraine, which is in
   agreement with recharge rates determined by conventional ground water
   hydraulics (10(-7) to 10(-10) m/sec) and another study using multiple
   environmental tracers (1 X 10(-9) to 6 X 10(-9) m/sec). The use of
   temperature to determine ground water flux is not limited by half-lives
   as many environmental tracers are, and this allows for cost-effective
   estimation of recharge and discharge rates over longer periods.}},
