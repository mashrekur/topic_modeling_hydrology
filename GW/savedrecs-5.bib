@article{ ISI:A1996TN46800021,
Author = {Musa, M and Kemblowski, MW},
Title = {{Transient capture zone for a single well}},
Journal = {{GROUND WATER}},
Year = {{1996}},
Volume = {{34}},
Number = {{1}},
Pages = {{168-170}},
Month = {{JAN-FEB}},
Abstract = {{Ground-water wells are frequently used to intercept contaminant plumes.
   The conventional design procedure is to plot the steady-state capture
   zone and check whether the contaminant plume lies inside it. This
   approach ignores the pollutant's movement during the transient
   development of the cone of depression. In this paper we present a new
   method to analyze the transient performance of a single well that
   combines the analytical solutions for the steady-state capture zones md
   transient velocity held with particle tracking. Using tbe particle
   trajectories, an effective capture zone can be estimated. No contaminant
   which is outside of this zone at the start of pumping can be captured by
   the well. The use of this method is illustrated by a hypothetical
   example.}},
