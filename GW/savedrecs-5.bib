@article{ ISI:A1995PZ57500012,
Author = {HADZISEHOVIC, M and DANGIC, A and MILJEVIC, N and SIPKA, V and
   GOLOBOCANIN, D},
Title = {{GEOTHERMAL-WATER CHARACTERISTICS IN THE SURDULICA AQUIFER}},
