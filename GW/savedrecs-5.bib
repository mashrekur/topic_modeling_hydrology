@article{ ISI:A1995TD12700016,
Author = {ARNOLD, JG and ALLEN, PM and MUTTIAH, R and BERNHARDT, G},
Title = {{AUTOMATED BASE-FLOW SEPARATION AND RECESSION ANALYSIS TECHNIQUES}},
