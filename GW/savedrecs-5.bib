@article{ ISI:A1994MP42400014,
Author = {CLEVELAND, TG},
Title = {{RECOVERY PERFORMANCE FOR VERTICAL AND HORIZONTAL WELLS USING
   SEMIANALYTICAL SIMULATION}},
Journal = {{GROUND WATER}},
Year = {{1994}},
Volume = {{32}},
Number = {{1}},
Pages = {{103-107}},
Month = {{JAN-FEB}},
Abstract = {{A semianalytical transient expression was used to evaluate the
   performance of a finite-length horizontal well as compared to a vertical
   well in a hypothetical recovery scenario. Recovery was simulated in two
   hypothetical confirmed aquifers to study the effect of aquifer thickness
   and well orientation on recovery performance. The results show that
   horizontal wells can initially mobilize larger volumes of ground water
   (and contaminants) but that vertical wells have the same ultimate
   recovery ability in a homogeneous, isotropic medium.}},
