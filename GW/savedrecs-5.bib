@article{ ISI:A1995TD12700013,
Author = {BREWSTER, ML and ANNAN, AP and GREENHOUSE, JP and KUEPER, BH and
   OLHOEFT, GR and REDMAN, JD and SANDER, KA},
Title = {{OBSERVED MIGRATION OF A CONTROLLED DNAPL RELEASE BY GEOPHYSICAL METHODS}},
