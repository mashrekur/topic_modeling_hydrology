@article{ ISI:A1996VF56900006,
Author = {Cole, JD and Woods, SL and Bricker, TR},
Title = {{Pentachlorophenol reductive dechlorination in an interceptor trench:
   Temperature effects}},
Journal = {{GROUND WATER}},
Year = {{1996}},
Volume = {{34}},
Number = {{5}},
Pages = {{784-790}},
Month = {{SEP-OCT}},
Abstract = {{Pentachlorophenol-acclimated methanogenic consortia were incubated at
   temperatures of 5, 10, 21, and 31 degrees C in physical models of an
   interceptor trench. Four identical 5.4 cm diameter brass columns packed
   with a uniform pea gravel were operated with a 2 day hydraulic retention
   time in a continuous upflow mode. The columns were fed 25 or 50 mg of
   acetate per liter, 0.32 or 4.0 mu M pentachlorophenol, and a dilute
   vitamin and nutrient mixture. In the columns operated at 31 and 21
   degrees C, pentachlorophenol was reductively dechlorinated at the ortho
   positions to form 2,3,4,5-tetrachlorophenol and 3,4,5-trichlorophenol.
   3,4,5-Trichlorophenol was the sole degradation product observed in the
   effluent. The lag period prior to pentachlorophenol biotransformation
   increased with decreasing temperature. At 31 degrees C, a lag of 72
   hours was observed; at 21 degrees C the lag was 120 hours; and at 10
   degrees C the lag was 744 hours. No biodegradation of pentachlorophenol
   was observed at 5 degrees C within an 888 hour period. Conversion of
   pentachlorophenol to 3,4,5-trichlorophenol was complete within the first
   7.6 cm of the column at 31 and 21 degrees C, Loss of 90\% of the
   pentachlorophenol was observed over the 30 cm column at 10 degrees C. An
   interceptor trench inoculated with a temperature- and
   pentachlorophenol-acclimated methanogenic consortia has the potential to
   anaerobically biotransform pentachlorophenol at temperatures as low as
   10 degrees C.}},
