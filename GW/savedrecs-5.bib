@article{ ISI:A1995PZ57500013,
Author = {LOAGUE, K and MIYAHIRA, RN and GREEN, RE and OKI, DS and GIAMBELLUCA, TW
   and SCHNEIDER, RC},
Title = {{CHEMICAL LEACHING NEAR THE WAIAWA SHAFT, OAHU, HAWAII .2. MODELING
   RESULTS}},
