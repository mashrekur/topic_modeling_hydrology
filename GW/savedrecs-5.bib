Type = {{Article}},
Language = {{English}},
Affiliation = {{KALUARACHCHI, JJ (Reprint Author), UTAH STATE UNIV, COLL ENGN, UTAH WATER RES LAB, LOGAN, UT 84322 USA.
   ENVIRONM MANAGEMENT DIRECTORATE, HILL AFB, UT 84056 USA.}},
DOI = {{10.1111/j.1745-6584.1995.tb00036.x}},
