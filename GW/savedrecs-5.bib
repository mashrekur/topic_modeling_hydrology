DOI = {{10.1111/j.1745-6584.1993.tb00864.x}},
ISSN = {{0017-467X}},
Keywords-Plus = {{NATURAL SEDIMENTS; WATER; SYSTEMS}},
Research-Areas = {{Geology; Water Resources}},
