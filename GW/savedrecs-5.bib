@article{ ISI:A1995QK56000008,
Author = {FALTA, RW},
Title = {{ANALYTICAL SOLUTIONS FOR GAS-FLOW DUE TO GAS INJECTION AND EXTRACTION
   FROM HORIZONTAL WELLS}},
Journal = {{GROUND WATER}},
Year = {{1995}},
Volume = {{33}},
Number = {{2}},
Pages = {{235-246}},
Month = {{MAR-APR}},
Abstract = {{Analytical solutions are developed for modeling the transient and
   steady-state gas pressure and the steady-state streamfunction fields
   resulting from gas injection and extraction from a pair of parallel
   horizontal wells. These solutions apply to cases in which the ground
   surface is open to the atmosphere, and in which the porous media is
   anisotropic but homogeneous. By neglecting end effects due to the finite
   length of the wells, the three-dimensional gas flow field is
   approximated as a two-dimensional cross section perpendicular to the
   wells. These solutions may be used to develop estimates of the
   horizontal well system behavior and to analyze horizontal well gas pump
   tests, and are useful for numerical model verification.}},
