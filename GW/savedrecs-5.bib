@article{ ISI:A1995RG50000018,
Author = {SZEKELY, F},
Title = {{ESTIMATION OF UNSTEADY, 3-DIMENSIONAL DRAWDOWN IN SINGLE, VERTICALLY
   HETEROGENEOUS AQUIFERS}},
Journal = {{GROUND WATER}},
Year = {{1995}},
Volume = {{33}},
Number = {{4}},
Pages = {{669-674}},
Month = {{JUL-AUG}},
Abstract = {{A quasi, a mixed, and a weighted three-dimensional model are applied to
   approximate the three-dimensional, unsteady drawdown in vertical pumping
   and observation wells, fully or partially penetrating a single,
   vertically heterogeneous, anisotropic aquifer. Cases of confined,
   semiconfined, and unconfined now conditions are considered. Numerical
   examples are used to quantify the numerical error of the methods,
   introduced by the vertical heterogeneity of the aquifer. Results of
   pumping test data analysis in a three-layer, anisotropic aquifer with
   multiple screens in the pumping well are presented.}},
