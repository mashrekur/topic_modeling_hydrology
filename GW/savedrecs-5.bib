@article{ ISI:A1996VQ57500004,
Author = {Allred, B and Brown, GO},
Title = {{Boundary condition and soil attribute impacts on anionic surfactant
   mobility in unsaturated soil}},
Journal = {{GROUND WATER}},
Year = {{1996}},
Volume = {{34}},
Number = {{6}},
Pages = {{964-971}},
Month = {{NOV-DEC}},
Abstract = {{Surfactant mobility in unsaturated soil will impact the effectiveness
   and efficiency of using these compounds for in situ environmental
   remediation above the water table. For this reason, transient
   unsaturated column tests were used to study the influence of boundary
   conditions and soil attributes on anionic surfactant transport. In these
   tests, aqueous surfactant solutions were injected into the inlet of
   horizontally mounted soil columns. Two commercial anionic surfactants
   were used, an alkyl ether sulfate (AES) and a linear alkylbenzene
   sulfonate (LAS).
   The overall study was divided into two parts. First, boundary condition
   effects including injected surfactant solution concentration, initial
   moisture content, and surfactant application rate were investigated.
   Increasing the injection solution concentration increased anionic
   surfactant mobility in the column while changes in the initial soil
   moisture content and surfactant application rate had no significant
   impact. Second, the impacts of soil attributes such as texture, dominant
   exchangeable cation, and resident organic matter were measured. With
   respect to texture, mobility was found to be greater in a sandy soil as
   compared with two loamy soils. Both surfactants, especially LAS, were
   found to be more mobile in a Na+ dominated soil rather than one
   dominated by Ca+2. The absence of soil organic matter increased LAS
   mobility.}},
