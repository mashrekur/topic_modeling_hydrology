@article{ ISI:A1994MP42400017,
Author = {MERGIA, G and KELLY, WE},
Title = {{MODELING GROUNDWATER REMEDIATION IN THE HIGH-PLAINS AQUIFER}},
Journal = {{GROUND WATER}},
Year = {{1994}},
Volume = {{32}},
Number = {{1}},
Pages = {{129-138}},
Month = {{JAN-FEB}},
Abstract = {{The Princeton Transport Code (PTC), a three-dimensional finite-element
   flow and transport model, was used to evaluate contaminant movement and
   recovery in the vicinity of a manufacturing facility in western
   Nebraska. The modeling was complicated by seasonal recharge from a
   nearby irrigation canal, a drainage ditch flowing across the site,
   municipal wells pumping at variable rates and screened at different
   levels in the underlying semiconfined aquifer, and the pumping of the
   three high-capacity wells being used to recover contaminated ground
   water. The flow model was calibrated in several steps: steady-state
   calibration using the unstressed (no pumping) conditions; steady-state
   calibration using the prerecovery pumping conditions; and finally,
   transient calibration using results obtained for the early recovery
   pumping period-first for the nonirrigation season and then for the
   irrigation season. The calibrated flow model was further tested by
   simulating transport during the early stages of the recovery pumping.}},
