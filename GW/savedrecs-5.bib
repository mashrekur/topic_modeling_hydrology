@article{ ISI:A1997WA67900023,
Author = {Lesage, S and Xu, H and Novakowski, KS},
Title = {{Distinguishing natural hydrocarbons from anthropogenic contamination in
   ground water}},
Journal = {{GROUND WATER}},
Year = {{1997}},
Volume = {{35}},
Number = {{1}},
Pages = {{149-160}},
Month = {{JAN-FEB}},
Abstract = {{Differentiation between natural and anthropogenic sources of
   ground-water contamination by petroleum hydrocarbons is necessary in
   areas where natural hydrocarbons may be present in the subsurface.
   Because of the similarity in composition between natural and refined
   petroleum, the use of statistical techniques to discern trends is
   required. In this study, both multivariate plotting techniques and
   principal component analysis were used to investigate the origin of
   hydrocarbons from a variety of study sites. Ground-water and gas samples
   were collected from the Niagara Falls area and from three gasoline
   stations where leaking underground storage tanks had been found.
   Although soil gas surveys are used to indicate the presence of
   hydrocarbons, they were not useful in differentiating between natural
   and anthropogenic sources of contamination in ground water. Propane and
