@article{ ISI:A1997WM74800009,
Author = {Shackelford, CD and Glade, MJ},
Title = {{Analytical mass leaching model for contaminated soil and soil stabilized
   waste}},
Journal = {{GROUND WATER}},
Year = {{1997}},
Volume = {{35}},
Number = {{2}},
Pages = {{233-242}},
Month = {{MAR-APR}},
Abstract = {{An analytical model for evaluating mass leaching from contaminated soil
   or soil stabilized waste is presented. The model is based on mass
   transport due to advection, dispersion, and retardation and can be used
   to evaluate the suitability and/or efficiency of soil washing solutions
   based on the results of column leaching studies. The model differs from
   more traditional models for column leaching studies in that the analysis
   is based on the cumulative mass of leachate instead of leachate
   concentration. A cumulative mass basis for leaching eliminates the
   requirement for determination of `'instantaneous'' effluent
   concentrations in the more traditional column leaching approach thereby
   allowing for the collection of relatively large effluent volumes. The
   cumulative masses of three heavy metals-Cd, Pb, and Zn-leached from two
   specimens of soil mixed with fly ash are analyzed with the mass leaching
   model to illustrate application and limitation of the model.}},
