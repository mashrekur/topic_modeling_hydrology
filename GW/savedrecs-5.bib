Affiliation = {{SHARP, JM (Reprint Author), UNIV TEXAS,DEPT GEOL SCI,AUSTIN,TX 78712, USA.}},
DOI = {{10.1111/j.1745-6584.1994.tb00609.x}},
ISSN = {{0017-467X}},
