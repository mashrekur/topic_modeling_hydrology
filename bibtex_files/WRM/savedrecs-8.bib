@article{ ISI:000208541300004,
Author = {Correia, Francisco Nunes and Fordham, Maureen and Saraiva, Maria Da
   Graca and Bernardo, Fatima},
Title = {{Flood Hazard Assessment and Management: Interface with the Public}},
