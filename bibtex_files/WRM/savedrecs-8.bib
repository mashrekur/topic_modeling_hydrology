@article{ ISI:000083824100001,
Author = {Correia, FN and Saraiva, MD and Da Silva, FN and Ramos, I},
Title = {{Floodplain management in urban developing areas. Part I. Urban growth
   scenarios and land-use controls}},
Journal = {{WATER RESOURCES MANAGEMENT}},
