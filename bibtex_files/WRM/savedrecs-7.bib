@article{ ISI:000260517400011,
Author = {Jha, Madan K. and Namgial, Deldan and Kamii, Y. and Peiffer, Stefan},
Title = {{Hydraulic Parameters of Coastal Aquifer Systems by Direct Methods and an
   Extended Tide - Aquifer Interaction Technique}},
Journal = {{WATER RESOURCES MANAGEMENT}},
