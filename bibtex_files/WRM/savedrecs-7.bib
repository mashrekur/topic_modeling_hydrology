@article{ ISI:000252946500002,
Author = {Yurdusev, M. A. and Kumanlioglu, A. A.},
Title = {{Survey-based estimation of domestic water saving potential in the case
   of manisa city}},
Journal = {{WATER RESOURCES MANAGEMENT}},
