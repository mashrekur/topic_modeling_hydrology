@article{ ISI:000265881700003,
Author = {Liberatore, S. and Sechi, G. M.},
Title = {{Location and Calibration of Valves in Water Distribution Networks Using
   a Scatter-Search Meta-heuristic Approach}},
Journal = {{WATER RESOURCES MANAGEMENT}},
