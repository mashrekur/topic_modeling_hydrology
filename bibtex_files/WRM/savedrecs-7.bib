@article{ ISI:000254208000005,
Author = {Regulwar, D. G. and Raj, P. Anand},
Title = {{Development of 3-D optimal surface for operation policies of a
   multireservoir in fuzzy environment using Genetic Algorithm for river
   basin development and management}},
