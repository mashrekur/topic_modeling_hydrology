@article{ ISI:000233530900003,
Author = {Sylaios, G and Stamatis, N and Kallianiotis, A and Vidoris, P},
Title = {{Monitoring water quality and assessment of land-based nutrient loadings
   and cycling in Kavala Gulf}},
