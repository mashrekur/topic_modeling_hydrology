@article{ ISI:000249272200007,
Author = {Liu, Zuoxin and Shu, Qiaosheng and Wang, Zhenying},
Title = {{Applying pedo-transfer functions to simulate spatial heterogeneity of
   cinnamon soil water retention characteristics in western liaoning
   province}},
