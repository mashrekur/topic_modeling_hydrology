@article{ ISI:000265881700001,
Author = {Wang, Guoqiang and Hapuarachchi, Prasantha and Ishidaira, Hiroshi and
   Kiem, Anthony S. and Takeuchi, Kuniyoshi},
Title = {{Estimation of Soil Erosion and Sediment Yield During Individual
   Rainstorms at Catchment Scale}},
Journal = {{WATER RESOURCES MANAGEMENT}},
