@article{ ISI:000262486100009,
Author = {Naik, M. Gopal and Rao, E. P. and Eldho, T. I.},
Title = {{Finite Element Method and GIS Based Distributed Model for Soil Erosion
   and Sediment Yield in a Watershed}},
Journal = {{WATER RESOURCES MANAGEMENT}},
