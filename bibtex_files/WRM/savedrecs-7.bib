@article{ ISI:000260517400002,
Author = {Artimo, Aki and Sarapera, Sami and Ylander, Iikka},
Title = {{Methods for Integrating an Extensive Geodatabase with 3D Modeling and
   Data Management Tools for the Virttaankangas Artificial Recharge
   Project, Southwestern Finland}},
