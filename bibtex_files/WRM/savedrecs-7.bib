@article{ ISI:000246186200003,
Author = {He, Bin and Takase, Keiji and Wang, Yi},
Title = {{Regional groundwater prediction model using automatic parameter
   calibration SCE method for a coastal plain of Seto Inland Sea}},
Journal = {{WATER RESOURCES MANAGEMENT}},
