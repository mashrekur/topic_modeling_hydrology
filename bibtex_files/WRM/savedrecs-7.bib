@article{ ISI:000262486100010,
Author = {Dawoud, Mohamed A. and Raouf, Abdel Rahman Abdel},
Title = {{Groundwater Exploration and Assessment in Rural Communities of Yobe
   State, Northern Nigeria}},
Journal = {{WATER RESOURCES MANAGEMENT}},
