@article{ ISI:000238362400003,
Author = {Paredes, Javier and Lund, Jay R.},
Title = {{Refill and drawdown rules for parallel reservoirs: Quantity and quality}},
Journal = {{WATER RESOURCES MANAGEMENT}},
