@article{ ISI:000257772300004,
Author = {Lu, H. W. and Huang, G. H. and Zeng, G. M. and Maqsood, I. and He, L.},
Title = {{An inexact two-stage fuzzy-stochastic programming model for water
   resources management}},
Journal = {{WATER RESOURCES MANAGEMENT}},
