@article{ ISI:000263741100002,
Author = {Anderson, E. I.},
Title = {{Explicit solutions to two problems of steady groundwater flow with a
   threshold gradient}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{FEB 28}},
Abstract = {{We present explicit analytic solutions to two problems of steady,
   two-dimensional groundwater flow, where Darcy's law is modified to
   incorporate a threshold gradient. The presence of a drain or well in
   both examples generates a free boundary, the stagnation zone boundary,
   separating flowing groundwater from stagnant groundwater. We use complex
   analysis, the hodograph, and the Milne-Thomson circle theorem to
   evaluate the position of the free boundary directly, without resorting
   to conformal mapping.}},
