@article{ ISI:000273058200001,
Author = {Minear, J. Toby and Kondolf, G. Matt},
Title = {{Estimating reservoir sedimentation rates at large spatial and temporal
   scales: A case study of California}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{DEC 25}},
Abstract = {{Previous reservoir sedimentation models have ignored two key factors for
   large spatial and temporal modeling of multiple reservoirs: trapping by
   upstream dams and decreasing sediment trapping as reservoirs fill. We
   developed a spreadsheet-based model that incorporates both factors.
   Using California as a case study, we used measured sedimentation rates
   to estimate sediment yields for distinct geomorphic regions and applied
   those rates to unmeasured reservoirs by region. Statewide reservoirs
   have likely filled with 2.1 billion m(3) of sediment to date, decreasing
   total reservoir capacity by 4.5\%. About 200 reservoirs have likely lost
   more than half their initial capacity to sedimentation.}},
