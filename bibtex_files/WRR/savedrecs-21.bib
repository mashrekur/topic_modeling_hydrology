@article{ ISI:000264442200003,
Author = {Boschan, A. and Auradou, H. and Ippolito, I. and Chertcoff, R. and
   Hulin, J. P.},
Title = {{Experimental evidence of the anisotropy of tracer dispersion in rough
   fractures with sheared walls}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{MAR 18}},
Abstract = {{Dispersion experiments are compared for two model fractures with
   identical complementary rough walls but with a relative shear
   displacement delta parallel (delta parallel to U) or perpendicular
   (delta perpendicular to U) to the flow velocity U. The flowing fluid is
   a shear thinning polymer solution with a Newtonian behavior at low shear
   rates. For delta perpendicular to U, the mixing fronts display large
   structures well reproduced by assuming parallel channels of conductance
   deduced from the aperture field. This model also explains the
   amplification of the structures in the shear thinning regime and the
   distribution of the local transit times (t) over bar (x, y). For delta
   parallel to U, the front is much flatter. The local thickness of the
   front is characterized by a dispersivity alpha(x, y): its distribution
   is narrow enough to define an effective value alpha(Pe) only for delta
