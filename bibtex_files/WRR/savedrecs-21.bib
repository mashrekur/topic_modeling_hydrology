@article{ ISI:000269359500002,
Author = {Gianotti, Rebecca L. and Bomblies, Arne and Eltahir, Elfatih A. B.},
Title = {{Hydrologic modeling to screen potential environmental management methods
   for malaria vector control in Niger}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{AUG 28}},
Abstract = {{This paper describes the first use of Hydrology-Entomology and Malaria
   Transmission Simulator (HYDREMATS), a physically based distributed
   hydrology model, to investigate environmental management methods for
   malaria vector control in the Sahelian village of Banizoumbou, Niger.
   The investigation showed that leveling of topographic depressions where
   temporary breeding habitats form during the rainy season, by altering
   pool basin microtopography, could reduce the pool persistence time to
   less than the time needed for establishment of mosquito breeding,
   approximately 7 days. Undertaking soil surface plowing can also reduce
   pool persistence time by increasing the infiltration rate through an
   existing pool basin. Reduction of the pool persistence time to less than
   the rainfall interstorm period increases the frequency of pool drying
   events, removing habitat for subadult mosquitoes. Both management
   approaches could potentially be considered within a given context. This
   investigation demonstrates that management methods that modify the
   hydrologic environment have significant potential to contribute to
   malaria vector control in water-limited, Sahelian Africa.}},
