@article{ ISI:000263741100003,
Author = {Simpson, M. J.},
Title = {{Depth-averaging errors in reactive transport modeling}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{FEB 28}},
Abstract = {{Depth-averaged reactive transport models are commonly used to represent
   field-scale contamination in groundwater aquifers. These models rely on
   simplifying the governing equations through defining a depth-averaged
   variable and vertically integrating the full reactive transport
   equation. This approach is exact provided that the governing equation is
   linear; however, vertically averaging a nonlinear conservation equation
   can introduce an error into the simplified model. We present a framework
   for analyzing this error so that the suitability of a depth-averaged
   reactive transport model can be assessed quantitatively before the model
   is implemented. This framework shows how the error introduced by
   depth-averaging reactive transport equations is governed by the
   parameters in the kinetic model and the geometry of the reactive
   transport problem.}},
