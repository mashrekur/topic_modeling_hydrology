@article{ ISI:000265332700002,
Author = {Dekker, Louis W. and Ritsema, Coen J. and Oostindie, Klaas and Moore,
   Demie and Wesseling, Jan G.},
Title = {{Methods for determining soil water repellency on field-moist samples}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{APR 14}},
Abstract = {{In this paper we describe a simple and quick method for determining the
   presence of water repellency in a soil by using a small core sampler
   (1.5 cm in diameter, 25 cm long) and applying the water drop penetration
   time (WDPT) test at different depths on the sandy soil cores. Obtained
   results provide spatial distribution patterns of water repellency in a
   soil profile, demonstrating seasonal changes in repellency. An advantage
   of the method is that the soil is not disturbed by the sampling. For
   assessment of the persistence of water repellency in strongly to
   extremely water repellent soils, and for determination of the critical
   soil water contents, the WDPT test and volumetric water content
   determinations should preferably be performed in the laboratory.}},
