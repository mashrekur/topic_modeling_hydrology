@article{ ISI:000270060100001,
Author = {Shao, Quanxi and Wang, You-Gan},
Title = {{Statistical power calculation and sample size determination for
   environmental studies with data below detection limits}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{SEP 16}},
Abstract = {{Power calculation and sample size determination are critical in
   designing environmental monitoring programs. The traditional approach
   based on comparing the mean values may become statistically
   inappropriate and even invalid when substantial proportions of the
   response values are below the detection limits or censored because
   strong distributional assumptions have to be made on the censored
   observations when implementing the traditional procedures. In this
   paper, we propose a quantile methodology that is robust to outliers and
   can also handle data with a substantial proportion of
   below-detection-limit observations without the need of imputing the
   censored values. As a demonstration, we applied the methods to a
   nutrient monitoring project, which is a part of the Perth Long-Term
   Ocean Outlet Monitoring Program. In this example, the sample size
   required by our quantile methodology is, in fact, smaller than that by
   the traditional t-test, illustrating the merit of our method.}},
