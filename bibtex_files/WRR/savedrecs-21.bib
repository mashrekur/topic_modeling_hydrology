@article{ ISI:000271586200002,
Author = {Hoitink, A. J. F. and Buschman, F. A. and Vermeulen, B.},
Title = {{Continuous measurements of discharge from a horizontal acoustic Doppler
   current profiler in a tidal river}},
Journal = {{WATER RESOURCES RESEARCH}},
