@article{ ISI:000263615900001,
Author = {Lundquist, Jessica and Huggett, Brian and Roop, Heidi and Low, Natalie},
Title = {{Use of spatially distributed stream stage recorders to augment rain
   gages by identifying locations of thunderstorm precipitation and
   distinguishing rain from snow}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2009}},
Volume = {{45}},
Month = {{FEB 20}},
Abstract = {{Precipitation measurements in complex terrain are difficult to obtain,
   particularly during convective thunderstorm precipitation or mixed rain
   and snow. In such cases, stream stage recorders distributed across many
   small, headwater streams can bridge the gap between point measurements
   of precipitation and larger-scale total basin discharge. We use case
   studies from two steep basins with shallow soils in the Sierra Nevada,
   California to illustrate how distributed stream sensors can help
   identify the location, timing, and intensity of spatially variable
   precipitation.}},
