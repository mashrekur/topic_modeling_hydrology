@article{ ISI:A1997XG95800003,
Author = {Abdin, AE and Kaluarachchi, JJ},
Title = {{Stochastic analysis of three-phase flow in heterogeneous porous media
   .1. Spectra/perturbation approach}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1997}},
Volume = {{33}},
Number = {{7}},
Pages = {{1549-1558}},
Month = {{JUL}},
Abstract = {{Stochastic analysis of steady state three-phase (water, oil, and air)
   flow in heterogeneous porous media was performed using the perturbation
   theory and spectral representation technique. The governing equations
   describing flow of water, oil, and air are coupled and nonlinear. The
   key stochastic input Variables are intrinsic permeability, k, and the
   soil grain size distribution index, alpha. Three different stochastic
   combinations of these two input parameters were considered in the
   analysis. The spectral/perturbation approach was used to develop
   closed-form expressions to describe stochastic variability of key output
   processes, such as capillary and individual phase pressures. The second
   paper of this two-part series {[}Abdin and Kaluarachchi, this issue]
   will discuss the applicability and the validity of the
   spectral/perturbation technique to study the stochastic behavior of
   three-phase flow using numerical simulations.}},
