@article{ ISI:A1997WQ77800035,
Author = {Rockhold, ML and Simmons, CS and Fayer, MJ},
Title = {{An analytical solution technique for one-dimensional, steady vertical
   water flow in layered soils}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1997}},
Volume = {{33}},
Number = {{4}},
Pages = {{897-902}},
Month = {{APR}},
Abstract = {{An analytical solution technique was developed for one-dimensional,
   steady vertical water flow in variably saturated, layered soils with
   arbitrary hydraulic properties. The solution technique is based on the
   exact integral solution for the Gardner exponential hydraulic
   conductivity function. The exact solution is extended for use with
   arbitrary hydraulic property functions, or measured K(h) data, by
   approximating In K(h) with piecewise-linear curve segments and
   integrating analytically, segment by segment. The resulting analytical
   solution technique is accurate, computationally efficient, and
   applicable to unsaturated and/or saturated conditions. Several
   application examples are presented, including a comparison with earlier
   experimental results.}},
