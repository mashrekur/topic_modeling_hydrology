@article{ ISI:A1997XP36600017,
Author = {Oliveira, A and Baptista, AM},
Title = {{Diagnostic modeling of residence times in estuaries}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1997}},
Volume = {{33}},
Number = {{8}},
Pages = {{1935-1946}},
Month = {{AUG}},
Abstract = {{The variability of estuarine residence times is elucidated through
   accurate, diagnostic numerical tracking of a large number of particles,
   complemented with statistical analysis. Residence times are shown to
   have strong spatial and temporal variability, which is accentuated by
   exchanges between the estuary and the coastal ocean due to chaotic
   stirring at the mouth. The concept of a single residence time per
   estuary, while convenient from both ecological and engineering
   viewpoints, is therefore shown to be an oversimplification. Maps of
   residence times and cumulative histograms are examined as alternatives.
   The former are attractive only for once-through tracers, while the
   latter appear useful over a broader range of tracer characteristics.}},
