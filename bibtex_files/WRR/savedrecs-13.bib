@article{ ISI:000180527800044,
Author = {Negrel, P and Petelet-Giraud, E and Kloppmann, W and Casanova, J},
Title = {{Boron isotope signatures in the coastal groundwaters of French Guiana}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2002}},
Volume = {{38}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{{[}1] Drinking water used by the population of French Guiana comes
   mostly from rivers and can induce major health impacts. Consequently,
   groundwaters are being studied in terms of an alternative drinking water
   supply. In this context, boron isotopes were applied to the coastal
   aquifers to better constrain the water-rock interactions. We found that
   the delta(11)B signatures vary between -2.98 and 49.35parts per thousand
   in the groundwaters and between 36.35 and 42.41parts per thousand in the
   river waters and that the local seawater gave a value of 40.76parts per
   thousand. We also noted an inverse correlation between groundwater pH
   and delta(11)B: this relationship, previously demonstrated
   experimentally, is particularly well defined in the French Guiana
   coastal aquifer because of the wide pH range (4.18 to 7.64) naturally
   present in the groundwaters. It would appear that the variations in
   delta(11)B values are linked to water-rock interaction processes (e.g.,
   dissolution/precipitation of silicates).}},
