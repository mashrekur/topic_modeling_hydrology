@article{ ISI:000180521100002,
Author = {Weyhenmeyer, CE and Burns, SJ and Waber, HN and Macumber, PG},
Title = {{Isotope study of moisture sources, recharge areas, and groundwater flow
   paths within the eastern Batinah coastal plain, Sultanate of Oman}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2002}},
Volume = {{38}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{Oxygen and hydrogen isotope analyses of rainfall samples collected on
   the eastern Batinah coastal plain of northern Oman between 1995 and 1998
   indicate two different principal water vapor sources for precipitation
   in the area: a northern, Mediterranean source and a southern, Indian
   Ocean source. As a result, two new local meteoric water lines were
   defined for the study area. Isotopic analyses of groundwater samples
   from over 200 springs and wells indicate that the main source of water
   to the Batinah coastal alluvial aquifer is high-altitude rainfall from
   the adjacent Jabal Akhdar Mountains, originating from a combination of
   northern and southern moisture sources. The groundwater recharged at
   high-altitude forms two plumes of water which is depleted in the heavy
   isotopes (18)O and (2)H and stretches from the mountains across the
   coastal plain to the sea, thereby retaining a chemical homogeneity
   horizontally and vertically down to a depth exceeding 300 m. In
   contrast, in areas adjacent to these two plumes the alluvial aquifer is
   geochemically stratified. Near the coast, saline intrusion results in
   abrupt changes in chloride concentrations and isotope values.}},
