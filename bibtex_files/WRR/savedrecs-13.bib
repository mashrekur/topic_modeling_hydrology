@article{ ISI:000178935000013,
Author = {Campbell, DH and Kendall, C and Chang, CCY and Silva, SR and Tonnessen,
   KA},
Title = {{Pathways for nitrate release from an alpine watershed: Determination
   using delta N-15 and delta O-18}},
