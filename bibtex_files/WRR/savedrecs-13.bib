@article{ ISI:000180521100030,
Author = {Morin, E and Georgakakos, KP and Shamir, U and Garti, R and Enzel, Y},
Title = {{Objective, observations-based, automatic estimation of the catchment
   response timescale}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2002}},
Volume = {{38}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{{[}1] A new characteristic timescale of a catchment is presented, the
   response timescale (RTS). It is a range of averaging time intervals
   which, when applied to catchment rainfall, yield smoothed time series
   that best approximate that of the resultant runoff. In determining the
   RTS, nothing is assumed about the nature of the rainfall-runoff
   transformation. In addition, this new measure is shown to be robust
   against measurement errors. An objective, automatic, observations-based
   algorithm is described that introduces the concept of peaks density for
   the estimation of RTS. Estimation is exemplified for single and multiple
   rainfall-runoff events through application to small catchments in Panama
   and Israel. In all cases, relatively stable values of response timescale
   are obtained. It is concluded that at least for the case studies, the
   response timescale is an intrinsic characteristic of the catchment and
   it is generally expected to be different from the catchment lag time and
   time of concentration.}},
