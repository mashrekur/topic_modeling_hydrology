Language = {{English}},
Affiliation = {{Pan, LH (Reprint Author), Univ Calif Berkeley, Lawrence Berkeley Lab, Div Earth Sci, Mail Stop 90-1116,1 Cyclotron Rd, Berkeley, CA 94720 USA.
   Univ Calif Berkeley, Lawrence Berkeley Lab, Div Earth Sci, Berkeley, CA 94720 USA.}},
DOI = {{10.1029/2001WR000901}},
