@article{ ISI:000459536500046,
Author = {Zhang, Yi and Xue, Ziqiu and Park, Hyuck and Shi, Ji-Quan and Kiyama,
   Tamotsu and Lei, Xinglin and Sun, Yankun and Liang, Yunfeng},
Title = {{Tracking CO2 Plumes in Clay-Rich Rock by Distributed Fiber Optic Strain
   Sensing (DFOSS): A Laboratory Demonstration}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2019}},
Volume = {{55}},
Number = {{1}},
