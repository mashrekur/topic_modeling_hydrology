@article{ ISI:000461858900022,
Author = {Roberts, D. C. and Moreno-Casas, P. and Bombardelli, F. A. and Hook, S.
   J. and Hargreaves, B. R. and Schladow, S. G.},
Title = {{Predicting Wave-Induced Sediment Resuspension at the Perimeter of Lakes
   Using a Steady-State Spectral Wave Model}},
