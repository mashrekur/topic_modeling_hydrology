@article{ ISI:000459536500042,
Author = {Shin, Sanghoon and Pokhrel, Yadu and Miguez-Macho, Gonzalo},
Title = {{High-Resolution Modeling of Reservoir Release and Storage Dynamics at
   the Continental Scale}},
Journal = {{WATER RESOURCES RESEARCH}},
