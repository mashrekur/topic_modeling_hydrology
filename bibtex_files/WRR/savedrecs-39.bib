@article{ ISI:000459536500036,
Author = {Dempsey, David and Riffault, Jeremy},
Title = {{Response of Induced Seismicity to Injection Rate Reduction: Models of
   Delay, Decay, Quiescence, Recovery, and Oklahoma}},
