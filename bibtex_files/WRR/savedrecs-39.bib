@article{ ISI:000461858900018,
Author = {Puyguiraud, Alexandre and Gouze, Philippe and Dentz, Marco},
Title = {{Stochastic Dynamics of Lagrangian Pore-Scale Velocities in
   Three-Dimensional Porous Media}},
