@article{ ISI:000461858900015,
Author = {Hasan, Sharul and Joekar-Niasar, Vahid and Karadimitriou, Nikolaos K.
   and Sahimi, Muhammad},
Title = {{Saturation Dependence of Non-Fickian Transport in Porous Media}},
Journal = {{WATER RESOURCES RESEARCH}},
