@article{ ISI:000461858900029,
Author = {Visser, A. and Thaw, M. and Deinhart, A. and Bibby, R. and Safeeq, M.
   and Conklin, M. and Esser, B. and Van der Velde, Y.},
Title = {{Cosmogenic Isotopes Unravel the Hydrochronology and Water Storage
   Dynamics of the Southern Sierra Critical Zone}},
