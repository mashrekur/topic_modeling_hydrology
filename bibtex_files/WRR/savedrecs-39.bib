@article{ ISI:000459536500002,
Author = {Benettin, Paolo and Queloz, Pierre and Bensimon, Michael and McDonnell,
   Jeffrey J. and Rinaldo, Andrea},
Title = {{Velocities, Residence Times, Tracer Breakthroughs in a Vegetated
   Lysimeter: A Multitracer Experiment}},
Journal = {{WATER RESOURCES RESEARCH}},
