@article{ ISI:000461858900008,
Author = {Baroni, Gabriele and Schalge, Bernd and Rakovec, Oldrich and Kumar,
   Rohini and Schueler, Lennart and Samaniego, Luis and Simmer, Clemens and
   Attinger, Sabine},
Title = {{A Comprehensive Distributed Hydrological Modeling Intercomparison to
   Support Process Representation and Data Collection Strategies}},
Journal = {{WATER RESOURCES RESEARCH}},
