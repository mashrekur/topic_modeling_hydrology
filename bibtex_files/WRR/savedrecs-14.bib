@article{ ISI:000182110800002,
Author = {Pittaluga, MB and Repetto, R and Tubino, M},
Title = {{Channel bifurcation in braided rivers: Equilibrium configurations and
   stability}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2003}},
Volume = {{39}},
Number = {{3}},
Month = {{MAR 7}},
Abstract = {{{[}1] We investigate the equilibrium configurations and the stability of
   river bifurcations in gravel braided networks. Within the context of a
   one-dimensional approach, the nodal point conditions play a crucial
   rule, as pointed out by Wang et al. {[}1995] who propose an empirical
   relationship relating water and sediment flow rates into the downstream
   branches. In the present paper, an alternative formulation of nodal
   point conditions is proposed based on a quasi two-dimensional approach.
   The results show that, if the Shields parameter of the upstream channel
   is large enough, the system only admits of one solution with both
   branches open, which is invariably stable. As the Shields parameter of
   the upstream channel decreases, two further stable solutions appear
   characterized by a different partition of water discharge into the
   downstream branches: in this case, the previous solution becomes
   unstable. Theoretical findings are confirmed by the numerical solution
   of the nonlinear one-dimensional equations.}},
