@article{ ISI:000184597000001,
Author = {Castro, MC and Goblet, P},
Title = {{Calibration of regional groundwater flow models: Working toward a better
   understanding of site-specific systems}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2003}},
Volume = {{39}},
Number = {{6}},
Month = {{JUN 28}},
Abstract = {{{[}1] This work highlights the lack of unique solutions for regional
   groundwater flow models and quantifies the degree of freedom concerning
   hydraulic conductivities for models calibrated on measured hydraulic
   heads. The potential of He-4 as an independent tracer at reducing the
   nonuniqueness problem is tested. Four different calibrated groundwater
   flow scenarios are presented for the Carrizo aquifer and surrounding
   formations in Texas. It is shown that variations of hydraulic
   conductivities up to 2 orders of magnitude in the Carrizo aquifer and
   overlying confining layer lead to similar calculated hydraulic heads. No
   clear-cut arguments are present to invalidate one groundwater flow
   scenario over a different one. In contrast, when tested with a He-4
   transport conceptual model, all groundwater flow scenarios except one
   failed to reproduce a coherent He-4 transport behavior in the system.
   This study exemplifies possible future contributions of He-4 at
   discerning which model most closely replicates natural conditions.}},
