@article{ ISI:000183111100004,
Author = {Yeo, IW and Lee, KK},
Title = {{Analytical solution for arbitrarily located multiwells in an anisotropic
   homogeneous confined aquifer}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2003}},
Volume = {{39}},
Number = {{5}},
Month = {{MAY 17}},
Abstract = {{Analytical solutions have been suggested to delineate the capture zone
   created by multiwells for the remediation design of contaminated
   groundwater. These previous analytical solutions are often restricted to
   pumping wells only, specific well locations, a limited number of wells,
   and isotropic aquifer. An alternative analytical steady state solution
   was suggested to deal with arbitrarily located multiinjection-pumping
   wells in an anisotropic homogeneous aquifer. The solution can be an
   alternative solution for easily and conveniently calculating the
   hydraulic head and gradient in a complex flow field caused by
   multiinjection-pumping wells at different rates, and will consequently
   be useful for pump-and-treat design.}},
