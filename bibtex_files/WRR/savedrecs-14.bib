@article{ ISI:000186201900001,
Author = {White, I and Smiles, DE and Santomartino, S and van Oploo, P and
   Macdonald, BCT and Waite, TD},
Title = {{Dewatering and the hydraulic properties of soft, sulfidic, coastal clay
   soils}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2003}},
Volume = {{39}},
Number = {{10}},
Month = {{OCT 22}},
Abstract = {{{[}1] Dewatering and consolidation of saturated swelling soils are
   governed by pressure-dependent soil hydraulic properties. Existing
   measurement techniques are difficult and slow. We illustrate a simple,
   rapid desorption technique, developed for industrial slurries, to
   measure hydraulic properties of a gel-like sulfidic, estuarine soil
   (similar to 70\% water content). Measured hydraulic conductivities,
   K(psi), were very small, similar to 10(-10) m/s, giving a representative
   capillary fringe thickness of similar to 7 m and characteristic gravity
   drainage times around 40 years. Capillarity therefore dominates flow in
   these soils. Estimated times for dewatering this soil under surface
   loading with closely spaced, vertical wick drains, are 2 to 70 years,
   consistent with experience. A Netherlands marine clay soil, saturated
   with seawater, is unexpectedly wetter than the brackish estuarine soil
   here at the same matric potential, psi. However, K(psi) for both soils
   overlap, suggesting the engineering approximation, K(psi) proportional
   to \textbackslash{}psi\textbackslash{}(-1), for marine-deposited clays.
   The functional dependencies of hydraulic properties surprisingly are not
   consistent with similar-media or double-layer theories.}},
Publisher = {{AMER GEOPHYSICAL UNION}},
Address = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009 USA}},
Type = {{Article}},
