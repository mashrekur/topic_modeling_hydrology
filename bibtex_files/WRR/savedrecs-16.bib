
@article{ ISI:000234508500002,
Author = {Hanson, CL and Moffet, CA and Wight, JR and Mowbray, JE},
Title = {{Ekalaka, Montana small watershed data set: Precipitation, runoff, air
   temperature, and soil water, 1968-1980}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2005}},
Volume = {{41}},
Number = {{12}},
Month = {{DEC 29}},
Abstract = {{A 12-year (1968-1980) hydrologic database has been developed for three
   sets of watersheds on the frail land resource area located near Ekalaka
   in southeast Montana. These records contain daily maximum and minimum
   air temperature, precipitation, runoff, soil water measured monthly and
   more often during the growing season, and breakpoint precipitation and
   runoff for summer runoff events. These data can be accessed from the U.
   S. Department of Agriculture, Agricultural Research Service, Northwest
   Watershed Research Center, Boise, Idaho database through the anonymous
   ftp site ftp.nwrc.ars.usda.gov.}},
