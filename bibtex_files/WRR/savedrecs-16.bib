@article{ ISI:000230172300001,
Author = {Crago, R and Hervol, N and Crowley, R},
Title = {{A complementary evaporation approach to the scalar roughness length}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2005}},
Volume = {{41}},
Number = {{6}},
Month = {{JUN 25}},
Abstract = {{{[} 1] A method is developed to estimate the scalar roughness length for
   water vapor Zeta(0v) using the complementary evaporation concept. On the
   basis of the advection-aridity approach, estimates of Zeta(0v) and of
   kB(-1) ( kB(-1) = ln( Zeta(0)/Zeta(0v)), where Zeta(0) is the momentum
   roughness length) are available that do not require any estimate of
   surface temperature or humidity or of stomatal resistance. When applied
   to grassland data from Cooperative Atmosphere-Surface Exchange
   Study-1997, the resulting values of kB(-1) corresponded well with those
   found from surface temperatures measured with a radiometer having an
   oblique view of the surface. They also displayed variability with
   Zeta(0+) (Zeta(0+) = u{*} Zeta(0)/nu, where u{*} is the friction
   velocity and nu is the kinematic viscosity), which corresponded well
   with theoretical and wind tunnel test results from the literature. These
   results imply that useful estimates of kB(-1) are available with this
   method, and they seem to support the complementary evaporation concept.
   Strict screening of the data was required to select only those data
   yielding the smallest uncertainties in the resulting values of kB(-1).}},
