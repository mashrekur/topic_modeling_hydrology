@article{ ISI:000231730000001,
Author = {Wu, CM and Yeh, TCJ and Zhu, JF and Lee, TH and Hsu, NS and Chen, CH and
   Folch, A},
Title = {{Traditional analysis of Comparing apples to oranges?}},
