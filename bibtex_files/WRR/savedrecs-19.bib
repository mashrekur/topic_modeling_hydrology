@article{ ISI:000257803900002,
Author = {Selvadurai, A. P. S.},
Title = {{On nonclassical analytical solutions for advective transport problems}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2008}},
Volume = {{44}},
Month = {{JUL 19}},
Abstract = {{The paper presents certain nonclassical analytical solutions for
   describing the one-dimensional advective transport of a chemical or a
   contaminant in a fluid-saturated porous medium. The advective transport
   is induced by potential flow. The nonclassical nature stems from the
   influences of time-dependent boundary concentrations, time-dependent
   flow velocities, and the presence of natural attenuation. These
   analytical results are novel and provide a useful set of solutions for
   benchmarking computational schemes that have been developed in the
   literature for solving transport of chemicals and contaminants in
   fluid-saturated porous media.}},
