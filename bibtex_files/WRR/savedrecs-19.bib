@article{ ISI:000259609500002,
Author = {Golden, Heather E. and Boyer, Elizabeth W. and Brown, Michael G. and
   Elliott, Emily M. and Lee, Don Koo},
Title = {{Simple approaches for measuring dry atmospheric nitrogen deposition to
   watersheds}},
Journal = {{WATER RESOURCES RESEARCH}},
