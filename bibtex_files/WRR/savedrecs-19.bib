Address = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Molnar, P (Reprint Author), ETH, Inst Environm Engn, CH-8093 Zurich, Switzerland.
   Molnar, Peter; Burlando, Paolo, ETH, Inst Environm Engn, CH-8093 Zurich, Switzerland.}},
DOI = {{10.1029/2007WR006142}},
Article-Number = {{W10404}},
