@article{ ISI:000254170100001,
Author = {Kuo, Yuan-Ching and Huang, Liang-Hsiung and Tsai, Tung-Lin},
Title = {{A hybrid three-dimensional computational model of groundwater solute
   transport in heterogeneous media}},
