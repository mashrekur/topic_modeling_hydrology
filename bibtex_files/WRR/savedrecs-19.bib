@article{ ISI:000255080300005,
Author = {Bauer-Gottwein, Peter and Rasmussen, Nikolaj F. and Feificova, Dagmar
   and Trapp, Stefan},
Title = {{Phytotoxicity of salt and plant salt uptake: Modeling ecohydrological
   feedback mechanisms}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2008}},
Volume = {{44}},
Number = {{4}},
Month = {{APR 12}},
Abstract = {{A new model of phytotoxicity of salt and plant salt uptake is presented
   and is coupled to an existing three-dimensional groundwater simulation
   model. The implementation of phytotoxicity and salt uptake relationships
   is based on experimental findings from willow trees grown in hydroponic
   solution. The data confirm an s-shaped phytotoxicity relationship as
   found in previous studies. Uptake data were explained assuming steady
   state salt concentration in plant roots, passive salt transport into the
   roots, and active enzymatic removal of salt from plant roots. On the one
   hand, transpiration strongly depends on groundwater salinity
   (phytotoxicity); on the other hand, transpiration significantly changes
   the groundwater salinity (uptake). This feedback loop generates
   interesting dynamic phenomena in hydrological systems that are dominated
   by transpiration and are influenced by significant salinity gradients.
   Generic simulations are performed for the Okavango island system and are
   shown to reproduce essential phenomena observed in nature.}},
