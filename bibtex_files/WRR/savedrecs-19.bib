@article{ ISI:000260998300001,
Author = {Devillers, P. and El Youssoufi, M. S. and Saix, C.},
Title = {{A framework for the construction of state surfaces of unsaturated soils
   in the elastic domain}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2008}},
Volume = {{44}},
Month = {{NOV 13}},
Abstract = {{This paper deals with volume changes of an unsaturated soil in the
   elastic domain. Two constitutive relations are established in the
   general framework of unsaturated porous media mechanics. They express
   the variations of the void ratio e and the water content w with respect
   to the mean net stress (sigma - p(g){*}) and the suction s. Associated
   with validated experimental results, these relations give access to the
   construction of the state surface e = f((sigma - p(g){*}), s) in the
   elastic domain. This surface shows two domains: the first, near
   saturation, is based on the hypothesis of a Biot's coefficient b(w)
   equal to 1; the second, for high suctions, is based on an experimental
   value of bw. Next, a preliminary construction of the state surface G(s)w
   = g((sigma - p(g){*}), s) is proposed for a given soil. Although
   restricted to the elastic domain, these constructions give a new
   understanding of the suction role in important problems related to
   overconsolidation, shrinkage, or swelling of unsaturated soils.}},
