
@article{ ISI:000317829900004,
Author = {Gaur, Nandita and Mohanty, Binayak P.},
Title = {{Evolution of physical controls for soil moisture in humid and subhumid
   watersheds}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2013}},
Volume = {{49}},
