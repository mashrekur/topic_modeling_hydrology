@article{ ISI:000317828600035,
Author = {Stoliker, Deborah L. and Liu, Chongxuan and Kent, Douglas B. and
   Zachara, John M.},
Title = {{Characterizing particle-scale equilibrium adsorption and kinetics of
   uranium(VI) desorption from U-contaminated sediments}},
Journal = {{WATER RESOURCES RESEARCH}},
