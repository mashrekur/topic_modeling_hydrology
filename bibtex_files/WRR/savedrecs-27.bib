@article{ ISI:000317828600010,
Author = {Mondal, Pulin K. and Sleep, Brent E.},
Title = {{Virus and virus-sized microsphere transport in a dolomite rock fracture}},
Journal = {{WATER RESOURCES RESEARCH}},
