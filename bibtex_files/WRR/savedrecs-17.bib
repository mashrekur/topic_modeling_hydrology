@article{ ISI:000241124700001,
Author = {Ramillien, G. and Frappart, F. and Guentner, A. and Ngo-Duc, T. and
   Cazenave, A. and Laval, K.},
Title = {{Time variations of the regional evapotranspiration rate from Gravity
   Recovery and Climate Experiment (GRACE) satellite gravimetry}},
Journal = {{WATER RESOURCES RESEARCH}},
