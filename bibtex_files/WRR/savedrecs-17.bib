Address = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Brummer, CJ (Reprint Author), Univ Washington, Dept Earth \& Space Sci, Box 351310, Seattle, WA 98195 USA.
   Univ Washington, Dept Earth \& Space Sci, Seattle, WA 98195 USA.}},
DOI = {{10.1029/2005WR004776}},
