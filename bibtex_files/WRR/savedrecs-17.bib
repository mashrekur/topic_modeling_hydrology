@article{ ISI:000240104000001,
Author = {Bales, Roger C. and Molotch, Noah P. and Painter, Thomas H. and
   Dettinger, Michael D. and Rice, Robert and Dozier, Jeff},
Title = {{Mountain hydrology of the western United States}},
