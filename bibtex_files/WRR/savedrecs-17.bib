@article{ ISI:000238606100003,
Author = {Tayfur, G and Singh, VP},
Title = {{Kinematic wave model of bed profiles in alluvial channels}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2006}},
Volume = {{42}},
Number = {{6}},
Month = {{JUN 21}},
Abstract = {{{[} 1] A mathematical model, based on the kinematic wave (KW) theory, is
   developed for describing the evolution and movement of bed profiles in
   alluvial channels. The model employs a functional relation between
   sediment transport rate and concentration, a relation between flow
   velocity and depth and Velikanov's formula relating suspended sediment
   concentration to flow variables. Laboratory flume and field data are
   used to test the model. Transient bed profiles in alluvial channels are
   also simulated for several hypothetical cases involving different water
   flow and sediment concentration characteristics. The model-simulated bed
   profiles are found to be in good agreement with what is observed in the
   laboratory, and they seem theoretically reasonable for hypothetical
   cases. The model results reveal that the mean particle velocity and
   maximum concentration ( maximum bed form elevation) strongly affect
   transient bed profiles.}},
