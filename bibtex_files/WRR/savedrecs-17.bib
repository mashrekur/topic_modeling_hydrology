@article{ ISI:000242759000001,
Author = {Winsemius, H. C. and Savenije, H. H. G. and van de Giesen, N. C. and van
   den Hurk, B. J. J. M. and Zapreeva, E. A. and Klees, R.},
Title = {{Assessment of Gravity Recovery and Climate Experiment (GRACE) temporal
   signature over the upper Zambezi}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2006}},
Volume = {{42}},
Number = {{12}},
Month = {{DEC 6}},
Abstract = {{{[}1] The temporal signature of terrestrial storage changes inferred
   from the Gravity Recovery and Climate Experiment ( GRACE) has been
   assessed by comparison with outputs from a calibrated hydrological model
   ( lumped elementary watershed (LEW)) of the upper Zambezi and
   surroundings and an inspection of the within-month ground track coverage
   of GRACE together with spatial-temporal rainfall patterns. The
   comparison of the hydrological model with GRACE reveals temporal
   inconsistencies between both data sets. Because the LEW model has been
   calibrated and validated with independent data sources, we believe that
   this is a GRACE artifact. The within-month ground track coverage shows
   an irregular orbit behavior which may well cause aliasing in the GRACE
   monthly deconvolutions. This aliasing is the most probable cause of
   observed temporal inconsistencies between GRACE and other data sets.}},
