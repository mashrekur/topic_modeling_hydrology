@article{ ISI:000336026000019,
Author = {Li, Han and Deng, Xinwei and Kim, Dong-Yun and Smith, Eric P.},
Title = {{Modeling maximum daily temperature using a varying coefficient
   regression model}},
Journal = {{WATER RESOURCES RESEARCH}},
