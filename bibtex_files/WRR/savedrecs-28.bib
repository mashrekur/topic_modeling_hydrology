@article{ ISI:000340430400007,
Author = {Ng, Gene-Hua Crystal and Bedford, David R. and Miller, David M.},
Title = {{A mechanistic modeling and data assimilation framework for Mojave Desert
   ecohydrology}},
Journal = {{WATER RESOURCES RESEARCH}},
