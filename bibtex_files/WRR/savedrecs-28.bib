@article{ ISI:000342632000048,
Author = {Sawada, Yohei and Koike, Toshio and Jaranilla-Sanchez, Patricia Ann},
Title = {{Modeling hydrologic and ecologic responses using a new eco-hydrological
   model for identification of droughts}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
Volume = {{50}},
