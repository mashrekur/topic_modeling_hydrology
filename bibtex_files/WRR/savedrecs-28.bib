@article{ ISI:000344783800043,
Author = {Griffin, Michael P. and Callahan, Timothy J. and Vulava, Vijay M. and
   Williams, Thomas M.},
Title = {{Storm-event flow pathways in lower coastal plain forested watersheds of
   the southeastern United States}},
Journal = {{WATER RESOURCES RESEARCH}},
