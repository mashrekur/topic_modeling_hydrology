@article{ ISI:000346654600024,
Author = {Sukop, Michael C. and Cunningham, Kevin J.},
Title = {{Lattice Boltzmann methods applied to large-scale three-dimensional
   virtual cores constructed from digital optical borehole images of the
   karst carbonate Biscayne aquifer in southeastern Florida}},
Journal = {{WATER RESOURCES RESEARCH}},
