@article{ ISI:000342632000011,
Author = {Brocca, L. and Zucco, G. and Mittelbach, H. and Moramarco, T. and
   Seneviratne, S. I.},
Title = {{Absolute versus temporal anomaly and percent of saturation soil moisture
   spatial variability for six networks worldwide}},
Journal = {{WATER RESOURCES RESEARCH}},
