@article{ ISI:000337672900037,
Author = {Gasser, Guy and Pankratov, Irena and Elhanany, Sara and Glazman, Hillel
   and Lev, Ovadia},
Title = {{Calculation of wastewater effluent leakage to pristine water sources by
   the weighted average of multiple tracer approach}},
Journal = {{WATER RESOURCES RESEARCH}},
