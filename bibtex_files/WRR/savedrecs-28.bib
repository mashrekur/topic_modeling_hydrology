@article{ ISI:000346654600001,
Author = {Bathurst, James C. and Iroume, Andres},
Title = {{Quantitative generalizations for catchment sediment yield following
   forest logging}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
Volume = {{50}},
