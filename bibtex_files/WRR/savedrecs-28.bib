@article{ ISI:000340430400010,
Author = {Erfani, Tohid and Binions, Olga and Harou, Julien J.},
Title = {{Simulating water markets with transaction costs}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
