@article{ ISI:000340430400041,
Author = {Stockinger, Michael Paul and Bogena, Heye Reemt and Luecke, Andreas and
   Diekkrueger, Bernd and Weiler, Markus and Vereecken, Harry},
Title = {{Seasonal soil moisture patterns: Controlling transit time distributions
   in a forested headwater catchment}},
