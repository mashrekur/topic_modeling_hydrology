@article{ ISI:000337672900029,
Author = {Ricardo, Ana M. and Koll, Katinka and Franca, Mario J. and Schleiss,
   Anton J. and Ferreira, Rui M. L.},
Title = {{The terms of turbulent kinetic energy budget within random arrays of
   emergent cylinders}},
