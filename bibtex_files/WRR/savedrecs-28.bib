@article{ ISI:000337672900015,
Author = {Luckmann, Jonas and Grethe, Harald and McDonald, Scott and Orlov, Anton
   and Siddig, Khalid},
Title = {{An integrated economic model of multiple types and uses of water}},
Journal = {{WATER RESOURCES RESEARCH}},
