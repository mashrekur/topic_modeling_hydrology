@article{ ISI:000337672900011,
Author = {Jha, Birendra and Juanes, Ruben},
Title = {{Coupled multiphase flow and poromechanics: A computational model of pore
   pressure effects on fault slip and earthquake triggering}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
Volume = {{50}},
