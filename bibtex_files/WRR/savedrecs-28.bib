@article{ ISI:000337672900016,
Author = {Ameli, Ali A. and Craig, James R.},
Title = {{Semianalytical series solutions for three-dimensional
   groundwater-surface water interaction}},
Journal = {{WATER RESOURCES RESEARCH}},
