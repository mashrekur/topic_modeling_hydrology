@article{ ISI:000342632000002,
Author = {Biederman, J. A. and Harpold, A. A. and Gochis, D. J. and Ewers, B. E.
   and Reed, D. E. and Papuga, S. A. and Brooks, P. D.},
Title = {{Increased evaporation following widespread tree mortality limits
   streamflow response}},
Journal = {{WATER RESOURCES RESEARCH}},
