@article{ ISI:000333563900005,
Author = {Mariethoz, Gregoire and Comunian, Alessandro and Irarrazaval, Inigo and
   Renard, Philippe},
Title = {{Analog-based meandering channel simulation}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
Volume = {{50}},
