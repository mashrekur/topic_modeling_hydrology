@article{ ISI:000334111600012,
Author = {Zheng, Feifei and Westra, Seth and Leonard, Michael and Sisson, Scott A.},
Title = {{Modeling dependence between extreme rainfall and storm surge to estimate
   coastal flooding risk}},
