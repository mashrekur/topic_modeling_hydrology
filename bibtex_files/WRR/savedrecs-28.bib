@article{ ISI:000336026000050,
Author = {Engdahl, Nicholas B.},
Title = {{Equivalence of the time and Laplace domain solutions for the steady
   state concentration of radiometric tracers and the groundwater age
   equation}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
Volume = {{50}},
Number = {{4}},
Pages = {{3602-3607}},
Month = {{APR}},
Abstract = {{It has been previously shown that the normalized concentration of a
   radioactively decaying tracer defines one point on the Laplace
   transformed age distribution and that point corresponds to the decay
   rate of the tracer. However, it is unclear how two time domain
   quantities, the decay rate and normalized concentration, are related to
   the elements of a Laplace transformed distribution; this note clarifies
   these relationships mechanistically. It is shown that the Laplace
   transformed age distribution is equivalent to the late-time
   concentration of a radiometric tracer if transport and age are both at
   steady state. The result is general and may be applied to any age
   distribution, including non-Fickian distributions. The significance of
   the generality is that the steady state concentrations and known decay
   rates may be used to help constrain the parameters of an unknown age
   distribution in a wide range of flow systems.
   Key Points
   <list id={''}wrcr20873-list-0001{''} list-type={''}bulleted{''}>
   <list-item id={''}wrcr20873-li-0001{''}>Laplace transformed age
   distributions are related to radiometric concentrations <list-item
   id={''}wrcr20873-li-0002{''}>The relationship is only valid for steady
   state age and transport <list-item id={''}wrcr20873-li-0003{''}>The
   relationship is general and applies to any decay rate or age
   distribution}},
