@article{ ISI:000333563900006,
Author = {Shang, Jianying and Liu, Chongxuan and Wang, Zheming and Zachara, John},
Title = {{Long-term kinetics of uranyl desorption from sediments under advective
   conditions}},
Journal = {{WATER RESOURCES RESEARCH}},
