@article{ ISI:000337672900038,
Author = {Clubb, Fiona J. and Mudd, Simon M. and Milodowski, David T. and Hurst,
   Martin D. and Slater, Louise J.},
Title = {{Objective extraction of channel heads from high-resolution topographic
   data}},
Journal = {{WATER RESOURCES RESEARCH}},
