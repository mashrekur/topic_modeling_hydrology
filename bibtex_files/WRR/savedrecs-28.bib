@article{ ISI:000334111600025,
Author = {Wang, Ping and Pozdniakov, Sergey P.},
Title = {{A statistical approach to estimating evapotranspiration from diurnal
   groundwater level fluctuations}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2014}},
