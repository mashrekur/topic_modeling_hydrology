@article{ ISI:000277266300006,
Author = {Anderson, Jr., William P. and Emanuel, Ryan E.},
Title = {{Effect of interannual climate oscillations on rates of submarine
   groundwater discharge}},
Journal = {{WATER RESOURCES RESEARCH}},
