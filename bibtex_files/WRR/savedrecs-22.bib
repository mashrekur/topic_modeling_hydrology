@article{ ISI:000285016200001,
Author = {Hut, R. W. and Weijs, S. V. and Luxemburg, W. M. J.},
Title = {{Using the Wiimote as a sensor in water research}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2010}},
Volume = {{46}},
Month = {{DEC 1}},
Abstract = {{The \$40 ``Wiimote{''} (an input device belonging with the Nintendo(R)
   Wii(TM) game system) can be used by hydrologists as a sensor. The device
   contains three accelerometers and an infrared camera with built-in
   source tracking. It communicates by Bluetooth(R). Because of the efforts
   of the hacking community it is now easy to let the Wiimote communicate
   with a standard personal computer. Using a floating evaporation pan as
   an example, we show that the Wiimote, although it may have potential
   drawbacks when used in field campaigns, is a good addition to the
   hydrologist's bag of tools, especially for proof of concept testing.}},
