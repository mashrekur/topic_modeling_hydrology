@article{ ISI:000349889800012,
Author = {Irvine, Dylan J. and Cranswick, Roger H. and Simmons, Craig T. and
   Shanafield, Margaret A. and Lautz, Laura K.},
Title = {{The effect of streambed heterogeneity on groundwater-surface water
   exchange fluxes inferred from temperature time series}},
Journal = {{WATER RESOURCES RESEARCH}},
