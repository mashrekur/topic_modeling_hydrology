@article{ ISI:000072816600025,
Author = {Dearmont, D and McCarl, BA and Tolman, DA},
Title = {{Costs of water treatment due to diminished water quality: A case study
   in Texas}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1998}},
Volume = {{34}},
Number = {{4}},
Pages = {{849-853}},
Month = {{APR}},
Abstract = {{The cost of municipal water treatment due to diminished water quality
   represents an important component of the societal costs of water
   pollution. Here the chemical costs of municipal water treatment are
   expressed as a function of raw surface water quality. Data are used for
   a 3-year period for 12 water treatment plants in Texas. Results show
   that when regional raw water contamination is present, the chemical cost
   of water treatment is increased by \$95 per million gallons (per 3785
   m(3)) from a base of \$75. A 1\% increase in turbidity is shown to
   increase chemical costs by 0.25\%.}},
