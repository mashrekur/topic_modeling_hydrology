@article{ ISI:000072816600029,
Author = {Wallbrink, PJ and Murray, AS and Olley, JM and Olive, LJ},
Title = {{Determining sources and transit times of suspended sediment in the
   Murrumbidgee River, New South Wales, Australia, using fallout Cs-137 and
   Pb-210}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1998}},
Volume = {{34}},
Number = {{4}},
Pages = {{879-887}},
Month = {{APR}},
Abstract = {{Sediment budgets typically require an estimate of the proportional yield
   from erosion sources to sediments in transport and storage. This becomes
   increasingly difficult as catchments become larger, and erosion,
   storage, and deposition processes become more complex. We demonstrate
   how fallout radionuclides can be used to estimate the proportional
   contributions to sediment load, from a tripartite classification of
   erosion sources in a large catchment (the mid-Murrumbidgee, 13,500
   km(2)). The three major potential sources of sediment within this
   catchment are cultivated lands (similar to 22\% of the surface area),
   uncultivated pastoral lands (similar to 78\%), and the numerous channels
   and gullies found in this region. Concentrations of the fallout
   radionuclides Pb-210(ex) and (CS)-C-137 in representative samples from
   each of these three sources are significantly different. Employing these
   values in a simple mixing model shows that the largest contribution of
   material is currently derived from subsoil channel/gully sources.
   Alterations to the suspended sediment Pb-210(ex) signature by in situ
   labeling and decay are also considered. Applying the model with
   different concentrations of Pb-210(ex) (decayed as a function of
   residence time in channels) suggests that the mean residence time of
   fine-grained material within this system is 10 +/- 5 years. However,
   differences in Cs-137 concentrations observed between flood and low-flow
   sediments, and the presence of the short-lived, cosmogenic Be-7, suggest
   that residence time of some of this fine-grained material may be of the
   order of only weeks to months.}},
