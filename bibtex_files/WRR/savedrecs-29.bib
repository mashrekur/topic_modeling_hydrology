@article{ ISI:000333563900058,
Author = {Farhadi, Leila and Entekhabi, Dara and Salvucci, Guido and Sun, Jian},
Title = {{Estimation of land surface water and energy balance parameters using
   conditional sampling of surface states}},
Journal = {{WATER RESOURCES RESEARCH}},
