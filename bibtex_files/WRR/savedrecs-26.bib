@article{ ISI:000328683800020,
Author = {Walter, B. and Horender, S. and Gromke, C. and Lehning, M.},
Title = {{Measurements of the pore-scale water flow through snow using Fluorescent
   Particle Tracking Velocimetry}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2013}},
