@article{ ISI:000322241300011,
Author = {Li, Chao and Singh, Vijay P. and Mishra, Ashok K.},
Title = {{Monthly river flow simulation with a joint conditional density
   estimation network}},
Journal = {{WATER RESOURCES RESEARCH}},
