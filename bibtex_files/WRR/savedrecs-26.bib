@article{ ISI:000325991100020,
Author = {Sun, Siao and Bertrand-Krajewski, Jean-Luc},
Title = {{Separately accounting for uncertainties in rainfall and runoff:
   Calibration of event-based conceptual hydrological models in small urban
   catchments using Bayesian method}},
