@article{ ISI:000323419100007,
Author = {Binley, Andrew and Ullah, Sami and Heathwaite, A. Louise and Heppell,
   Catherine and Byrne, Patrick and Lansdown, Katrina and Trimmer, Mark and
   Zhang, Hao},
Title = {{Revealing the spatial variability of water fluxes at the
   groundwater-surface water interface}},
Journal = {{WATER RESOURCES RESEARCH}},
