@article{ ISI:000329929100017,
Author = {Raleigh, Mark S. and Landry, Christopher C. and Hayashi, Masaki and
   Quinton, William L. and Lundquist, Jessica D.},
Title = {{Approximating snow surface temperature from standard temperature and
   humidity data: New possibilities for snow model and remote sensing
   evaluation}},
