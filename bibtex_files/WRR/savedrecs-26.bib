@article{ ISI:000328683800022,
Author = {Jouzel, Jean and Delaygue, Gilles and Landais, Amaelle and
   Masson-Delmotte, Valerie and Risi, Camille and Vimeux, Francoise},
Title = {{Water isotopes as tools to document oceanic sources of precipitation}},
