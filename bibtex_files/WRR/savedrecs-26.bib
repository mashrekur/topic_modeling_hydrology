@article{ ISI:000329929100053,
Author = {Maghrebi, Mahdi and Jankovic, Igor and Fiori, Aldo and Dagan, Gedeon},
Title = {{Effective retardation factor for transport of reactive solutes in highly
   heterogeneous porous formations}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2013}},
Volume = {{49}},
Number = {{12}},
Pages = {{8600-8604}},
Month = {{DEC}},
Abstract = {{The effective retardation factor of highly heterogeneous porous
   formations is investigated using an analytical derivation and numerical
   simulations based on the self-consistent approximation and the
   multi-indicator conductivity model, used in the past for the effective
   hydraulic conductivity estimation. Both positive and negative
   correlation between the logarithm of hydraulic conductivity K and the
   logarithm of sorption distribution coefficient K-d are investigated,
   including the effects of random noise. The arithmetic average of the
   random retardation factor is found to represent accurately the effective
   retardation factor of isotropic and anisotropic formations regardless of
   correlation between ln K-d and ln K, and for variance of ln K as large
   as 8.}},
