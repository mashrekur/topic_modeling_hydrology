@article{ ISI:000319282100002,
Author = {Mueller, Julie M. and Swaffar, Wes and Nielsen, Erik A. and Springer,
   Abraham E. and Lopez, Sharon Masek},
Title = {{Estimating the value of watershed services following forest restoration}},
