@article{ ISI:000327432500054,
Author = {Arora, Bhavna and Mohanty, Binayak P. and McGuire, Jennifer T. and
   Cozzarelli, Isabelle M.},
Title = {{Temporal dynamics of biogeochemical processes at the Norman Landfill
   site}},
