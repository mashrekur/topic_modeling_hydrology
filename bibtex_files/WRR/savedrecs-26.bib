@article{ ISI:000325991100015,
Author = {Cvetkovic, Vladimir},
Title = {{How accurate is predictive modeling of groundwater transport? A case
   study of advection, macrodispersion, and diffusive mass transfer at the
   Forsmark site (Sweden)}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2013}},
Volume = {{49}},
Number = {{9}},
Pages = {{5317-5327}},
Month = {{SEP}},
Abstract = {{We study predictive modeling of groundwater transport that accounts for
   three mechanisms: mean advection, macrodispersion, and mass transfer. A
   general methodology is presented and applied to a series of nonsorbing
   tracer tests along multiple pathways on scales ranging from ca. 70 to
   300 m, in a highly heterogeneous aquifer at Forsmark (Sweden). The mean
   water residence time cannot be predicted well using a simple water
   balance model. Longitudinal macrodispersivity (L) (L) and a mass
   transfer parameter group (1/root T) are extrapolated from the control
   tracer experiments, to yield accurate predictions of tracer discharge,
   once the mean water residence time is constrained. A relatively simple
   modeling framework based on Fickian macrodispersion and diffusion seems
   to be adequate for reproducing the tracer discharge in this complex and
   highly heterogeneous porous media.}},
