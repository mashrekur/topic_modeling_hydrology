@article{ ISI:000327432500035,
Author = {Seed, Alan W. and Pierce, Clive E. and Norman, Katie},
Title = {{Formulation and evaluation of a scale decomposition-based stochastic
   precipitation nowcast scheme}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2013}},
