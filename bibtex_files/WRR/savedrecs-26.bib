@article{ ISI:000329929100037,
Author = {Tang, Youneng and Valocchi, Albert J. and Werth, Charles J. and Liu,
   Haihu},
Title = {{An improved pore-scale biofilm model and comparison with a microfluidic
   flow cell experiment}},
Journal = {{WATER RESOURCES RESEARCH}},
