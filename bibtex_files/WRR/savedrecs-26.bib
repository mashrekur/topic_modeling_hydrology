@article{ ISI:000325991100057,
Author = {Nabi, M. and de Vriend, H. J. and Mosselman, E. and Sloff, C. J. and
   Shimizu, Y.},
Title = {{Detailed simulation of morphodynamics: 3. Ripples and dunes}},
Journal = {{WATER RESOURCES RESEARCH}},
