@article{ ISI:000323419100045,
Author = {O'Malley, Daniel and Cushman, John H.},
Title = {{Ubiquity of, and geostatistics for, nonstationary increment random
   fields}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2013}},
Volume = {{49}},
Number = {{7}},
Pages = {{4525-4529}},
Month = {{JUL}},
Abstract = {{Nonstationary random fields such as fractional Brownian motion and
   fractional Levy motion have been studied extensively in the hydrology
   literature. On the other hand, random fields that have nonstationary
   increments have seen little study. A mathematical argument is presented
   that demonstrates processes with stationary increments are the exception
   and processes with nonstationary increments are far more abundant. The
   abundance of nonstationary increment processes has important
   implications, e.g., in kriging where a translation-invariant variogram
   implicitly assumes stationarity of the increments. An approach to
   kriging for processes with nonstationary increments is presented and
   accompanied by some numerical results.}},
