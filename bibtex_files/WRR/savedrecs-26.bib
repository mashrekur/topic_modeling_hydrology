@article{ ISI:000327432500058,
Author = {Pollacco, Joseph A. P. and Mohanty, Binayak P. and Efstratiadis, Andreas},
Title = {{Weighted objective function selector algorithm for parameter estimation
   of SVAT models with remote sensing data}},
