@article{ ISI:000319282100009,
Author = {Dlubac, Katherine and Knight, Rosemary and Song, Yi-Qiao and Bachman,
   Nate and Grau, Ben and Cannia, Jim and Williams, John},
Title = {{Use of NMR logging to obtain estimates of hydraulic conductivity in the
   High Plains aquifer, Nebraska, USA}},
Journal = {{WATER RESOURCES RESEARCH}},
