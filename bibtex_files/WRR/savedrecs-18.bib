@article{ ISI:000250452600003,
Author = {Ghezzehei, Teamrat A. and Kneafsey, Timothy J. and Su, Grace W.},
Title = {{Correspondence of the Gardner and van Genuchten-Mualem relative
   permeability function parameters}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2007}},
Volume = {{43}},
Number = {{10}},
Month = {{OCT 17}},
Abstract = {{The Gardner and van Genuchten-Mualem models of relative permeability are
   widely used in analytical and numerical solutions to flow problems,
   respectively. Comparison of analytical and numerical solutions therefore
   requires defining some correspondence between the Gardner and van
   Genuchten-Mualem models. In this paper, we introduce generalized
   conversion formulae that reconcile these two models in the midrange of
   saturation. In general, we find that the Gardner parameter a G is
   related to the van Genuchten parameters alpha(vG) and n as alpha(G)
   approximate to 1.3 alpha(vG) n. The performance of the proposed
   conversion formulae is poor when n is much smaller than 2.}},
