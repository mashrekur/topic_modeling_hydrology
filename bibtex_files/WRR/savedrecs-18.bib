@article{ ISI:000246500500004,
Author = {Emmerton, Craig A. and Lesack, Lance F. W. and Marsh, Philip},
Title = {{Lake abundance, potential water storage, and habitat distribution in the
   Mackenzie River Delta, western Canadian Arctic}},
