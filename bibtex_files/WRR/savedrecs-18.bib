
@article{ ISI:000252017300004,
Author = {Jimenez-Hornero, F. J. and Giraldez, J. V. and Laguna, A. M.},
Title = {{Multifractal analysis of flow velocity simulated with the lattice model
   approach in idealized three-dimensional porous media}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2007}},
Volume = {{43}},
Number = {{12}},
Month = {{DEC 29}},
Abstract = {{{[}1] The strange attractor formalism has been used to check whether the
   flow velocity in idealized three-dimensional porous media could be
   described by the multifractal approach. The flow velocity magnitude was
   simulated with a lattice model in six media with different porosities.
   The results obtained from the analysis of the mass exponent functions
   and the multifractal and generalized dimensions spectra show that the
   velocity of the simulated flows has a multifractal type of scaling that
   becomes stronger as the porosity decreases. A correlation analysis has
   been performed showing the relationships between the statistical
   properties and the multifractal parameters of the velocity magnitude
   distribution.}},
