@article{ ISI:000250712000002,
Author = {Visser, Ate and Broers, Hans Peter and Bierkens, Marc F. P.},
Title = {{Dating degassed groundwater with H-3/He-3}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2007}},
Volume = {{43}},
Number = {{10}},
Month = {{OCT 30}},
Abstract = {{The production of gases in groundwater under contaminated locations by
   geochemical and biological processes is not uncommon. Degassing of these
   gases from groundwater and repartitioning of noble gases between water
   and gas phase distorts groundwater dating by H-3/He-3. We observed noble
   gas concentrations below atmospheric equilibrium in 20 out of 34
   groundwater samples from agriculturally polluted sandy areas in the
   Netherlands. From the absence of nitrate in degassed samples, we
   conclude that denitrification causes degassing. The Ne-22/Ne-20 ratios
   show that degassing had attained solubility equilibrium and had not
   caused isotopic fractionation by diffusion. To correct for the loss of
   tritiogenic He-3 due to degassing, we present a single-step equilibrium
   degassing model. We use the total dissolved gas pressure at the
   monitoring screen to estimate the depth and timing of degassing, which
   is essential to estimate travel times from degassed samples. By
   propagating the uncertainties in the underlying measurements and
   assumptions through the travel time calculations, we found a travel time
   uncertainty of 3 years (a). We therefore conclude that H-3/He-3 dating
   can produce valuable information on groundwater flow even at sites with
   strong degassing.}},
