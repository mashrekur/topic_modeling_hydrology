@article{ ISI:000249373900003,
Author = {Bosch, D. D. and Sheridan, J. M.},
Title = {{Stream discharge database, little river experimental watershed, georgia,
   united states}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2007}},
Volume = {{43}},
Number = {{9}},
Month = {{SEP 5}},
Abstract = {{{[}1] The U. S. Department of Agriculture Agricultural Research Service
   Southeast Watershed Research Laboratory (SEWRL) initiated a hydrologic
   research program on the Little River Experimental Watershed (LREW) in
   1967. Long-term (up to 37 years) research quality streamflow data are
