@article{ ISI:000246841800002,
Author = {Yue, Wusi and Meneveau, Charles and Parlange, Marc B. and Zhu, Weihong
   and van Hout, Rene and Katz, Joseph},
Title = {{A comparative quadrant analysis of turbulence in a plant canopy}},
