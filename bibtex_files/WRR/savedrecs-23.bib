@article{ ISI:000287819000001,
Author = {Jaeger, Kristin L. and Wohl, Ellen},
Title = {{Channel response in a semiarid stream to removal of tamarisk and Russian
   olive}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2011}},
Volume = {{47}},
Month = {{FEB 22}},
Abstract = {{We report observed short-term (3 years) channel adjustment in an
   incised, semiarid stream to the removal of invasive plants, tamarisk
   (Tamarix spp.) and Russian olive (Elaeagnus angustifolia) by (1)
   removing the above-ground portion of the plant (cut-stump method) and
   (2) removing the entire plant (whole-plant method). The stream flows
   through Canyon de Chelly National Monument in Arizona, USA., draining an
   similar to 1500 km(2) catchment. Average channel width is 13 m; average
   thalweg depth is 2-3 m, although channel banks exceed 8 m locally.
   Channels adjusted primarily through widening, with significantly larger
   changes occurring in whole-plant removal reaches; however, neither plant
   removal method elicited large-scale bank destabilization, and the
   channels remained entrenched. Particular site conditions limiting
   large-scale destabilization include the absence of sufficient streamflow
   magnitudes, the presence of clay layers at the bank toe, the remaining
   presence of native vegetation, and the entrenched morphology. Our
   findings serve as a cautionary note regarding the temporal and spatial
   variability in channel response to invasive plant removal and underscore
   the importance of considering site-specific conditions in future
   restoration projects that include invasive plant removal.}},
