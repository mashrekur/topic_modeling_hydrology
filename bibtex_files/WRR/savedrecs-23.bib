@article{ ISI:000295979800001,
Author = {Berg, Steven J. and Illman, Walter A.},
Title = {{Three-dimensional transient hydraulic tomography in a highly
   heterogeneous glaciofluvial aquifer-aquitard system}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2011}},
Volume = {{47}},
Month = {{OCT 11}},
Abstract = {{Hydraulic tomography has been proposed as an alternative site
   characterization method, however, relatively few field scale studies
   have been attempted. In this paper, we characterize the highly
   heterogeneous glaciofluvial aquifer-aquitard system at the North Campus
   Research Site, located at the University of Waterloo, Waterloo, Ontario,
   Canada using transient hydraulic tomography (THT). In particular, we
   performed 9 pumping tests in a network of wells to image the hydraulic
   conductivity (K) and specific storage (S(s)) distributions (or
   tomograms) as well as their uncertainties in three-dimensions using the
   THT code of J. Zhu and T.-C. J. Yeh (2005). We first performed
   stochastic inverse modeling of the 9 pumping tests individually to gain
   insight into the level of detail that can be imaged. Then, we
   sequentially included 4 of the pumping tests in a THT analysis. The
