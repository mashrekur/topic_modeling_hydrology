@article{ ISI:000297419500001,
Author = {Cirpka, Olaf A. and de Barros, Felipe P. J. and Chiogna, Gabriele and
   Nowak, Wolfgang},
Title = {{Probability density function of steady state concentration in
   two-dimensional heterogeneous porous media}},
Journal = {{WATER RESOURCES RESEARCH}},
