@article{ ISI:000373117300047,
Author = {Bendoni, M. and Mel, R. and Solari, L. and Lanzoni, S. and Francalanci,
   S. and Oumeraci, H.},
Title = {{Insights into lateral marsh retreat mechanism through localized field
   measurements}},
Journal = {{WATER RESOURCES RESEARCH}},
