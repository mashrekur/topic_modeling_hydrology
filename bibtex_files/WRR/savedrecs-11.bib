@article{ ISI:000089184100002,
Author = {Fuller, ME and Dong, HL and Mailloux, BJ and Onstott, TC and DeFlaun, MF},
Title = {{Examining bacterial transport in intact cores from Oyster, Virginia:
   Effect of sedimentary facies type on bacterial breakthrough and
   retention}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2000}},
Volume = {{36}},
Number = {{9}},
Pages = {{2417-2431}},
Month = {{SEP}},
Abstract = {{The effects of the physical/chemical properties of sediment on the
   transport of bacteria were examined. An adhesion-deficient, groundwater
   isolate was injected into intact cores of three different sedimentary
   facies. Total effluent recovery of bacteria varied with facies type and
   appeared to be most influenced by mean grain size and total metal
   hydroxide content. Bacterial concentrations within the core were highest
   near the point of injection, but cell distribution exhibited
   considerable lateral and longitudinal variation. Calculations of the
   collision efficiency (a) indicated that the bacterial inoculum injected
   into the cores was composed of a range of subpopulations of cells, each
   possessing different ct values. Although predictions based on previously
   published research were generally correct, this research highlights the
   need for further studies of bacterial transport under environmentally
   relevant conditions and the necessity of examining how the physiological
   heterogeneity of the injected organisms affects their transport in
   subsurface sediments.}},
