@article{ ISI:000090146100003,
Author = {Saiers, JE and Guha, H and Jardine, P and Brooks, S},
Title = {{Development and evaluation of a mathematical model for the transport and
   oxidation-reduction of CoEDTA}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2000}},
Volume = {{36}},
Number = {{11}},
Pages = {{3151-3165}},
Month = {{NOV}},
Abstract = {{Oxidation-reduction reactions influence the subsurface mobility of a
   wide variety of toxic contaminants. In the work reported here, we
   develop a new model for coupled transport and oxidation-reduction, and
   we test the model against published data on the movement of CoEDTA
   (cobalt ethylenediaminetetraacetic acid) through columns packed with
   beta -MnO2-coated sand. The model solves equations for the
   advective-dispersive transport of three aqueous species: Co(II)EDTA,
   Co(III)EDTA, and dissolved oxygen. These transport equations are linked
   with nonlinear kinetics expressions that describe (1) oxidation of
   Co(II)EDTA to Co(III)EDTA by beta -MnO2, (2) inhibition of Co(II)EDTA
   oxidation due to precipitation of Mn2O3, an insoluble reaction product
   that occludes the beta -MnO2 surface, and (3) regeneration of the
   redox-reactive beta -MnO2 surface through oxidation of the Mn2O3
   precipitate by dissolved oxygen. Comparison of experimental and
   calculated results demonstrates that the model describes the response of
   the coupled hydrological and geochemical processes to changes in flow
   rate, influent concentrations of Co(II)EDTA, and beta -MnO2 surface
   coverages.}},
