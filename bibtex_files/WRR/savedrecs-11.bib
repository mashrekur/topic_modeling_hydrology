@article{ ISI:000087928800029,
Author = {Hogarth, WL and Parlange, JY},
Title = {{Application and improvement of a recent approximate analytical solution
   of Richards' equation}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2000}},
Volume = {{36}},
Number = {{7}},
Pages = {{1965-1968}},
Month = {{JUL}},
Abstract = {{A method for developing wetting front profiles for the one-dimensional
   Richards' equation is given. The method is obtained by simplifying and
   extending a recent approximate solution and brings together features
   from several different studies. Difficulties associated with profile
   development are discussed by applying the method to several examples,
   providing a deeper understanding of the solutions of Richards' equation.
   For simplicity, we illustrate the improvements for soil-water
   diffusivities and conductivities which have a power law dependence on
   the water content although they should apply in general, and the
   appropriate generalization is given in the conclusion.}},
