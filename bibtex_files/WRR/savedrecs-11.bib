@article{ ISI:000086814100008,
Author = {Montanari, A and Rosso, R and Taqqu, MS},
Title = {{A seasonal fractional ARIMA model applied to the Nile River monthly
   flows at Aswan}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2000}},
Volume = {{36}},
Number = {{5}},
Pages = {{1249-1259}},
Month = {{MAY}},
Abstract = {{A seasonal fractional autoregressive integrated moving average (ARIMA)
   model is introduced, with both short- and long-term persistent periodic
   components. The estimation of the parameters is carried out by applying
   the Whittle's approximation to the Gaussian maximum likelihood function,
   which yields asymptotically consistent estimates. This method is
   particularly useful for hydrological time series. It is applied here to
   the Nile River monthly flows at Aswan in order to detect whether long
   memory is present. The results are compared with ones obtained by
   applying heuristic procedures, some of which were developed recently, in
   order to see how these perform on seasonal data.}},
