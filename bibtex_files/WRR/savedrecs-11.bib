@article{ ISI:000089184100031,
Author = {Zhang, XX and Ewen, J},
Title = {{Efficient method for simulating gravity-dominated water flow in
   unsaturated soils}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2000}},
Volume = {{36}},
Number = {{9}},
Pages = {{2777-2780}},
Month = {{SEP}},
Abstract = {{Numerical convergence difficulties associated with nonlinearity in the
   hydraulic property functions are often encountered when simulating
   gravity-dominated water flow in unsaturated soils using Richards'
   equation. A new approach for approximating the gravity term in Richard's
   equation is developed. Tests on a wide variety of soils show it is
   superior to existing Picard and Newton approaches. It is more reliable
   and efficient and allows larger time steps to be used.}},
