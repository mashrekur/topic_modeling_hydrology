@article{ ISI:000166582800007,
Author = {Chu, Y and Jin, Y and Flury, M and Yates, MV},
Title = {{Mechanisms of virus removal during transport in unsaturated porous media}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2001}},
Volume = {{37}},
Number = {{2}},
Pages = {{253-263}},
Month = {{FEB}},
Abstract = {{Previous studies suggest that sorption of colloidal particles onto the
   air-water interface is an important mechanism for enhanced retention and
   retardation during transport in unsaturated systems. In this study,
   bacteriophages phi X174 and MS-2 and Br tracer were introduced into sand
   columns of various water contents as a step function under constant flow
   rates. The results showed that when a ``reactive{''} (water washed) sand
   was used, the retention of both phi X174 and MS-2 increased
   significantly at low water saturation levels. However, when an
   ``inert{''} (metals and metal oxides removed) sand was used, the effect
   of water content was minimal, although observable. These results suggest
   that in the presence of reactive solid surfaces, increased reactions at
   the solid-water interface rather than at the air-water interface
   dominates in virus removal and transport under unsaturated conditions. A
   model that incorporated reactions at both the solid-water and air-water
   interfaces was developed and successfully applied to the data.}},
