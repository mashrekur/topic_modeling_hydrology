@article{ ISI:000167801500021,
Author = {Chan, CY and Knight, RJ},
Title = {{Laboratory measurements of electromagnetic wave velocity in layered
   sands}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2001}},
Volume = {{37}},
Number = {{4}},
Pages = {{1099-1105}},
Month = {{APR}},
Abstract = {{The velocity of an electromagnetic (EM) wave in a system of layered
   geologic materials is determined by the velocities and volume fractions
   of the individual layers and by the ratio between the wavelength of the
   EM wave (lambda) and the average layer thickness (t). For the case pi <
   t the EM wave velocity is given by ray theory; for the case <lambda> > t
   the velocity is given by effective medium theory (EMT), Previous
   numerical studies have shown the transition from ray theory to effective
   medium theory to occur at lambda /t approximately equal to 4. Using
   laboratory time domain reflectometry (TDR), we measured the EM
   velocities of six layered samples composed of 50\% dry coarse sand and
   50\% saturated fine sand. The hit for these measurements range from
