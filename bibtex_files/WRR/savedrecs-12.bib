@article{ ISI:000168266400033,
Author = {Genereux, D and Guardiario, J},
Title = {{A borehole flowmeter investigation of small-scale hydraulic conductivity
   variation in the Biscayne Aquifer, Florida}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2001}},
Volume = {{37}},
Number = {{5}},
Pages = {{1511-1517}},
Month = {{MAY}},
Abstract = {{Geostatistical analysis of closely spaced borehole flowmeter
   measurements was used to estimate the variance (2.53) and vertical and
   horizontal correlation lengths of In K (0.57 and 7.3 m) in the Biscayne
   Aquifer, a limestone aquifer critical for Florida's water supply and for
   Everglades restoration efforts. The variance and correlation lengths of
   the Biscayne Aquifer art: similar to same of the values for
   unconsolidated siliciclastic sediments (especially these at Columbus,
   Mississippi). The larger lambda (h) for the Biscayne Aquifer (7.3 m) is
   thought to be due at least in part to the lower lateral variability of
   the carbonate platform depositional environment, compared to the fluvial
   environments in which the siliciclastic sediments were deposited. An
   improved down hole packer would allow for data with finer vertical
   resolution; the current system is adequate for work inside well screens
   but cannot adequately seal many spots in open, irregular rock boreholes.
   Research in rock presents additional logistical difficulties but is
   important for addressing fundamental questions about solute transport in
   a wider range of geological media, beyond unconsolidated siliciclastic
   deposits.}},
