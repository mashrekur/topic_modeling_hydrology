@article{ ISI:000167801500020,
Author = {Li, L and Barry, DA and Jeng, DS},
Title = {{Tidal fluctuations in a leaky confined aquifer: Dynamic effects of an
   overlying phreatic aquifer}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2001}},
Volume = {{37}},
Number = {{4}},
Pages = {{1095-1098}},
Month = {{APR}},
Abstract = {{Tidal fluctuations in a leaky confined coastal aquifer are damped
   significantly due to leakage into an overlying phreatic aquifer. Jiao
   and Tang {[}1999] presented an analytical solution to a simple model
   describing this phenomenon. Their solution assumes that the tidal
   fluctuations in the overlying phreatic aquifer are negligible (i.e,, a
   static phreatic aquifer), Here we examine dynamic effects of the
   overlying aquifer based on a new approximate analytical solution. The
   numerical results indicate that the dynamic effects can be significant
   for a relatively large leakage and a high transmissivity of the phreatic
   aquifer.}},
