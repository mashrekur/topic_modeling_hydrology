@article{ ISI:000173283000042,
Author = {Singer, MB and Dunne, T},
Title = {{Identifying eroding and depositional reaches of valley by analysis of
   suspended sediment transport in the Sacramento River, California}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2001}},
Volume = {{37}},
Number = {{12}},
Pages = {{3371-3381}},
Month = {{DEC}},
Abstract = {{Spatial patterns in suspended sediment transport and storage along the
   Sacramento River were assessed by evaluating the suspended sediment
   budget for the main channel accounting for all tributaries and
   diversions. Time series analysis was employed to quantify the
   relationship between streamflow and suspended sediment concentration for
   gauging stations along the main channel and signature tributaries.
   Sediment concentration records (of 2-yr duration) were extended using
   Box-Jenkins transfer function models to calculate annual rates of
   suspended sediment discharge over a 32-year period since dam
   construction on the Sacramento River. The suspended sediment budget was
   evaluated to identify reaches of net erosion or deposition. The results
   of the budget suggest the influence of tectonics and anthropogenic
   channel modification.}},
