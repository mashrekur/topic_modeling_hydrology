@article{ ISI:000173283000010,
Author = {Dupin, HJ and Kitanidis, PK and McCarty, PL},
Title = {{Pore-scale modeling of biological clogging due to aggregate expansion: A
   material mechanics approach}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2001}},
Volume = {{37}},
