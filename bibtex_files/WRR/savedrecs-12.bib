@article{ ISI:000170090100009,
Author = {Russo, D and Zaidel, J and Laufer, A},
Title = {{Numerical analysis of flow and transport in variably saturated bimodal
   heterogeneous porous media}},
Journal = {{WATER RESOURCES RESEARCH}},
