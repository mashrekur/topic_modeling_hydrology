@article{ ISI:A1991FV77700004,
Author = {HAZENBERG, G and PANU, US},
Title = {{THEORETICAL-ANALYSIS OF FLOW-RATE INTO PERFORATED DRAIN TUBES}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1991}},
Volume = {{27}},
Number = {{7}},
Pages = {{1411-1418}},
Month = {{JUL}},
Abstract = {{Perforated drain tubes have, in many places, supplanted the use of drain
   tiles in drainage from agricultural lands, airports and the like. 
   However, the few existent formulae for predicting drainage rates are
   complex and unwieldy.  In this paper, a formula is derived by using a
   series solution of Laplace's equation and techniques of solution of dual
   trigonometric series, for the rate of water flow into a perforated drain
   tube, showing the effects of such parameters as hole size, number and
   spacing of holes, and drain size and depth.  The formula applies to
   pipes perforamated by small circular holes or by longitudinal slits. 
   Recommendations for the design of perforated pipes are made, based on
   the delivery ratio.}},
