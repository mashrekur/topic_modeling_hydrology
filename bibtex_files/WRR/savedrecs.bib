@article{ ISI:A1991GD92700001,
Author = {COOPER, LW and OLSEN, CR and SOLOMON, DK and LARSEN, IL and COOK, RB and
   GREBMEIER, JM},
Title = {{STABLE ISOTOPES OF OXYGEN AND NATURAL AND FALLOUT RADIONUCLIDES USED FOR
   TRACING RUNOFF DURING SNOWMELT IN AN ARCTIC WATERSHED}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1991}},
Volume = {{27}},
Number = {{9}},
Pages = {{2171-2179}},
Month = {{SEP}},
Abstract = {{Stable isotopes of oxygen, and natural and fallout radionuclides, have
   been used, respectively, to identify water sources and to quantify
   watershed fluxes of precipitation-borne solutes and particles that come
   in contact with tundra vegetation at Imnavait Creek, Alaska (68-degrees
   37'N, 149-degrees 17'W).  Oxygen 18/oxygen 16 ratios of water in the
   snowpack, stream, and unfrozen soil during the peak of snowmelt showed
   that less than approximately 14\% of streamwater was derived from
   sources other than snow.  Within a month of snowmelt, O-18/O-16 ratios
   in the stream indicated that only negligible amounts of water derived
   from snow remained in the watershed.  In contrast to the low degree of
   mixing between snow and underlying soil water, greater than 90\% of the
   atmospherically derived Be-7 (53-day half-life) that was deposited in
   the snowpack was adsorbed onto the surface 2-3 cm of frozen organic
   soils and vegetation.  A slightly lower degree of absorption
   (approximately 2/3) on tundra was observed for atmospherically derived
   S-35 (87-day half-life) released during snowmelt.  These data indicate
   that atmospherically derived substances in the snowpack are often
   retained strongly by sorption or chemical interaction with vegetation
   and soil during snowmelt.  This conclusion is also supported by the
   vegetation and soil inventories of Cs-137 (30.2-year half-life), which
   are similar to values expected as a result of atmospheric deposition
   over the past 40 years.}},
