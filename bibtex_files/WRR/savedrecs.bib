@article{ ISI:A1991FP65100013,
Author = {KANE, DL and HINZMAN, LD and BENSON, CS and LISTON, GE},
Title = {{SNOW HYDROLOGY OF A HEADWATER ARCTIC BASIN .1. PHYSICAL MEASUREMENTS AND
   PROCESS STUDIES}},
Journal = {{WATER RESOURCES RESEARCH}},
