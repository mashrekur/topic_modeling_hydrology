@article{ ISI:A1991GD92700024,
Author = {WANG, QJ},
Title = {{THE GENETIC ALGORITHM AND ITS APPLICATION TO CALIBRATING CONCEPTUAL
   RAINFALL-RUNOFF MODELS}},
