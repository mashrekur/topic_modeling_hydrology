@article{ ISI:A1991GJ13600018,
Author = {WIERENGA, PJ and HILLS, RG and HUDSON, DB},
Title = {{THE LAS-CRUCES TRENCH SITE - CHARACTERIZATION, EXPERIMENTAL RESULTS, AND
   ONE-DIMENSIONAL FLOW PREDICTIONS}},
Journal = {{WATER RESOURCES RESEARCH}},
