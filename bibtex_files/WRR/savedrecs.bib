@article{ ISI:A1991GJ13600021,
Author = {VOGEL, T and CISLEROVA, M and HOPMANS, JW},
Title = {{POROUS-MEDIA WITH LINEARLY VARIABLE HYDRAULIC-PROPERTIES}},
Journal = {{WATER RESOURCES RESEARCH}},
