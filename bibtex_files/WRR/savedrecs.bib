@article{ ISI:A1991FP65100008,
Author = {PHILIP, JR},
Title = {{INFILTRATION AND DOWNSLOPE UNSATURATED FLOWS IN CONCAVE AND CONVEX
   TOPOGRAPHIES}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1991}},
Volume = {{27}},
Number = {{6}},
Pages = {{1041-1048}},
Month = {{JUN}},
Abstract = {{The solution of the nonlinear unsaturated flow equation is given for a
   problem of infiltration (and associated subsurface flows) into concave
   and convex topographies made up of homogeneous isotropic soil with
   uniform initial moisture content.  The analysis embraces concave and
   convex surfaces which are parts of the surfaces of circular cylinders
   and spheres.  We thus model concave and convex slopes (of both ridges
   and isolated hills), valley bottoms and hollows, and ridgecrests and
   hillcrests.  The major geometrical influence on the flow processes is
   the slope angle.  Only when sigma-1 (sigma = surface total curvature) is
   less than about 10 times the characteristic infiltration length l(grav)
   (or 10 times the sorptive length) does the perturbation due to surface
   curvature need to be taken into account.  Accordingly, the earlier
   analysis of infiltration on planar hillslopes has wide applicability to
   slopes with surface curvature.}},
