@article{ ISI:A1991FV77700037,
Author = {PONCE, VM},
Title = {{NEW PERSPECTIVE ON THE VEDERNIKOV NUMBER}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1991}},
Volume = {{27}},
Number = {{7}},
Pages = {{1777-1779}},
Month = {{JUL}},
Abstract = {{There are three characteristic speeds in open channel hydraulics:  (1)
   the mean flow velocity u, (2) the relative celerity of kinematic waves
   v, and (3) the relative celerity of dynamic waves w.  These three speeds
   give rise to two independent dimensionless ratios:  (1) the
   dimensionless relative celerity of kinematic waves c(k) = v/u, and (2)
   the dimensionless relative celerity of dynamic waves c(d) = w/u.  The
   reciprocal of c(d) is the Froude number F(r) = u/w.  The Vedernikov
   number is the third ratio V = v/w.  Recent developments in the linear
   theory of surface runoff show that the Vedernikov number has a
   significant role in extending the concept of hydraulic diffusivity to
   the realm of dynamic waves.  This new perspective makes possible the
   modeling of catchment dynamics using diffusion waves and including
   inertial effects, under a wide range of boundary friction and
   cross-sectional shape specifications.}},
