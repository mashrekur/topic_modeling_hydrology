@article{ ISI:000310072500001,
Author = {Masbruch, Melissa D. and Chapman, David S. and Solomon, D. Kip},
Title = {{Air, ground, and groundwater recharge temperatures in an alpine setting,
   Brighton Basin, Utah}},
