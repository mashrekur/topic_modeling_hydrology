@article{ ISI:000308599700001,
Author = {Chen, Huali and Ding, Guoping and Han, Weon Shik and Kim, Kue-Young and
   Park, Eungyu},
Title = {{Multispecies transport coupled to a first-order reaction network in a
   double-domain medium}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2012}},
Volume = {{48}},
Month = {{SEP 7}},
Abstract = {{One-dimensional analytical solutions for multispecies solute transport
   in a domain with two distinctive subdomains are developed based on exact
   interdomain boundary conditions. The derived solutions are tested
   through transient and steady state scenarios. In the global sensitivity
   analysis (SA) of the developed solutions, the influences of the up- and
   down-gradient subdomains apportioned to the interfacial concentrations
   are 75.7 and 24.3\%, respectively, while in the local SA, they are
   similar, at 65 and 35\%, respectively. The overall SA results suggest
   that the common considerations of up- and down-gradient influences are
   essential when the precision of the estimation is an important issue.}},
