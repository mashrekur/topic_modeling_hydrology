@article{ ISI:000400160500046,
Author = {Kelly, James F. and Bolster, Diogo and Meerschaert, Mark M. and
   Drummond, Jennifer D. and Packman, Aaron I.},
Title = {{FracFit: A robust parameter estimation tool for fractional calculus
   models}},
Journal = {{WATER RESOURCES RESEARCH}},
