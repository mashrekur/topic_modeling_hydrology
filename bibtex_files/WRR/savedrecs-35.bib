@article{ ISI:000394911200008,
Author = {Wang, Quanrong and Zhan, Hongbin},
Title = {{Intrawellbore kinematic and frictional losses in a horizontal well in a
   bounded confined aquifer}},
Journal = {{WATER RESOURCES RESEARCH}},
