@article{ ISI:000394911200025,
Author = {Weber, Tobias K. D. and Iden, Sascha C. and Durner, Wolfgang},
Title = {{Unsaturated hydraulic properties of Sphagnum moss and peat reveal
   trimodal pore-size distributions}},
Journal = {{WATER RESOURCES RESEARCH}},
