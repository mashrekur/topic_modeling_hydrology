@article{ ISI:000394911200012,
Author = {Cihan, Abdullah and Birkholzer, Jens and Trevisan, Luca and
   Gonzalez-Nicolas, Ana and Illangasekare, Tissa},
Title = {{Investigation of representing hysteresis in macroscopic models of
   two-phase flow in porous media using intermediate scale experimental
   data}},
