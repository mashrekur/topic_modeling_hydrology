@article{ ISI:000400160500049,
Author = {Anel, Juan A.},
Title = {{Comment on ``Most computational hydrology is not reproducible, so is it
   really science?{''} by Christopher Hutton et al.}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2017}},
Volume = {{53}},
Number = {{3}},
Pages = {{2572-2574}},
Month = {{MAR}},
Abstract = {{Nowadays, the majority of the scientific community is not aware of the
   risks and problems associated with an inadequate use of computer systems
   for research, mostly for reproducibility of scientific results. Such
   reproducibility can be compromised by the lack of clear standards and
   insufficient methodological description of the computational details
   involved in an experiment. In addition, the inappropriate application or
   ignorance of copyright laws can have undesirable effects on access to
   aspects of great importance of the design of experiments and therefore
   to the interpretation of results.
   Plain Language Summary This article highlights several important issues
   to ensure the scientific reproducibility of results within the current
   scientific framework, going beyond simple documentation. Several
   specific examples are discussed in the field of hydrological modeling.}},
