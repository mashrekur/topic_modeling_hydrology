@article{ ISI:000398568800017,
Author = {Lin, Liao-Fan and Ebtehaj, Ardeshir M. and Wang, Jingfeng and Bras,
   Rafael L.},
Title = {{Soil moisture background error covariance and data assimilation in a
   coupled land-atmosphere model}},
Journal = {{WATER RESOURCES RESEARCH}},
