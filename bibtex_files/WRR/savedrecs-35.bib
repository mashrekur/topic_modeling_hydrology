@article{ ISI:000394911200006,
Author = {Carles Brangari, Albert and Sanchez-Vila, Xavier and Freixa, Anna and M.
   Romani, Anna and Rubol, Simonetta and Fernandez-Garcia, Daniel},
Title = {{A mechanistic model (BCC-PSSICO) to predict changes in the hydraulic
   properties for bio-amended variably saturated soils}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2017}},
Volume = {{53}},
Number = {{1}},
