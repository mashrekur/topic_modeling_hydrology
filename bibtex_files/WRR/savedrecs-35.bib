@article{ ISI:000394911200039,
Author = {Li, Dongyue and Durand, Michael and Margulis, Steven A.},
Title = {{Estimating snow water equivalent in a Sierra Nevada watershed via
   spaceborne radiance data assimilation}},
Journal = {{WATER RESOURCES RESEARCH}},
