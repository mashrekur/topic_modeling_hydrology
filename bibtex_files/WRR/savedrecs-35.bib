@article{ ISI:000394911200055,
Author = {Ameli, Ali A. and Beven, Keith and Erlandsson, Martin and Creed, Irena
   F. and McDonnell, Jeffrey J. and Bishop, Kevin},
Title = {{Primary weathering rates, water transit times, and
   concentration-discharge relations: A theoretical analysis for the
   critical zone}},
Journal = {{WATER RESOURCES RESEARCH}},
