@article{ ISI:A1992HT43800025,
Author = {DENG, FW and CUSHMAN, JH},
Title = {{ON SPECTRAL MEASURE AND MONTE-CARLO APPROACHES TO FLOW IN HETEROGENEOUS
   MEDIA}},
