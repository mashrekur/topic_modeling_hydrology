@article{ ISI:A1992JR40900011,
Author = {GRAYSON, RB and MOORE, ID and MCMAHON, TA},
Title = {{PHYSICALLY BASED HYDROLOGIC MODELING .2. IS THE CONCEPT REALISTIC}},
