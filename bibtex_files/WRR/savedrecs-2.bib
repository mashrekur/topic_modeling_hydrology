Type = {{Article}},
Language = {{English}},
Affiliation = {{IJJASZVASQUEZ, EJ (Reprint Author), MIT,RALPH M PARSONS LAB,CAMBRIDGE,MA 02139, USA.}},
DOI = {{10.1029/92WR01561}},
ISSN = {{0043-1397}},
