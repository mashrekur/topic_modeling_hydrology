@article{ ISI:A1992GY77200017,
Author = {BRUSSEAU, ML and JESSUP, RE and RAO, PSC},
Title = {{MODELING SOLUTE TRANSPORT INFLUENCED BY MULTIPROCESS NONEQUILIBRIUM AND
   TRANSFORMATION REACTIONS}},
Journal = {{WATER RESOURCES RESEARCH}},
