@article{ ISI:A1992JL48200029,
Author = {SELKER, J and LECLERQ, P and PARLANGE, JY and STEENHUIS, T},
Title = {{FINGERED FLOW IN 2 DIMENSIONS .1. MEASUREMENT OF MATRIC POTENTIAL}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1992}},
Volume = {{28}},
Number = {{9}},
Pages = {{2513-2521}},
Month = {{SEP}},
Abstract = {{Precise management of the changing matric potential during infiltration
   into unsaturated soil required the development of miniature, high-speed,
   planar tensiometers. A novel design was developed, with response time of
   less than 1 s. The applicability of the devices is shown through
   measurements of the matric potential in growing instabilities, both in
   the induction zone and along the vertical finger profile. Tensiometry is
   demonstrated to be a practical method of obtaining data with high
   temporal and spatial resolution for the study of dynamic flow fields and
   facilitates testing of theoretical results for unstable flow fields.}},
