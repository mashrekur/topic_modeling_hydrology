@article{ ISI:A1992JL48200023,
Author = {DYKHUIZEN, RC},
Title = {{DIFFUSIVE MATRIX FRACTURE COUPLING INCLUDING THE EFFECTS OF FLOW
   CHANNELING}},
Journal = {{WATER RESOURCES RESEARCH}},
