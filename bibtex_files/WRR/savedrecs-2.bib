@article{ ISI:A1992HT43800022,
Author = {JACKSON, CR and CUNDY, TW},
Title = {{A MODEL OF TRANSIENT, TOPOGRAPHICALLY DRIVEN, SATURATED SUBSURFACE FLOW}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1992}},
Volume = {{28}},
Number = {{5}},
Pages = {{1417-1427}},
Month = {{MAY}},
Abstract = {{A two-dimensional, finite difference model of saturated subsurface
   hillslope flow, capable of handling varied topography, is developed. The
   model uses rainfall, elevation data, saturated conductivities, and
   available soil porosity to predict saturated thicknesses in time and
   space over the hillslope. The model is tested against piezometric data
   collected in a steep hillslope hollow in the Cascade Range of Washington
   state during the winter wet season. The model closely tracks the timing
   and magnitude of piezometric response.}},
