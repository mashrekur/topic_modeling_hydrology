@article{ ISI:A1992HN11300007,
Author = {RUBIN, Y and DAGAN, G},
Title = {{CONDITIONAL ESTIMATION OF SOLUTE TRAVEL TIME IN HETEROGENEOUS FORMATIONS
   - IMPACT OF TRANSMISSIVITY MEASUREMENTS}},
Journal = {{WATER RESOURCES RESEARCH}},
