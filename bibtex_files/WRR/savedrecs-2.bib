Address = {{2000 FLORIDA AVE NW, WASHINGTON, DC 20009}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{DESTOUNI, G (Reprint Author), ROYAL INST TECHNOL,DEPT HYDRAUL ENGN,S-10044 STOCKHOLM 70,SWEDEN.}},
DOI = {{10.1029/92WR00166}},
ISSN = {{0043-1397}},
Keywords-Plus = {{TRANSFER-FUNCTION MODEL; HYDRAULIC CONDUCTIVITY; SPATIAL VARIABILITY;
   POROUS FORMATIONS; WATER-FLOW; TRANSPORT; DISPERSION; SIMULATION;
