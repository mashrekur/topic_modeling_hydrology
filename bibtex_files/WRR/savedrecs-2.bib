@article{ ISI:A1992HG17500020,
Author = {DESTOUNI, G},
Title = {{PREDICTION UNCERTAINTY IN SOLUTE FLUX THROUGH HETEROGENEOUS SOIL}},
Journal = {{WATER RESOURCES RESEARCH}},
