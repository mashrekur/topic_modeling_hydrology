@article{ ISI:A1992JF70100012,
Author = {STAGNITTI, F and PARLANGE, JY and STEENHUIS, TS and PARLANGE, MB and
   ROSE, CW},
Title = {{A MATHEMATICAL-MODEL OF HILLSLOPE AND WATERSHED DISCHARGE}},
Journal = {{WATER RESOURCES RESEARCH}},
