@article{ ISI:A1992HG17500018,
Author = {REEVES, TL and ELGEZAWI, SM},
Title = {{TIME DOMAIN REFLECTOMETRY FOR MEASURING VOLUMETRIC WATER-CONTENT IN
   PROCESSED OIL-SHALE WASTE}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1992}},
Volume = {{28}},
Number = {{3}},
Pages = {{769-776}},
Month = {{MAR}},
Abstract = {{Time domain reflectometry (TDR) was evaluated and developed to monitor
   volumetric water content (theta(v)) in oil shale solid waste retorted
   and combusted by the Lurgi-Ruhrgas process.  A TDR probe was designed
   and tested that could be buried and compacted in waste embankments and
   provide in situ measurements for theta(v) in the high-saline and
   high-alkaline conditions exhibited by this waste.  TDR was found to be
   accurate for measurement of theta(v) across a broad range of water
   contents in the processed oil shale waste.  A computer algorithm to
   automate the analysis of TDR traces to determine theta(v) was developed
   and tested.  A sensitivity test was performed to analyze differences
   between three smoothing algorithms on the measurement.  No significant
   differences were found between smoothing algorithms or between the
   number of points applied for smoothing.}},
