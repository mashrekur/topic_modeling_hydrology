@article{ ISI:A1992JC48300012,
Author = {HUNTER, C and MCDONALD, A and BEVEN, K},
Title = {{INPUT OF FECAL-COLIFORM BACTERIA TO AN UPLAND STREAM CHANNEL IN THE
   YORKSHIRE DALES}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1992}},
Volume = {{28}},
Number = {{7}},
Pages = {{1869-1876}},
Month = {{JUL}},
Abstract = {{The input of fecal coliform bacteria to the River Skell stream from a
   small, approximately 0.5 ha, instrumented research site on Pateley Moor
   in the Yorkshire Dales was investigated.  Bacterial concentrations were
   determined for a total of 433 water samples collected between October
   and December 1984 from 31 stream bank sites representing three types of
   near-surface water flow:  matrix through flow, nonmatrix through flow,
   and overland flow.  Bacterial concentrations in matrix through flow were
   found to be very low, but much higher input rates were found in both
   nonmatrix through flow and overland flow.  Overland flow sites were the
   most important group for the input of bacteria to the channel, both
   between and during storm events.  The rate of fecal coliform input was
   found to increase during storm events, and there was a strong, positive
   correlation between bacterial input rate and stream stage height.}},
