@article{ ISI:A1994PJ32500010,
Author = {BRUTSAERT, W},
Title = {{THE UNIT RESPONSE OF GROUNDWATER OUTFLOW FROM A HILLSLOPE}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1994}},
Volume = {{30}},
Number = {{10}},
Pages = {{2759-2763}},
Month = {{OCT}},
Abstract = {{Subsurface flow from a hillslope can be described by hydraulic
   groundwater theory, as formulated by the Boussinesq equation.  Solution
   of its linear version brings out some of the essential features of this
   phenomenon.  Arbitrary inputs can be accommodated by simple convolution
   of the sudden drawdown problem.  The relative magnitude of the driving
   mechanisms, namely the streamwise pressure gradient, resulting in
   diffusion and gravity (i.e., slope), resulting in advection, can be
   determined by a simple dimensionless parameter.}},
