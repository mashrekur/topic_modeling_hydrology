@article{ ISI:A1994MP40100004,
Author = {NOVAKOWSKI, KS and LAPCEVIC, PA},
Title = {{FIELD MEASUREMENT OF RADIAL SOLUTE TRANSPORT IN FRACTURED ROCK}},
