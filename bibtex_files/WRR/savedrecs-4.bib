@article{ ISI:A1994MV70700030,
Author = {CHEN, ZQ and GOVINDARAJU, RS and KAVVAS, ML},
Title = {{SPATIAL AVERAGING OF UNSATURATED FLOW EQUATIONS UNDER INFILTRATION
   CONDITIONS OVER AREALLY HETEROGENEOUS FIELDS .1. DEVELOPMENT OF MODELS}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1994}},
Volume = {{30}},
