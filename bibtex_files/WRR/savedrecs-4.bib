@article{ ISI:A1994NV57900008,
Author = {CHENG, XX and ANDERSON, MP},
Title = {{SIMULATING THE INFLUENCE OF LAKE POSITION ON GROUNDWATER FLUXES}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1994}},
Volume = {{30}},
Number = {{7}},
Pages = {{2041-2049}},
Month = {{JUL}},
Abstract = {{Groundwater flow around three hypothetical flow-through lakes, located
   in the upper, middle, and lower portions of a watershed, was numerically
   simulated in cross section under steady state and transient conditions.
   Results showed that (1) groundwater fluxes into and out of the lake
   located in the lowest position were higher and more stable than for
   lakes farther upgradient, and (2) periods of intense groundwater
   recharge caused flow reversals and the formation of a stagnation point
   downgradient of the uppermost lake, thereby causing a net increase in
   groundwater inflow. These simulation results provide guidance on
   monitoring network design, including the frequency of monitoring and/or
   sampling.}},
