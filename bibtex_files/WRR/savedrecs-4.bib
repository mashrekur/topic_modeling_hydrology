@article{ ISI:A1994NA85300002,
Author = {GROVES, CG and HOWARD, AD},
Title = {{MINIMUM HYDROCHEMICAL CONDITIONS ALLOWING LIMESTONE CAVE DEVELOPMENT}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1994}},
Volume = {{30}},
Number = {{3}},
Pages = {{607-615}},
Month = {{MAR}},
Abstract = {{Early theoretical investigations into initiation of limestone cave
   development suggested that water entering small fractures approaches
   saturation within a short distance and thus appeared to be unable to
   produce much further enlargement of these fractures. The answer to this
   problem seems to be White's (1977) `'kinetic trigger'' idea, emphasizing
   the precipitous drop in dissolution rates as equilibrium is approached.
   However, minimum conditions of flow rate, passage geometry, and initial
   CO2 concentration may exist, below which the waters will so nearly
   achieve saturation that caves will be unable to develop. Modeling
   results indicate that under geologically reasonable PCO2 and head
   gradient values, the minimum initial aperture widths in which caves will
   develop is of the order of 100 mum. Initial apertures wider than about
   500 mum will form passages very easily, even under low head gradients.
   There will thus be strong differential selection among initial flow
   paths with regard to cavern passage development.}},
