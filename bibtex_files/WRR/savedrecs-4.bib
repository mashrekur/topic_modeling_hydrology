@article{ ISI:A1994NJ81700022,
Author = {NAFF, RL},
Title = {{AN EULERIAN SCHEME FOR THE 2ND-ORDER APPROXIMATION OF SUBSURFACE
   TRANSPORT MOMENTS}},
