@article{ ISI:A1994MV70700004,
Author = {MEROT, P and CRAVE, A and GASCUELODOUX, C and LOUHALA, S},
Title = {{EFFECT OF SATURATED AREAS ON BACKSCATTERING COEFFICIENT OF THE ERS-1
   SYNTHETIC-APERTURE RADAR - FIRST RESULTS}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1994}},
Volume = {{30}},
Number = {{2}},
Pages = {{175-179}},
Month = {{FEB}},
Abstract = {{One way to estimate spatial distribution of water content at the soil
   surface consists of using active microwave remote sensing. It has been
   theoretically and experimentally demonstrated for unsaturated
   conditions. Nevertheless, radar data are ambiguous when ponding
   conditions occur, as in variable source areas, due to the contradictory
   influence of the dielectric effect and the specular effect on the
   backscattering attenuation coefficient, sigma(0). A procedure is
   considered, based on a topographic analysis, to take into account the
   influence of the two effects on radar response. The present results
   stress the capabilities of the ERS (European Remote Sensing Satellite)
   radar to survey saturated areas in time and space.}},
