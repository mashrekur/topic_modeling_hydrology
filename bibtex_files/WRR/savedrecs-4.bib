@article{ ISI:A1994MV70700033,
Author = {HANTUSH, MM and MARINO, MA},
Title = {{2-DIMENSIONAL STOCHASTIC-ANALYSIS AND OPTIMAL ESTIMATION IN AQUIFERS -
   RANDOM RECHARGE}},
