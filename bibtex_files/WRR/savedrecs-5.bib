@article{ ISI:A1995QW76800021,
Author = {SAWYER, CS and AHLFELD, DP and KING, AJ},
Title = {{GROUNDWATER REMEDIATION DESIGN USING A 3-DIMENSIONAL SIMULATION-MODEL
   AND MIXED-INTEGER PROGRAMMING}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{5}},
Pages = {{1373-1385}},
Month = {{MAY}},
Abstract = {{A three-dimensional groundwater flow management model for making
   decisions on the design of hydrodynamic control of a groundwater flow
   system using a combination of extraction and/or injection wells is
   developed. The model takes into account constraints imposed on the
   system to stop the horizontal spread of contaminants and to ensure a net
   upward flow in areas where downward vertical gradients exist. The
   mathematical formulation of the groundwater remediation problem as a
   mixed-integer model and the strategy for solving the model are
   presented. Numerical results are presented for the Toms River Plant
   site, which is modeled as a five-layer aquifer system with
   interconnecting aquitards. A sensitivity analysis on the relative
   magnitude of the continuous operating costs and the fixed-charge costs
   is also presented.}},
