@article{ ISI:A1995QL42700013,
Author = {PARKIN, GW and KACHANOSKI, RG and ELRICK, DE},
Title = {{CONSTANT RATE RAINFALL INFILTRATION IN A FIELD OF VARIABLE-ALPHA}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{3}},
Pages = {{593-600}},
Month = {{MAR}},
Abstract = {{Spatial variability of hydraulic parameters is an important factor in
   prediction of water flow and solute transport in unsaturated soils. An
   analytical solution for one-dimensional constant rate rainfall
   infiltration in a homogeneous soil is incorporated into a model of a
   spatially variable field. The model is a composite of vertically
   homogeneous soil columns that differ in their hydraulic properties. In
   this study, horizontal variation in infiltration profiles is due to
   variation in the inverse macroscopic capillary length (alpha) of each
   column. The sill in travel time variance of wetting profiles with depth
   for the homogeneous column confirms the notion of a long-time traveling
   wave of fixed shape. Variation in alpha slightly enhances the spread of
   wetting profiles and delays formation of the traveling wave.
   Identification of the mean alpha value is more critical than inclusion
   of variability in alpha for wetting profile simulations under the
   examined conditions.}},
