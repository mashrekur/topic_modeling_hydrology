@article{ ISI:A1995RB07500010,
Author = {OGDEN, FL and RICHARDSON, JR and JULIEN, PY},
Title = {{SIMILARITY IN CATCHMENT RESPONSE .2. MOVING RAINSTORMS}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{6}},
Pages = {{1543-1547}},
Month = {{JUN}},
Abstract = {{The influence of storm motion on runoff is explored, with a focus on
   dimensionless hydrologic similarity parameters. One- and two-dimensional
   physically based runoff models are subjected to moving rainstorms. A
   dimensionless storm speed parameter Ut(e)/L(p), where U is the storm
   speed, t(e) is the runoff plane kinematic time to equilibrium, and L(p)
   is the length of the runoff plane, is identified as a similarity
   condition. Storm motion effects on the peak discharge are greatest when
   the storm is traversing a one-dimensional runoff plane in the downslope
   direction at a dimensionless speed of Ut(e)/L(p) = 0.5. This conclusion
   holds for all values of the dimensionless storm sizes L(s)/L(p) where
   L(s) is the length of the storm in the direction of motion. Simulations
   with a two-dimensional rainfall-runoff model confirm the applicability
   of this similarity parameter on natural watershed topography. Results
   indicate that the detailed simulation of storm motion is necessary when
   the storm is moving near the velocity of maximum effect, which is
   considerably slower than typical storm velocities.}},
