@article{ ISI:A1995TD80500009,
Author = {TENBERGE, HFM and METSELAAR, K and JANSEN, MJW and DESANAGUSTIN, EM},
Title = {{THE SAWAH RICELAND HYDROLOGY MODEL}},
Journal = {{WATER RESOURCES RESEARCH}},
