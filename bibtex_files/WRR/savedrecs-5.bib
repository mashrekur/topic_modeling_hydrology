@article{ ISI:A1995QH49300008,
Author = {HART, DR},
Title = {{PARAMETER-ESTIMATION AND STOCHASTIC INTERPRETATION OF THE TRANSIENT
   STORAGE MODEL FOR SOLUTE TRANSPORT IN STREAMS}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{2}},
Pages = {{323-328}},
Month = {{FEB}},
Abstract = {{A stochastic model for solute transport in streams with transient
   storage zones is presented. This model has an analytic solution in terms
   of a rapidly converging infinite series. Besides being computationally
   efficient, this solution is of interest because the terms of the series
   can be interpreted physically. Simple methods for roughly estimating
   storage zone parameters directly from tracer injection data are
   introduced. These methods give quick estimates of these parameters
   without requiring solutions to the model equations.}},
