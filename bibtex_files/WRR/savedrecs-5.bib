@article{ ISI:A1995RG41100015,
Author = {PETERS, T and ADAMOWICZ, WL and BOXALL, PC},
Title = {{INFLUENCE OF CHOICE SET CONSIDERATIONS IN MODELING THE BENEFITS FROM
   IMPROVED WATER-QUALITY}},
