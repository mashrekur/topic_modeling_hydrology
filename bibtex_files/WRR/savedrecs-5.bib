@article{ ISI:A1995QL42700004,
Author = {LOWRY, MI and MILLER, CT},
Title = {{PORE-SCALE MODELING OF NONWETTING-PHASE RESIDUAL IN POROUS-MEDIA}},
Journal = {{WATER RESOURCES RESEARCH}},
