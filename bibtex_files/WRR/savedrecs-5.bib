@article{ ISI:A1995QL42700028,
Author = {STORMONT, JC},
Title = {{THE EFFECT OF CONSTANT ANISOTROPY ON CAPILLARY BARRIER PERFORMANCE}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{3}},
Pages = {{783-785}},
Month = {{MAR}},
Abstract = {{Ross' solution for the divergence capacity of a capillary barrier is
   modified to incorporate constant hydraulic conductivity anisotropy of
   the fine layer. The analysis assumes steady infiltration and that the
   principal directions of the hydraulic conductivity tenser are parallel
   and normal to the fine-coarse contact. The solutions for divergence
   capacity and length have two terms. The first term, similar to the
   solution of Ross, represents the diversion due to the presence of the
   capillary barrier and is proportional to the hydraulic conductivity
   parallel to the contact. The second term, which arises from infiltration
   into an anisotropic medium independent of the capillary barrier, is a
   function of the fine layer thickness and will be small for engineered
   barriers. Thus an anisotropic fine layer with a greater hydraulic
   conductivity in the direction parallel to the fine-coarse contact will
   extend the performance of a capillary barrier as measured by its
   divergence capacity and length.}},
