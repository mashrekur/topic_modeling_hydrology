@article{ ISI:A1995RG41100018,
Author = {CROUCHER, AE and OSULLIVAN, MJ},
Title = {{THE HENRY PROBLEM FOR SALTWATER INTRUSION}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{7}},
Pages = {{1809-1814}},
Month = {{JUL}},
Abstract = {{Previous solutions of the idealized saltwater intrusion problem known as
   the Henry problem are discussed, and possible reasons for the observed
   discrepancies between them are given. High-accuracy finite difference
   techniques are used to solve the nondimensionalized equations governing
   the problem, and a fine grid is used so that the solutions obtained
   contain only very small truncation errors. Such errors are investigated
   by means of grid refinement. Comparison of past results with the present
   solutions indicate, first, the presence of significant inaccuracy in
   certain earlier results and, second, the effects of numerical dispersion
   in other previous solutions calculated using relatively few grid points.}},
