@article{ ISI:A1995TK64700037,
Author = {Belitz, K and Dripps, W and Fairchild, JE},
Title = {{Slug tests in elastic, unconfined aquifers: Incorporating the free
   surface}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1995}},
Volume = {{31}},
Number = {{12}},
Pages = {{3329-3336}},
Month = {{DEC}},
Abstract = {{In this paper we evaluate slug tests in shallow, fully screened wells
   that completely penetrate homogeneous, anisotropic, elastic, unconfined
   aquifers. If the free surface is incorporated into the initial boundary
   value problem, the dimensionless head (H-D) is a function of five
   parameters: dimensionless time (t(D)), system geometry and anisotropy
   (eta), elastic storativity (S), specific yield (S-y), and the size of
   the initial perturbation relative to the initial saturated thickness
   (lambda). If the water table is assumed to be a constant head boundary,
   the dependence on S-y and lambda is lost. Numerical solution of the
   problem indicates that the dimensionless response of the well (H-D
   versus t(D)) is largely a function of lambda, S-y, and eta; the response
   is less sensitive to S. Application of type curves derived for wells
   that are cased above a fixed-head water table and screened beneath will
   result in an overestimate of transmissivity. The overestimate will be
   minimized if lambda and S-y are small.}},
