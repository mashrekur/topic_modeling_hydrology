@article{ ISI:000223893600004,
Author = {Liu, FJ and Williams, MW and Caine, N},
Title = {{Source waters and flow paths in an alpine catchment, Colorado Front
   Range, United States}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2004}},
Volume = {{40}},
Number = {{9}},
Month = {{SEP 9}},
Abstract = {{{[}1] Source waters and flow paths of streamflow draining high-elevation
   catchments of the Colorado Rocky Mountains were determined using
   isotopic and geochemical tracers during the 1996 snowmelt runoff season
   at two subcatchments of the Green Lakes Valley, Colorado Front Range. A
   two-component hydrograph separation using delta(18)O indicates that new
   water dominated ( 82 +/- 6\%) streamflow at the 8-ha Martinelli
   catchment and old water dominated ( 64 +/- 2\%) at the 225-ha Green Lake
   4 (GL4) catchment. Snowmelt became isotopically enriched as the melt
   season progressed, complicating the interpretation of source water
   models. Thus old water may be underestimated if the temporal variation
   in delta(18)O of snowmelt is ignored or extrapolated from point
   measurements to the catchment. Two-component hydrograph separations for
   unreacted and reacted waters using a single geochemical tracer were not
   always meaningful. Three-component hydrograph separations using
   end-member mixing analysis indicated that subsurface flow contributed
   more than two thirds to the streamflow at both catchments. Talus fields
   contributed more than 40\% of the total discharge during summer at the
   GL4 catchment. A conceptual model was established for flow generation
   based on these results. It is suggested that surface water and
   groundwater interactions are much more important to the quantity and
