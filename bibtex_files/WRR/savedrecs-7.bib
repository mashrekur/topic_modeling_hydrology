@article{ ISI:A1996VE90100015,
Author = {Lall, U and Rajagopalan, B and Tarboton, DG},
Title = {{A nonparametric wet/dry spell model for resampling daily precipitation}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1996}},
Volume = {{32}},
Number = {{9}},
Pages = {{2803-2823}},
Month = {{SEP}},
Abstract = {{A nonparametric wet/dry spell model is developed for resampling daily
   precipitation at a site. The model considers alternating sequences of
   wet and dry days in a given season of the year. All marginal, joint, and
   conditional probability densities of interest (e.g., dry spell length,
   wet spell length, precipitation amount, and wet spell length given prior
   to dry spell length) are estimated nonparametrically using at-site data
   and kernel probability density estimators. Procedures for the
   disaggregation of wet spell precipitation into daily precipitation and
   for the generation of synthetic sequences are proffered. An application
