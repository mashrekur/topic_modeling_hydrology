@article{ ISI:A1996VA83400014,
Author = {Qualls, RJ and Brutsaert, W},
Title = {{Evaluation of spatially distributed ground-based and remotely sensed
   data to estimate spatially distributed sensible heat fluxes}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1996}},
Volume = {{32}},
Number = {{8}},
Pages = {{2489-2495}},
Month = {{AUG}},
Abstract = {{The First International Satellite Land Surface Climatology Project
   (ISLSCP) Field Experiment (FIFE) was initiated partly to improve our
   ability to model spatial distributions of surface-atmospheric fluxes
   over hilly prairie. Monin-Obukhov similarity was used to calculate
   sensible heat fluxes (H-c) at an array of ten FIFE flux measurement
   sites for comparison with measured sensible heat fluxes (H-m). Data were
   collected within the dynamic sublayer on a clear morning when there was
   strong solar heating of the surface, winds in excess of 5 m s(-1), and
   uniformly wet soil conditions. The sensitivity of correlations between
   H-c and H-m to spatial variability of air (T-a) and aircraft-based
   (thermal infrared multispectral scanner (TIMS)) remotely sensed surface
   (T-r) temperatures, wind speed (u), and an atmospheric stability
   parameterization (psi(h)) was examined. H-c was found to depend on the
   spatial variability of T-r and u but not on T-a and psi(h). Furthermore,
   approximately half the discrepancy between H-c and H-m may be attributed
   to uncertainty in H-m.}},
