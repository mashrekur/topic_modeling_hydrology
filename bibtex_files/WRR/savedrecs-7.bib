@article{ ISI:A1996UN70400003,
Author = {Cornett, RJ and Cramer, J and Andrews, HR and Chant, LA and Davies, W
   and Greiner, BF and Imahori, Y and Koslowsky, V and McKay, J and Milton,
   GM and Milton, JCD},
Title = {{In situ production of Cl-36 in uranium ore: A hydrogeological assessment
   tool}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1996}},
Volume = {{32}},
Number = {{6}},
Pages = {{1511-1518}},
Month = {{JUN}},
Abstract = {{In situ neutron activation of Cl-35 within the rock and groundwater of
   geologic deposits that have elevated concentrations of uranium provides
   a hydrogeological tracer. We determine the production rate and mobility
   of Cl-36 in the 1.3-billion-year-old Cigar Lake uranium ore deposit.
   Accelerator mass spectrometry was used to map the concentrations of
   Cl-36 in the ore and in the groundwater that were up to 100 times
   greater than those encountered in unmineralized portions of the host
   sandstone aquifer. The residence time of this mobile anion in
   groundwater within the mineralized zone ranged from 14 to 280 kyr. These
   residence times are consistent with the hydraulic and geochemical data,
   suggesting significant control of Cl- and groundwater movement by the
   clay-rich matrix of the mineralized zone.}},
