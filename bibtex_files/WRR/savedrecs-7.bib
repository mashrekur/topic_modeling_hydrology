@article{ ISI:A1996TN96100016,
Author = {Eichinger, WE and Parlange, MB and Stricker, H},
Title = {{On the concept of equilibrium evaporation and the value of the
   Priestley-Taylor coefficient}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1996}},
Volume = {{32}},
Number = {{1}},
Pages = {{161-164}},
Month = {{JAN}},
Abstract = {{An analytical expression is derived for the Priestley-Taylor
   coefficient, a. This coefficient is generally interpreted as the ratio
   between the actual evaporation rate and the equilibrium evaporation
   rate. The derived expression is shown to be equal to the accepted value
   of 1:26 for typically observed atmospheric conditions and to be
   relatively insensitive to small changes in atmospheric parameters, A
   comparison is made between the new expression and data taken over an
   irrigated held in California, The concept of equilibrium evaporation is
   called into question and it is suggested that the Priestley-Taylor
   expression with alpha equal to 1.26 is a more accurate representation of
   evaporation under `'equilibrium'' wet surface conditions.}},
