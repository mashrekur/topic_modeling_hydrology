@article{ ISI:A1996UH88200016,
Author = {Park, N},
Title = {{Closed-form solutions for steady state density-dependent flow and
   transport in a vertical soil column}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1996}},
Volume = {{32}},
Number = {{5}},
Pages = {{1317-1322}},
Month = {{MAY}},
Abstract = {{Closed-form solutions are derived for steady state density-dependent
   fluid flow and solute transport problems in a one-dimensional vertical
   soil column. Transport mechanisms considered are advection, molecular
   diffusion, and velocity-dependent dispersion. Solutions can be obtained
   for a wide variety of cases. Characteristics of density-dependent
   solutions, including effects of variations in density and the thickness
   of transition zones, are investigated for a simple case. Unique features
   not found in density-independent solutions are observed. Nonuniform
   concentration profiles with vanishing density effects are also
   identified.}},
