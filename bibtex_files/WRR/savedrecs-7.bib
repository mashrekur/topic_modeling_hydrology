@article{ ISI:A1996TT02500023,
Author = {Branson, J and Lawler, DM and Glen, JW},
Title = {{Sediment inclusion events during needle ice growth: A laboratory
   investigation of the role of soil moisture and temperature fluctuations}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1996}},
Volume = {{32}},
Number = {{2}},
Pages = {{459-466}},
Month = {{FEB}},
Abstract = {{This paper reports on a series of laboratory experiments designed to
   determine the impact of imposed temperature and moisture variations on
   the inclusion of sediment within growing needle ice. The results
   demonstrate that needle-ice growth reflects coupled heat and water
   fluxes, and thus provide experimental evidence to support theoretical
   studies by other authors. Sediment is shown to be incorporated into the
   ice as layers or dispersed concentrations when-there is an imbalance of
   these two fluxes, that is, restrictions on moisture supply and/or over
   rapid heat loss from the surface, which causes the freezing front to
   descend into the soil profile.}},
