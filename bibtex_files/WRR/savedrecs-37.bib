@article{ ISI:000428474500007,
Author = {Bartlett, M. S. and Porporato, A.},
Title = {{A Class of Exact Solutions of the Boussinesq Equation for Horizontal and
   Sloping Aquifers}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2018}},
Volume = {{54}},
Number = {{2}},
Pages = {{767-778}},
Month = {{FEB}},
Abstract = {{The nonlinear equation of Boussinesq (1877) is a foundational approach
   for studying groundwater flow through an unconfined aquifer, but solving
   the full nonlinear version of the Boussinesq equation remains a
   challenge. Here, we present an exact solution to the full nonlinear
   Boussinesq equation that not only applies to sloping aquifers but also
   accounts for source and sink terms such as bedrock seepage, an often
   significant flux in headwater catchments. This new solution captures the
   hysteretic relationship (a loop rating curve) between the groundwater
   flow rate and the water table height, which may be used to provide a
   more realistic representation of streamflow and groundwater dynamics in
   hillslopes. In addition, the solution provides an expression where the
   flow recession varies based on hillslope parameters such as bedrock
   slope, bedrock seepage, aquifer recharge, plant transpiration, and other
   factors that vary across landscape types.}},
