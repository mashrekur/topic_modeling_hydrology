@article{ ISI:000428474000006,
Author = {Clifford, Eoghan and Mulligan, Sean and Comer, Joanne and Hannon, Louise},
Title = {{Flow-Signature Analysis of Water Consumption in Nonresidential Building
   Water Networks Using High-Resolution and Medium-Resolution Smart Meter
   Data: Two Case Studies}},
