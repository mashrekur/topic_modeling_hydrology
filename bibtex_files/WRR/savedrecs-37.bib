@article{ ISI:000428474500022,
Author = {Kastner, K. and Hoitink, A. J. F. and Torfs, P. J. J. F. and Vermeulen,
   B. and Ningsih, N. S. and Pramulya, M.},
Title = {{Prerequisites for Accurate Monitoring of River Discharge Based on
   Fixed-Location Velocity Measurements}},
