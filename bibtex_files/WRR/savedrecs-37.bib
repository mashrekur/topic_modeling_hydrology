@article{ ISI:000428474000020,
Author = {Mekonnen, Mesfin M. and Hoekstra, Arjen Y.},
Title = {{Global Anthropogenic Phosphorus Loads to Freshwater and Associated Grey
   Water Footprints and Water Pollution Levels: A High-Resolution Global
   Study}},
Journal = {{WATER RESOURCES RESEARCH}},
