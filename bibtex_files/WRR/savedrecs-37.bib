@article{ ISI:000428474500023,
Author = {Song, Wenhui and Yao, Jun and Ma, Jingsheng and Sun, Hai and Li, Yang
   and Yang, Yongfei and Zhang, Lei},
Title = {{Numerical Simulation of Multiphase Flow in Nanoporous Organic Matter
   With Application to Coal and Gas Shale Systems}},
Journal = {{WATER RESOURCES RESEARCH}},
