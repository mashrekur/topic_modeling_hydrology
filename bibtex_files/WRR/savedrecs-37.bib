@article{ ISI:000428474000014,
Author = {Bracken, C. and Holman, K. D. and Rajagopalan, B. and Moradkhani, H.},
Title = {{A Bayesian Hierarchical Approach to Multivariate Nonstationary
   Hydrologic Frequency Analysis}},
Journal = {{WATER RESOURCES RESEARCH}},
