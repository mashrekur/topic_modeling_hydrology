@article{ ISI:000428474000029,
Author = {Zhang, Shulei and Yang, Yuting and McVicar, Tim R. and Yang, Dawen},
Title = {{An Analytical Solution for the Impact of Vegetation Changes on
   Hydrological Partitioning Within the Budyko Framework}},
Journal = {{WATER RESOURCES RESEARCH}},
