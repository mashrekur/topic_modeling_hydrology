@article{ ISI:000428474000016,
Author = {Hansen, Scott K. and Haslauer, Claus P. and Cirpka, Olaf A. and
   Vesselinov, Velimir V.},
Title = {{Direct Breakthrough Curve Prediction From Statistics of Heterogeneous
   Conductivity Fields}},
