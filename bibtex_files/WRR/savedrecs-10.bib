@article{ ISI:000078123800018,
Author = {Whitley, R and Hromadka, TV},
Title = {{Approximate confidence intervals for design floods for a single site
   using a neural network}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1999}},
Volume = {{35}},
Number = {{1}},
Pages = {{203-209}},
Month = {{JAN}},
Abstract = {{A basic problem in hydrology is the computation of confidence levels for
   the value of the T-year flood when it is obtained from a log Pearson III
   distribution using the estimated mean, estimated standard deviation, and
   estimated skew. Here we give a practical method for finding approximate
   one-sided or mio-sided confidence intervals for the 100-year flood based
   on data from a single site. These confidence intervals are generally
   accurate to within a percent or two, as tested by simulations, and are
   obtained by use of a neural network.}},
