@article{ ISI:000083342400022,
Author = {Montesinos, P and Garcia-Guzman, A and Ayuso, JL},
Title = {{Water distribution network optimization using a modified genetic
   algorithm}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1999}},
Volume = {{35}},
Number = {{11}},
Pages = {{3467-3473}},
Month = {{NOV}},
Abstract = {{A modified genetic algorithm (GA) is proposed for water distribution
   network optimization. Several changes are introduced in the selection
   and mutation processes of a simple GA. In each generation a constant
   number of solutions is eliminated, the selected ones are ranked for
   crossover, and the new solutions are allowed to undergo at most one
   mutation. All these modifications greatly increase the algorithm
   convergence. The modified GA is tested on the New York City water supply
   expansion problem. It obtains the lowest-cost feasible solution reported
   in the literature in far fewer generations than any previous GA.}},
