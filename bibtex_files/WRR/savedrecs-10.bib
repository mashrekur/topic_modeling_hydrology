@article{ ISI:000079992400031,
Author = {Kumar, B and Nachiappan, RP},
Title = {{On the sensitivity of Craig and Gordon model for the estimation of the
   isotopic composition of lake evaporates}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1999}},
Volume = {{35}},
Number = {{5}},
Pages = {{1689-1691}},
Month = {{MAY}},
Abstract = {{The effect of errors in the estimated isotopic composition of
   atmospheric moisture by delta(a)-delta(p) equilibrium assumption method
   on the isotopic composition of lake evaporate (delta(E)) estimated using
   the Craig and Gordon linear resistance model has been studied. It has
   been found that the propagated error in estimated delta(E) increases
   with normalized relative humidity (h) but is less significant for h up
   to 0.5. The propagated error becomes unrealistic for h above 0.8. Since
   h is the major factor influencing the propagated error, an error in h
   would result in large uncertainty in the estimated delta(E). Simple
   analytical expressions for determining the propagated errors have been
   presented. The study also reveals that for the isotopic enrichment of a
   water body to continue, the difference between its isotopic composition
   and that of atmospheric air (Delta(la)) should be greater than the
   equilibrium enrichment factor (epsilon{*}). Using the relationship
   between Delta(la), and epsilon{*}, the maximum isotopic enrichment that
   a water body can achieve at different temperatures can be deduced.}},
