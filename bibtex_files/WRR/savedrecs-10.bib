@article{ ISI:000078123800025,
Author = {Davisson, ML and Smith, DK and Kenneally, J and Rose, TP},
Title = {{Isotope hydrology of southern Nevada groundwater: Stable isotopes and
   radiocarbon}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1999}},
Volume = {{35}},
Number = {{1}},
Pages = {{279-294}},
Month = {{JAN}},
Abstract = {{A new delta(18)O map of southern Nevada groundwater shows a systematic
   decrease in delta(18)O of similar to 5 parts per thousand from 36
   degrees to 39 degrees N latitude. The variation is consistent with
   higher-latitude recharge following continuous flow paths along
   north-south trending graben valleys and systematically increasing in
   delta(18)O due to mixing with lower-latitude, higher-delta(18)O
   recharge. The data do not suggest that large masses of groundwater with
   unusually low delta(18)O values were recharged during the last pluvial
   period as suggested by previous workers. Many delta(18)O-delta D pairs
   in groundwater indicate variable amounts of evaporation relative to the
   global meteoric water line. The precipitation rate and type (rain versus
   snow) for a given geographic area controls the extent of evaporation. A
   ``model{''} delta(18)O value was calculated from evaporated groundwaters
   by subtracting postcloud evaporation. The distribution of these model
   delta(18)O values suggest new regional groundwater flow paths previously
   undocumented. Dissolved inorganic carbon in groundwaters collected from
   deep regional flow systems typically has low C-14 concentrations (less
   than or equal to 12\% modern carbon). In contrast, groundwaters
   collected from the carbonate rock of the Spring Mountains and alluvium
   of Forty Mile Canyon have higher C-14 contents, indicating more recent
   recharge. Because of nonlinear mixing of C-14, groundwater in the
   regional flow system likely acquired most of its observable C-14 from
   mixtures of young, locally recharged groundwater.}},
