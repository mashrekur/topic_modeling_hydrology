@article{ ISI:000081145400019,
Author = {Hendry, MJ and Mendoza, CA and Kirkland, RA and Lawrence, JR},
Title = {{Quantification of transient CO2 production in a sandy unsaturated zone}},
Journal = {{WATER RESOURCES RESEARCH}},
