@article{ ISI:A1993LL33500010,
Author = {LOVEI, L and WHITTINGTON, D},
Title = {{RENT-EXTRACTING BEHAVIOR BY MULTIPLE AGENTS IN THE PROVISION OF
   MUNICIPAL WATER-SUPPLY - A STUDY OF JAKARTA, INDONESIA}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{7}},
Pages = {{1965-1974}},
Month = {{JUL}},
Abstract = {{A framework is presented for the analysis of rent-extracting behavior by
   multiple agents involved in the provision of municipal water supplies in
   Jakarta, Indonesia. It is shown that such behavior can dramatically
   affect the terms and conditions under which water service is offered to
   the public. A water supply system based on limited numbers of public
   taps, relatively few house connections, and water vendors can generate
   substantial monopoly rents that can be appropriated by both public and
   private agents. Most professionals involved with water supply projects
   in developing countries typically assume that the objective of municipal
   water authorities is to serve the public interest. In fact, agents
   involved in the water delivery system may pursue strategies designed for
   private gain, which can have important and pervasive implications for
   how a water system is actually designed and operated. Proposals to
   change the technical, engineering aspects of a water distribution system
   can thus threaten the interests of powerful groups. Effective public
   policy and donor involvement in the water sector must be based on an
   understanding of the structure of water markets and the political power
   supporting existing institutional arrangements.}},
