@article{ ISI:A1993KM52000024,
Author = {BROWN, GO and STONE, ML and GAZIN, JE},
Title = {{ACCURACY OF GAMMA-RAY COMPUTERIZED-TOMOGRAPHY IN POROUS-MEDIA}},
Journal = {{WATER RESOURCES RESEARCH}},
