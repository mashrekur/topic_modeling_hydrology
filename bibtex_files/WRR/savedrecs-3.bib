@article{ ISI:A1993LR88400028,
Author = {CHENG, AHD and MOROHUNFOLA, OK},
Title = {{MULTILAYERED LEAKY AQUIFER SYSTEMS .2. BOUNDARY-ELEMENT SOLUTIONS}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{8}},
Pages = {{2801-2811}},
Month = {{AUG}},
Abstract = {{The boundary element algorithm for the multilayered leaky aquifer system
   of Neuman-Witherspoon and Herrera is presented. By applying the Laplace
   transform to the integrodifferential equations of Herrera, a set of
   coupled two-dimensional elliptic equations results. The boundary
   integral representation is derived from Green's theorem. Green's
   functions governed by an adjoint system of the original governing
   equations are found from the pumping well solutions given in part 1 by a
   coefficient exchange. The resultant numerical system requires a
   discretization only at the boundaries of the planar aquifers and is free
   from aquitard unknowns. Several numerical examples, including
   one-dimensional flow, pumping well problems, and two-dimensional
   problems are provided for the purpose of validation and demonstration.}},
