@article{ ISI:A1993LL33500032,
Author = {KALATZIS, A and GARCIADELGADO, RA and PANG, TK and KOUSSIS, AD and
   BOWERS, AR},
Title = {{2-DIMENSIONAL GROUNDWATER TRANSPORT OF REACTIVE SOLUTES WITH COMPETITIVE
   ADSORPTION}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{7}},
Pages = {{2241-2248}},
Month = {{JUL}},
Abstract = {{Chemical contamination of groundwater is typically associated with
   multicomponent solutions, the migration of which is affected by
   preferential adsorption and solution reactions. Transport models should,
   therefore, account for advection and dispersion and for the reactions in
   the solution and on the solid phase. The two-dimensional model METLI has
   been developed for the personal computer (PC) simulation of groundwater
   transport with competitive adsorption of a three-species system
   consisting of a metal, a ligand, and their dominant complex at a given
   pH. The model structure, which employs spatial and process splitting
   algorithms, is described. One- and two-dimensional application examples
   are used to demonstrate how chemical reactivity and affinity for
