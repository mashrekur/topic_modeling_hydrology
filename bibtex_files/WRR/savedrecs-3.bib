@article{ ISI:A1993LE49500002,
Author = {YAO, HM and GEORGAKAKOS, AP},
Title = {{NEW CONTROL CONCEPTS FOR UNCERTAIN WATER-RESOURCES SYSTEMS .2. RESERVOIR
   MANAGEMENT}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{6}},
Pages = {{1517-1525}},
Month = {{JUN}},
Abstract = {{Reservoir management decisions continuously strive to balance
   conflicting risks and benefits. Hydrologic uncertainty is a major
   complicating factor, especially during extreme events when data are
   sparse and probabilistic characterizations are less reliable. The set
   control approach offers an alternative by assuming that inputs are
   unknown but bounded. The solution provides a set of control actions
   which guarantee that the system will operate within the stated
   constraints for a certain time horizon. The method is applied to a
   three-reservoir system in the southeastern United States.}},
