@article{ ISI:A1993MA36800023,
Author = {NIKORA, VI and SAPOZHNIKOV, VB and NOEVER, DA},
Title = {{FRACTAL GEOMETRY OF INDIVIDUAL RIVER CHANNELS AND ITS
   COMPUTER-SIMULATION}},
