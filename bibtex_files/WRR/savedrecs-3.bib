@article{ ISI:A1993LR88400027,
Author = {CHENG, AHD and MOROHUNFOLA, OK},
Title = {{MULTILAYERED LEAKY AQUIFER SYSTEMS .1. PUMPING WELL SOLUTIONS}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{8}},
Pages = {{2787-2800}},
Month = {{AUG}},
Abstract = {{This paper presents an analytical solution of a radial flow to a pumping
   well at constant discharge located in a multilayered leaky aquifer
   system. The aquifer system is composed of alternating layer of aquifers
   and aquitards and is modeled after the theory of Neuman-Witherspoon and
   Herrera. The resulting coupled integrodifferential equation system is
   handled by the Laplace transform technique which removes not only the
   time variable but also the convolutional integral. Green's functions for
   the coupled elliptic partial differential equations are then sought.
   Utilizing a numerical Laplace inversion algorithm, the drawdown at any
   given time and location in any layer can be evaluated. Several examples
   are given for validation and exploration purposes.}},
