@article{ ISI:A1993LE49500029,
Author = {FENTON, GA and GRIFFITHS, DV},
Title = {{STATISTICS OF BLOCK CONDUCTIVITY THROUGH A SIMPLE BOUNDED STOCHASTIC
   MEDIUM}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{6}},
Pages = {{1825-1830}},
Month = {{JUN}},
Abstract = {{A Monte Carlo approach is employed to estimate the distribution of an
   equivalent conductivity measure, the block conductivity, which
   characterizes the total flow rate through a two-dimensional bounded
   domain and which is itself a random variable. Chi-square goodness-of-fit
   tests indicate that the lognormal distribution is an appropriate choice
   for the block conductivity distribution. For square domains, the mean
   and variance of the block conductivity are seen to be closely
   approximated using the statistics of local averages of log conductivity.}},
