@article{ ISI:A1993LR88400019,
Author = {MAUL, A and ELSHAARAWI, AH},
Title = {{STOCHASTIC-MODELS APPLIED TO CLUSTER-ANALYSIS OF CENSORED WATER-QUALITY
   DATA}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{1993}},
Volume = {{29}},
Number = {{8}},
Pages = {{2705-2711}},
Month = {{AUG}},
Abstract = {{A clustering method is presented which is appropriate when a mixture of
   multivariate distributions is used to model the data. The procedure
   assumes that the measurements made on several independent
   characteristics follow probability distributions of different nature
   (e.g., normal, Poisson, multinomial). Likelihood methods are used to
   both estimate the parameters of the models and perform the cluster
   analysis. The procedure is applicable when the data are subject to
   censoring. A data set on the concentrations of volatile halocarbon
   contaminant in Lake Ontario is used to illustrate the methods of this
   paper.}},
