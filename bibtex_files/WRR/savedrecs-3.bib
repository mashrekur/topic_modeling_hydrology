@article{ ISI:A1993LR88400036,
Author = {CHENG, AHD and ABOUSLEIMAN, Y and RUAN, F and LAFE, OE},
Title = {{BOUNDARY-ELEMENT SOLUTION FOR STOCHASTIC GROUNDWATER-FLOW - TEMPORAL
   WEAKLY STATIONARY PROBLEMS}},
