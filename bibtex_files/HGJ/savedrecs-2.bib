@article{ ISI:000401787800003,
Author = {Hunt, Randall J. and Johnson, William P.},
Title = {{Pathogen transport in groundwater systems: contrasts with traditional
   solute transport}},
Journal = {{HYDROGEOLOGY JOURNAL}},
