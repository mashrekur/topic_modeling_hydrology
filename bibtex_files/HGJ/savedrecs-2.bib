Title = {{Exploratory experiments to determine the effect of alternative
   operations on the efficiency of subsurface arsenic removal in rural
   Bangladesh}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2015}},
Volume = {{23}},
