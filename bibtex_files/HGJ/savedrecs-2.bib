@article{ ISI:000371083400009,
Author = {Strauhal, Thomas and Loew, Simon and Holzmann, Michael and Zangerl,
   Christian},
Title = {{Detailed hydrogeological analysis of a deep-seated rockslide at the
   Gepatsch reservoir (Klasgarten, Austria)}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2016}},
Volume = {{24}},
