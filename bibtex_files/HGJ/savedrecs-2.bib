@article{ ISI:000359776900005,
Author = {Gurarslan, Gurhan and Karahan, Halil},
Title = {{Solving inverse problems of groundwater-pollution-source identification
   using a differential evolution algorithm}},
Journal = {{HYDROGEOLOGY JOURNAL}},
