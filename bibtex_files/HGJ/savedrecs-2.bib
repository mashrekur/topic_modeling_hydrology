@article{ ISI:000354620800014,
Author = {Shapoori, V. and Peterson, T. J. and Western, A. W. and Costelloe, J. F.},
Title = {{Top-down groundwater hydrograph time-series modeling for climate-pumping
   decomposition}},
Journal = {{HYDROGEOLOGY JOURNAL}},
