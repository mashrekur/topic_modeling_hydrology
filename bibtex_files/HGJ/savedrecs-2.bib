@article{ ISI:000380090000015,
Author = {Bhusari, Vijay and Katpatal, Y. B. and Kundal, Pradeep},
Title = {{An innovative artificial recharge system to enhance groundwater storage
   in basaltic terrain: example from Maharashtra, India}},
Journal = {{HYDROGEOLOGY JOURNAL}},
