@article{ ISI:000345285600011,
Author = {Smerdon, Brian D. and Smith, Laura A. and Harrington, Glenn A. and
   Gardner, W. Payton and Delle Piane, Claudio and Sarout, Joel},
Title = {{Estimating the hydraulic properties of an aquitard from in situ pore
   pressure measurements}},
Journal = {{HYDROGEOLOGY JOURNAL}},
