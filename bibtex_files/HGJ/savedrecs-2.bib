@article{ ISI:000371083400017,
Author = {Filimonova, Elena A. and Baldenkov, Mikhail G.},
Title = {{A combined-water-system approach for tackling water scarcity:
   application to the Permilovo groundwater basin, Russia}},
Journal = {{HYDROGEOLOGY JOURNAL}},
