@article{ ISI:000394239800007,
Author = {Liu, Dan and Zhou, Jingjing and Zhang, Wenjing and Huan, Ying and Yu,
   Xipeng and Li, Fulin and Chen, Xuequn},
Title = {{Column experiments to investigate transport of colloidal humic acid
   through porous media during managed aquifer recharge}},
Journal = {{HYDROGEOLOGY JOURNAL}},
