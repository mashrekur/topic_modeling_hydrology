@article{ ISI:000363550100006,
Author = {Campana, Claudia and Fidelibus, Maria Dolores},
Title = {{Reactive-transport modelling of gypsum dissolution in a coastal karst
   aquifer in Puglia, southern Italy}},
Journal = {{HYDROGEOLOGY JOURNAL}},
