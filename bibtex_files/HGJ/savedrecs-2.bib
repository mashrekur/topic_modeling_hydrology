@article{ ISI:000371083400007,
Author = {Comunian, Alessandro and De Micheli, Leonardo and Lazzati, Claudio and
   Felletti, Fabrizio and Giacobbo, Francesca and Giudici, Mauro and
   Bersezio, Riccardo},
Title = {{Hierarchical simulation of aquifer heterogeneity: implications of
   different simulation settings on solute-transport modeling}},
Journal = {{HYDROGEOLOGY JOURNAL}},
