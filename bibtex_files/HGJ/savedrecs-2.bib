@article{ ISI:000334933000013,
Author = {Dafny, Elad and Silburn, D. Mark},
Title = {{The hydrogeology of the Condamine River Alluvial Aquifer, Australia: a
   critical assessment}},
Journal = {{HYDROGEOLOGY JOURNAL}},
