@article{ ISI:000228976800006,
Author = {Baker, VR and Dohm, JM and Fairen, AG and Ferre, TPA and Ferris, JC and
   Miyamoto, H and Schulze-Makuch, D},
Title = {{Extraterrestrial hydrogeology}},
Journal = {{HYDROGEOLOGY JOURNAL}},
