@article{ ISI:000175382900004,
Author = {Dim, JR and Sakura, Y and Fukami, H and Miyakoshi, A},
Title = {{Spatial characteristics of groundwater temperature in the Ishikari
   Lowland, Hokkaido, northern Japan: analytical and numerical applications}},
Journal = {{HYDROGEOLOGY JOURNAL}},
