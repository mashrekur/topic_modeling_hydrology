@article{ ISI:000174618800006,
Author = {Walker, GR and Zhang, L and Ellis, TW and Hatton, TJ and Petheram, C},
Title = {{Estimating impacts of changed land use on recharge: review of modelling
   and other approaches appropriate for management of dryland salinity}},
Journal = {{HYDROGEOLOGY JOURNAL}},
