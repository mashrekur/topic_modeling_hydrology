@article{ ISI:000083504300006,
Author = {Heinrichs, G and Udluft, P},
Title = {{Natural arsenic in Triassic rocks: A source of drinking-water
   contamination in Bavaria, Germany}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{1999}},
Volume = {{7}},
Number = {{5}},
Pages = {{468-476}},
Month = {{OCT}},
Abstract = {{The aquifer system of the Upper Triassic Keuper Sandstone, an important
   source of drinking water in northern Bavaria, is affected by elevated
   arsenic concentrations. Within the study area of 8000 km(2), no evidence
   exists for any artificial source of arsenic. Data from about 500 deep
   water wells show that in approximately 160 wells arsenic concentrations
   are 10-150 mu g/L. The regional distribution of arsenic in the
   groundwater shows that elevated arsenic concentrations are probably
   related to specific lithofacies of the aquifers that contain more
   sediments of terrestrial origin. Geochemical measurements on samples
   from four selected well cores show that arsenic has accumulated in the
   rocks. This indigenous arsenic is the source of arsenic in the
   groundwater of certain facies of the middle unit of the Keuper
   Sandstone.}},
