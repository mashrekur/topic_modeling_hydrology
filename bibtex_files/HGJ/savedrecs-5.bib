@article{ ISI:000082304000006,
Author = {Cresswell, R and Wischusen, J and Jacobson, G and Fifield, K},
Title = {{Assessment of recharge to groundwater systems in the arid southwestern
   part of Northern Territory, Australia, using chlorine-36}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{1999}},
Volume = {{7}},
Number = {{4}},
Pages = {{393-404}},
Month = {{AUG}},
Abstract = {{The sustainability of community water supplies drawn from shallow
   aquifers in the arid southwest of the Northern Territory has been
   evaluated using the radioactive isotope chlorine-36 (Cl-36). These
   aquifers include fractured sandstones of the Ngalia Basin, fractured
   metamorphic rocks and Cainozoic sands and gravels. Cl-36/Cl ratios for
   these shallow, regional groundwaters exhibit a bimodal distribution with
   peaks at 205 (+/-7) and 170 (+/-7)x10(-15). Th, higher ratio probably
   represents modern (Holocene) recharge, diluted with windblown salts from
   local playa lakes, and occurs mostly around the margin of the basin. The
   lower ratio corresponds to a Cl-36 ``age{''}, or mean residence time, of
   80-100 ka, implying that the last major recharge occurred during the
   last interglacial interval (Oxygen Isotope Stage 5). These values are
   mainly observed in the interior of the Ngalia Basin. Lower values of the
   Cl-36/Cl ratio measured near playa lakes are affected by addition of
   chloride from remobilised salts. Finite carbon-14 (C-14) data for the
   groundwaters are at variance with the Cl-36 results, but a depth profile
   suggests low recharge, allowing diffusion of recent atmospheric carbon
   to the water table. The Cl-36 results have important implications for
   groundwater management in this region, with substantial recharge only
   occurring during favourable, wet, interglacial climatic regimes; most
   community water supplies are dependent on these ``old{''} waters.}},
