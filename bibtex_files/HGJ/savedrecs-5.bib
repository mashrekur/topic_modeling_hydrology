@article{ ISI:000233528700017,
Author = {Perrochet, P},
Title = {{A simple solution to tunnel or well discharge under constant drawdown}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2005}},
Volume = {{13}},
Number = {{5-6}},
Pages = {{886-888}},
Month = {{OCT}},
Abstract = {{A simple analytical formula is developed to calculate transient
   discharge inflow rates into a tunnel or a well under constant drawdown.
   The agreement with the classical, but cumbersome
   diffusion-equation-based solution of Jacob and Lohman is excellent
   throughout the range of dimensionless times. By using only a
   straightforward logarithmic function, this explicit solution may
   therefore be used with great computational benefits in practice, and
   also when further mathematical manipulations such as differentiation or
   integration are required.}},
