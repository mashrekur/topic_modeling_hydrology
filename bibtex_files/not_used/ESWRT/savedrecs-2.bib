Author = {Petersen, Joerdis and Paschke, Albrecht and Gunold, Roman and
   Schueuermann, Gerrit},
Title = {{Calibration of Chemcatcher (R) passive sampler for selected highly
   hydrophobic organic substances under fresh and sea water conditions}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
