@article{ ISI:000366891000004,
Author = {Dwivedi, Charu and Pathak, Sanjay K. and Kumar, Manmohan and Tripathi,
   Subhash C. and Bajaj, Parma N.},
Title = {{Preparation and characterization of potassium nickel
   hexacyanoferrate-loaded hydrogel beads for the removal of cesium ions}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
Year = {{2015}},
Volume = {{1}},
Number = {{2}},
Pages = {{153-160}},
Abstract = {{A novel synthetic approach has been used to prepare sorbent hydrogel
   composite beads of potassium nickel hexacyanoferrate (KNiHCF), using PVA
   and cross-linked alginate as the binding matrix. The characterization
   study indicates the high hydrophilicity and high surface area of the
   synthesized beads. The application potential of these beads for removal
   of cesium ions from low level liquid nuclear waste has been studied in
   batch mode using a Cs-137 radiotracer. The equilibrium sorption and
   kinetic data are obtained at different initial cesium ion
   concentrations. These data are analysed using various sorption isotherm
   and kinetic models. It is observed that the sorption process can be best
   described by the Langmuir isotherm model, and the monolayer capacity of
   the beads is determined to be 7 mg per g of the swollen beads, which
   corresponds to similar to 64 mg per g of the dry beads. The sorption of
   cesium ions onto the sorbent beads is found to follow pseudo
   second-order kinetics over the entire studied concentration range. The
   ion-exchange mechanism involves the exchange of cesium ions with
   potassium ions. The mechanism of the sorption process is also
   investigated using the intraparticle diffusion model and Boyd's plot,
   and the results indicate that the sorption of cesium ions onto the beads
   is a complex process, involving both intraparticle diffusion and film
   diffusion.}},
