@article{ ISI:000379869600008,
Author = {Santillana, Gem E. and Smith, Heidi J. and Burr, Mark and Camper, Anne
   K.},
Title = {{Archaeal ammonium oxidation coupled with bacterial nitrite oxidation in
   a simulated drinking water premise plumbing system}},
