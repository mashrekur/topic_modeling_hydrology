@article{ ISI:000366891000001,
Author = {Davies, P. A.},
Title = {{Solar thermal decomposition of desalination reject brine for carbon
   dioxide removal and neutralisation of ocean acidity}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
Year = {{2015}},
Volume = {{1}},
Number = {{2}},
Pages = {{131-137}},
Abstract = {{Desalination plants could become net absorbers (rather than net
   emitters) of CO2. Thermal decomposition of salts in desalination reject
   brine can yield MgO which, added to the ocean, would take up CO2 through
   conversion to bicarbonate. The process proposed here comprises
   dewatering of brine followed by decomposition in a solar receiver using
   a heliostat field.}},
