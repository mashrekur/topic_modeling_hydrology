@article{ ISI:000368859800009,
Author = {Mautner, Andreas and Maples, Henry A. and Sehaqui, Houssine and
   Zimmermann, Tanja and Perez de Larraya, Uxua and Mathew, Aji P. and Lai,
   Chi Yan and Li, Kang and Bismarck, Alexander},
Title = {{Nitrate removal from water using a nanopaper ion-exchanger}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
Year = {{2016}},
Volume = {{2}},
Number = {{1}},
Pages = {{117-124}},
Abstract = {{Nitrates seriously affect drinking water quality. We herein present a
   process for the efficient removal of nitrates from water using a
   nanopaper ion-exchanger, which can be operated in flow-through
   conditions. The nanopaper ion-exchanger was produced from
   nanofibrillated cellulose obtained from fibre sludge, a paper-production
   waste stream, using a simple paper-making process. The cellulose
   nanofibrils were modified with quaternary trimethylammonium groups. The
   performance of these cationic nanopaper ion-exchangers was assessed with
   respect to their permeance and nitrate adsorption. Nitrates could be
   successfully captured onto the cationic nanopaper and thus rejected from
   contaminated water during dynamic filtration experiments. The
