@article{ ISI:000426117200010,
Author = {Shrestha, Narayan K. and Qamer, Faisal M. and Pedreros, Diego and
   Murthy, M. S. R. and Wahid, Shahriar Md. and Shrestha, Mandira},
Title = {{Evaluating the accuracy of Climate Hazard Group (CHG) satellite rainfall
   estimates for precipitation based drought monitoring in Koshi basin,
   Nepal}},
