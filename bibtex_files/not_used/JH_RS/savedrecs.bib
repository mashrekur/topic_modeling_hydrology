@article{ ISI:000426118300003,
Author = {Deemy, James B. and Rasmussen, Todd C.},
Title = {{Hydrology and water quality of isolated wetlands: Stormflow changes
   along two episodic flowpaths}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
Year = {{2017}},
