@article{ ISI:000500502700004,
Author = {Sabzi, Hamed Zamani and Moreno, Hernan A. and Fovargue, Rachel and Xue,
   Xianwu and Hong, Yang and Neeson, Thomas M.},
Title = {{Comparison of projected water availability and demand reveals future
   hotspots of water stress in the Red River basin, USA}},
