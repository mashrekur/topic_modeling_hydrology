@article{ ISI:000431062600003,
Author = {Pradhan, Nawa Raj and Loney, Drew},
Title = {{An analysis of the unit hydrograph peaking factor: A case study in Goose
   Creek Watershed, Virginia}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
