@article{ ISI:000415815000012,
Author = {Bern, Carleton R. and Stogner, Sr., Robert W.},
Title = {{The Niobrara Formation as a challenge to water quality in the Arkansas
   River, Colorado, USA}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
Year = {{2017}},
