@article{ ISI:000452568000009,
Author = {Lee, Eunhee and Jayakumar, Ramasamy and Shrestha, Sangam and Han,
   Zaisheng},
Title = {{Assessment of transboundary aquifer resources in Asia: Status and
   progress towards sustainable groundwater management}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
