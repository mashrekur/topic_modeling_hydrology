@article{ ISI:000430386500003,
Author = {Mohor, Guilherme Samprogna and Rodriguez, Daniel Andres and Tomasella,
   Javier and Siqueira Junior, Jose Lazaro},
Title = {{Exploratory analyses for the assessment of climate change impacts on the
   energy production in an Amazon run-of-river hydropower plant}},
