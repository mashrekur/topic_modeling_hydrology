@article{ ISI:000435608100005,
Author = {Diwakar, Jasmine and Johnston, Scott G. and Burton, Edward D. and Das
   Shrestha, Suresh},
Title = {{Arsenic mobilization in an alluvial aquifer of the Terai region, Nepal}},
