@article{ ISI:000431062600011,
Author = {Adane, Zablon A. and Nasta, Paolo and Zlotnik, Vitaly and Wedin, David},
Title = {{Impact of grassland conversion to forest on groundwater recharge in the
   Nebraska Sand Hills}},
