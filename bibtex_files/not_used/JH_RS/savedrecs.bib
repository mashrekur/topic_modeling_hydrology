@article{ ISI:000407357500017,
Author = {Jayakaran, A. D. and Smoot, Z. T. and Park, D. M. and Hitchcock, D. R.},
Title = {{Relating stream function and land cover in the Middle Pee Dee River
   Basin, SC}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
