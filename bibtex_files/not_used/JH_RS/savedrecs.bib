@article{ ISI:000435635700017,
Author = {Bishop, James M. and Glenn, Craig R. and Amato, Daniel W. and Dulai,
   Henrietta},
Title = {{Effect of land use and groundwater flow path on submarine groundwater
   discharge nutrient flux}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
