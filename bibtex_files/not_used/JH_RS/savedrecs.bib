@article{ ISI:000430386500032,
Author = {von Gunten, D. and Wohling, T. and Haslauer, C. P. and Merchan, D. and
   Causape, J. and Cirpka, O. A.},
Title = {{Estimating climate-change effects on a Mediterranean catchment under
   various irrigation conditions}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
