@article{ ISI:000477759200006,
Author = {Tsering, Tenzin and Wahed, Mahmoud S. M. Abdel and Iftekhar, Sidra and
   Sillanpaa, Mika},
Title = {{Major ion chemistry of the Teesta River in Sikkim Himalaya, India:
   Chemical weathering and assessment of water quality}},
