Title = {{Transboundary aquifers between Chihuahua, Coahuila, Nuevo Leon and
   Tamaulipas, Mexico, and Texas, USA: Identification and categorization}},
Journal = {{JOURNAL OF HYDROLOGY-REGIONAL STUDIES}},
Year = {{2018}},
