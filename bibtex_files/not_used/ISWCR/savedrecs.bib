@article{ ISI:000438879500004,
Author = {Akanji, Mutair A. and Oshunsanya, Suarau O. and Alomran, Abdulrasoul},
Title = {{Electrical conductivity method for predicting yields of two yam
   (Dioscorea alata) cultivars in a coarse textured soil}},
Journal = {{INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH}},
Year = {{2018}},
Volume = {{6}},
Number = {{3}},
Pages = {{230-236}},
Month = {{SEP}},
Abstract = {{Apparent Soil Electrical Conductivity (ECa) measurement is a rapid and
   accurate tool for measuring soil physical and chemical properties
   affecting crop productivity. This study uses ECa to predict yam yield.
   Soil ECa was measured at the depth of 0-15 cm, 15-30 cm and 30-45 cm
   using Miller 400D resistance meter with multi-electrode Wenner array.
   Soil samples were collected at the aforementioned depths and analyzed
   for selected physical and chemical properties. Two cultivars of water
   yam, Discorea alata L. (TDa 00/00194 (Dl) and TDa 00/00006 (D2)) were
   planted and yield data were collected after harvesting. Data collected
   were analyzed using correlation, nonlinear and multiple linear
   regression analysis using Origin statistical software (Pro. V.8.1). Soil
   ECa at 0-15 cm correlated with the yields of Dl and D2 with correlations
   (r) of 0.83 and 0.84, respectively. The relationship between ECa and Dl
   and D2 were best fit with a cubic function (with r(2) = 0.70 and 0.75,
   respectively). A Multiple linear regression model showed the interactive
   effect of soil physical and chemical properties as it affected the
   yields. The generated models showed that soil properties needed for
   growth and yields of Dl and D2 are different. Therefore, farmers should
   not plant both cultivars into the same soil environment or use blanket
   fertilizer application to achieve optimum performance. (C) 2018
   International Research and Training Center on Erosion and Sedimentation
   and China Water and Power Press. Production and Hosting by Elsevier B.V.}},
