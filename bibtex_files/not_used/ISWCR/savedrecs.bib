@article{ ISI:000517833900002,
Author = {Sadeghi, Seyed Hamidreza and Ghaffari, Gholam Ali and Rangavar,
   Abdulsaleh and Hazbavi, Zeinab and Singh, Vijay P.},
Title = {{Spatiotemporal distribution of soil moisture in gully facies}},
Journal = {{INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH}},
Year = {{2020}},
