@article{ ISI:000467786800005,
Author = {Asfaw, Daniel and Workineh, Getachew},
Title = {{Quantitative analysis of morphometry on Ribb and Gumara watersheds:
   Implications for soil and water conservation}},
Journal = {{INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH}},
