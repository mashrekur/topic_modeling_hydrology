@article{ ISI:000478576600001,
Author = {Alewell, Christine and Borrelli, Pasquale and Meusburger, Katrin and
   Panagos, Panos},
Title = {{Using the USLE: Chances, challenges and limitations of soil erosion
   modelling}},
Journal = {{INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH}},
