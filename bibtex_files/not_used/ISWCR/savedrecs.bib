@article{ ISI:000417101600005,
Author = {Busari, Mutiu Abolanle and Kukal, Surinder Singh and Kaur, Amanpreet and
   Bhatt, Rajan and Dulazi, Ashura Ally},
Title = {{Conservation tillage impacts on soil, crop and the environment}},
Journal = {{INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH}},
