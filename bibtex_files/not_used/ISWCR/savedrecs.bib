@article{ ISI:000417101900002,
Author = {Dong, Yuequn and Lei, Tingwu and Li, Shuqin and Yuan, Cuiping and Zhou,
   Shumei and Yang, Xiusheng},
Title = {{Effects of rye grass coverage on soil loss from loess slopes}},
Journal = {{INTERNATIONAL SOIL AND WATER CONSERVATION RESEARCH}},
Year = {{2015}},
Volume = {{3}},
Number = {{3}},
Pages = {{170-182}},
Month = {{SEP}},
Abstract = {{Vegetative coverage is commonly used to reduce urban slope soil erosion.
   Laboratory experimental study on soil erosion under grass covered slopes
   is conventionally time and space consuming. In this study, a new method
   is suggested to study the influences of vegetation coverage on soil
   erosion from a sloped loess sudlice under three slope gradients of 5
   degrees, 15 degrees, and 25 degrees; four rye grass coverages of 0\%,
   25\%, 50\%, and 75\%; and three rainfall intensities of 60, 90, and 120
   min/h with a silt-loamy loess soil. Rye grasses were planted in the
   field with the studied soil before being transplanted into a laboratory
   flume. Grass was allowed to resume growth for a period before the rain
   simulation experiment. Results showed that the grass cover reduced soil
   erosion by 63.90\% to 92.75\% and sediment transport rate by 80.59\% to
   96.17\% under different slope gradients and rainfall intensities. The
   sediment concentration/sediment transport rate from bare slope was
   significantly higher than from a grass-covered slope. The sediment
   concentration/transport rate from grass-covered slopes decreased
   linearly with grass coverage and increased with rainfall intensity. The
   sediment concentration/transport rate from the bare slope increased as a
   power function of slope and reached the maximum value at the gradient of
   about 25 degrees, whereas that from grass-covered slope increased
   linearly and at much lower levels. The results of this study can he used
   to estimate the effect of vegetation on soil erosion from loess slopes.
   (C) 2015 International Research and Training Center on Erosion and
   Sedimentation and China Water and Power Press. Production and Hosting by
   Elsevier B.V. This is an open access article under the CC BY-NC-ND
