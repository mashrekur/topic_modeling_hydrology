@article{ ISI:000458695600007,
Author = {Benioug, M. and Golfier, F. and Fischer, P. and Oltean, C. and Bues, M.
   A. and Yang, X.},
Title = {{Interaction between biofilm growth and NAPL remediation: A pore-scale
   study}},
Journal = {{ADVANCES IN WATER RESOURCES}},
