@article{ ISI:000432555400006,
Author = {Schmidt, Susanne I. and Kreft, Jan-Ulrich and Mackay, Rae and
   Picioreanu, Cristian and Thullner, Martin},
Title = {{Elucidating the impact of micro-scale heterogeneous bacterial
   distribution on biodegradation}},
