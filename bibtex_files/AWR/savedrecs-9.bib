@article{ ISI:000485107000012,
Author = {Pahar, Gourabananda and Dhar, Anirban},
Title = {{On information coupling in hybrid ISPH framework for fluidized granular
   systems}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2019}},
Volume = {{131}},
Month = {{SEP}},
Abstract = {{An improved information transfer mechanism is developed to couple fluid
   and granular modules for Incompressible Smoothed Particle Hydrodynamics
   modelling of sediment transport. The interaction force pair necessitates
   effective projection of information among parallel continuum based
   modules. The particle position in either module is susceptible to
   considerable variation due to their Lagrangian nature. An attempt has
   been made to minimize the effective thickness of the diffused interface
   between the fluid and granular particles. The proposed model reduces
   artificial drag force resulting from diffused computation of effective
   porosity. The revised framework outperforms in coarse resolution
   compared to its existing counterpart.}},
