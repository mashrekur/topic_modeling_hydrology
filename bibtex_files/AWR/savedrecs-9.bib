@article{ ISI:000418592800003,
Author = {Craciun, Maria and Vamos, Calin and Suciu, Nicolae},
Title = {{Analysis and generation of groundwater concentration time series}},
Journal = {{ADVANCES IN WATER RESOURCES}},
