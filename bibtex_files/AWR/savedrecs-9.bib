@article{ ISI:000427990700015,
Author = {Bertassello, Leonardo Enrico and Rao, P. Suresh C. and Park, Jeryang and
   Jawitz, James W. and Botter, Gianluca},
Title = {{Stochastic modeling of wetland-groundwater systems}},
