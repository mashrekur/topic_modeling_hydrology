@article{ ISI:000475554300003,
Author = {Katzourakis, Vasileios E. and Chrysikopoulos, V, Constantinos},
Title = {{Two-site colloid transport with reversible and irreversible attachment:
   Analytical solutions}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2019}},
Volume = {{130}},
Pages = {{29-36}},
Month = {{AUG}},
Abstract = {{Analytical solutions for colloid transport in water saturated,
   one-dimensional, homogeneous porous media, under fully developed uniform
   flow are presented. The colloids can be either suspended in the aqueous
   phase or attached reversibly and/or irreversibly onto the solid matrix.
   Both attachment rates are assumed linear, and any possible blocking or
   ripening effects are neglected. The colloids can be either of neutral
   density or denser than the water. For dense colloids, gravity effects
   are accounted for. Gravity may enhance or hinder the migration of dense
   colloids, depending on the flow direction. Upstream boundary conditions
   for both instantaneous and broad-pulse colloid injections are accounted
   for. The new analytical solutions were used to successfully fit
   available experimental data.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND}},
