@article{ ISI:000181617200012,
Author = {Thomson, AM and Brown, RA and Rosenberg, NJ and Izaurralde, RC and
   Legler, DM and Srinivasan, R},
Title = {{Simulated impacts of El Nino/Southern Oscillation on United States water
   resources}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2003}},
Volume = {{39}},
Number = {{1}},
Pages = {{137-148}},
Month = {{FEB}},
Abstract = {{The El Ni (n) over tildeo/Southern Oscillation (ENSO) phenomena alter
   global weather patterns with consequences for fresh water supply. ENSO
   events impact regions and their natural resource sectors around the
   globe. For example, in 1997 and 1998, a strong El Ni (n) over tildeo
   brought warm ocean temperatures, flooding, and record snowfall to the
   west coast of the United States. Research on ENSO events has improved
   long range climate predictions, affording the potential to reduce the
   damage and economic cost of these weather patterns. Here, using the
   Hydrologic Unit Model for the United States (HUMUS), we simulate the
   impacts of four types of ENSO states (Neutral, El Ni (n) over tildeo, La
   Ni (n) over tildea, and strong El Ni (n) over tildeo) on water resources
   in the conterminous United States. The simulations show that La Ni (n)
   over tildea conditions increase water yield across much of the country.
   We find that water yield increases during El Nift years across the south
   while declining in much of the rest of the country. However, under
   strong El Ni (n) over tildeo conditions, regional water yields are much
   higher than Neutral, especially along the West Coast. Strong El Ni (n)
   over tildeo is not simply an amplification of El Ni (n) over tildeo; it
   leads to strikingly different patterns of water resource response.}},
