@article{ ISI:000228985800009,
Author = {Bakhsh, A and Kanwar, RS},
Title = {{Spatial clusters of subsurface drainage water NO3-N leaching losses}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2005}},
Volume = {{41}},
