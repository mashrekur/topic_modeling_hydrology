@article{ ISI:000176769300015,
Author = {Zipper, CE and Holtzman, GL and Darken, PF and Gildea, JJ and Stewart,
   RE},
Title = {{Virginia USA water quality, 1978 to 1995: Regional interpretation}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2002}},
Volume = {{38}},
Number = {{3}},
Pages = {{789-802}},
Month = {{JUN}},
Abstract = {{Nine surface water-quality variables were analyzed for trend at 180
   Virginia locations over the 1978 to 1995 period. Median values and
   seasonal Kendall's tau, a trend indicator statistic, were generated for
   dissolved oxygen saturation (DO), biochemical oxygen demand (BOD), pH
   (PH), total residue (TR), nonfilterable residue (NFR), nitrate-nitrite
   nitrogen (NN), total Kjeldahl nitrogen (TKN), total phosphorus (TP), and
   fecal coliform (FC) at each location. Each location was assigned to one
   of four physiographic regions, and mean state and regional medians and
   taus were calculated. Widespread BOD and NFR improvements were detected
   and FC improvements occurred in the state's western regions. TR and TKN
   exhibited predominantly increasing trends at locations throughout the
   state. BOD, TKN, NFR, and TR medians were higher at coastal locations
   than in other regions, NN, TKN, and TR exhibited predominantly
   increasing trends in regions with high median concentrations, while
   declining trends predominated in regions with relatively high BOD, FC,
   and NFR medians. Appalachian locations exhibited the greatest regional
   water-quality improvements for BOD, FC, NFR, and TKN. Factors
   responsible for regional differences appear to include geology, land
   use, and landscape features; these factors vary regionally.}},
