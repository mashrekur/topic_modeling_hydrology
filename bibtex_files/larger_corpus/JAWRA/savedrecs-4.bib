@article{ ISI:000175188700010,
Author = {Eisen-Hecht, JI and Kramer, RA},
Title = {{A cost-benefit analysis of water quality protection in the Catawba basin}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2002}},
Volume = {{38}},
Number = {{2}},
Pages = {{453-465}},
Month = {{APR}},
Abstract = {{The primary objective of this study was to perform a cost-benefit
   analysis of maintaining the current level of water quality in the
   Catawba River basin. Economic benefits were estimated using a stated
   preference survey method designed to value respondents' willingness to
   pay for a management plan to protect water quality in the Catawba basin
   over time. From the surveys conducted with 1,085 area residents, we
   calculated an annual mean willingness to pay of \$139 for the management
   plan, or more than \$75.4 million for all taxpayers in the area. Over
   the five-year time horizon in which respondents were asked to pay for
   the management plan, this resulted in a total economic benefit of
   \$340.1 million. The Watershed Analysis Risk Management Framework model
   was used to estimate the amount of management activities needed to
   protect the current level of water quality in the basin over time. Based
   on the model results, the total cost of the management plan was
   calculated to be \$244.8 million over a ten-year period. The resulting
   cost-benefit analysis indicated that the potential benefits of this
   management plan would outweigh the costs by more than \$95 million.}},
