@article{ ISI:000236554000019,
Author = {Dwire, KA and Ryan, SE and Shirley, LJ and Lytjen, D and Otting, N and
   Dixon, MK},
Title = {{Influence of herbivory on regrowth of riparian shrubs following a
   wildland fire}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2006}},
Volume = {{42}},
Number = {{1}},
