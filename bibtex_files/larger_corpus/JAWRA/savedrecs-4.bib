@article{ ISI:000186238800012,
Author = {Chen, L},
Title = {{Real coded genetic algorithm optimization of long term reservoir
   operation}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
