@article{ ISI:000186238800020,
Author = {Richards, RP and Grabow, GL},
Title = {{Detecting reductions in sediment loads associated with Ohio's
   conservation reserve enhancement program}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2003}},
Volume = {{39}},
Number = {{5}},
Pages = {{1261-1268}},
Month = {{OCT}},
Abstract = {{Small systematic changes in loads or concentrations of water quality
   constituents are difficult to detect against the background of short
   term fluctuations ({''}noise{''}) that result from weather and climate
   effects. Minimum Detectable Change Analysis (MDCA) uses prior knowledge
   of a water quality constituent to determine how much change must occur
   (e.g., from implementation of conservation practices) for the change to
   be statistically significant. In this paper we use MDCA to determine
   whether the goal of the Ohio Lake Erie Conservation Reserve Enhancement
   Program (CREP), to reduce sediment loads by an average of 6 percent over
   10 years, represents a large enough change to be detected. We conclude
   that this amount of change is unlikely to be detected as statistically
   significant, even with the high frequency sampling program planned for
   evaluating it. The minimum detectable change ranges from about 7 to 9
   percent for three different rivers.}},
