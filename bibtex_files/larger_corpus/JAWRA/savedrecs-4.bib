@article{ ISI:000224721900016,
Author = {Mishra, SK and Sansalone, JJ and Glenn, DW and Singh, VP},
Title = {{PCN based metal partitioning in urban snowmelt, rainfall/runoff, and
   river flow systems}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2004}},
Volume = {{40}},
Number = {{5}},
Pages = {{1315-1337}},
Month = {{OCT}},
Abstract = {{Drawing an analogy between the popular Soil Conservation Service curve
   number (SCS-CN) method based infiltration and metal sorption processes,
   a new partitioning curve number (PCN) approach is suggested for
   partitioning of heavy metals into dissolved and particulate bound forms
   in urban snowmelt, rainfall/runoff, and river flow environments. The
   parameters, the potential maximum desorption, psi, and the PCN analogous
   to the SCS-CN parameters S and CN, respectively, are introduced. Under
   the condition of snowmelt, PCN (or psi) is found to generally rely on
   temperature, relative humidity, pH, and chloride content; during a
   rainstorm, W is found to depend on the alkalinity and the pH of the
   rainwater; and in the river flow situation, PCN is found to generally
   depend on the temperature, pH, and chloride content. The advantage of
   using PCN instead of the widely used partitioning parameter, K-d, is
   found to lie in the PCN's efficacy to distinguish the adsorption (or
   sorption) behavior of metals in the above snowmelt, rainfall/runoff, and
   river flow situations, analogous to the hydrological behavior of
   watersheds.}},
