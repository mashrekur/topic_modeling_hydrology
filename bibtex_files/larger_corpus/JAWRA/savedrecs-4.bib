@article{ ISI:000222484600015,
Author = {Fox, GA},
Title = {{Evaluation of a stream aquifer analysis test using analytical solutions
   and field data}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
