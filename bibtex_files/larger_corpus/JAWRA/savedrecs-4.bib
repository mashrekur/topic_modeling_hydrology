@article{ ISI:000179273400012,
Author = {Schneiderman, EM and Pierson, DC and Lounsbury, DG and Zion, MS},
Title = {{Modeling the hydrochemistry of the Cannonsville watershed with
   Generalized Watershed Loading Functions (GWLF)}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
