@article{ ISI:000221322300014,
Author = {Liu, CW},
Title = {{Decision support system for managing ground water resources in the
   Choushui River alluvial in Taiwan}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
