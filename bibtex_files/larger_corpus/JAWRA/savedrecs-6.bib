@article{ ISI:A1997WX81100009,
Author = {Fontaine, TA and Jacomino, VMF},
Title = {{Sensitivity analysis of simulated contaminated sediment transport}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
