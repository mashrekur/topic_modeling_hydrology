@article{ ISI:A1997XD79900001,
Author = {AbuZeid, M and Lum, K},
Title = {{Global water issues and the World Water Council}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
