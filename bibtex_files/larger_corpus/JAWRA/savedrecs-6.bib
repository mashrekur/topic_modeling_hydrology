@article{ ISI:A1997XD79900008,
Author = {Chang, WYB},
Title = {{ENSO: Extreme climate events and their impacts on Asian deltas}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
