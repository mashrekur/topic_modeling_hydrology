@article{ ISI:000087115000003,
Author = {Morehouse, BJ},
Title = {{Climate impacts on urban water resources in the southwest: The
   importance of context}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2000}},
Volume = {{36}},
Number = {{2}},
Pages = {{265-277}},
Month = {{APR}},
Abstract = {{Stresses on water resources in the Southwest take many forms and emanate
   from many different sources, among which are complex institutional
   arrangements, significant areal and temporal climatic variability, and
   high urban growth rates. Further challenges to managing supply and
   demand in this water-scarce region are posed by environmental, social,
   and legal differences within and between the individual urban areas.
   Analysis of the sensitivity of the urban water sector in the Southwest
   to climatic variability requires careful consideration of these factors.
   Such analysis, in turn, provides an essential foundation for effective
   evaluation of the region's sensitivity to longer term climate change.}},
