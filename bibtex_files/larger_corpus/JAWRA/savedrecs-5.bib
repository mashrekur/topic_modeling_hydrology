@article{ ISI:000075700000008,
Author = {Dunne, T},
Title = {{Critical data requirements for prediction of erosion and sedimentation
   in mountain drainage basins}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
