@article{ ISI:000075700000015,
Author = {Qiu, ZY and Prato, T},
Title = {{Economic evaluation of riparian buffers in an agricultural watershed}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{1998}},
