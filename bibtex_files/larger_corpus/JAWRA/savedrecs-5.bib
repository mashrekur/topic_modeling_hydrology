@article{ ISI:000078134800012,
Author = {Wang, KH and Cleveland, TG and Towsley, C and Umrigar, D},
Title = {{Head loss at manholes in surcharged sewer systems}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{1998}},
Volume = {{34}},
Number = {{6}},
Pages = {{1391-1400}},
Month = {{DEC}},
Abstract = {{A laboratory investigation was conducted to determine the head losses at
   sewer pipe junctions (manholes) under surcharged conditions. A physical
   model of a manhole/pipeline system was constructed for head-loss
   measurements. Head-loss coefficients were determined for a variety of
   outlet-flow Reynolds numbers, surcharge levels, pipe sizes, flow
   configurations, and inlet-flow rates. Empirical formulas were also
   developed to estimate head-loss coefficients. The results indicate that
   head loss is insensitive to the amount of surcharge, but depends heavily
   on the now configuration, relative flow rate, and the change of pipe
   diameter within the pipelines.}},
