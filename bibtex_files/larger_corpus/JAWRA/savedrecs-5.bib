@article{ ISI:000083224900023,
Author = {Szilagyi, J and Parlange, MB},
Title = {{Defining watershed-scale evaporation using a normalized difference
   vegetation index}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{1999}},
Volume = {{35}},
Number = {{5}},
Pages = {{1245-1255}},
Month = {{OCT}},
Abstract = {{Monthly composites of the Normalized Difference Vegetation Indices
   (NDVI), derived from the National Oceanic and Atmospheric
   Administration's (NOAA) Advanced Very High Resolution Radiometer
   (AVHRR), were transformed linearly into monthly evaporation rates and
   compared with detailed hydrologic-model simulation results for five
   watersheds across the United States. Model-simulated monthly evaporation
   values showed high correlations (mean R-2 =.77) with NDVI-derived
   evaporation estimates. These latter estimates, used in a classical water
   balance model, resulted in equally accurate simulations of monthly
   runoff than when the model was run to estimate monthly evaporation via
   soil moisture accounting. Comparison of NDVI-derived evaporation
   estimates with pan data showed promise for transforming NDVI values into
   evaporation estimates under both wet and water-limiting conditions
   without resorting to the application of any kind of calibrated
   hydrologic models.}},
