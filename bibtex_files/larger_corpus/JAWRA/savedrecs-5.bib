@article{ ISI:000088737800017,
Author = {Keplinger, KO and McCarl, BA},
Title = {{An evaluation of the 1997 Edwards Aquifer Irrigation Suspension}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2000}},
Volume = {{36}},
Number = {{4}},
Pages = {{889-901}},
Month = {{AUG}},
Abstract = {{In early 1997, the Texas Edwards Aquifer Authority implemented a pilot
   Irrigation Suspension Program with the objectives of increasing
   springflow and providing relief to municipalities during drought.
   Irrigators were paid an average of \$234 per acre to suspend water use,
   a price higher than regional land rental rates. Auction theory and
   program implementation details suggest that the program implementation
   partially caused inflated bids. The irrigation Suspension Program is
   also compared to two alternative programs: (1) subsidizing more
   efficient irrigation technology and (2) buying land. The irrigation
   suspension is found to be more cost-effective relative to subsidizing
   improved irrigation efficiency because it can be put in place only when
   aquifer levels are low. Land purchase is a cheaper alternative if the
   bid levels remain at the levels observed.}},
