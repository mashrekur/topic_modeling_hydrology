@article{ ISI:000170052100008,
Author = {Shultz, SD and Fridgen, PM},
Title = {{Floodplains and housing values: Implications for flood mitigation
   projects}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2001}},
Volume = {{37}},
Number = {{3}},
Pages = {{595-603}},
Month = {{JUN}},
Abstract = {{The hedonic valuation method was used to quantify the impact of
   floodplain location on housing values in Fargo-Moorhead. Being located
   in the 100-year floodplain lowered the home values by \$8,990, and such
   homes were worth \$10,241 less than similar homes located outside the
   floodplain before the major flood event of 1997. Required flood
   insurance premiums for these homes were determined to account for
   approximately 81 percent of this price depreciation. In contrast, homes
   in the 500-year floodplain were worth \$3,100 more than similar homes
   not in the floodplain. It was concluded that more disclosure is needed
   regarding the location of the 500-year floodplain, and that the hedonic
   valuation method can be used to calculate the economic gains and losses
   associated with flood mitigation projects or floodplain remapping
   efforts that result in the reclassification of the legal floodplain
   status of individual homes.}},
