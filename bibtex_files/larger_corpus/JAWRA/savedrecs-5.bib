@article{ ISI:000173379400018,
Author = {Toledo, ZO and Kauffman, JB},
Title = {{Root biomass in relation to channel morphology of headwater streams}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2001}},
Volume = {{37}},
