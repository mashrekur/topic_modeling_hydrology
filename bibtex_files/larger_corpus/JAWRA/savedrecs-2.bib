@article{ ISI:000349151900005,
Author = {Doll, Barbara A. and Jennings, Gregory D. and Spooner, Jean and Penrose,
   David L. and Usset, Joseph L.},
Title = {{EVALUATING THE ECO-GEOMORPHOLOGICAL CONDITION OF RESTORED STREAMS USING
   VISUAL ASSESSMENT AND MACROINVERTEBRATE METRICS}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
