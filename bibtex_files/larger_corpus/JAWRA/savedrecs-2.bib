@article{ ISI:000291224900006,
Author = {Koutsoyiannis, Demetris},
Title = {{Hurst-Kolmogorov Dynamics and Uncertainty}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
