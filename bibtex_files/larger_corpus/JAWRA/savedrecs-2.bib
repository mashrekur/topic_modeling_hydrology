@article{ ISI:000333687000003,
Author = {Battaglin, W. A. and Meyer, M. T. and Kuivila, K. M. and Dietze, J. E.},
Title = {{GLYPHOSATE AND ITS DEGRADATION PRODUCT AMPA OCCUR FREQUENTLY AND WIDELY
   IN US SOILS, SURFACE WATER, GROUNDWATER, AND PRECIPITATION(1)}},
