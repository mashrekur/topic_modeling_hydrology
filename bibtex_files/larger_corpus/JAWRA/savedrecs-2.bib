@article{ ISI:000293092200010,
Author = {Andrews, Danielle M. and Barton, Christopher D. and Kolka, Randall K.
   and Rhoades, Charles C. and Dattilo, Adam J.},
Title = {{SOIL AND WATER CHARACTERISTICS IN RESTORED CANEBRAKE AND FOREST RIPARIAN
   ZONES}},
