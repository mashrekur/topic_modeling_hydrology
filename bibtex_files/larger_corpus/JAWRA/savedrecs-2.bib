@article{ ISI:000284769000010,
Author = {Meierdiercks, Katherine L. and Smith, James A. and Baeck, Mary Lynn and
   Miller, Andrew J.},
Title = {{Heterogeneity of Hydrologic Response in Urban Watersheds}},
