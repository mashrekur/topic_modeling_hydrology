@article{ ISI:000322580500011,
Author = {Grady, Caitlin A. and Reimer, Adam P. and Frankenberger, Jane and
   Prokopy, Linda Stalker},
Title = {{LOCATING EXISTING BEST MANAGEMENT PRACTICES WITHIN A WATERSHED: THE
   VALUE OF MULTIPLE METHODS}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2013}},
Volume = {{49}},
Number = {{4}},
Pages = {{883-895}},
Month = {{AUG}},
Abstract = {{There is an increasing need to document the impacts of
   conservation-related best management practices (BMPs) on water quality
   within a watershed. However, this impact analysis depends upon accurate
   geospatial locations of existing practices, which are difficult to
   obtain. This study demonstrates and evaluates three different methods
   for obtaining geospatial information for BMPs. This study was focused on
   the Eagle Creek Watershed, a mixed use watershed in central Indiana. We
   obtained geospatial information for BMPs through government records,
   producer interviews, and remote-sensing aerial photo interpretation.
   Aerial photos were also used to validate the government records and
   producer interviews. This study shows the variation in results obtained
   from the three sources of information as well as the benefits and
   drawbacks of each method. Using only one method for obtaining BMP
   information can be incomplete, and this study demonstrates how multiple
   methods can be used for the most accurate picture.}},
