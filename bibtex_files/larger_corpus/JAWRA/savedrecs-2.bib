@article{ ISI:000314755700013,
Author = {Kinzel, Paul J. and Legleiter, Carl J. and Nelson, Jonathan M.},
Title = {{Mapping River Bathymetry With a Small Footprint Green LiDAR:
   Applications and Challenges}},
