@article{ ISI:000322580500004,
Author = {Stein, Eric D. and Cover, Matthew R. and Fetscher, A. Elizabeth and
   O'Reilly, Clare and Guardado, Roxana and Solek, Christopher W.},
Title = {{REACH-SCALE GEOMORPHIC AND BIOLOGICAL EFFECTS OF LOCALIZED STREAMBANK
   ARMORING}},
