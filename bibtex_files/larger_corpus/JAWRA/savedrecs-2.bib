@article{ ISI:000345773900007,
Author = {Evans, Daniel M. and Zipper, Carl E. and Donovan, Patricia F. and
   Daniels, W. Lee},
Title = {{LONG-TERM TRENDS OF SPECIFIC CONDUCTANCE IN WATERS DISCHARGED BY
   COAL-MINE VALLEY FILLS IN CENTRAL APPALACHIA, USA}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2014}},
Volume = {{50}},
Number = {{6}},
Pages = {{1449-1460}},
Month = {{DEC}},
Abstract = {{Anthropogenic salinization of freshwaters is a global concern. Coal
   surface mining causes release of dissolved sulfate, bicarbonate,
   calcium, magnesium, and other ions to surface waters in central
   Appalachia, USA, through practices that include mine rock disposal in
   valley fills (VFs). This region's surface waters naturally have low
   salinity, with specific conductance (SC, a salinity indicator) generally
   <200S/cm, and aquatic impacts have been found when SC exceeds the 300 to
   500S/cm range. We analyzed SC in waters emerging from 137 VFs over
   periods of 1 to 23years. Mean SCs for these VFs ranged from 227 to
   2,866S/cm, generally rose during and immediately following construction,
   but often declined during latter portions of longer monitoring records.
   Seventy-four of 103 VFs with postconstruction data had SC trends that
   fit negative quadratic forms. Of the 16 revegetated VFs with at least
   five years of SC data past the quadratic maximum, the mean quadratic
   maximum was 1,464 (+/- 696) S/cm and the model projected time required
   to approach natural conditions (by declining to <500S/cm) was 19.6 (+/-
   6.6) years after VF construction initiation, indicating long-lasting but
   not permanent aquatic impacts due to elevated (>500S/cm) SC.}},
