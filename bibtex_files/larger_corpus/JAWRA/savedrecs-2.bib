@article{ ISI:000313264000003,
Author = {Ludwig, Andrea and Matlock, Marty and Haggard, Brian and Chaubey,
   Indrajeet},
Title = {{PERIPHYTON NUTRIENT LIMITATION AND MAXIMUM POTENTIAL PRODUCTIVITY IN THE
   BEAVER LAKE BASIN, UNITED STATES}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
