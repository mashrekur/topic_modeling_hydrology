Author = {Sun, Ranhao and Wang, Zhaoming and Chen, Liding and Wang, Wei},
Title = {{ASSESSMENT OF SURFACE WATER QUALITY AT LARGE WATERSHED SCALE: LAND-USE,
   ANTHROPOGENIC, AND ADMINISTRATIVE IMPACTS}},
