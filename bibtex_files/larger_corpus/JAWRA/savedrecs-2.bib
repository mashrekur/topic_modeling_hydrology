@article{ ISI:000278522700001,
Author = {Elcock, Deborah},
Title = {{Future US Water Consumption: The Role of Energy Production1}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
