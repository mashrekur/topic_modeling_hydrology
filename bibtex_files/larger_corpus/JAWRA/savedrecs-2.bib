@article{ ISI:000284769000011,
Author = {Colby, Elizabeth J. Hilkert and Young, Thomas M. and Green, Peter G. and
   Darby, Jeannie L.},
Title = {{Costs of Arsenic Treatment for Potable Water in California and
   Comparison to U.S. Environmental Protection Agency Affordability Metrics}},
