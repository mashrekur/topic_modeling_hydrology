@article{ ISI:000313264000007,
Author = {Walton-Day, Katherine and Runkel, Robert L. and Kimball, Briant A.},
Title = {{USING SPATIALLY DETAILED WATER-QUALITY DATA AND SOLUTE-TRANSPORT
   MODELING TO SUPPORT TOTAL MAXIMUM DAILY LOAD DEVELOPMENT}},
