@article{ ISI:000296147200005,
Author = {Maupin, Molly A. and Ivahnenko, Tamara},
Title = {{NUTRIENT LOADINGS TO STREAMS OF THE CONTINENTAL UNITED STATES FROM
   MUNICIPAL AND INDUSTRIAL EFFLUENT}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
