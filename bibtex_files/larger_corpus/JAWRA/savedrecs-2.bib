@article{ ISI:000333687000005,
Author = {Thomas, Kevin V. and Araujo da Silva, Felipe M. and Langford, K. H. and
   Leao de Souza, Afonso D. and Nizzeto, L. and Waichman, A. V.},
Title = {{SCREENING FOR SELECTED HUMAN PHARMACEUTICALS AND COCAINE IN THE URBAN
