@article{ ISI:000345773900014,
Author = {Kinoshita, Alicia M. and Hogue, Terri S. and Napper, Carolyn},
Title = {{EVALUATING PRE-AND POST-FIRE PEAK DISCHARGE PREDICTIONS ACROSS WESTERN
   US WATERSHEDS}},
