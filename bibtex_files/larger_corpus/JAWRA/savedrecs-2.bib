@article{ ISI:000342890600009,
Author = {Salman, Dina A. and Amer, Saud A. and Ward, Frank A.},
Title = {{WATER APPROPRIATION SYSTEMS FOR ADAPTING TO WATER SHORTAGES IN IRAQ}},
