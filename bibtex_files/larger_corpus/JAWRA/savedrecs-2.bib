@article{ ISI:000340462900014,
Author = {Wang, Hsiao-Wen and Kondolf, G. Mathias},
Title = {{UPSTREAM SEDIMENT-CONTROL DAMS: FIVE DECADES OF EXPERIENCE IN THE
   RAPIDLY ERODING DAHAN RIVER BASIN, TAIWAN}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
