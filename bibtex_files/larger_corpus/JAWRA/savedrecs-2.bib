@article{ ISI:000313264200001,
Author = {Ator, Scott W. and Denver, Judith M.},
Title = {{Estimating Contributions of Nitrate and Herbicides From Groundwater to
   Headwater Streams, Northern Atlantsdic Coastal Plain, United States}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
