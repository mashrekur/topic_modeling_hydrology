@article{ ISI:000322580500015,
Author = {Tavernia, Brian G. and Nelson, Mark D. and Caldwell, Peter and Sun, Ge},
Title = {{WATER STRESS PROJECTIONS FOR THE NORTHEASTERN AND MIDWESTERN UNITED
   STATES IN 2060: ANTHROPOGENIC AND ECOLOGICAL CONSEQUENCES}},
