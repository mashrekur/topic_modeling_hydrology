@article{ ISI:000286629700016,
Author = {Brooks, Robert T. and Colburn, Elizabeth A.},
Title = {{Extent and Channel Morphology of Unmapped Headwater Stream Segments of
   the Quabbin Watershed, Massachusetts1}},
