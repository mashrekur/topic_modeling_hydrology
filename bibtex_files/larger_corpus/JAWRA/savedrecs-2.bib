@article{ ISI:000349151900006,
Author = {Callahan, Michael K. and Rains, Mark C. and Bellino, Jason C. and
   Walker, Coowe M. and Baird, Steven J. and Whigham, Dennis F. and King,
   Ryan S.},
Title = {{CONTROLS ON TEMPERATURE IN SALMONID-BEARING HEADWATER STREAMS IN TWO
   COMMON HYDROGEOLOGIC SETTINGS, KENAI PENINSULA, ALASKA}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
