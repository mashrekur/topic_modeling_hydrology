@article{ ISI:000293092200011,
Author = {Laforce, Serge and Simard, Marie-Claude and Leconte, Robert and
   Brissette, Francois},
Title = {{CLIMATE CHANGE AND FLOODPLAIN DELINEATION IN TWO SOUTHERN QUEBEC RIVER
   BASINS}},
