@article{ ISI:000297850000007,
Author = {Poff, Boris and Koestner, Karen A. and Neary, Daniel G. and Henderson,
   Victoria},
Title = {{Threats to Riparian Ecosystems in Western North America: An Analysis of
   Existing Literature}},
