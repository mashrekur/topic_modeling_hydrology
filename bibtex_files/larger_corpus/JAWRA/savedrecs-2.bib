Author = {DiFrancesco, Kara N. and Tullos, Desiree D.},
Title = {{FLEXIBILITY IN WATER RESOURCES MANAGEMENT: REVIEW OF CONCEPTS AND
   DEVELOPMENT OF ASSESSMENT MEASURES FOR FLOOD MANAGEMENT SYSTEMS}},
