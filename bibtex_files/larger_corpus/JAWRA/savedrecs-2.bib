@article{ ISI:000317001700013,
Author = {Waite, Ian R.},
Title = {{Development and Application of an Agricultural Intensity Index to
   Invertebrate and Algal Metrics from Streams at Two Scales}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2013}},
Volume = {{49}},
