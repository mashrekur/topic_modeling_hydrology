@article{ ISI:000315543100008,
Author = {Zhang, Wei and Rattanaudompol, U-sa and Li, Hui and Bouchard, Dermont},
Title = {{Effects of humic and fulvic acids on aggregation of aqu/nC(60)
   nanoparticles}},
Journal = {{WATER RESEARCH}},
