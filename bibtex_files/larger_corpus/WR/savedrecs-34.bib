@article{ ISI:000315543100009,
Author = {Cavalcanti, Eliane Bezerra and Garcia-Segura, Sergi and Centellas,
   Francesc and Brillas, Enric},
Title = {{Electrochemical incineration of omeprazole in neutral aqueous medium
   using a platinum or boron-doped diamond anode: Degradation kinetics and
   oxidation products}},
