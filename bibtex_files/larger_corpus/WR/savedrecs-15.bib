@article{ ISI:000180763300012,
Author = {Ouyang, Y},
Title = {{Simulating dynamic load of naturally occurring TOC from watershed into a
   river}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{4}},
Pages = {{823-832}},
Month = {{FEB}},
Abstract = {{Naturally occurring total organic carbon (TOC) is an important feature
   of stream water quality. This study investigates the dynamic load of TOC
   from the deep creek watershed into the lower St. Johns River (LSJR), FL,
   USA, using numerical simulations and field measurements. An existing St.
   Johns River watershed assessment model for simultaneous loading of
   nutrients from watersheds into rivers is modified to include the TOC
   component for the purpose of this study. Three simulation scenarios
   (i.e., daily, monthly, and annually) are performed to estimate the
   dynamic load of TOC in response to rainfall events. Simulations show
   that rainfall events have decisive effects on TOC loads from the Deep
   Creek watershed into the LSJR. In general, the highly frequent the
   rainfall events occur, the higher the TOC loads into the river.
   Simulations also illustrate that effects of rainfall events on daily
   changes of TOC are minimal in winter, but are profound in late summer.
   Results suggest that TOC load into the river is not only a
   rainfall-driven but also a temperature-driven biological process. (C)
   2002 Elsevier Science Ltd. All rights reserved.}},
