@article{ ISI:000184168700006,
Author = {Rivera-Utrilla, J and Sanchez-Polo, M},
Title = {{Adsorption of Cr(III) on ozonised activated carbon. Importance of C
   pi-cation interactions}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{14}},
Pages = {{3335-3340}},
Month = {{AUG}},
Abstract = {{The adsorption of Cr(Ill) in aqueous solution was investigated on a
   series of ozonised activated carbons, analysing the effect of oxygenated
   surface groups on the adsorption process. A study was carried out to
   determine the adsorption isotherms and the influence of the pH on the
   adsorption of this metal. The adsorption capacity and affinity of the
   adsorbent for Cr(III) increased with the increase in oxygenated acid
   groups on the surface of the activated carbon. These findings imply that
   electrostatic-type interactions predominate in the adsorption process,
   although the adsorption of Cr(Ill) on the original (basic) carbon
   indicates that other forces also participate in the adsorption process.
   Thus, the ionic exchange of protons in the -Cpi-H3O+ interaction for
   Cr(Ill) accounts for the adsorption of cationic species in basic carbons
   with positive charge density. Study of the influence of pH on the
   adsorption of Cr(111) showed that, in each system, the maximum
   adsorption occurred when the charge of the carbon surface was opposite
   that of the species of Cr(III) present at the pH of the experiment.
   These results confirmed that electrostatic interactions predominate in
   the adsorption process. (C) 2003 Elsevier Ltd. All rights reserved.}},
