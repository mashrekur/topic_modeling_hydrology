@article{ ISI:000184857700029,
Author = {Kameda, T and Yoshioka, T and Mitsuhashi, T and Uchida, M and Okuwaki, A},
Title = {{The simultaneous removal of calcium and chloride ions from calcium
   chloride solution using magnesium-aluminum oxide}},
