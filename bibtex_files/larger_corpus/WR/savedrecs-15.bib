@article{ ISI:000185260600019,
Author = {Salehizadeh, H and Shojaosadati, SA},
Title = {{Removal of metal ions from aqueous solution by polysaccharide produced
   from Bacillus firmus}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{17}},
Pages = {{4231-4235}},
Month = {{OCT}},
Abstract = {{This paper deals with adsorption of Pb, Cu and Zn on the polysaccharide
   produced by Bacillus firmus. The adsorption of metal ions was
   significantly affected by the initial pH of solution, initial metal ion
   and polysaccharide concentrations, and presence of other ions in
   solution. At optimum pH, the uptakes of Pb, Cu and Zn were 98.3\%,
   74.9\% and 61.8\%, respectively. The metal ions removal was lower at
   neutral and generally the initial adsorption rate was rapid and reached
   equilibrium after 10 min. The process of uptake obeys both Langmuir and
   Freundlich isotherms. (C) 2003 Elsevier Ltd. All rights reserved.}},
