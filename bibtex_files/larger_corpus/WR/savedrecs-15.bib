@article{ ISI:000183839100023,
Author = {Filipovic, V and Raspor, B},
Title = {{Metallothionein and metal levels in cytosol of liver, kidney and brain
   in relation to growth parameters of Mullus surmuletus and Liza aurata
   from the Eastern Adriatic Sea}},
