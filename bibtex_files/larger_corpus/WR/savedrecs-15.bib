@article{ ISI:000184168700014,
Author = {Park, SK and Bielefeldt, AR},
Title = {{Equilibrium partitioning of a non-ionic surfactant and pentachlorophenol
   between water and a non-aqueous phase liquid}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{14}},
Pages = {{3412-3420}},
Month = {{AUG}},
Abstract = {{The partitioning of the non-ionic surfactant Tergitol NP-10 (TNP10) and
   pentachlorophenol (PCP) into a mineral oil light non-aqueous phase
   liquid (NAPL) were quantified in batch tests. Due to the ionizable
   nature of PCP, the effects of pH and ionic strength (mu) on the
   equilibrium partitioning were evaluated. NAPL:water partition
   coefficients (K-new) of TNP10 ranged from 3 to 7L(water)/L-NAPL.
   Enhanced PCP dissolution into water from the NAPL was achieved at
   aqueous TNP10 concentrations greater than or equal to200mg/L. Surfactant
   addition of 1200mg/L TNP10 increased the aqueous PCP concentrations by
   14-fold at pH 5 versus 2 to 3-fold at pH 7 as compared to PCP aqueous
   solubility. The more significant response at the lower pH is likely due
   to the greater hydrophobicity of PCP at the lower pH, which is
   approaching PCP's pK(a) of 4.7. Higher ionic strength (mu 0.11 versus
   0.001 M) increased K-new of PCP by 10-33\% without surfactant, compared
   to a more than 150\% increase with a dose of 4000mg/L TNP10. This work
   contributes information relevant to the application of surfactants to
   remediate sites contaminated with NAPLs. (C) 2003 Elsevier Science Ltd.
   All rights reserved.}},
