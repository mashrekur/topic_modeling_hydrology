@article{ ISI:000185812200002,
Author = {Majumder, PS and Gupta, SK},
Title = {{Hybrid reactor for priority pollutant nitrobenzene removal}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{18}},
Pages = {{4331-4336}},
Month = {{NOV}},
Abstract = {{The performance of a hybrid reactor, comprising of trickling filter and
   activated sludge process, in treating nitrobenzene wastewater was
   investigated. Acetate induced cells of mixed consortia was acclimatized
   with gradual increase of nitrobenzene concentration up to 90mg/l in 100
   days using sodium acetate as co-substrate and considering COD and
   nitrobenzene concentration as paramount parameters for assessing the
   growth of biofilm and acclimation. A removal of 60-95.80\% COD and
   80-90.23\% nitrobenzene was observed during acclimation. During
   hydraulic retention time (HRT) studies, the optimum HRT was found to be
   29.55 h at which a maximum of 95.83\% COD and 97.93\% nitrobenzene
