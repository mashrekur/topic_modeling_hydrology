@article{ ISI:000183430000025,
Author = {Aulenta, F and Dionisi, D and Majone, M and Parisi, A and Ramadori, R
   and Tandoi, V},
Title = {{Effect of periodic feeding in sequencing batch reactor on substrate
   uptake and storage rates by a pure culture of Amaricoccus kaplicensis}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{11}},
Pages = {{2764-2772}},
Month = {{JUN}},
Abstract = {{A pure culture of Amaricoccus kaplicensis was aerobically cultured at a
   long culture residence time (Theta(C) > 12 d), under periodic acetate
   feeding in a sequencing batch reactor (SBR). The cycle length and,
   correspondingly, the volumetric organic load rate (vOLR) were varied in
   the range 4-24h and 0.76-0.12 g COD l(-1)d(-1), respectively. The
   transient response of the microorganism to the acetate spike was
   investigated throughout batch tests, as a function of SBR cycle length
   and vOLR. In all tested conditions, a rapid transient response was
   observed, mainly due to acetate storage in the form of
   polyhydroxybutyrate, since growth (production of active biomass) played
   a minor role. Apart from this general trend, the maximum rates under
   transient conditions increased as the cycle length increased from 4 to
   24 h. In the SBR, the longest cycle also caused a decrease in floc size
