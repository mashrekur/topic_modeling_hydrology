@article{ ISI:000184857700024,
Author = {Choi, SD and Hong, HB and Chang, YS},
Title = {{Adsorption of halogenated aromatic pollutants by a protein released from
   Bacillus pumilus}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
