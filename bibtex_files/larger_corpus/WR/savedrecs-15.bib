@article{ ISI:000184512300009,
Author = {Craik, SA and Smith, DW and Chandrakanth, M and Belosevic, M},
Title = {{Effect of turbulent gas-liquid contact in a static mixer on
   Cryptosporidium parvum oocyst inactivation by ozone}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{15}},
Pages = {{3622-3631}},
Month = {{SEP}},
Abstract = {{Static mixers may be used to dissolve gaseous ozone in water treatment
   facilities in order to provide protection against the waterborne
   parasite Cryptosporidium parvum. The objective of this study was to
   determine the effect of a brief exposure to turbulent gas-liquid mixing
   conditions in a static mixer on inactivation of C parvum oocysts by
   ozone. Inactivation measured in an ozone contacting apparatus that
   employed a static mixer for ozone dissolution was compared to
   predictions based on a previously published kinetic model of C parvum
   inactivation by dissolved ozone in gently stirred batch reactors.
   Although initial contact in the static mixer had no immediate effect on
   the oocysts, a 20\% increase in the rate of inactivation during
   subsequent contact with dissolved ozone was observed. Increasing the
   degree of turbulence within the static mixer did not yield additional
   inactivation. Use of static mixers for dissolution of ozone in drinking
   water treatment systems may provide limited enhancement of C parvum
   inactivation by dissolved ozone. (C) 2003 Elsevier Science Ltd. All
   rights reserved.}},
