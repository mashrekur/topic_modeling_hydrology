@article{ ISI:000392788900014,
Author = {Yang, Jingxin and Li, Ji and Dong, Wenyi and Ma, Jun and Yang, Yi and
   Li, Jiayin and Yang, Zhichao and Zhang, Xiaolei and Gu, Jia and Xie,
   Wanying and Cang, Yan},
Title = {{Enhancement of bromate formation by pH depression during ozonation of
   bromide-containing water in the presence of hydroxylamine}},
Journal = {{WATER RESEARCH}},
Year = {{2017}},
Volume = {{109}},
Pages = {{135-143}},
Month = {{FEB 1}},
Abstract = {{This work investigated the fate of bromate formation during ozonation in
   the presence of hydroxylamine (HA). Results indicated that pH
   depression, as a commonly feasible control strategy for bromate
   formation during ozonation, unexpectedly enhanced the bromate formation
   during ozonation in the presence of HA. A dramatically high level of
   bromate was observed at acidic pH in the ozone/HA process. The
   scavenging experiments demonstrated the essential role of (OH)-O-center
   dot produced in the reaction of ozone with HA in bromate formation. In
   the process, (OH)-O-center dot mainly oxidizes bromide to Br-center dot,
   which is further oxidized by ozone and eventually converts to bromate.
   Further investigations suggested that the unexpected enhancement on
   bromate formation by pH depression can be mainly ascribed to the pH
   dependent ozone decay, (OH)-O-center dot exposures and formation rate of
   Br-center dot. As pH decreased from 7 to 5, the reduced (OH)-O-center
   dot scavenging capacity of HA led to higher (OH)-O-center dot exposures,
   which contributed to the enhancement of bromate formation. As pH
   decreased from 5 to 3, the enhanced formation rate of Br-center dot
   largely augmented the formation of bromate. In addition, the ozone decay
   slowed down by pH depression provided more available ozone for the
   oxidation of the formed Br-center dot to bromate. The enhanced effect of
   pH depression on bromate formation was still observed in the real water
   samples in the ozone/HA process. Accordingly, pH depression might be
   avoided to control the bromate formation during ozonation in the
   presence of HA. (C) 2016 Elsevier Ltd. All rights reserved.}},
