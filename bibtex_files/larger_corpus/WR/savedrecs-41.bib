@article{ ISI:000395612400021,
Author = {Dong, Xin and Zhang, Xinyi and Zeng, Siyu},
Title = {{Measuring and explaining eco-efficiencies of wastewater treatment plants
   in China: An uncertainty analysis perspective}},
