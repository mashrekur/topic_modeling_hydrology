@article{ ISI:000395218900024,
Author = {Motshekga, Sarah C. and Ray, Suprakas Sinha},
Title = {{Highly efficient inactivation of bacteria found in drinking water using
   chitosan-bentonite composites: Modelling and breakthrough curve analysis}},
