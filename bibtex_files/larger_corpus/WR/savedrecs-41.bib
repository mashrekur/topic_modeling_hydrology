Author = {Luo, Wenhai and Phan, Hop V. and Xie, Ming and Hai, Faisal I. and Price,
   William E. and Elimelech, Menachem and Nghiem, Long D.},
Title = {{Osmotic versus conventional membrane bioreactors integrated with reverse
   osmosis for water reuse: Biological stability, membrane fouling, and
   contaminant removal}},
