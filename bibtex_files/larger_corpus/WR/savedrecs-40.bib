@article{ ISI:000375819000038,
Author = {Insam, Heribert and Markt, Rudolf},
Title = {{Comment on ``Synergistic co-digestion of solid-organic-waste and
   municipal-sewage-sludge: 1 plus 1 equals more than 2 in terms of biogas
   production and solids reduction{''} {[}Water Research 87, 416-423]}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{95}},
Pages = {{392-393}},
Month = {{MAY 15}},
Abstract = {{Co-digestion of organic waste and sewage sludge enhances biogas
   production and reduces the mass of remaining solids. This phenomenon of
   enhanced organic matter decomposition by adding labile substrate is
   known from other habitats like soils and sediments where it is called
   priming effect. It is thus suggested to adopt the term priming effect
   also in environmental biotechnology, and in particular for
   biomethanisation of wastewater sludges by the addition of energy-rich
   co-substrates. (C) 2016 Elsevier Ltd. All rights reserved.}},
