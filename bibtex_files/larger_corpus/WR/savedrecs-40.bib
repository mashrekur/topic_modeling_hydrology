@article{ ISI:000374360900018,
Author = {Thompson Brewster, Emma and Mehta, Chirag M. and Radjenovic, Jelena and
   Batstone, Damien J.},
Title = {{A mechanistic model for electrochemical nutrient recovery systems}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{94}},
