@article{ ISI:000287054500010,
Author = {Upadhyay, Nabin and Sun, Qinyue and Allen, Jonathan O. and Westerhoff,
   Paul and Herckes, Pierre},
Title = {{Synthetic musk emissions from wastewater aeration basins}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
