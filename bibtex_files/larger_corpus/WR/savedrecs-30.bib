@article{ ISI:000287054500034,
Author = {Cantwell, Raymond E. and Hofmann, Ron},
Title = {{Ultraviolet absorption properties of suspended particulate matter in
   untreated surface waters}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{3}},
Pages = {{1322-1328}},
Month = {{JAN}},
Abstract = {{Previous research has shown that wastewater disinfection using UV light
   can be impaired by attenuation of the UV light as it passes through
   particles to reach embedded and protected microorganisms. This study
   determined that the UV absorption (at 254 nm) of particles present in 10
   untreated surface waters was similar to the absorption of wastewater
   particles. As such, it provides evidence that UV disinfection of surface
   waters during drinking water treatment may be impaired by the same
   mechanism if particles are present. The study also demonstrated that
   among the 10 untreated surface waters examined, there was no correlation
   between the UV absorption (254 nm) of the solid particulate material,
   total organic carbon, total suspended solids, turbidity, or UV
   absorbance (254) of the bulk water. (C) 2010 Elsevier Ltd. All rights
   reserved.}},
