@article{ ISI:000286790500055,
Author = {Zhou Xiao-Hong and Yu Tong and Shi Han-Chang and Shi Hui-Ming},
Title = {{Temporal and spatial inhibitory effects of zinc and copper on wastewater
   biofilms from oxygen concentration profiles determined by
   microelectrodes}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{2}},
Pages = {{953-959}},
Month = {{JAN}},
Abstract = {{To understand the temporal and spatial toxic effect of heavy metals on
   the microbial activities of biofilms, microelectrodes were used to
   measure the inhibitory oxygen (O-2) concentration profiles resulted from
   the effects of zinc (Zn2+) and copper (Cu2+). Using the O-2
   microprofiles as bases, the spatial distributions of net specific O-2
   respiration were determined in biofilms with and without treatment of 5
   mg/L Zn2+ or 1 mg/L Cu2+. Results show that microbial activities were
   inhibited only in the outer layer (similar to 400 mu m) of the biofilms
   and bacteria present in the deeper sections of the biofilms became even
   more active. The inhibition caused by the heavy metals was evaluated by
   two methods. One was derived from the oxygen influx at the interface and
   the other was based on the integral of the oxygen consumption calculated
   from the entire O-2 profile. The two methods yielded significantly
   different results. We argue that the integral method results in more
   accurate assessment of toxicity than the surface flux determination. (C)
   2010 Elsevier Ltd. All rights reserved.}},
