@article{ ISI:000286790500021,
Author = {Razavi, Behnaz and Ben Abdelmelek, Sihem and Song, Weihua and O'Shea,
   Kevin E. and Cooper, William J.},
Title = {{Photochemical fate of atorvastatin (lipitor) in simulated natural waters}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{2}},
Pages = {{625-631}},
Month = {{JAN}},
Abstract = {{Cholesterol-lowering statin drugs are among the most frequently
   prescribed for reducing human blood cholesterol and they have been
   detected as contaminants in natural waters. In this study the
   photochemical behavior of atorvastatin (lipitor) was investigated at two
   different concentrations of 35.8 mu M (20 mg L-1) and 35.8 nM (20 mu g
   L-1) using a solar simulator and a UV reactor. Photochemical fate in
   natural waters can be described in most cases by the sum of the loss due
   to hydrolysis, direct photolysis, and, reaction with hydroxyl radical
   ((OH)-O-center dot), singlet oxygen (O-1(2)) (or O-2 (D-1)), and excited
   state dissolved organic matter (DOM). The absolute bimolecular reaction
   rate constant with (OH)-O-center dot was measured, using pulsed
   radiolysis, (1.19 +/- 0.04) x 10(10) M-1 s(-1). The reaction rate
   constant of O-1(2) was determined to be (3.1 +/- 0.2) x 10(8) M-1 s(-1).
   Under the experimental conditions used, at high atorvastatin
   concentration (35.8 mu M) the contribution of singlet oxygen (O-1(2)) to
   the photodegradation of atorvastatin in natural waters was higher than
   that of hydroxyl radical, and accounted for up to 23\% of the loss in
   aqueous solutions. Whereas, at a concentration of 35.8 nM, O-1(2) (and
   (OH)-O-center dot) both played a minor role in the removal of this
   compound. Lastly, it also appears that atorvastatin reacts with (DOM
   center dot)-D-3 contributing to its loss in simulated natural waters.
   (c) 2010 Elsevier Ltd. All rights reserved.}},
