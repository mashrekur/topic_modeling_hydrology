@article{ ISI:000298527300016,
Author = {Rodriguez, Eva M. and Fernandez, Guadalupe and Alvarez, Pedro M. and
   Beltran, Fernando J.},
Title = {{TiO2 and Fe (III) photocatalytic ozonation processes of a mixture of
   emergent contaminants of water}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
Volume = {{46}},
Number = {{1}},
Pages = {{152-166}},
Month = {{JAN 1}},
Abstract = {{A mixture of three emergent contaminants: testosterone (TST), bisphenol
   A (BPA) and acetaminophen (AAP) has been treated with different
   photocatalytic oxidation systems. Homogeneous catalysts as Fe(III) alone
   or complexed with oxalate or citrate ions, heterogeneous catalysts as
   titania, and oxidants such as hydrogen peroxide and/or ozone have been
   used to constitute the oxidation systems. For the radiation type, black
   light lamps mainly emitting at 365 nm have been used. The effects of pH
   (3 and 6.5) have been investigated due to the importance of this
   variable both in ozone and Fe(III) systems. Removal of initial compounds
   and mineralization (total organic carbon: TOC) were followed among other
   parameters. For the initial compounds removal ozonation alone, in many
   cases, allows the highest elimination rates, regardless of the presence
   or absence of UVA light and catalyst. For mineralization, however, ozone
   photocatalytic processes clearly leads to the highest oxidation rates.
   (C) 2011 Elsevier Ltd. All rights reserved.}},
