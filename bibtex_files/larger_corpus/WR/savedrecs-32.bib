Author = {Pisarenko, Aleksey N. and Stanford, Benjamin D. and Yan, Dongxu and
   Gerrity, Daniel and Snyder, Shane A.},
Title = {{Effects of ozone and ozone/peroxide on trace organic contaminants and
   NDMA in drinking water and water reuse applications}},
