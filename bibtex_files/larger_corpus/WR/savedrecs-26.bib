@article{ ISI:000263658700028,
Author = {Iversen, Vera and Koseoglu, Hasan and Yigit, Nevzat O. and Drews, Anja
   and Kitis, Mehmet and Lesjean, Boris and Kraume, Matthias},
Title = {{Impacts of membrane flux enhancers on activated sludge respiration and
   nutrient removal in MBRs}},
