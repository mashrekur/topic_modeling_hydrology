@article{ ISI:000263658700030,
Author = {Falk, Michael W. and Song, Kyung-Guen and Matiasek, Michael G. and
   Wuertz, Stefan},
Title = {{Microbial community dynamics in replicate membrane bioreactors - Natural
   reproducible fluctuations}},
