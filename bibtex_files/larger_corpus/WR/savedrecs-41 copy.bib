@article{ ISI:000414818700015,
Author = {Su, Zhaoyang and Liu, Ting and Yu, Wenzheng and Li, Xing and Graham,
   Nigel J. D.},
Title = {{Coagulation of surface water: Observations on the significance of
   biopolymers}},
