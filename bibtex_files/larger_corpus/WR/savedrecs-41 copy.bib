@article{ ISI:000414818700048,
Author = {Feldman, H. and Flores-Alsina, X. and Ramin, P. and Kjellberg, K. and
   Jeppsson, U. and Batstone, D. J. and Gernaey, K. V.},
Title = {{Modelling an industrial anaerobic granular reactor using a multi-scale
   approach}},
