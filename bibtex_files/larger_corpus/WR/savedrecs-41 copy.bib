@article{ ISI:000402494600018,
Author = {Dykstra, J. E. and Keesman, K. J. and Biesheuvel, P. M. and van der Wal,
   A.},
Title = {{Theory of pH changes in water desalination by capacitive deionization}},
Journal = {{WATER RESEARCH}},
