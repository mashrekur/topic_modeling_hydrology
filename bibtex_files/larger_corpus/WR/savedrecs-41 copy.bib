@article{ ISI:000407404300024,
Author = {Bourgin, Marc and Borowska, Ewa and Helbing, Jakob and Hollender,
   Juliane and Kaiser, Hans-Peter and Kienle, Cornelia and McArdell,
   Christa S. and Simon, Eszter and von Gunten, Urs},
Title = {{Effect of operational and water quality parameters on conventional
   ozonation and the advanced oxidation process O-3/H2O2: Kinetics of
   micropollutant abatement, transformation product and bromate formation
   in a surface water}},
Journal = {{WATER RESEARCH}},
Year = {{2017}},
Volume = {{122}},
Pages = {{234-245}},
Month = {{OCT 1}},
Abstract = {{The efficiency of ozone-based processes under various conditions was
   studied for the treatment of a surface water (Lake Zurich water,
   Switzerland) spiked with 19 micropollutants (pharmaceuticals,
   pesticides, industrial chemical, X-ray contrast medium, sweetener) each
