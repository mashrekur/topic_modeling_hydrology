@article{ ISI:000407404300018,
Author = {Kwon, Minhwan and Kye, Homin and Jung, Youmi and Yoon, Yeojoon and Kang,
   Joon-Wun},
Title = {{Performance characterization and kinetic modeling of ozonation using a
   new method: R-OH,R-O3 concept}},
Journal = {{WATER RESEARCH}},
Year = {{2017}},
Volume = {{122}},
Pages = {{172-182}},
Month = {{OCT 1}},
Abstract = {{Ozonation is an effective treatment for removing various organic
   pollutants from aquatic systems. The Rct concept, which is defined as
   the ratio of center dot OH exposure to O-3 exposure, has been widely
   used to predict the removal efficiency of target compounds, but it has
   significant variations by water temperature and initial O-3 dose which
   are crucial parameters in drinking water plant. The ROH,O-3 concept,
   which is defined as the center dot OH exposure by O-3 consumption, was
   proposed as a kinetic parameter for characterization and kinetic
   modeling for ozonation. The RoH,O-3 concept is independent of
   temperature and initial O-3 dose. A higher ROH,O-3 value indicates a
   higher center dot OH formation when the same amount of O-3 is consumed
   in different water samples; therefore, the center dot OH yield from O-3
   decomposition of the water samples can be compared using the \&Kw
   values. The ROH,O-3 concept can also be used to characterize and model
   the initial ozone demand phase, and it is more convenient method
