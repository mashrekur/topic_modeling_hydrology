Language = {{English}},
Affiliation = {{Taghipour, F (Reprint Author), Univ British Columbia, Dept Chem \& Biol Engn, 2360 East Mall, Vancouver, BC V6T 1Z3, Canada.
   Kheyrandish, Ataollah; Mohseni, Madjid; Taghipour, Fariborz, Univ British Columbia, Dept Chem \& Biol Engn, 2360 East Mall, Vancouver, BC V6T 1Z3, Canada.}},
DOI = {{10.1016/j.watres.2017.06.015}},
