@article{ ISI:000401386300018,
Author = {Grandcoin, Alexis and Piel, Stephanie and Baures, Estelle},
Title = {{AminoMethylPhosphonic acid (AMPA) in natural waters: Its sources,
   behavior and environmental fate}},
