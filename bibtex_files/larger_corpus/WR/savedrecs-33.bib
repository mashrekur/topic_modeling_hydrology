@article{ ISI:000327911700012,
Author = {Baek, Kitae and Ciblak, Ali and Mao, Xuhui and Kim, Eun-Jung and
   Alshawabkeh, Akram},
Title = {{Iron anode mediated transformation of selenate in sand columns}},
