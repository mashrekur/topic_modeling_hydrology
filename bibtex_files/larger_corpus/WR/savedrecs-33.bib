@article{ ISI:000326212500026,
Author = {Tang, Youneng and Valocchi, Albert J.},
Title = {{An improved cellular automaton method to model multispecies biofilms}},
Journal = {{WATER RESEARCH}},
Year = {{2013}},
Volume = {{47}},
Number = {{15}},
Pages = {{5729-5742}},
Month = {{OCT 1}},
Abstract = {{Biomass-spreading rules used in previous cellular automaton methods to
   simulate multispecies biofilm introduced extensive mixing between
   different biomass species or resulted in spatially discontinuous biomass
   concentration and distribution; this caused results based on the
   cellular automaton methods to deviate from experimental results and
   those from the more computationally intensive continuous method. To
   overcome the problems, we propose new biomass-spreading rules in this
   work: Excess biomass spreads by pushing a line of grid cells that are on
   the shortest path from the source grid cell to the destination grid
   cell, and the fractions of different biomass species in the grid cells
   on the path change due to the spreading. To evaluate the new rules,
   three two-dimensional simulation examples are used to compare the
   biomass distribution computed using the continuous method and three
   cellular automaton methods, one based on the new rules and the other two
   based on rules presented in two previous studies. The relationship
   between the biomass species is syntrophic in one example and competitive
   in the other two examples. Simulation results generated using the
   cellular automaton method based on the new rules agree much better with
   the continuous method than do results using the other two cellular
   automaton methods. The new biomass-spreading rules are no more complex
   to implement than the existing rules. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
