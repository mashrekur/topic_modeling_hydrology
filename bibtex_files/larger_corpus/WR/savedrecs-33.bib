@article{ ISI:000318051900002,
Author = {Ayache, C. and Pidou, M. and Croue, J. P. and Labanowski, J. and
   Poussade, Y. and Tazi-Pain, A. and Keller, J. and Gernjak, W.},
Title = {{Impact of effluent organic matter on low-pressure membrane fouling in
   tertiary treatment}},
Journal = {{WATER RESEARCH}},
Year = {{2013}},
