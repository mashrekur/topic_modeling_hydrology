@article{ ISI:000319644800013,
Author = {Bournazou, M. N. Cruz and Hooshiar, K. and Arellano-Garcia, H. and
   Wozny, G. and Lyberatos, G.},
Title = {{Model based optimization of the intermittent aeration profile for SBRs
   under partial nitrification}},
