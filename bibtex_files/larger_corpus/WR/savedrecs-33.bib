@article{ ISI:000326212500044,
Author = {Wang, Ling-Ling and Wang, Long-Fei and Ye, Xiao-Dong and Yu, Han-Qing},
Title = {{Hydration interactions and stability of soluble microbial products in
   aqueous solutions}},
