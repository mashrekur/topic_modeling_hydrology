@article{ ISI:000319644800010,
Author = {Peiris, R. H. and Jaklewicz, M. and Budman, H. and Legge, R. L. and
   Moresoli, C.},
Title = {{Assessing the role of feed water constituents in irreversible membrane
   fouling of pilot-scale ultrafiltration drinking water treatment systems}},
