@article{ ISI:000326910000029,
Author = {Hierro, A. and Martin, J. E. and Olias, M. and Vaca, F. and Bolivar, J.
   P.},
Title = {{Uranium behaviour in an estuary polluted by mining and industrial
   effluents: The Ria of Huelva (SW of Spain)}},
Journal = {{WATER RESEARCH}},
Year = {{2013}},
Volume = {{47}},
