@article{ ISI:000321084000029,
Author = {Wang, Xiaoping and Lim, Teik-Thye},
Title = {{Highly efficient and stable Ag-AgBr/TiO2 composites for destruction of
   Escherichia coli under visible light irradiation}},
Journal = {{WATER RESEARCH}},
