@article{ ISI:000318379800027,
Author = {Sushchik, Nadezhda N. and Kuchkina, Anna Yu and Gladyshev, Michail I.},
Title = {{Fatty acid content and composition of sediments from Siberian eutrophic
   water bodies: Implications for biodiesel production}},
Journal = {{WATER RESEARCH}},
