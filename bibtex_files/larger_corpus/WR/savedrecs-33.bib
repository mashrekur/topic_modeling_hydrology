@article{ ISI:000327911700022,
Author = {Jeong, Hoon Y. and Anantharaman, Karthik and Hyun, Sung P. and Son, Moon
   and Hayes, Kim F.},
Title = {{pH impact on reductive dechlorination of cis-dichloroethylene by Fe
   precipitates: An X-ray absorption spectroscopy study}},
Journal = {{WATER RESEARCH}},
