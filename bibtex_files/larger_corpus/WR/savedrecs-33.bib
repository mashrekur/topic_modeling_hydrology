@article{ ISI:000320497500013,
Author = {Papa, M. and Pedrazzani, R. and Bertanza, G.},
Title = {{How green are environmental technologies? A new approach for a global
   evaluation: The case of WWTP effluents ozonation}},
Journal = {{WATER RESEARCH}},
Year = {{2013}},
