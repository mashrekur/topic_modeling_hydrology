@article{ ISI:000327911700005,
Author = {Huebner, U. and Keller, S. and Jekel, M.},
Title = {{Evaluation of the prediction of trace organic compound removal during
   zonation of secondary effluents using tracer substances and second order
   rate kinetics}},
Journal = {{WATER RESEARCH}},
Year = {{2013}},
