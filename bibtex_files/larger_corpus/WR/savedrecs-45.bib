@article{ ISI:000458224000013,
Author = {Wu, Xianwei and Huang, Ju and Lu, Zichuan and Chen, Gaofeng and Wang,
   Jianmin and Liu, Guoqiang},
Title = {{Thiothrix eikelboomii interferes oxygen transfer in activated sludge}},
Journal = {{WATER RESEARCH}},
Year = {{2019}},
Volume = {{151}},
Pages = {{134-143}},
Month = {{MAR 15}},
Abstract = {{This study revealed that, Thiothrix eikelboomii, a well-known
   filamentous bacterium that causes sludge bulking, could also interfere
   oxygen transfer during wastewater treatment. The volumetric oxygen
   transfer coefficient (K(L)a) in filamentous-bulking sludge (FBS) was
   found to be 43\% lower than that in floc-forming sludge (FFS) at similar
   biomass concentrations, partially because the filamentous bacteria had
   increased the sludge apparent viscosity. The K(L)a value for FBS,
   however, was still significantly lower than that for FFS even if both
   sludges had similar apparent viscosity. Numerous tiny and free-swimming
   filaments were observed to attach on the air bubble surface, presumably
   reducing the liquid film renewal and increasing the liquid film
   thickness. Moreover, the filaments were co-coated with extracellular
   polymeric substances of protein and polysaccharide, which could make
   them performing like ``amphiphilic molecules{''} of surfactants to
   hinder oxygen transfer. Therefore, the particular surface property of
   filaments and their interaction with air bubbles could also impact
   oxygen transfer. Thiothrix eikelboomii was identified to be the
   responsible filamentous bacterium that lowered the K(L)a value, while
   other filamentous bacteria with short filaments did not interfere oxygen
   transfer. This study implies that controlling sludge bulking benefits
   not only sludge settling but also oxygen transfer. (C) 2018 Elsevier
   Ltd. All rights reserved.}},
