@article{ ISI:000458224000026,
Author = {Wang, Jieqiong and Chen, Jingwen and Qiao, Xianliang and Zhang, Ya-nan
   and Uddin, Misbah and Guo, Zhongyu},
Title = {{Disparate effects of DOM extracted from coastal seawaters and
   freshwaters on photodegradation of 2,4-Dihydroxybenzophenone}},
Journal = {{WATER RESEARCH}},
Year = {{2019}},
Volume = {{151}},
Pages = {{280-287}},
Month = {{MAR 15}},
Abstract = {{Dissolved organic matter (DOM) plays an important role in degradation of
   organic pollutants by photochemically-produced reactive intermediates
   (RIs), such as excited triplet-states of DOM ((DOM)-D-3{*}), singlet
   oxygen (O-1(2)) and hydroxyl radical (center dot OH). However, it is not
   clear whether DOM extracted from coastal seawaters (CS-DOM) and DOM
   derived from freshwaters (FW-DOM) exhibit similar effects on
   photodegradation of organic micropollutants. Herein,
   2,4-dihydroxybenzophenone (BP-1) was adopted as a model compound to
   probe the effects of different DOM on photodegradation kinetics of
   organic micropollutants. Results show that the CS-DOM promotes the
   photodegradation of BP-1 mainly via the pathway involving (DOM)-D-3{*};
   while (DOM)-D-3{*}, O-1(2) and center dot OH are responsible for BP-1
   photodegradation in the presence of the FW-DOM. Compared with the
   FW-DOM, the CS-DOM undergoes more photobleaching, and contains less
   aromatic C=C and C=O functional groups. Although (DOM)-D-3{*} formation
   quantum yields for the CS-DOM are relatively higher than those for the
   FW-DOM, the CS-DOM has lower rates of light absorption, leading to lower
   steady-state RI concentrations for the CS-DOM. BP-1 photodegradation in
   the presence of the CS-DOM is faster than in the presence of the FW-DOM,
   due to higher second-order reaction rate constants between BP-1 and
   CS-(DOM)-D-3{*} and fewer antioxidants contained in the CS-DOM. (C) 2018
   Elsevier Ltd. All rights reserved.}},
