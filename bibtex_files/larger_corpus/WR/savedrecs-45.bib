@article{ ISI:000468253500057,
Author = {Jalali, Lida and Zarei, Mehdi and Gutierrez, Francisco},
Title = {{Salinization of reservoirs in regions with exposed evaporites. The
   unique case of Upper Gotvand Dam, Iran}},
Journal = {{WATER RESEARCH}},
Year = {{2019}},
Volume = {{157}},
Pages = {{587-599}},
Month = {{JUN 15}},
Abstract = {{Construction of the great Upper Gotvand Dam in the Karun River, Iran,
   with an impoundment capacity of 4.5 billion cubic meters, has resulted
   in an environmental disaster: accumulation of 66.5 million metric tonnes
   of dissolved salt in the reservoir and a dramatic increase in the
   salinity of the reservoir water up to 200 g/L. This paper aims to
   identify and assess the main sources of the salinization of the
   reservoir water integrating multiple data: (1) geological and
   geomorphological evidence; (2) continuous vertical records of electrical
   conductivity at 11 stations along the reservoir; (3) total dissolved
   solids and major ion concentrations of 108 water samples at 58 sampling
   stations; (4) delta O-18, delta H-2 in 35 water samples and delta Cl-37
   in 16 water samples from the reservoir and surrounding rivers and
   springs. Geological and geomorphological evidence, hydrogeochemical data
   and isotopic signature reveal that halite dissolution is the main cause
   of salinization in the Gotvand Reservoir. The results show that salt
   dissolution in the Gachsaran Formation (especially at Anbar Ridge),
   which was under-estimated in the study phase of the project, has
   provided about 86 per cent of the dissolved salt accumulated in the
   reservoir over a 2-year period since the impoundment of the reservoir in
   2011. (C) 2019 Elsevier Ltd. All rights reserved.}},
