@article{ ISI:000468253500013,
Author = {Ye, Gui and Yu, Zhiyong and Li, Yiming and Li, Lei and Song, Li and Gu,
   Li and Cao, Xuebo},
Title = {{Efficient treatment of brine wastewater through a flow-through
   technology integrating desalination and photocatalysis}},
Journal = {{WATER RESEARCH}},
