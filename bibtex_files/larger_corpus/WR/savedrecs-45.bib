@article{ ISI:000458221200010,
Author = {Rames, Emily and Macdonald, Joanne},
Title = {{The QuantiPhage assay: A novel method for the rapid colorimetric
   detection of coliphages using cellulose pad materials}},
Journal = {{WATER RESEARCH}},
