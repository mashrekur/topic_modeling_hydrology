@article{ ISI:A1993KV56400025,
Author = {SRIVASTAVA, RM},
Title = {{EFFECT OF SEQUENCE OF MEASUREMENT ON PARTICLE COUNT AND SIZE
   MEASUREMENTS USING A LIGHT BLOCKAGE (HIAC) PARTICLE COUNTER}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{5}},
Pages = {{939-942}},
Month = {{MAY}},
Abstract = {{Recent legislation in the U.S. has forced the water treatment plants to
   use particle size measurements in conjunction with turbidity
   measurements to ensure the removal of pathogenic entities from drinking
   water. There is a general lack of information among the water treatment
   professionals about the intricacies and pitfalls of particle size
   measurements, particularly for samples with low particulate
   concentrations. This work focuses on how the sequence of sample analysis
   can affect the results obtained, if an improper analysis protocol is
   used. Experimental results are presented to demonstrate this point, the
   conclusion being that care should be taken in sequencing of samples for
   batch particle counts as well as in one-line particle size measurements.
   A protocol is presented that avoids such problems.}},
