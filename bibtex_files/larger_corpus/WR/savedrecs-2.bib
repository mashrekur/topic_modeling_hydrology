@article{ ISI:A1993KP13700007,
Author = {YEOMAN, S and LESTER, JN and PERRY, R},
Title = {{PHOSPHORUS REMOVAL AND ITS INFLUENCE ON METAL SPECIATION DURING
   WASTE-WATER TREATMENT}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{3}},
Pages = {{389-395}},
Month = {{MAR}},
Abstract = {{The effect of chemical phosphate precipitants on heavy metal
   distribution in sewage sludges was investigated. The proportion of
   solids contained within each particle size range was affected by
   treatment. For the heavy metals, chemical precipitation appeared to
   shift the distribution throughout various chemical fractions in the
