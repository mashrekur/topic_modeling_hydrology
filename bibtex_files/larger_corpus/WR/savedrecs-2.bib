@article{ ISI:A1993LT91900001,
Author = {JACOBSEN, BN and NYHOLM, N and PEDERSEN, BM and POULSEN, O and OSTFELDT,
   P},
Title = {{REMOVAL OF ORGANIC MICROPOLLUTANTS IN LABORATORY ACTIVATED-SLUDGE
