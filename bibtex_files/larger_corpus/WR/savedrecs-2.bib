@article{ ISI:A1993KV56400009,
Author = {NIRMALAKHANDAN, N and SPEECE, RE and PEACE, JL and JANG, W},
Title = {{OPERATION OF COUNTERCURRENT AIR-STRIPPING TOWERS AT HIGHER LOADING RATES}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{5}},
Pages = {{807-813}},
Month = {{MAY}},
Abstract = {{A survey of air and water loading rates in counter-current packed towers
   used in air-stripping applications revealed that they are designed to
   operate at low loading rates relative to incipient flooding limits. This
   paper presents a theoretical analysis of some advantages and
   disadvantages of operating counter-current packed towers at high loading
   rates. A simple modification of the conventional counter-current system
   proposed previously as a cascade air-stripping system is shown here to
   be capable of accomodating high loading rates. Results of previous
   prototype tests are presented to demonstrate the possible benefits of
   operating counter-current packed towers at higher loading rates.}},
