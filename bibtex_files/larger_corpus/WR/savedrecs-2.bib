@article{ ISI:A1993KP13700020,
Author = {XIE, YF and RECKHOW, DA},
Title = {{A RAPID AND SIMPLE ANALYTICAL METHOD FOR CYANOGEN CHLORIDE AND
   CYANOGEN-BROMIDE IN DRINKING-WATER}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{3}},
Pages = {{507-511}},
Month = {{MAR}},
Abstract = {{A simple gas chromatographic method was developed for the determination
   of cyanogen chloride and cyanogen bromide in drinking waters. Saturated
   headspace is injected directly onto a capillary column and the analytes
   are quantified by electron capture detection. Method detection limits
   are 0.04 and 0.2 mug/l for CNCl and CNBr, respectively. Effects of
   temperature and salt addition on analyte response were investigated.
   Other tests were conducted to determine the effects of sulfite and free
   chlorine on analyte stability. A concluding set of experiments showed
   the relationship between bromide concentration and cyanogen halide
   speciation.}},
