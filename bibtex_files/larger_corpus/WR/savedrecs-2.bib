@article{ ISI:A1993KV56400013,
Author = {LEE, CM and SUBLETTE, KL},
Title = {{MICROBIAL TREATMENT OF SULFIDE-LADEN WATER}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{5}},
Pages = {{839-846}},
Month = {{MAY}},
Abstract = {{Water containing up to 25 mM soluble sulfide has been successfully
   treated microbially in an upflow bubble column. Sulfides were completely
   oxidized to sulfate by a sulfide-tolerant strain of the chemoautotroph,
   Thiobacillus denitrificans, flocculated by co-culture with floc-forming
   heterotrophs. The sulfide-active floc was shown to be stable for 9
   months of continuous operation with no external organic carbon required
   to support the growth of the heterotrophs. The floc exhibited excellent
   settling properties throughout the experiment.}},
