@article{ ISI:A1993KP13700016,
Author = {BHARGAVA, DS and OJHA, CSP},
Title = {{MODELS FOR DESIGN OF FLOCCULATING BAFFLED CHANNELS}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{3}},
Pages = {{465-475}},
Month = {{MAR}},
Abstract = {{In developing countries, particularly in power-scarce locations and
   where water treatment Plants are less mechanized and have a reduced
   staff level, the baffled channels type of flocculating device continues
   to be a preferred device. The existing method of flocculating baffled
   channel design involves several trials to evaluate the flocculating
   channel dimensions to satisfy mean velocity gradient, flow velocities,
   detention time, channel configuration geometry and several other
   requirements. An integrated approach through models and nomographs has
   been presented in this paper to instantly evolve the design elements for
   such flocculating channels. The presented models and nomographs also
   provide design options for various values of the velocity in the
   channels (to suit the different floc characteristics) and for the
   different channel width to flow depth ratios. The presented methodology,
   models and nomographs add to the existing literature.}},
