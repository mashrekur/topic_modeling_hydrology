@article{ ISI:A1993LF18000005,
Author = {MERTOOETOMO, E and VALSARAJ, KT and WETZEL, DM and HARRISON, DP},
Title = {{CASCADE CROSS-FLOW AIR STRIPPING OF MODERATELY VOLATILE COMPOUNDS USING
   HIGH AIR-TO-WATER RATIOS}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{7}},
Pages = {{1139-1144}},
Month = {{JUL}},
Abstract = {{The cascade crossflow air stripper provides the capability at the design
   stage of independently varying the cross-sectional areas for air and
   water flow. This flexibility results in a greatly reduced gas-phase
   pressure drop and permits stable operation at air and water flow rates
   which would cause flooding in a countercurrent column. Experimental
   results from a laboratory-scale cascade crossflow packed column have
   been coupled with design calculations to show the feasibility of
   stripping compounds such as 1,2-dichloroethane and methyl ethyl ketone
   which are not easily amenable to countercurrent air stripping. For
   1,2-dichloroethane, comparable experimental stripping efficiency in
   countercurrent and cascade crossflow modes was achieved with a pressure
   drop reduction in the cascade crossflow mode of approximately two orders
   of magnitude. High efficiency stripping of less volatile methyl ethyl
   ketone requires large G/L ratios which would flood a countercurrent
   column. In contrast, the large G/L ratios can be achieved in the cascade
   crossflow mode, and design calculations based upon the experimental
   results suggest that 95\% efficiency can be achieved using a column of
   reasonable size.}},
