@article{ ISI:A1993KP13700015,
Author = {BHARGAVA, DS and RAJAGOPAL, K},
Title = {{DIFFERENTIATION BETWEEN TRANSITION ZONE AND COMPRESSION IN ZONE SETTLING}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{3}},
Pages = {{457-463}},
Month = {{MAR}},
Abstract = {{To explore the effects of initial suspended solids concentrations and
   the nature of the suspended material on the settling behaviour (in the
   initial uniform settling region, transition zone and compression zone),
   batch settling tests were conducted for the different kinds of suspended
   materials (chemically flocculated and clay slurry types) present in
   different initial suspended solids concentrations. Using the
   experimental data, models for predicting the first-order rate constants
   for both the transition zone and compression zone were developed in
   relation to the initial suspended solids concentration and sludge volume
   index (indicating in some form, the characteristics of the suspended
   material present in the waters). The ratio of the two rate constants
   also correlated with the initial suspended solids concentration and
   sludge volume index the parameters of which also affect the minimum
   concentration for zone settling to occur. The use of the developed
   models will avoid the need of conducting enormous and time consuming
   settling tests.}},
