@article{ ISI:A1993LZ03100004,
Author = {MARTIN, AB and COOKE, GD and CARLSON, RE},
Title = {{LAKE-SEDIMENTS AS POTENTIAL SOURCES OF TRIHALOMETHANE PRECURSORS}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{12}},
Pages = {{1725-1729}},
Month = {{DEC}},
Abstract = {{Trihalomethanes (THMs) form when chlorine reacts with certain
   naturally-occurring organic molecules (THM precursors) during the
   disinfection of drinking water. THMs are believed to have significant
   carcinogenic and mutagenic properties. The elucidation of THM precursor
   sources is important because source management may lower costs and
   increase effectiveness of in-plant treatments. This study investigated
   the importance of lake and reservoir sediments as a THM precursor
   source.
   Sediments collected from littoral and profundal areas of Lake Rockwell
   Reservoir (Portage County, Ohio) and from a nearby natural take (East
   Twin Lake) were kept in the dark for 21 days at 20-degrees-C to estimate
   release rates of THM precursors. All sediments had significant THM
   precursor releases relative to non-sediment controls. Anaerobic
   conditions produced significantly less precursors than during an aerobic
   incubation. Littoral sediment precursor release rate (mug THMs/m2/day)
   was significantly greater than precursor release rates for lake and
   reservoir profundal sediments. The importance of sediment THM precursor
   release relative to other sources is discussed.}},
