@article{ ISI:A1993KP13700009,
Author = {KRAUTH, K and STAAB, KF},
Title = {{PRESSURIZED BIOREACTOR WITH MEMBRANE FILTRATION FOR WASTE-WATER
   TREATMENT}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{3}},
Pages = {{405-411}},
Month = {{MAR}},
Abstract = {{In the following paper, results of investigations with a newly developed
   technology for the treatment of wastewater are reported. This technology
   is based on a pressurized aerobic activated sludge bioreactor with a
   membrane unit for separation of the activated sludge from the purified
   wastewater.
   In respect to epidemic hygiene, the effluent quality corresponded to the
   quality of drinking water with almost no bacteria to be found in the
   effluent.
   During process development, tests were carried out with wastewater of
   different origins, different pressures and different solid
   concentrations within the activated sludge reactor. Using the membrane
   unit, tests were carried out with different types of membranes under
   variation of the velocity in the membrane tubes. In the paper presented,
   the results of tests with wastewater from the canning of sour vegetables
   are reported.}},
