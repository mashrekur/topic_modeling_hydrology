@article{ ISI:A1993LJ47500015,
Author = {BANNOUD, AH and PERSIN, F and RUMEAU, M},
Title = {{A STUDY OF THE PERFECTION OF AN ELECTROCHEMICAL REACTOR FOR SOFTENING
   WATER}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{8}},
Pages = {{1385-1391}},
Month = {{AUG}},
Abstract = {{An electrochemical reactor for softening water has been perfected. The
   best results, without excessive scaling of the electrodes or the
   membrane, have been obtained with a cationic membrane as a diaphragm. In
   the cathodic compartment, an increase of the concentrations of Ca2+ and
   CO32- can be noted. The cations are transferred through the membrane
   under the effect of an electrical field. Anions are produced by
   electrolysis or by chemical reactions. These favour the acceleration of
   calcite crystalline growth by reducing the time necessary for the first
   germ to form. The softening of the anolyte depends on the current
   strength, the flow rate and the number of Ca2+ transport ions in the
   membrane.}},
