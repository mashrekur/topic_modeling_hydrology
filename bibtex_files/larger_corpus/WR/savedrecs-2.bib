@article{ ISI:A1993LZ03100015,
Author = {MARA, DD and EDWARDS, P and CLARK, D and MILLS, SW},
Title = {{A RATIONAL APPROACH TO THE DESIGN OF WASTE-WATER-FED FISHPONDS}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{12}},
Pages = {{1797-1799}},
Month = {{DEC}},
Abstract = {{A procedure is given for the minimal treatment of wastewater in a 1-day
   anaerobic pond followed by a 5-day facultative pond prior to discharge
   into a fishpond. The criterion for the design of the fishpond is a
   surface loading of total nitrogen of 4 kg N ha-1 d-1. The number of
   faecal coliforms in the fishpond is then determined; this should be not
   greater than 1000 per 100 ml to ensure that the fish are
   microbiologically safe for human consumption. Fish (carp and tilapia)
   yields are of the order of 13 t ha-1 a-1, assuming that the pond is
   drained and harvested three times a year and that there is a fish loss
   of 25 percent. Preliminary financial analysis indicates that such a
   wastewater-fed fishpond system is commercially viable.}},
