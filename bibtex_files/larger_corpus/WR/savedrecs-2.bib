@article{ ISI:A1993LZ03100010,
Author = {PENNA, N and RINALDI, A and MONTANARI, G and DIPAOLO, A and PENNA, A},
Title = {{MUCILAGINOUS MASSES IN THE ADRIATIC SEA IN THE SUMMER OF 1989}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{12}},
Pages = {{1767-1771}},
Month = {{DEC}},
Abstract = {{This paper reports on one of the most recent episodes of mucilage
   formation in the central northern part of the Adriatic coast in
   July-August 1989. The stretch of coast examined goes from the estuary of
   the River Po to the border of the Marche-Abruzzo coast (Italy).
   The possible events (nutrients, N/P ratio, particular climatic
   conditions) that cause the formation of the mucilage masses were
   examined and are discussed here. Furthermore, the oxygenation state of
   the water on the surface and on the bottom as well as the possible
   effects on molluscs were also examined. The field results indicated that
   no anoxia phenomena were found in bottom waters caused by mucilages, and
   that mollusc death was probably due to mechanical reasons. Finally the
   places where the mucilages formed and their subsequent movements are
   described.}},
