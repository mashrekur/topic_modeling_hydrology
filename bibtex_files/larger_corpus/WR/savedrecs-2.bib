@article{ ISI:A1993KC08000001,
Author = {ZHANG, J and HUANG, WW},
Title = {{DISSOLVED TRACE-METALS IN THE HUANGHE - THE MOST TURBID LARGE RIVER IN
   THE WORLD}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{1}},
Pages = {{1-8}},
Month = {{JAN}},
Abstract = {{With a water discharge of 41.0 x 10(9) m3/yr and sediment load of 1.1 x
   10(9) tons/yr, the Huanghe is the most turbid large world river. This
   study examines concentrations of eight dissolved trace metals (Cd, Co,
   Cu, Fe, Mn, Ni, Pb and Zn) in the Huanghe. Trace metals occur at levels
   lower than frequently reported in China. There are correlations between
   the metals and major elements (e.g. SiO44-) in the fiver, indicating the
   significance of weathering and erosion in controlling water chemistry.
   Anthropogenic contributions cannot be assessed accurately from this
   study, but do not appear lo change element levels dramatically.}},
