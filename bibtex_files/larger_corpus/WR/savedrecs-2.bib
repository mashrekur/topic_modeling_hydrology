@article{ ISI:A1993KP13700001,
Author = {IJZERMAN, MM and HAGEDORN, C and RENEAU, RB},
Title = {{MICROBIAL TRACERS TO EVALUATE AN ON-SITE SHALLOW-PLACED LOW-PRESSURE
   DISTRIBUTION-SYSTEM}},
Journal = {{WATER RESEARCH}},
Year = {{1993}},
Volume = {{27}},
Number = {{3}},
Pages = {{343-347}},
Month = {{MAR}},
Abstract = {{Two antibiotic resistant Escherichia coli strains and two host-specific
   coliphages were used to evaluate a shallow-placed low pressure
   distribution (LPD) system installed in a soil that was unsuitable for a
   conventional system because of the shallow depth to shale bedrock. Three
   independent LPD subsystems were installed with design hydraulic loading
   rates of 7.7, 14.3 (based on Virginia regulations) and 30.6 l. d-1 m-2
