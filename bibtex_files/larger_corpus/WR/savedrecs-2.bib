@article{ ISI:A1993LJ47500007,
Author = {BRAMBILLA, AM and CALVOSA, L and MONTEVERDI, A and POLESELLO, S and
   RINDONE, B},
Title = {{OZONE OXIDATION OF POLYETHOXYLATED ALCOHOLS}},
