@article{ ISI:A1993LJ47500005,
Author = {KINOSHITA, T and BALES, RC and YAHYA, MT and GERBA, CP},
Title = {{BACTERIA TRANSPORT IN A POROUS-MEDIUM - RETENTION OF BACILLUS AND
