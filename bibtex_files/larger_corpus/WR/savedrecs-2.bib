@article{ ISI:A1993KC08000022,
Author = {NARAYANAN, B and SUIDAN, MT and GELDERLOOS, AB and BRENNER, RC},
Title = {{TREATMENT OF SEMIVOLATILE COMPOUNDS IN HIGH-STRENGTH WASTES USING AN
   ANAEROBIC EXPANDED-BED GAC REACTOR}},
