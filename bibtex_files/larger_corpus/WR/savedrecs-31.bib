@article{ ISI:000305053200006,
Author = {Loiselle, Steven and Vione, Davide and Minero, Claudio and Maurino,
   Valter and Tognazzi, Antonio and Dattilo, Arduino M. and Rossi, Claudio
   and Bracchini, Luca},
Title = {{Chemical and optical phototransformation of dissolved organic matter}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
Volume = {{46}},
Number = {{10}},
Pages = {{3197-3207}},
Month = {{JUN 15}},
Abstract = {{Dissolved organic matter represents the main reservoir of organic carbon
   in most aquatic ecosystems. In the present study, we determined the
   optical changes and the quantum yields of transient species formation
   for chromophoric dissolved organic matter (CDOM) samples undergoing
   photodegradation. The results show that the triplet states (CDOM)-C-3{*}
   are potentially key players in CDOM photodegradation and that such
   transformations are strongly influenced by small differences in CDOM
   sources and sinks. In contrast, center dot OH radicals are very unlikely
   to play a key role in phototransformation. These results represent an
   important first step in combining optical and transient species analyses
   to understand photodegradation processes of dissolved organic matter.
   (c) 2012 Elsevier Ltd. All rights reserved.}},
