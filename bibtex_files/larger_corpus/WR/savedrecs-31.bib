@article{ ISI:000306862500014,
Author = {Raynaud, Mickael and Vaxelaire, Jean and Olivier, Jeremy and
   Dieude-Fauvel, Emilie and Baudez, Jean-Christophe},
Title = {{Compression dewatering of municipal activated sludge: Effects of salt
   and pH}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
Volume = {{46}},
