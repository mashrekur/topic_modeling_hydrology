@article{ ISI:000301400500006,
Author = {Goncalves, M. R. and Costa, J. C. and Marques, I. P. and Alves, M. M.},
Title = {{Strategies for lipids and phenolics degradation in the anaerobic
   treatment of olive mill wastewater}},
