@article{ ISI:000306623200035,
Author = {O'Toole, Joanne and Sinclair, Martha and Malawaraarachchi, Manori and
   Hamilton, Andrew and Barker, S. Fiona and Leder, Karin},
Title = {{Microbial quality assessment of household greywater}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
