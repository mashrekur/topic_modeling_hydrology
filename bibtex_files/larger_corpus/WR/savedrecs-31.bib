@article{ ISI:000311130100030,
Author = {Maere, Thomas and Villez, Kris and Marsili-Libelli, Stefano and
   Naessens, Wouter and Nopens, Ingmar},
Title = {{Membrane bioreactor fouling behaviour assessment through principal
   component analysis and fuzzy clustering}},
Journal = {{WATER RESEARCH}},
