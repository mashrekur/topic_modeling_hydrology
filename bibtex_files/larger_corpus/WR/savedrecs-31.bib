@article{ ISI:000311130100007,
Author = {Talbot, Pauline and Martinelli, Laure and Talvy, Samuel and Chauveheid,
   Eric and Haut, Benoit},
Title = {{Ozone inactivation of resistant microorganisms: Laboratory analysis and
   evaluation of the efficiency of plants}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
