@article{ ISI:000303035400023,
Author = {Zhang, Yifeng and Angeliclaki, Irini},
Title = {{Innovative self-powered submersible microbial electrolysis cell (SMEC)
   for biohydrogen production from anaerobic reactors}},
Journal = {{WATER RESEARCH}},
