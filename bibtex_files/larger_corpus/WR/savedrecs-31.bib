@article{ ISI:000306862500001,
Author = {Rajasekhar, Pradeep and Fan, Linhua and Thong Nguyen and Roddick,
   Felicity A.},
Title = {{A review of the use of sonication to control cyanobacterial blooms}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
Volume = {{46}},
