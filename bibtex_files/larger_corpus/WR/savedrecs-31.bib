@article{ ISI:000309095800040,
Author = {Lotito, Adriana Maria and Di Iaconi, Claudio and Lotito, Vincenzo},
Title = {{Physical characterisation of the sludge produced in a sequencing batch
   biofilter granular reactor}},
Journal = {{WATER RESEARCH}},
