Author = {Zhang, Bing and Song, Xianfang and Zhang, Yinghua and Han, Dongmei and
   Tang, Changyuan and Yu, Yilei and Ma, Ying},
Title = {{Hydrochemical characteristics and water quality assessment of surface
   water and groundwater in Songnen plain, Northeast China}},
