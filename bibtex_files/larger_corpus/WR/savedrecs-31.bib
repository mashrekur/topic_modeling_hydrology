@article{ ISI:000306623200028,
Author = {Lee, Do Gyun and Zhao, Fuman and Rezenom, Yohannes H. and Russell, David
   H. and Chu, Kung-Hui},
Title = {{Biodegradation of triclosan by a wastewater microorganism}},
