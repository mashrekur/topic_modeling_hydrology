@article{ ISI:000312972200030,
Author = {Li, Guozheng and Park, Seongjun and Rittmann, Bruce E.},
Title = {{Developing an efficient TiO2-coated biofilm carrier for intimate
   coupling of photocatalysis and biodegradation}},
Journal = {{WATER RESEARCH}},
Year = {{2012}},
