@article{ ISI:000304585300009,
Author = {Kishida, Naohiro and Morita, Hisao and Haramoto, Eiji and Asami, Mari
   and Akiba, Michihiro},
Title = {{One-year weekly survey of noroviruses and enteric adenoviruses in the
   Tone River water in Tokyo metropolitan area, Japan}},
Journal = {{WATER RESEARCH}},
