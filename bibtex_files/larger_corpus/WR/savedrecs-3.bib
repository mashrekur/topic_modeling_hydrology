@article{ ISI:A1994NK88000029,
Author = {RAMAMURTHY, AS and TRAN, DM and CARBALLADA, LB},
Title = {{INCREASED HYDRAULIC RESISTANCE IN COMBINING OPEN-CHANNEL FLOWS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{6}},
Pages = {{1505-1508}},
Month = {{JUN}},
Abstract = {{In combining open channel flows, blockage at the junction is manifested
   by the rise in the stage due to the effect of lateral inflow from the
   branch channel. Blockage results in pockets of flow where the flow
   velocity is very small and the depth of flow is large. The entry of the
   lateral flow into the main channel also throttles the main channel flow
   and results in an increase in the hydraulic resistance to flow which in
   turn leads to considerable energy losses. In the design of open channel
   networks, the expected increase in the flow depth near the channel
   junction permits one to estimate the height of the channel walls in the
   vicinity of the junction. A relationship between blockage and the ratio
   of the discharges is proposed on the basis of the energy principle.
   Using this relationship, one can predict blockage for a right-angled
   junction in combining flow. Using experimental results, the proposed
   relationship is verified.}},
