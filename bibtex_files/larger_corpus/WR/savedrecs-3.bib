@article{ ISI:A1994NA22700024,
Author = {JIH, CG and HUANG, JS},
Title = {{EFFECT OF BIOFILM THICKNESS DISTRIBUTION ON SUBSTRATE-INHIBITED KINETICS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{4}},
Pages = {{967-973}},
Month = {{APR}},
Abstract = {{The effect of biofilm thickness distribution (BTD) on
   substrate-inhibited kinetics in anaerobic filters was evaluated. The
   modeling results show that the kinetic behavior of normal BTD is
   different from that of uniform biofilm thickness when the substrate
   utilization rate of some biofilms is limited by diffusion or reaction.
   However, when all biofilms are either diffusion-limited or
   reaction-limited, kinetic behaviors of normal BTD and uniform biofilm
   thickness are identical. In addition, the BTD can reduce the apparent
   maximum substrate utilization rate and narrow the multiple steady-state
   region of substrate-inhibited kinetics. In this study, phenol was used
   as an inhibitory substrate. A general agreement was found between the
   numerical simulation results using the normal BTD and experimental
   results for anaerobic filters. On the contrary, numerical simulation
   using uniform biofilm thickness failed to predict the outcome of the
   experiments. This indicates that an anaerobic filter is composed of a
   biofilm with varying thickness and thus, the BTD of a filter is of great
   importance, especially for treating wastewater containing inhibitory
   substrates.}},
