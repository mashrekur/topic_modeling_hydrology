@article{ ISI:A1994NC44800015,
Author = {KIM, CW and KOOPMAN, B and BITTON, G},
Title = {{INT-DEHYDROGENASE ACTIVITY TEST FOR ASSESSING CHLORINE AND
   HYDROGEN-PEROXIDE INHIBITION OF FILAMENTOUS PURE CULTURES AND
   ACTIVATED-SLUDGE}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{5}},
Pages = {{1117-1121}},
Month = {{MAY}},
Abstract = {{The INT-dehydrogenase activity (DHA) test was compared with OUR and ATP
   tests. DHA determined by extraction technique (DHA,) correlated linearly
   well with OUR and ATP in S. natans, type 021N pure cultures and
   activated sludge. DHA determined by direct counting technique (DHA,) was
   comparable to DHA, in pure cultures. Among three activity parameters
   (DHA, OUR and ATP) used to assess chlorine and hydrogen peroxide
   toxicity to the pure cultures and activated sludge, ATP was the least
   sensitive and the sensitivity of the other parameters was similar. S.
   natans and type 021N were equally sensitive to chlorine, whereas type
   021N was somewhat more sensitive to hydrogen peroxide than was S.
   natans.}},
