@article{ ISI:A1994MW93800012,
Author = {PICER, M and HOCENSKI, V},
Title = {{EVALUATION OF MODIFICATIONS OF THE SIMPLE SPECTROFLUOROMETRY METHOD FOR
   ESTIMATING PETROLEUM HYDROCARBON LEVELS IN SEA SEDIMENT SAMPLES}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{3}},
Pages = {{619-629}},
Month = {{MAR}},
Abstract = {{The monitoring of petroleum hydrocarbons in the water environment very
   often involves complicated methods. There are many difficulties
   encountered in analyzing large numbers of samples in the monitoring
   programs. In such cases, a simpler, though less precise and specific,
   technique using inexpensive equipment may be more desirable. This idea
   was realized in an assessment of the petroleum hydrocarbon pollution of
   the Mediterranean Sea where the oil pollutant (dissolved and/or
   dispersed in sea water) was measured by spectrofluorometry. This paper
   describes an analytical evaluation of three modifications of two simple
   spectrofluorometry methods, used on numerous sediment samples collected
   from various areas of the Adriatic Sea.}},
