@article{ ISI:A1994PB66200004,
Author = {KIA, SF and SMITH, JC},
Title = {{DISTRIBUTION OF LIGHT ORGANIC CONTAMINANTS IN THE GROUNDWATER TABLE
   VICINITY}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{10}},
Pages = {{2075-2086}},
Month = {{OCT}},
Abstract = {{The previously developed multiphase model is applied to numerically
   simulate migration of a lighter-than-water immiscible organic
   contaminant in the vicinity of the groundwater table in a field-scale
   contamination scenario. The numerical simulations show that the
   contaminated zone consists of two regions: the saturated region where
   the soil saturation with respect to the contaminant is at a maximum
   level, and the diffused region where the soil saturation varies from a
   maximum to a residual level. Strong spatial variation is predicted in
   the contaminant saturation profile, and an inflection point may exist in
   the profile. The simulations clearly show the development of a so called
   pancake over the saturated groundwater system with time, and project the
   resultant depression of the water interface. It is shown that the
   contaminant penetrates deep into the groundwater system and spreads
   laterally in both directions and at a rate much greater than that
   commonly derived based on the groundwater flow.}},
