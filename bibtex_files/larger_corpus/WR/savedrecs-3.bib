@article{ ISI:A1994NK88000007,
Author = {ESPLUGAS, S and YUE, PL and PERVEZ, MI},
Title = {{DEGRADATION OF 4-CHLOROPHENOL BY PHOTOLYTIC OXIDATION}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{6}},
Pages = {{1323-1328}},
Month = {{JUN}},
Abstract = {{The degradation of 4-chlorophenol (4-CP) by photolytic oxidation with
   ultraviolet light and ozone has been studied in a laboratory
   photoreactor. Ultraviolet irradiation on its own produced negligible
   degradation of 4-CP. Some degradation of 4-CP was obtained by ozonolysis
   without u.v. radiation. The mineralization of 4-CP to carbon dioxide and
   water was most effective when the organic was subjected to photolytic
   oxidation with u.v./ozone. The degradation kinetics, measured in terms
   of the rate of reduction of total organic carbon (TOC), was found to be
   first order with respect to TOC. The rate constant, however, is a
   function of absorbed radiation, a parameter which should be included in
   the modelling of the photolytic reaction kinetics.}},
