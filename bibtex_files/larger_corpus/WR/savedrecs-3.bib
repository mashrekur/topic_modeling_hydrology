@article{ ISI:A1994PJ23800020,
Author = {LISHMAN, LA and MURPHY, KL},
Title = {{THE SIGNIFICANCE OF HYDROLYSIS IN MICROBIAL DEATH AND DECAY}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{11}},
Pages = {{2417-2419}},
Month = {{NOV}},
Abstract = {{One of the distinctive features of the IAWPRC model is the
   death-regeneration cycle. The death-regeneration cycle is equivalent to
   the endogenous approach under conditions where there is no accumulation
   of intermediates. Experimental evidence would appear to indicate that
   hydrolysis may be the rate controlling step even at 20 degrees C. At
   lower temperatures preliminary studies indicate that hydrolysis is
   appreciably slower than either death or synthesis. Changes in the
   quantity of particulate slowly biodegradable COD will influence the
   determination of the net rate of endogenous respiration by batch
   digestion and the gross death and decay coefficient used in the IAWPRC
   model. Where hydrolysis is substantially slower than death a net
   accumulation of slowly biodegradable COD can be expected. This could
   explain the higher production of solids at lower temperatures even when
   a large percentage of the feed is rapidly biodegradable COD.}},
