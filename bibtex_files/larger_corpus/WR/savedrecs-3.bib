@article{ ISI:A1994PJ23800002,
Author = {VERMA, S and VALSARAJ, KT and WETZEL, DM and HARRISON, DP},
Title = {{DIRECT COMPARISON OF COUNTERCURRENT AND CASCADE CROSS-FLOW AIR STRIPPING
   UNDER FIELD CONDITIONS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{11}},
Pages = {{2253-2261}},
Month = {{NOV}},
Abstract = {{Field tests using `'identical'' cascade crossflow and countercurrent air
   strippers to treat contaminated ground water from an industrial site
   confirmed the advantages of the cascade crossflow configuration which
   had been previously demonstrated in the laboratory. In parametric tests,
   1,2-dichloroethane stripping efficiency and column pressure drop were
   measured as a function of air-to-water ratio using both equal water
   loading rates and equal water flow rates. Pressure drop in the cascade
   crossflow unit was more than one order of magnitude lower, and maximum
   air rates were four to five times greater than the comparable values in
   countercurrent Row. Because of the larger air rates, higher stripping
   efficiency was possible in the cascade crossflow stripper. For example,
   at a water loading rate of 18.9 kg/m(2).s, the maximum 1,2-dichlorethane
   stripping efficiency in countercurrent how was limited to 0.92, while
   greater than 0.99 efficiency was possible using a seven-pass crossflow
   column. Long-term tests, in which both columns were operated
   continuously for as long as two months, showed that the cascade
   crossflow stripper was more resistant to fouling than the countercurrent
   stripper.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD, ENGLAND OX5 1GB}},
