@article{ ISI:A1994MW93800020,
Author = {BLUM, DJW and SUFFET, IH and DUGUET, JP},
Title = {{QUANTITATIVE STRUCTURE-ACTIVITY RELATIONSHIP USING MOLECULAR
   CONNECTIVITY FOR THE ACTIVATED CARBON ADSORPTION OF ORGANIC-CHEMICALS IN
   WATER}},
