@article{ ISI:A1994ME86300012,
Author = {NEWMAN, ME and FILELLA, M and CHEN, YW and NEGRE, JC and PERRET, D and
   BUFFLE, J},
Title = {{SUBMICRON PARTICLES IN THE RHINE RIVER .2. COMPARISON OF FIELD
   OBSERVATIONS AND MODEL PREDICTIONS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{1}},
Pages = {{107-118}},
Month = {{JAN}},
Abstract = {{The particle size distribution in the Rhine River near Basle
   (Switzerland) was measured for a period of over 1 year. Peaks in the
   measured size distributions were consistently observed in the ranges of
   100-200, 300-700 nm and 1-3 mum. The size distributions observed did not
   vary greatly with time or flow rate. Predictions of a classical
   coagulation/sedimentation model agreed well with these field
   observations. This agreement between model predictions and field
   measurements indicated that the unknown (and probably dynamic)
   `'initial'' particle distribution may have been quickly transformed,
   either in the river or in the interstitial soil solution, through
   coagulation and sedimentation into the characteristic and relatively
   stable shape experimentally observed.}},
