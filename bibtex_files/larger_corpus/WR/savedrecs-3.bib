@article{ ISI:A1994MK29500014,
Author = {PASCOE, D and KEDWARDS, TJ and MAUND, SJ and MUTHI, E and TAYLOR, EJ},
Title = {{LABORATORY AND FIELD-EVALUATION OF A BEHAVIORAL BIOASSAY - THE
   GAMMARUS-PULEX (L) PRECOPULA SEPARATION (GAPPS) TEST}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{2}},
Pages = {{369-372}},
Month = {{FEB}},
Abstract = {{A bioassay, based upon the reproductive behaviour of the amphipod
   Gammarus pulex, was evaluated for its application in detecting
   freshwater pollutants. In the laboratory the bioassay proved to be
   useful in rapidly detecting a wide range of pollutants at concentrations
   significantly below those causing acute lethal toxicity and in the field
   it responded rapidly to a variety of pollutant inputs. The results
   indicate that this bioassay is simple, cost-effective and reliable and
   could be employed effectively for detecting and quantifying pollutants.}},
