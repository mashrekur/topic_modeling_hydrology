@article{ ISI:A1994PP84500018,
Author = {NETO, JSA and MARTINS, AJ and SANTANNA, GL},
Title = {{USE OF RAT-BRAIN NA+, K+-ATPASE ASSAY TO DETERMINE THE EFFECTIVENESS OF
   BIOLOGICAL TREATMENT TO REDUCE TOXICITY OF PAPER-MILL EFFLUENTS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{12}},
Pages = {{2583-2584}},
Month = {{DEC}},
Abstract = {{This work evaluates the toxicity of kraft mill wastewaters and the
   extent of detoxification attained after biological treatment of these
   effluents with the fungus Phanerochaete chrysosporium. The degree of
   inhibition of the enzyme Na+,K+-ATPase was used as an indicator of
   toxicity. The untreated alkali extraction stage effluent significantly
   inhibited the enzyme activity. When this effluent was treated
   biologically the degree of enzyme inhibition was practically reduced to
   zero. The inhibition of the enzyme Na+,K+-ATPase was found to be a
   suitable indicator of toxicity for bleached pulp and paper mill waste
   waters.}},
