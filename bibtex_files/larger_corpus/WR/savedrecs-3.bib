@article{ ISI:A1994NC44800002,
Author = {JAFVERT, CT and VANHOOF, PL and HEATH, JK},
Title = {{SOLUBILIZATION OF NONPOLAR COMPOUNDS BY NONIONIC SURFACTANT MICELLES}},
