@article{ ISI:A1994NW95000015,
Author = {FAHMY, M and KUT, OM and HEINZLE, E},
Title = {{ANAEROBIC-AEROBIC FLUIDIZED-BED BIOTREATMENT OF SULFITE PULP BLEACHING
   EFFLUENTS .1. GLOBAL PARAMETERS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{9}},
Pages = {{1987-1996}},
Month = {{SEP}},
Abstract = {{Four parallel biological fluidized bed reactor systems-one single
   aerobic, one single anaerobic and two combined anaerobic-aerobic-have
   been used to treat sulphite chlorine pulp bleaching wastewater.
   It has been observed that the AOX removal was limited at 30\%,
   independently of the wastewater dilution, mode of operation and
   retention time. Only small differences could be observed between the
   three systems involving an aerobic stage. The single aerobic reactor
   also gave rather competitive results with those having an additional
   anaerobic step, whereas the removal in the single anaerobic reactor was
   less than the three other systems. Only at high loading rates did the
   anaerobic-aerobic recycle reactor (AAR) perform better than the other
   systems.
   Constant NPOC/AOX ratio, before and after treatment, showed that the
   chlorinated material was degraded to the same extent as the
   non-chlorinated one.
   The removal by adsorption to biomass and biomass support material was
   less than 1\% for AOX.}},
