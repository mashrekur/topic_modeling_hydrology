@article{ ISI:A1994NR03100020,
Author = {ALBERICI, RM and JARDIM, WF},
Title = {{PHOTOCATALYTIC DEGRADATION OF PHENOL AND CHLORINATED PHENOLS USING
   AG-TIO2 IN A SLURRY REACTOR}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{8}},
Pages = {{1845-1849}},
Month = {{AUG}},
Abstract = {{Photodegradation of phenol (PhOH), 2,4-dichlorophenol (2,4-DCP),
   2,3,5-trichlorophenol (2,3,5-TCP) and pentachlorophenol (PCP) was
   investigated using an open upflow slurry reactor and TiO2 (Ag-loaded) as
   photocatalyst. Light was provided by three 125 W high-pressure mercury
   lamps. Best degradation rates were obtained using 250 mg 1(-1) of
   Ag-TiO2 in the presence of H2O2 and a constant supply of air (1.71
   min(-1)). Under this optimized condition, destruction rate constants
   obtained for a 1x10(-4)M solution of PhOH, 2,4-DCP, 2,3,5-TCP and PCP
   were 0.0493, 0.0430, 0.0367 and 0.0356 min(-1), respectively. Each gram
   of phenol required 1.70 kW for its photodestruction.}},
