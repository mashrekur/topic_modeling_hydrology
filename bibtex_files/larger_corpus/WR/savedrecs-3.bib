@article{ ISI:A1994NC44800016,
Author = {MACKIE, GL and WRIGHT, CA},
Title = {{ABILITY OF THE ZEBRA MUSSEL, DREISSENA-POLYMORPHA TO BIODEPOSIT AND
   REMOVE PHOSPHORUS AND BOD FROM DILUTED ACTIVATED SEWAGE-SLUDGE}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{5}},
Pages = {{1123-1130}},
Month = {{MAY}},
Abstract = {{Zebra mussels were suspended in sediment traps within 10-1. containers
   for 96 h in 1.5, 3.0, 7.5, 15 and 30\% activated sewage sludge. Water
   samples were removed at 1, 6, 12, 24 and 48 h and analyzed for
   turbidity, total phosphorus and BOD (5 days, 20 degrees C) and compared
   to values obtained from controls (i.e; containers with the same
   concentrations of activated sewage sludge but lacking zebra mussels).
   The amount of material deposited in the sediment traps (i.e.
   biodeposition) was compared to the amount deposited outside the sediment
   traps (i.e. biocontrols) and to control containers after 96 h. The rate
   of increase in water clarity and the rate of decrease in total
   phosphorus levels were significantly greater in containers with zebra
   mussels than in controls for all concentrations of activated sewage
   sludge tested. The mussels significantly reduced the BOD levels in
   containers with less than 3.0\% sludge within 48 h. In containers with
   zebra mussels, nearly all the seston and nutrients were biodeposited in
   the sediment traps. The results indicate the potential use of zebra
   mussels for removing from diluted sludge turbidity, phosphorus and BOD
   by means of biodeposition.}},
