@article{ ISI:A1994MK29500003,
Author = {PAVLOSTATHIS, SG and MORRISON, D},
Title = {{RESPONSE OF CONTINUOUS-FLOW ACTIVATED-SLUDGE REACTORS TO PHOTOPROCESSING
   WASTEWATERS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{2}},
Pages = {{269-276}},
Month = {{FEB}},
Abstract = {{Continuous-flow activated sludge reactors were used for the treatment of
   photoprocessing wastewaters from two commonly used photoprocesses,
   namely KODAK Flexicolor Process C-41 (film processing) and KODAK
   Ektacolor Process RA-4 (paper processing). Simulated wastewaters were
   fed to laboratory-scale reactors at a loading level of 10\% (v/v) along
   with a synthetic base feed. The biodegradation of the base feed was not
   affected by the photoprocessing wastewaters. In addition, more than 36\%
   of the photoeffluent-derived chemical oxygen demand (COD) was destroyed,
   primarily as a result of sulfite and thiosulfate oxidation. Ethylene
   diamine tetraacetate (EDTA) was recalcitrant and accounted for a
   significant fraction of the effluent COD. The photoeffluent-amended
   reactors showed a decrease in ammonia removal as compared to the control
   reactor (fed only the base feed). Significant consumption of alkalinity
   was observed in the photoeffluent-amended reactors due to the oxidation
   of sulfite, thiosulfate and ammonia. Acute toxicity bioassays using two
   species, fathead minnow (Pimephales promelas) and a daphnid (Daphnia
   magna), revealed that the activated sludge treatment rendered the
   photoprocessing wastewaters non-toxic to the test organisms.}},
