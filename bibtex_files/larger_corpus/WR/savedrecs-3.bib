@article{ ISI:A1994NA22700014,
Author = {KOYAMA, O and KAMAGATA, Y and NAKAMURA, K},
Title = {{DEGRADATION OF CHLORINATED AROMATICS BY FENTON OXIDATION AND
   METHANOGENIC DIGESTER SLUDGE}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{4}},
Pages = {{895-899}},
Month = {{APR}},
Abstract = {{Chlorinated aromatic compounds were oxidatively degraded by Fenton
   reaction (a chemical oxidation by H2O2 in the presence of Fe2+ as
   catalyst). The breakdown products, mainly consisting of formate and
   oxalate, were readily converted to methane by methanogenic digester
   sludge without any acclimation with the products. This process was quite
   effective for decomposition of chlorobenzoates, chlorophenols,
   dichlorophenols and p-chlorobiphenyl. The methanogenic degradation of
   oxalate was found to be responsible for Oxalobacter formigenes and
   Methanobacterium-like methanogens in the digester sludge.}},
