@article{ ISI:A1994NA22700001,
Author = {REICHEL, G and NACHTNEBEL, HP},
Title = {{SUSPENDED SEDIMENT MONITORING IN A FLUVIAL ENVIRONMENT - ADVANTAGES AND
   LIMITATIONS APPLYING AN ACOUSTIC-DOPPLER-CURRENT-PROFILER}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{4}},
Pages = {{751-761}},
Month = {{APR}},
Abstract = {{Acoustic Doppler Current Profilers (ADCPs) are state of the art in
   oceanography and hydrometry for current velocity and discharge
   measurements. Additionally, efforts are made to derive characteristics
   of suspended particles from ADCP data. This paper summarizes experiences
   from the application of an ADCP for suspended sediment monitoring in a
   fluvial environment with special respect to available state of the art
   monitoring methods. Both theoretical and some experimental results are
   presented based on investigations in a reservoir in the Austrian section
   of the Danube. The ADCP appears to be a valuable tool for detecting
   inhomogeneities in the suspended sediment transport and for determining
   sample locations. Additionally, it can be instrumental in temporal
   interpolation and regionalization between point values of parameters
   obtained by standard monitoring methods. Due to methodical limitations
   and the character of the fluvial suspended sediments, however, it cannot
   serve as an instrument to measure the particle size distribution and the
   particle concentration.}},
