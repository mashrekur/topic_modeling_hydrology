@article{ ISI:A1994MK29500005,
Author = {HAO, OJ and PHULL, KK and CHEN, JM},
Title = {{WET OXIDATION OF TNT RED WATER AND BACTERIAL TOXICITY OF TREATED WASTE}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{2}},
Pages = {{283-290}},
Month = {{FEB}},
Abstract = {{This laboratory study determined the composition of the off-gas after
   wet oxidation (WO) of trinitrotoluene (TNT) red water, and monitored
   bacterial toxicity of treated waste. The amount of sulfate concentration
   in the treated sample is much higher than the sum of the measured
   inorganic sulfate and the organic sulfur associated with the
   dinitrotoluene sulfonates initially present in the raw red water. This
   indicates the presence of other unidentified S-containing organic
   compounds in red water. The nitrogen analyses indicate a significant
   NH4+ concentration (32 mg N/1) in treated waste and a high N-2
   concentration (14.6\%; net increase of 4.6\%) in the off-gas. The
   toxicity results indicate that the WO-treated red water exerts toxic
   effects on Acinetobacter and Nitrosomonas growth, but has less impact on
   activated sludge. Furthermore, treated samples at 340 degrees C appear
   to exhibit a more inhibitory effect on both Acinetobacter and
   Nitrosomonas than those treated at 260 degrees C.}},
