@article{ ISI:A1994NC44800030,
Author = {KIM, BR and SUIDAN, MT and TRAEGNER, UK},
Title = {{CONSIDERING AGE DISTRIBUTION FOR A PAC/WATER SLURRY REACTOR MODEL}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{5}},
Pages = {{1241-1245}},
Month = {{MAY}},
Abstract = {{In a powdered activated carbon (PAC)/water slurry reactor, each PAC
   particle has a different residence time, resulting in a distribution of
   residence times, called the exit age distribution. Two mathematical
   modeling approaches were compared with regard to handling the age
   distribution for a PAC/water slurry reactor which removes an adsorbate
   by adsorption. One approach implicitly includes the age distribution as
   part of the governing equations by developing them based on an
   age-averaged PAC particle whereas the other approach explicitly includes
   the age distribution by developing the equations based on individual PAC
   particles with various ages. The two approaches converged for a
   steady-state case where adsorption isotherm is linear. In addition, the
   former approach resulted in a much simpler set of equations as compared
   to that of the latter, resulting in much less numerical complexity for
   general cases.}},
