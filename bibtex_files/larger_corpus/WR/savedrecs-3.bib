@article{ ISI:A1994NK88000025,
Author = {DRNDARSKI, N and KRIZMAN, M},
Title = {{RADIOISOTOPES IN PERIPHYTON AND SEDIMENTS FROM THE KOLUBARA RIVER AND
   ITS TRIBUTARIES}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{6}},
Pages = {{1471-1474}},
Month = {{JUN}},
Abstract = {{The site-specific concentrations of the fission radionuclide Cs-137, an
   activation radionuclide Cs-134 and the naturally occurring radionuclides
   K-40, U-238, Ra-226 and Th-232 were determined by gamma-spectroscopy in
   periphyton and sediment samples collected from the Kolubara River and
   its tributaries. Various concentration ratios were calculated in order
   to explain the bioaccumulation and distribution of radioisotopes in the
   Kolubara River ecosystem. The total gamma activities of river sediments
   collected at the selected locations were also calculated. The
   concentrations of radiocesium per square meter of sediments were
   compared to the concentrations of Cs-137 for the regional soil after the
   Chernobyl accident. The presence of radioactive isotopes, such as Cs-137
   and Cs-134, in the Kolubara River environment from the Chernobyl fallout
   was evident.}},
