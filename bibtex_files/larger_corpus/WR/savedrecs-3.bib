@article{ ISI:A1994NW95000013,
Author = {MHAMDI, MA and ALEYA, L and DEVAUX, J},
Title = {{PHOSPHORUS EXCHANGES BETWEEN SEDIMENT AND WATER IN TROPHICALLY DIFFERENT
   RESERVOIRS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{9}},
Pages = {{1971-1980}},
Month = {{SEP}},
Abstract = {{Phosphorus exchanges within the sediment-water interface coupled with
   several parameters were studied in several reservoirs with geologically
   different catchment basins and different trophic levels in Morocco and
   France.
   The results showed that these exchanges were regulated by a combination
   of the physical-chemical variability of the environment, the geological
   composition of the catchment basins and the trophic status of the lake.
   In the hypereutrophic Villerest reservoir, iron-bound phosphorus is the
   major form of phosphorus trapped by the sediment whereas, in Moroccan
   reservoirs, calcium-bound phosphorus prevailed.
   We suggest that drastic control of phosphorus input into waters be
   carried out via a large dephosphorization program of the tributaries to
   avoid bloom formation in the Villerest reservoir and calcium-bound
   phosphorus dissociation with upward release of bioavailable phosphorus.}},
