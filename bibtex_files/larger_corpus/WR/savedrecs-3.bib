@article{ ISI:A1994PB66200023,
Author = {ZAHID, W and GANCZARCZYK, J},
Title = {{A TECHNIQUE FOR A CHARACTERIZATION OF RBC BIOFILM SURFACE}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{10}},
Pages = {{2229-2231}},
Month = {{OCT}},
Abstract = {{A new technique was developed to evaluate geometric irregularities of
   biofilm surface. Vertical microtome sections of RBC biofilm samples
   embedded in glycol methacrylate resin were cut in two perpendicular
   directions and their profiles were examined by light microscopy image
   analysis. The measured fractal dimensions of the studied RBC biofilm
   surface were in the broad range from 2.1 to 2.8. The hydrodynamic and
   mass transfer models for biofilm systems may be greatly improved by
   inclusion of numerical values characterizing the surfaces involved.}},
