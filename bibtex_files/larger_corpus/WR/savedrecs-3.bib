@article{ ISI:A1994NR03100008,
Author = {LAPSIRIKUL, W and CORDRUWISCH, R and HO, G},
Title = {{ANAEROBIC BIOFLOCCULATION OF WOOL SCOURING EFFLUENT}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{8}},
Pages = {{1743-1747}},
Month = {{AUG}},
Abstract = {{A two-stage anaerobic bioflocculation process (ABP) successfully
   flocculated a high grease (> 15 g/1) wool scouring effluent (WSE).
   During 110 days of continuous operation, a two-stage laboratory process
   removed between 70 and 90\% grease and 60-86\% COD at a combined
   hydraulic residence time (HRT) of 4-10 days. Grease removal was more
   difficult with low grease (< 10 g/1) WSE. At a HRT of 3 days a
   single-stage ABP removed 40 and 44\% grease at 20 and 37 degrees C,
   respectively.
   Methane production and volatile fatty acids consumption were negligible.
   Most of the grease was removed by flocculation as a result of anaerobic
   bacterial activity. The supernatant was successfully treated by an
   aerobic activated sludge process reducing grease concentration from
   about 1.5 to less than 0.1 g/1 in the final effluent at an HRT of 3
   days.}},
