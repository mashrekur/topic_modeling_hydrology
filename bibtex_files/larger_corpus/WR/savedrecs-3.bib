
@article{ ISI:A1994PP84500001,
Author = {VILKER, VL and LYKLEMA, J and NORDE, W and VERDUIN, BJM},
Title = {{INTERACTION BETWEEN A PLANT-VIRUS AND COLLOIDAL HEMATITE}},
Journal = {{WATER RESEARCH}},
