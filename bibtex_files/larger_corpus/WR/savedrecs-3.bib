@article{ ISI:A1994NW95000018,
Author = {HE, WS and PING, Z},
Title = {{ORGANIC-MATTER DEGRADATION KINETICS IN A FLUIDIZED-BED BIOREACTOR}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{9}},
Pages = {{2021-2028}},
Month = {{SEP}},
Abstract = {{An organic matter degradation kinetics model in a fluidized bed
   bioreactor (FBBR) has been established based on the porous pellet model
   in this paper. Synthetic phenolic wastewater has been used as substrate
   to verify the validity of the model which has good precision. The
   transfer and degradation mechanism of organic matter in biofilm and the
   effects of biofilm thickness on reactor performance have also been
   tested and analyzed in detail. The research results indicate that the
   high efficiency of the fluidized bed bioreactor lies in the high value
   of eta X and that biofilm thickness is a significant factor affecting
   eta, X and reactor performance.}},
