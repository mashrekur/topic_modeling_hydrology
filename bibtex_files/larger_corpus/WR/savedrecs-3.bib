@article{ ISI:A1994ME86300002,
Author = {SAKAKIBARA, Y and FLORA, JRV and SUIDAN, MT and BISWAS, P and KURODA, M},
Title = {{MEASUREMENT OF MASS-TRANSFER COEFFICIENTS WITH AN ELECTROCHEMICAL METHOD
   USING DILUTE ELECTROLYTE-SOLUTIONS}},
Journal = {{WATER RESEARCH}},
Year = {{1994}},
Volume = {{28}},
Number = {{1}},
Pages = {{9-16}},
Month = {{JAN}},
Abstract = {{An electrochemical method for measuring mass transfer coefficients using
   dilute electrolyte solutions is described. Oxidation-reduction potential
   measurements are used to determine the limiting current density. A
   mathematical model taking into account the diffusion and migration of
   the various electrolyte species was developed to evaluate the mass
   transfer coefficient. The range of the Sherwood numbers obtained with
   the present method were of the same order of magnitude as those obtained
   from classical mass transfer studies. The proposed method and the
   analytical procedure is applicable to electrochemical bioreactors where
   the use of excess electrolytes is not recommended.}},
