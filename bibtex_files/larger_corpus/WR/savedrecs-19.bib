@article{ ISI:000226727600010,
Author = {Casanova, J and Negrel, P and Blomqvist, R},
Title = {{Boron isotope fractionation in groundwaters as an indicator of past
   permafrost conditions in the fractured crystalline bedrock of the
   fennoscandian shield}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{2-3}},
Pages = {{362-370}},
Month = {{JAN-FEB}},
Abstract = {{The Fennoscandian Shield has been subjected to several glaciations over
   the past million years, the last of which (Weichselian Ice Age) ended
   only at about 10 Ka. Here we used boron isotopes and B contents to (a)
   establish the degree of water-rock interaction (WRI) and (b) clarify
   freezing processes within groundwaters from the Aspo site in Sweden and
   from various sites in Finland. The high delta(11) B values recorded by
   all groundwaters (up to 51.9parts per thousand) including diluted,
   boron-poor, inland groundwaters suggest selective uptake of B-10 into
   ice related to freezing processes under permafrost conditions. According
   to co-existing ice and residual brines in a Canadian frozen mine, this
   fractionation process, enhanced by Rayleigh fractionation, can generate
   a natural field of isotopic variation around 60parts per thousand and
   provides a new application of B isotope that makes possible to easily
   characterise groundwaters that underwent past permafrost conditions. (C)
   2005 Elsevier Ltd. All rights reserved.}},
