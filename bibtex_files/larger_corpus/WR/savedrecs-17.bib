@article{ ISI:000221776200018,
Author = {Qi, YN and Shang, C and Lo, IMC},
Title = {{Formation of haloacetic acids during monochloramination}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{9}},
Pages = {{2375-2383}},
Month = {{MAY}},
Abstract = {{Factors that affect the formation of haloacetic acids (HAAs) during
   monochloramination, such as monochloramine application techniques, the
   initial chlorine (Cl) to ammonia-N (N) ratios, the bromide
   concentrations, and the wastewater quality, were studied. Aqueous humic
   acid solutions and undisinfected wastewater effluent samples obtained
   from two Hong Kong Sewage Treatment Works (STWs) were monochloraminated
   under various conditions. HAA formation was strongly affected by the
   monochloramine application techniques. The formation of trichloroacetic
   acid (TCAA) and total HAAs was reduced by adding preformed
   monochloramine. A higher initial Cl:N ratio indicated a higher chlorine
   demand and consequently led to higher HAA yields. Increasing the bromide
   concentration shifted HAAs from chlorinated species to brominated
   species and increased the yields of total HAAs, concurrent with
   decreases in the yields of dichloroacetic acid (DCAA) and TCAA but with
   increases in those of the other HAAs measured. Variations in the
   patterns of HAA formation were observed in monochloraminated wastewater
   effluent samples. The variations cannot be simply explained by the
   chlorine chemistry involving ammonia and/or bromide but are likely
   attributable to the combining effects of the water quality and the
   characteristics of the organics in the wastewater. (C) 2004 Elsevier
   Ltd. All rights reserved.}},
