@article{ ISI:000187881200011,
Author = {Lima, SAC and Raposo, MFJ and Castro, PML and Morais, RM},
Title = {{Biodegradation of p-chlorophenol by a microalgae consortium}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{1}},
Pages = {{97-102}},
Month = {{JAN}},
Abstract = {{An aquatic community was recovered from a waste discharge container fed
   with several aromatic pollutants. After 3 months of selective enrichment
   with p-chlorophenol and p-nitrophenol, two microalgae species, Chlorella
   vulgaris and Coenochloris pyrenoidosa, were recovered from the microbial
   consortium. As an axenic culture, this microalgae consortium was able to
   remove p-chlorophenol under different photo-regimes. Cultures grown
   under a 24 h light regime were capable of biodegrading 50 mg l(-1) of
   p-chlorophenol within 5 days. Addition of zeolite, an adsorbing
   material, did not improve the p-chlorophenol removal. However, when
   p-chlorophenol at 150 mgl(-1) was fed to the culture supplemented with
   zeolite, the growth rate of the consortium improved, but the lag phase
   was longer (16 against 14 days in the absence of zeolite). (C) 2003
   Elsevier Ltd. All rights reserved.}},
