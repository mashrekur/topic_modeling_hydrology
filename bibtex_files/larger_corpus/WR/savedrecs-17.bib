@article{ ISI:000188061900025,
Author = {McCurdy, K and Carlson, K and Gregory, D},
Title = {{Floc morphology and cyclic shearing recovery: comparison of alum and
   polyaluminum chloride coagulants}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{2}},
Pages = {{486-494}},
Month = {{JAN}},
Abstract = {{This study investigated the floe formation and re-aggregation potential
   for alum, polyaluminum chloride (PACl) and a blend of these coagulants.
   Bench-scale testing included floc morphology characterization for
   well-developed floe, post-shear floe, and non-settleable (filter
   influent) floe. Different applications of coagulants were observed to
   produce non-settleable floe that was morphologically different. The alum
   treatment had a decrease in average floe size from coagulated to
   non-settleable conditions, whereas the PACl and PACl/alum treatments
   resulted in similar sized floe between these processes. Zeta potential
   distribution measurements showed that the alum treatment resulted in a
   negative shift from coagulated to non-settleable conditions whereas the
   PACl and PACl/alum treatments had no significant shift. A photometric
   dispersion analyzer (PDA) was employed to compare the differences
   between coagulant treatments with respect to shear induced aggregate
   breakup and recovery. The PDA allowed a dynamic monitoring of initial
   floe aggregation and measured the degree of recovery from cyclic
   shearing. The degree of recovery from shearing was greatest for the
   PACl/alum treatment likely as a result of increased collision efficiency
   due to more effective charge neutralization. (C) 2003 Elsevier Ltd. All
   rights reserved.}},
