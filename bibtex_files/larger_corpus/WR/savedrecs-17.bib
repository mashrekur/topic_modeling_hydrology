@article{ ISI:000221776200019,
Author = {Le Truong, G and De Laat, J and Legube, B},
Title = {{Effects of chloride and sulfate on the rate of oxidation of ferrous ion
   by H2O2}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{9}},
Pages = {{2384-2394}},
Month = {{MAY}},
Abstract = {{The rates of oxidation of Fe(II) by H2O2 in the presence of sodium
   perchlorate, sodium nitrate, sodium chloride and sodium sulfate salts
   (0-1 M) have been compared in the study. Experiments were carried out in
   a batch reactor, in the dark, at pH < 3, 25 +/- 0.5degreesC and at
   controlled ionic strength (less than or equal to 1M). The experimental
   results showed that the rates of oxidation of Fe(II) in the presence of
   chloride, nitrate and perchlorate were identical. In the presence of
   sulfate, the rate of oxidation of Fe(II) was faster and depended on the
   pH and the concentration of sulfate. The pseudo second-order rate
   constants for the reaction of H2O2 with Fe2+, FeCl+ and FeSO4 were
   determined as 55 +/- 1, 55 +/- 1 and 78 +/- 3 M-1 s(-1), respectively.
   (C) 2004 Elsevier Ltd. All rights reserved.}},
