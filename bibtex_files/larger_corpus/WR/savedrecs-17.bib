@article{ ISI:000188364500017,
Author = {Tadesse, I and Green, FB and Puhakka, JA},
Title = {{Seasonal and diurnal variations of temperature, pH and dissolved oxygen
   in advanced integrated wastewater pond system (R) treating tannery
   effluent}},
