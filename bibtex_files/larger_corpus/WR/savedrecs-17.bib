@article{ ISI:000220501400010,
Author = {Dumortier, R and Rodil, E and Weber, ME and Vera, JH},
Title = {{Selective removal of gallium(iii) from aqueous solutions containing zinc
   or aluminum using sodium di-(n-octyl) phosphinate}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{7}},
Pages = {{1745-1752}},
Month = {{APR}},
Abstract = {{Gallium was removed selectively from aqueous solutions containing zinc
   or aluminum using sodium di-(n-octyl) phosphinate as a ligand (NaL). At
   low pH or low mole ratios, the gallium was removed by complexation with
   the ligand as GaL3(S), while the zinc or the aluminum remained in the
   solution. Nearly complete separation of gallium was obtained. By
   increasing the amount of ligand or by increasing the pH, the zinc or
   aluminum remaining in the solution was then removed as a solid complex:
   ZnL2(S) or AIL(3(S)), respectively. At a pH between 1.5 and 2 and a mole
   ratio ligand to total metals of 0.75 for zinc solutions and 1.0 for
   aluminum solutions, more than 98\% of the gallium was selectively
   removed with a high molar selectivity, alpha(Ga/Zn) and alpha(Ga/Al),
   respectively. Over 95\% of gallium was recovered from the solid GaL3(S)
   complex by treatment of the complex with a 3 M NaOH solution and diethyl
   ether. The gallium was concentrated in the aqueous solution to 4 times
   its initial concentration and the ligand was extracted into the ether
   phase. After evaporation of the ether, 95\% of the ligand was
   regenerated in its sodium form as a solid. (C) 2004 Elsevier Ltd. All
   rights reserved.}},
