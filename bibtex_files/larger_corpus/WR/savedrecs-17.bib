@article{ ISI:000223414000014,
Author = {Jankowska, A and Biesaga, M and Drzewicz, P and Trojanowicz, M and
   Pyrzynska, K},
Title = {{Chromatographic separation of chlorophenoxy acid herbicides and their
   radiolytic degradation products in water samples}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{14-15}},
Pages = {{3259-3264}},
Month = {{AUG-SEP}},
Abstract = {{HPLC procedure for simultaneous determination of chlorophenoxy acid
   herbicides and their radiolytic degradation products in waters is
   described with the use of octadecylsilica column and spectrophotometric
   detection at 280 nm. The satisfactory separation was achieved with a
   mobile phase of pH 2.5 consisting of 43.7 mM acetic acid with 40\% (v/v)
   acetonitrile. Limit of detection values for herbicides and phenol
   derivatives were in the range of 19-41 mug/l and 1060 mug/l,
   respectively. The developed method was applied for monitoring the
   effectiveness of radiolytic degradation of herbicides. Studies of
   products of gamma-radiolysis of 2,4-dichlorophenol have shown that the
   efficiency of this process is affected by the presence of naturally
   occurring scavengers of gamma-radiation such as carbonates or nitrates.
   (C) 2004 Elsevier Ltd. All rights reserved.}},
