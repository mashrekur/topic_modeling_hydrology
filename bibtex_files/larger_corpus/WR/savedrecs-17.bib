@article{ ISI:000224014900011,
Author = {Phe, MH and Dossot, M and Block, JC},
Title = {{Chlorination effect on the fluorescence of nucleic acid staining dyes}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{17}},
Pages = {{3729-3737}},
Month = {{OCT}},
Abstract = {{An alternative to culture methods for the control of drinking water
   disinfection would use fluorescent dyes that could evidence the nucleic
   acid damages provoked by sodium hypochlorite treatment. The two dyes
   selected in this study, SYBR Green II RNA gel stain and TOTO-1 iodide,
   efficiently stain nucleic acids (DNA and RNA) and quite poorly the other
   biomolecules considered (Bovine serum albumin, palmitic acid and
   dextrane). After treatment of nucleic acid solutions with increasing
   amounts of sodium hypochlorite, a decrease of fluorescence intensity is
   observed for both DNA and RNA stained with either SYBR-II or TOTO-1.
   However, the two fluorochromes do not lead to the same results, which
   shows that the two dyes are not bound to nucleic acids in the same way.
   Contrary to TOTO-1, SYBR-II reveals to be sufficiently sensitive to
   indicate both DNA or RNA damages as soon as the latter are in contact
   with hypochlorite even at concentrations of HClO lower than 10 mumol/L.
   Moreover, SYBR-II offers the opportunity to make quantitative titration
   of chlorine treated DNA and therefore seems to be the appropriate
   candidate to control the efficiency of the hypochlorite disinfection
   process of drinking water samples. (C) 2004 Elsevier Ltd. All rights
   reserved.}},
