@article{ ISI:000187881200018,
Author = {Fujita, H and Izumi, J and Sagehashi, M and Fujii, T and Sakoda, A},
Title = {{Decomposition of trichloroethene on ozone-adsorbed high silica zeolites}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{1}},
Pages = {{166-172}},
Month = {{JAN}},
Abstract = {{We developed a novel ozonation process for water treatment using high
   silica zeolites as an adsorptive concentrator of water-dissolved ozone
   and organic pollutants, resulting in a significant increase in reaction
   rate. In experiments involving trichloroethene (TCE) decomposition using
   a tubular flow reactor, TCE decomposition was much greater in the
   presence of ZSM-5 (SiO2/Al2O3 ratio = 3000) than in its absence,
   possibly due to the high concentrations of ozone and TCE inside the
   adsorbent. The TCE conversion obtained in our experiments was found to
   reach its theoretically maximum limit. (C) 2003 Elsevier Ltd. All rights
   reserved.}},
