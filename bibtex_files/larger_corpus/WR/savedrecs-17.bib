@article{ ISI:000223414000017,
Author = {Morel, E and Santamaria, K and Perrier, M and Guiot, SR and Tartakovsky,
   B},
Title = {{Application of multi-wavelength fluorometry for on-line monitoring of an
   anaerobic digestion process}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{14-15}},
Pages = {{3287-3296}},
Month = {{AUG-SEP}},
Abstract = {{This work examined the use of multi-wavelength fluorometry for on-line
   monitoring of an anaerobic digestion process. Experiments were carried
   out in a laboratory-scale anaerobic digestor fed with either synthetic
   or agricultural (cheese factory) wastewater. An in-line fiber optic
   probe installed in the external recirculation loop of the reactor was
   used to acquire fluorescence spectra with an interval of 5-10 min. The
   spectra were compared with analytical measurements taken at the same
   time to develop regression models, which were then used to predict
   concentrations of chemical oxygen demand, volatile fatty acids, and
   other key process parameters. A comparison of partial least squares
   (PLS), nonlinear principal components regression, and step-wise
   regression models on an independent set of data showed that the PLS
   model gave the best prediction accuracy. Crown Copyright (C) 2004
   Published by Elsevier Ltd. All rights reserved.}},
