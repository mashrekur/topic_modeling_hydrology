@article{ ISI:000220501400007,
Author = {Komilis, DP and Ham, RK and Park, JK},
Title = {{Emission of volatile organic compounds during composting of municipal
   solid wastes}},
Journal = {{WATER RESEARCH}},
Year = {{2004}},
Volume = {{38}},
Number = {{7}},
Pages = {{1707-1714}},
Month = {{APR}},
Abstract = {{The objective of this study was to identify and quantify volatile and
   semi-volatile organic compounds (VOCs) produced during composting of the
   organic fraction of municipal solid wastes (MSW). A laboratory
   experiment was conducted using organic components of MSW that were
   decomposed under controlled aerobic conditions. Mixed paper primarily
   produced alkylated benzenes, alcohols and alkanes. Yard wastes primarily
   produced terpenes, alkylated benzenes, ketones and alkanes, while food
   wastes primarily produced sulfides, acids and alcohols. Among 13
   aromatic VOCs found in MSW composting facilities, toluene, ethylbenzene,
   1,4-dichlorobenzene, p-isopropyl toluene, and naphthalene were in the
   largest amounts. Unseeded mixed paper, seeded mixed paper, seeded yard
   wastes, unseeded yard wastes, seeded food wastes and unseeded food
   wastes produced approximately 6.5, 6.1, 2.1, 0.83, 2.5 and 0.33 mg of 13
   volatile and semi-volatile aromatic organic compounds combined,
   respectively, per dry kg. All VOCs were emitted early during the
   composting process and their production rates decreased with time at
