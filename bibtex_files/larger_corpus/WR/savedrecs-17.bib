@article{ ISI:000225194700020,
Author = {Kwan, CY and Chu, W},
Title = {{A study of the reaction mechanisms of the degradation of
   2,4-dichlorophenoxyacetic acid by oxalate-mediated photooxidation}},
Journal = {{WATER RESEARCH}},
