@article{ ISI:000256335400038,
Author = {Schaefer, Ralf Bernhard and Paschke, Albrecht and Vrana, Branislav and
   Mueller, Ralf and Liess, Matthias},
Title = {{Performance of the Chemcatcher (R) passive sampler when used to monitor
   10 polar and semi-polar pesticides in 16 Central European streams, and
   comparison with two other sampling methods}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{10-11}},
Pages = {{2707-2717}},
Month = {{MAY}},
Abstract = {{We investigated the performance of the Chemcatcher (R), an aquatic
   passive sampling device consisting of a sampler body and an Empore (R)
   disk as receiving phase, when used to monitor acetochlor, alachlor,
   carbofuran, chlorfenvinphos, alpha-endosulfan, fenpropidin, linuron,
   oxadiazon, pirimicarb and tebuconazole in 16 Central European streams.
   The Chemcatcher (R), equipped with an SDB-XC Empore (R) disk, detected
   seven of the aforementioned pesticides with a total of 54 detections.
   The time-weighted average (TWA) concentrations reached up to 1 mu g/L
   for acetochlor and alachlor. Toxic units derived from these
   concentrations explained reasonably well the observed ecological effects
   of pesticide stress, measured with the SPEAR index. In a follow-up
   analysis, we compared the Chemcatcher (R) performance with those of two
   other sampling systems. The results obtained with the Chemcatcher (R)
   closely matched those of the event-driven water sampler. By contrast,
   the TWA concentrations were not significantly correlated with
   concentrations on suspended particles. We conclude that the Chemcatcher
   (R) is suitable for the monitoring of polar organic toxicants and
   presents an alternative to conventional spot sampling in the monitoring
   of episodically occurring pollutants. (c) 2008 Elsevier Ltd. All rights
   reserved.}},
