@article{ ISI:000253512200021,
Author = {de la Rubia, Angeles and Rodriguez, Manuel and Leon, Victor M. and
   Prats, Daniel},
Title = {{Removal of natural organic matter and THM formation potential by ultra-
   and nanofiltration of surface water}},
