@article{ ISI:000259753000046,
Author = {Berney, Michael and Vital, Marius and Huelshoff, Iris and Weilenmann,
   Hans-Ulrich and Egli, Thomas and Hammes, Frederik},
Title = {{Rapid, cultivation-independent assessment of microbial viability in
   drinking water}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{14}},
Pages = {{4010-4018}},
Month = {{AUG}},
Abstract = {{Fast and accurate monitoring of chemical and microbiological parameters
   in drinking water is essential to safeguard the consumer and to improve
   the understanding of treatment and distribution systems. However, most
   water utilities and drinking water guidelines still rely solely on
   time-requiring heterotrophic plate counts (HPC) and plating for faecal
   indicator bacteria as regular microbiological control parameters. The
   recent development of relative simple bench-top flow cytometers has made
   rapid and quantitative analysis of cultivation-independent microbial
   parameters more feasible than ever before. Here we present a study using
   a combination of cultivation-independent methods including fluorescence
   staining (for membrane integrity, membrane potential and esterase
   activity) combined with flow cytometry and total adenosine tri-phosphate
   (ATP) measurements, to assess microbial viability in drinking water. We
   have applied the methods to different drinking water samples including
   non-chlorinated household tap water, untreated natural spring water, and
   commercially available bottled water. We conclude that the
   esterase-positive cell fraction, the total ATP values and the high
   nucleic acid (HNA) bacterial fraction (from SYBR (R) Green I staining)
   were most representative of the active/viable population in all of the
   water samples. These rapid methods present an alternative way to assess
   the general microbial quality of drinking water as well as specific
   events that can occur during treatment and distribution, with equal
   application possibilities in research and routine analysis. (C) 2008
   Elsevier Ltd. All rights reserved.}},
