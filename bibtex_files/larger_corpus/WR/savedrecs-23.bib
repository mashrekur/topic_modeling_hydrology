@article{ ISI:000256335400041,
Author = {Zhu, Xingdong and Nanny, Mark A. and Butler, Elizabeth C.},
Title = {{Photocatalytic oxidation of aqueous ammonia in model gray waters}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{10-11}},
Pages = {{2736-2744}},
Month = {{MAY}},
Abstract = {{This study investigated the TiO2 photocatalytic degradation of aqueous
   ammonia (NH4+/NH3) in the presence of surfactants and monosaccharides at
   pH similar to 10.1. Initial rates of NH4+/NH3 photocatalytic degradation
   decreased by approximately 50-90\% in the presence of anionic, cationic,
   and nonionic surfactants and monosaccharides. Through correlation
   analysis, we concluded that scavenging of hydroxyl radical
   ((OH)-O-center dot) by the products of surfactant/monosaccharide
   photocatalytic degradation, including carbonate and formate, could
   explain approximately 80\% of the variance in initial rates of NH4+/NH3
   removal in our system. Addition of a supplemental (OH)-O-center dot
   source (H2O2) enhanced the rate of NH4+/NH3 degradation in the presence
   of the surfactant Brij (R) 23 lauryl ether (Brij (R) 35), further
   supporting the idea that (OH)-O-center dot scavenging is the mechanism
   by which surfactants and monosaccharides decreased initial rates of
   NH4+/NH3 photocatalytic degradation. Despite slowed rates of NH4+/NH3
   degradation, both surfactants/monosaccharides and NH4+/NH3 were removed
   by TiO2 photocatalysis, indicating that this process can effectively
   remove both carbonaceous and nitrogenous biochemical oxygen demand from
   gray water. (c) 2008 Elsevier Ltd. All rights reserved.}},
