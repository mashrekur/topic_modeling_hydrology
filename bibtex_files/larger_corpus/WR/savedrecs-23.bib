@article{ ISI:000256335400025,
Author = {Traenckner, Jens and Wricke, Burkhard and Krebs, Peter},
Title = {{Estimating nitrifying biomass in drinking water filters for surface
   water treatment}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{10-11}},
Pages = {{2574-2584}},
Month = {{MAY}},
Abstract = {{The objective of this work is to estimate active nitrifying biomass and
   its main influencing factors in low-loaded biofilters based on
   operational data. An analytical approach based on balancing growth,
   decay and biomass removed by backwashing is proposed. The method is
   developed and applied in pilot-scale rapid sand filters for drinking
   water treatment. Decay rate was measured directly in the filter for
   different temperatures. To assess the amount of active biomass in
   backwash water, a technique based on respiration measurements was used.
   Backwash losses increased overproportional with balanced biomass in the
   filter. The impact of both parameters on active biomass is quantified
   exemplarily for a given constant nitrification rate. (c) 2008 Elsevier
   Ltd. All rights reserved.}},
