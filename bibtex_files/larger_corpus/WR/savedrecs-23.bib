@article{ ISI:000255825500047,
Author = {Wilen, Britt-Marie and Onuki, Motoharu and Hermansson, Malte and Lumley,
   Doug and Mino, Takashi},
Title = {{Microbial community structure in activated sludge floc analysed by
   fluorescence in situ hybridization and its relation to floc stability}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{8-9}},
Pages = {{2300-2308}},
Month = {{APR}},
Abstract = {{The efficiency of activated sludge treatment plants is dependent on the
   solid-liquid separation properties of the activated sludge. A critical
   parameter is the stability of the microbial flocs. Weak flocs
   deflocculate easily leaving increased concentrations of suspended solids
   in the effluent. The knowledge about how different bacteria are attached
   to the flocs and their influence on the bioflocculation is limited. in
   this study, the deflocculation of different phylogenetic groups of
   bacteria in activated sludge from a full scale plant was investigated.
   The experiments were carried out by using a shear method where the
   sludge flocs are deflocculated under controlled shear conditions. The
   degree of deflocculation was measured as increase in turbidity of the
   supernatant. identification and quantification of the microbial
   community structure of both total activated sludge and deflocculated
   bacteria were conducted with group-specific gene probes for broad groups
   of bacteria (Alphaproteobacteria, Betaproteobacteria,
   Gammaproteobacteria and Actinobacteria) and fluorescence in situ
   hybridization (FISH). The microbial community structure of the
   deflocculated bacteria was different compared to the total activated
   sludge with a higher abundance of Gammaproteobacteria in the supernatant
   indicating that different groups of bacteria are bound with different
   strength to the floc surface. The results show that the bacteria in the
   outer parts of the flocs are relatively loosely bound to the floc matrix
   and can be easily eroded from the surface when exposed to shear. (C)
   2008 Elsevier Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
