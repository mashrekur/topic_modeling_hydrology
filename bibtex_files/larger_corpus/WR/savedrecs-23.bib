@article{ ISI:000255166600030,
Author = {Humbert, Hugues and Gallard, Herve and Suty, Herve and Croue,
   Jean-Philippe},
Title = {{Natural organic matter (NOM) and pesticides removal using a combination
   of ion exchange resin and powdered activated carbon (PAC)}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{6-7}},
Pages = {{1635-1643}},
Month = {{MAR}},
Abstract = {{The combination of anion exchange resins (AERs) and powdered activated
   carbon (PAC) was studied to remove both natural organic matter (NOM) and
   pesticides. Experiments were conducted with high dissolved organic
   carbon (DOC) surface water (about 6.0 mg DOC/L) spiked with both
   atrazine and isoproturon. AERs, like MIEX (R) and IRA938 (R), showed up
   to 7S\% removal of DOC after 30 min contact time. The addition of PAC
   after treatment with these AERs only slightly decreased the residual DOC
   from 1.4 to 1.2mg/L. Experiments conducted with high (200 mu g/L) and
   low (1 mu g/L) initial pesticide concentrations showed that simultaneous
   and successive combinations of AER and PAC significantly improve the
   removal of both pesticides compared with PAC treatment on raw water. The
   improvement of short-term adsorption kinetics was explained by the
   adsorption of pesticides on AERs (about 5\%) and the removal of high
   molecular weight (MW) NOM structures by AERs that reduce pore blockage
   phenomena. For 24h contact time with PAC (adsorption isotherms), the
   benefit of AER treatment was lower, which indicates that the refractory
   DOC to AER treatment still competes through direct site competition
   mechanism. MIEX (R) resin had a distinct behavior since the simultaneous
   treatment with PAC showed no benefit on pesticide adsorption. The
   presence of fine residues of MIEX (R) was shown to interfere with PAC
   adsorption. (c) 2007 Elsevier Ltd. All rights reserved.}},
