@article{ ISI:000255166600023,
Author = {Zhang, Tao and Lu, Jinfeng and Ma, Jun and Qiang, Zhimin},
Title = {{Comparative study of ozonation and synthetic goethite-catalyzed
   ozonation of individual NOM fractions isolated and fractionated from a
   filtered river water}},
Journal = {{WATER RESEARCH}},
