@article{ ISI:000257982400001,
Author = {Masango, Mxolisi and Myburgh, Jan and Botha, Christo and Labuschagne,
   Leonie and Naicker, Dharmarai},
Title = {{A comparison of in vivo and in vitro assays to assess the toxicity of
   algal blooms}},
Journal = {{WATER RESEARCH}},
Year = {{2008}},
Volume = {{42}},
Number = {{13}},
Pages = {{3241-3248}},
Month = {{JUL}},
Abstract = {{The toxicity of purified microcystin-LR (MC-LR) and algal material
   collected during the winter and summer seasons (2005/2006) from the
   Hartebeespoort dam, South Africa, was investigated using the
   enzyme-linked immunosorbent assay (ELISA), mouse bioassay, catfish
   primary hepatocytes (in vitro assay) and protein phosphatase inhibition
   (PPi) assays. Microcystis aeruginosa, known producer of microcystins,
   was the dominant cyanobacteria present in the water samples.
   Exceptionally high cell numbers per millilitre were observed, especially
   with the summer samples (similar to 1.442 x 10(8) cells/ml), indicating
   a severe algal bloom in the dam. The toxin concentration as detected by
   ELISA and PPi assay in the winter and summer extracts was at least 1000
   times more than the provisional guideline value (1 mu g/l) set by the
   World Health Organization (WHO) for MC-LR in drinking water. Hepatotoxic
   effects and death of mice were observed after dosing with the summer
   extracts, while no hepatotoxic effects were observed with winter
   extracts. The EC50 values obtained after exposure of the catfish primary
   hepatocytes for 72 h to MC-LR, winter and summer extracts was about
   0.091, 0.053 and 0.014mg/l, respectively. Similar toxicity results were
   obtained when the mouse bioassay and primary hepatocytes were used. (c)
   2007 Elsevier Ltd. All rights reserved.}},
