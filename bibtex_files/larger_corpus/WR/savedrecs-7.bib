@article{ ISI:000073383700029,
Author = {Brouwer, H and Klapwijk, A and Keesman, KJ},
Title = {{Identification of activated sludge and wastewater characteristics using
   respirometric batch-experiments}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{4}},
Pages = {{1240-1254}},
Month = {{APR}},
Abstract = {{This paper presents a procedure to obtain activated sludge kinetics and
   wastewater characteristics from respirometric batch-experiments. For
   identification of state variables and model parameters a modified
   version of the ASM no. 1 that describes the oxygen uptake rate of the
   nitrification process and organic matter elimination was used. It is
   demonstrated that the maximum nitrification rate of activated sludge and
   the concentration nitrifiable nitrogen, and the rapid hydrolysis rate
   together with the concentration slowly biodegradable organic compounds
   can be estimated accurately. Suggestions to improve identifiability
   problems are given. it is concluded that the proposed method offers
   possibilities to follow changes in sludge capacities, for both
   heterotrophic and autotrophic micro-organisms, and wastewater
   composition. (C) 1998 Elsevier Science Ltd. All rights reserved.}},
