@article{ ISI:000073383700040,
Author = {Roche, KF},
Title = {{Growth potential of Daphnia magna Straus in the water of dairy waste
   stabilization ponds}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{4}},
Pages = {{1325-1328}},
Month = {{APR}},
Abstract = {{The waters from two ponds of a series of waste stabilization ponds
   treating dairy effluent, were studied as a growth medium for Daphnia
   magna Straus individuals in the laboratory. Body growth and reproduction
   varied between seasons, years and ponds, with performance being
   generally superior in the later pond of the series. Maximum values of
   final mean body length and intrinsic rate of increase (r) Of 5.2 and
   0.38 mm, respectively, were recorded, implying that the effluent water
   shows potential as a growth medium for D. magna. (C) 1998 Published by
   Elsevier Science Ltd. All rights reserved.}},
