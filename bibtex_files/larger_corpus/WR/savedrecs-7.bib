@article{ ISI:000072231600050,
Author = {Abd-Allah, AMA and Srorr, T},
Title = {{Biodegradation of anionic surfactants in the presence of organic
   contaminants}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{3}},
Pages = {{944-947}},
Month = {{MAR}},
Abstract = {{Surfactant (XP-100) biodegradability with yeast extract was studied in
   the presence of the pollution active hydrocarbons naphthalene and
   hexadecane. Surfactant biodegradation was faster with yeast extract than
   without. Increased surfactant concentration did not inhibit its
   biodegradation over the period studied. The addition of organic
   contaminants, on the other hand, enhanced surfactant biodegradation due
   to their synergistic effect. Naphthalene degraded more than hexadecane.
   The results outlined contribute to a better understanding of the
