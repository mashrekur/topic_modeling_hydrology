@article{ ISI:000077429600021,
Author = {Greene, EA and Gieg, LM and Coy, DL and Fedorak, PM},
Title = {{Sulfolane biodegradation potential in aquifer sediments at sour natural
   gas plant sites}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{12}},
Pages = {{3680-3688}},
Month = {{DEC}},
Abstract = {{Sulfolane is used in the treatment of sour natural gas. It is a highly
   water soluble compound that has been introduced into soils and
   groundwaters at a number of sour gas processing plant sites. Aquifer
   sediments from contaminated locations at three sites in western Canada
   were assessed for microbial activity and their ability to degrade
   sulfolane under aerobic and five anaerobic (nitrate-, Mn(IV)-, Fe(III)-,
   sulfate- and CO2-reducing) conditions. The microcosms were supplemented
   with 200 mg/L sulfolane and adequate supplies of N, P, and the
   appropriate terminal electron acceptor. Microcosms containing
   contaminated aquifer sediments from each of the three sites were able to
   degrade sulfolane aerobically at 8 degrees C and 28 degrees C, and the
   biodegradation followed zero-order kinetics. The lag limes before the
   onset of sulfolane biodegradation were shorter when
   sulfolane-contaminated sediments were used as inocula than when
   uncontaminated soils were used. No anaerobic sulfolane biodegradation
   was observed at 28 degrees C, nor was sulfolane biodegradation observed
   at 8 degrees C under Fe(III)-, sulfate- and CO2-reducing conditions. At
   8 degrees C, anaerobic degradation of sulfolane coupled to Mn(IV)
   reduction was observed in microcosms from two sites, and degradation
   coupled to nitrate reduction was seen in a microcosm from one of the
   contaminated sites. (C) 1998 Elsevier Science Ltd. All rights reserved.}},
