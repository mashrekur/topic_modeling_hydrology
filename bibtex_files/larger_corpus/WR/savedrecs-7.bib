@article{ ISI:000073795400031,
Author = {Mizuno, O and Li, YY and Noike, T},
Title = {{The behavior of sulfate-reducing bacteria in acidogenic phase of
   anaerobic digestion}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{5}},
Pages = {{1626-1634}},
Month = {{MAY}},
Abstract = {{The effect of sulfate reduction on the acidogenic phase was investigated
   using anaerobic chemostat systems at 35 degrees C. Sucrose (10,000 mg
   COD/1) was used as the sole organic substrate. Chemostat systems were
   maintained at hydraulic retention times (HRTs) of 2, 4, 6, 8 and 10 h.
   The sulfate concentration in the substrate was increased to 0 (control),
   600, 1200 and 2400 mg/l. Sulfate reduction occurred even at an HRT of 2
   h, while the hydrogen production rate evidently decreased with
   increasing sulfate concentration at HRTs of 2 and 4 h. Hydrogen was a
   key electron donor for the sulfate-reducing bacteria (SRB). The sulfate
   removal efficiencies were over 90\% at sulfate concentrations of 600 and
   1200 mg/l at HRTs of longer than 8 h. No inhibition in the degradation
   of sucrose was observed although the free-H2S concentration was up to 99
   mg S/1. Hydrogen-consuming and lactate-consuming SRB were maximally
   enumerated at 10(8) and 10(9) MPN/ml at a sulfate concentration of 2400
   mil and an HRT of 2 h. The SRB were enumerated at 10(5) to 10(7) MPN/ml
   even in the absence of sulfate. The results of this study showed that
   SRB could grow under acidogenic condition, and almost sulfate could be
   removed in the acidogenic phase. (C) 1998 Elsevier Science Ltd. All
   rights reserved.}},
