@article{ ISI:000071506600025,
Author = {Ventura, F and Boleda, MR and Lloret, R and Martin-Alonso, J},
Title = {{Strategies for the identification of compounds causing odours in water:
   A study of creosote spills}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{2}},
Pages = {{503-509}},
Month = {{FEB}},
Abstract = {{Odour events detected in river water entering waterworks plants have
   been studied. Creosote, a distillate from coal tar used as a raw
   material by two wood-preserving factories located in the watershed was
   the product causing some of the incidents envisaged. Several analytical
   techniques such as closed-loop stripping analysis (CLSA), liquid-liquid
   extraction (LLE), gas chromatography-mass spectrometry (GC-MS) and
   high-performance liquid chromatography-fluorescence detection (HPLC-FL)
   have been employed to identify creosote spills as the source of the
   problem. The strategies followed in order to clarify which factory was
   responsible fbr the pollution detected are described. (C) 1998 Elsevier
   Science Ltd. All rights reserved.}},
