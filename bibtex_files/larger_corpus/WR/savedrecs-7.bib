@article{ ISI:000075311200026,
Author = {Lei, LC and Hu, XJ and Yue, PL},
Title = {{Improved wet oxidation for the treatment of dyeing wastewater
   concentrate from membrane separation process}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{9}},
Pages = {{2753-2759}},
Month = {{SEP}},
Abstract = {{In this paper, a series of wet oxidation experiments was carried out in
   a 2-1 autoclave reactor. To achieve a significant reduction of the
   reaction temperature and pressure, two new oxidation methods were
   applied. One was to partially or totally replace oxygen with a stronger
   oxidant, viz. hydrogen peroxide, and the other was to introduce a
   catalyst. Both processes were found to achieve a higher initial reaction
   rate of TOC and colour reduction at lower reaction temperatures and
   pressures as compared with the conventional wet air oxidation. By
   combining the two new improved methods to treat the dyeing wastewater
   concentrate from a membrane separation process, almost 80\% of TOC
   reduction and 90\% of colour removal were achieved within half-an-hour
   at a reaction temperature of 110 degrees C and a system total pressure
   of 50 kPa. (C) 1998 Elsevier Science Ltd. All rights reserved.}},
