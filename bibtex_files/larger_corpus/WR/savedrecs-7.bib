@article{ ISI:000075837300035,
Author = {Casado, AG and Hernandez, EJA and Vilchez, JL},
Title = {{Determination of fatty acids (C-8-C-22) in urban wastewater by GC-MS}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{10}},
Pages = {{3168-3172}},
Month = {{OCT}},
Abstract = {{A simple method for determination of fatty acids (C-8-C-22) in
   wastewater, extracted with dichloromethane. was developed. The
   extraction time was drastically reduced using ultrasound. Fatty acids
   were esterified using BF3 methanolic solution followed by an 11 min run
   using gas chromatography-mass spectrometry with selected ion monitoring
   (GC/MS-SIM). A clean-up is not necessary using SIM Mode. Tridecanoic
   acid was used as a surrogate internal standard. Detection limits go from
   0.008 to 0.016 mg l(-1). The applicable concentration range was 0.010 to
   10 mg l(-1). The method was validated by using the Standard Addition
   Methodology and it was applied satisfactorily to wastewater From Granada
   (Spain). (C) 1998 Elsevier Science Ltd. All rights reserved.}},
