@article{ ISI:000072231600001,
Author = {Kettunen, RH and Rintala, JA},
Title = {{Performance of an on-site UASB-reactor treating leachate at low
   temperature}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{3}},
Pages = {{537-546}},
Month = {{MAR}},
Abstract = {{A pilot-scale UASB reactor was used to study treatment of municipal
   landfill leachate (COD 1.5-3.2 g l(-1)) at low temperatures (13-23
   degrees C) and in on-site conditions for 226 days. The reactor was
   successfully operated by decreasing the process temperature as leachate
   temperature decreased in winter. Despite changes in leachate quality,
   65-75\% COD and up to 95\% BOD7 removals were achieved at 18-23 degrees
   C with organic loading rates (OLR) of 2-4 kgCOD m(-3) d(-1). At 13-14
   degrees C, 50-55\% COD and 72\% BOD7 removals were obtained with OLRs of
   1.4-2 kgCOD m(-3) d(-1). Methane yield averaged 320 mlCH(4) g(-1)
   CODremoved. Excessive solids washout was not observed despite low
   process temperatures. Inorganic material accumulated in the reactor
   sludge throughout the study. The main precipitants were calcium and iron
   which, at the end of the run, accounted for 24\% and 7\% of total solids
   of the sludge, respectively. The specific methanogenic activity (SMA) of
   the mesophilic (37 degrees C) inoculum used in the study increased 7
   fold at 11 degrees C and 3 fold at 22-24 degrees C during the run. Yet
   SMA at 11 degrees C (0.05 gCH(4)COD g VSS-1 d(-1)) remained one third of
   that at 22-24 degrees C. (C) 1998 Elsevier Science Ltd. All rights
   reserved.}},
