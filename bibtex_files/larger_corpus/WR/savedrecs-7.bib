@article{ ISI:000074735800014,
Author = {Akgiray, O and Saatci, AM},
Title = {{An algorithm for bank operation of declining rate filters}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{7}},
Pages = {{2095-2105}},
Month = {{JUL}},
Abstract = {{A mathematical theory and a computer algorithm for modeling variable
   declining rate filtration systems are presented. The use of harmonic
   mean conductivity in the calculation of laminar head loss through a
   filter medium is explained. It is mathematically proved that harmonic
   mean conductivity, as opposed to conductivity evaluated at average
   specific deposit, must be used if Darcy's law holds. The differences
   between the predictions based on the two approaches to calculating
   average hydraulic conductivity are demonstrated. The computer algorithm
   presented here is capable of predicting water level variations, effluent
   concentrations, individual filter velocities and productions as
   functions of rime. (C) 1998 Elsevier Science Ltd. All rights reserved.}},
