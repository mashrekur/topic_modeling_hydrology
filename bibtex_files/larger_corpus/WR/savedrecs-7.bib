@article{ ISI:000075311200041,
Author = {Wu, WW and Chadik, PA and Schmidt, CJ},
Title = {{An in situ synthesis of cyanogen chloride as a safe and economical
   aqueous standard}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{9}},
Pages = {{2865-2869}},
Month = {{SEP}},
Abstract = {{This study developed the in situ synthesis of CNCI as a Safe and
   economical aqueous standard, based on Standard Method 500-CN- E. This
   study demonstrated that the synthesized CNCI standards are comparable
   with commercial standards based on results from purge-and-trap GC/MS
   analysis and micro-liquid/liquid extraction with GC/ECD analysis.
   Furthermore, the synthesized CNCI solution provides a source of
   externally prepared standards, which avoids gas-liquid transfer
   procedures. Only microgram quantities of this hazardous substance were
   generated and may be safely converted to the relatively innocuous
   cyanate ion immediately after use by simply raising the pH. This
   performance-based approach led to a successful means to synthesize
   aqueous CNCI solutions as either quality control standards or
   calibration standards. It may be especially useful for laboratories in
   other countries where CNCI is difficult to acquire. (C) 1998 Elsevier
   Science Ltd. All rights reserved.}},
