@article{ ISI:000075837300020,
Author = {Nameche, T and Vasel, JL},
Title = {{Hydrodynamic studies and modelization for aerated lagoons and waste
   stabilization ponds}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{10}},
Pages = {{3039-3045}},
Month = {{OCT}},
Abstract = {{All attempts to model a wastewater treatment process necessarily require
   knowledge of the reactor hydraulic mixing characteristics. However, very
   few tracer experiments have been conducted on full-scale facilities,
   especially aerated lagoons and waste stabilization ponds. Several
   equations based on a dispersion model have been proposed to estimate the
   hydrodynamic behavior of ponds and lagoons. This paper compares these
   equations with the results of dye tracer experiments conducted on more
   than thirty existing facilities to determine which pond characteristics
   influence the flow pattern. It concludes by suggesting new dispersion
   prediction formulas that relate Peclet number values to aerator power
   input and pond shape, i.e., the length/width and length/depth ratios.
   (C) 1998 Elsevier Science Ltd. All rights reserved.}},
