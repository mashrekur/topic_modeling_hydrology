@article{ ISI:000073383700019,
Author = {Pavelic, P and Ragusa, SR and Flower, RL and Rinck-Pfeiffer, SM and
   Dillon, PJ},
Title = {{Diffusion chamber method for in situ measurement of pathogen
   inactivation in groundwater}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{4}},
Pages = {{1144-1150}},
Month = {{APR}},
Abstract = {{A method has been developed to measure pathogen inactivation in
   groundwater in situ by installing diffusion chambers in wells. This
   method has advantages over others in terms of utility, reduction of
   artefacts, and the ability to accommodate porous media. The diffusion
   chambers have been tasted in the laboratory for their ease of
   sterilization, and the effect of pressure on leakage. Exchange processes
   across the membranes were examined, and the effective diffusion
   coefficients for movement of KCI through two different pore-size
   membranes are given. An expression to account for the dilution in
   microbial concentrations due to periodic subsampling from the chamber
   has been determined. Finally, an example in a held application of the
   use of diffusion chambers in situ provided inactivation rates for
   Enterococcus faecalis from chambers with and without a porous medium.
   (C) 1998 Elsevier Science Ltd. All rights reserved.}},
