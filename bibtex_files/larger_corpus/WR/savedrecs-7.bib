@article{ ISI:000071108600020,
Author = {Karanfil, T and Kilduff, JE and Schlautman, MA and Weber, WJ},
Title = {{The oxygen sensitivity of organic macromolecule sorption by activated
   carbon: Effects of solution chemistry}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{1}},
Pages = {{154-164}},
Month = {{JAN}},
Abstract = {{The influence of solution chemistry (pH, ionic strength and calcium
   concentration) on the oxygen sensitivity of natural and synthetic
   dissolved organic macromolecule sorption by granular activated carbon
   was investigated. Sorption sensitivity to the presence of oxygen was
   found to decrease with decreasing pH, with increasing ionic strength and
   with increasing calcium concentration, The influence of oxygen on the
   extent of sorption by a basic GAC (pH(PZC) = 8.5) appears to be enhanced
   by increasing phenolic acidity and/or decreasing macromolecule size. The
   results suggest that open, flexible molecular configurations of organic
   macromolecules and the direct interactions of these substances with
   carbon surfaces are important factors with respect to the effects of
   oxygen on their sorption from aqueous phase. (C) 1998 Elsevier Science
   Ltd. All rights reserved.}},
