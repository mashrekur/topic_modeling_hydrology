@article{ ISI:000073383700020,
Author = {Cornu, S and Ambrosi, JP and Lucas, Y and Desjardins, T},
Title = {{Origin and behaviour of dissolved chlorine and sodium in Brazilian
   rainforest}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{4}},
Pages = {{1151-1161}},
Month = {{APR}},
Abstract = {{Results for Cl and Na in rainfall, throughfall and stemflow at a
   rainforest site located near Manaus (Central Amazonia, Brazil) cannot be
   systematically assigned to marine contribution in rainfall. Rather, the
   results show that 50\% of the whole input (stemflow plus throughfall)
   for Cl and 10\% for Na are due to atmospheric deposition the remaining
   component being derived from biological release. Thus Cl and Na cannot
   always be used as tracers of marine origin for atmospheric input for
   forest environments far from the seashore. (C) 1998 Elsevier Science
   Ltd. All rights reserved.}},
