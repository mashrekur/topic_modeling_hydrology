@article{ ISI:000072231600005,
Author = {Lawrence, AJ and Poulter, C},
Title = {{Development of a sub-lethal pollution bioassay using the estuarine
   amphipod Gammarus duebeni}},
Journal = {{WATER RESEARCH}},
Year = {{1998}},
Volume = {{32}},
Number = {{3}},
Pages = {{569-578}},
Month = {{MAR}},
Abstract = {{Bioassays have been developed and compared to assess the potential role
   of the estuarine amphipod Gammarus duebeni in sub-lethal ecotoxicology
   studies. The assays involved the monitoring of pleopod beat frequency
   and swimming endurance against a head flow of water. Pleopod beat
   frequency showed a complex response to copper which was concentration
   and time dependent. Swimming endurance however, showed a clear dose
   response relationship and was identified as the more consistent
   technique. Using this assay significant impairment of swimming endurance
   was observed at 45 mg l(-1) copper, 20 mg l(-1) pentachlorophenol (PCP)
   and 8 mg l(-1) benzo{[}a]pyrene (B{[}a]P). These results indicate that
   Gammarus duebeni is a useful species with which to assess pollution
