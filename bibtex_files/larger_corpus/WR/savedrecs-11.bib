@article{ ISI:000170177300004,
Author = {Mersiowsky, I and Weller, M and Ejlertsson, J},
Title = {{Fate of plasticised PVC products under landfill conditions: A
   laboratory-scale landfill simulation reactor study}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{13}},
Pages = {{3063-3070}},
Month = {{SEP}},
Abstract = {{he long-term behaviour of plasticised PVC products was investigated in
   laboratory-scale landfill simulation reactors. The examined products
   included a cable material and a flooring with different combinations of
   plasticisers. The objective of the study was to assess whether a
   degradation of the PVC polymer or a loss of plasticisers occurred under
   landfill conditions. A degradation of the polymer matrix was not
   observed. The contents of plasticisers in a.-ed samples was determined
   and compared to the respective original products. The behaviour of the
   various plasticisers was found to differ significantly. Losses of DEHP
   and BBP from the flooring were too low for analytical quantification. No
   loss of DIDP from the cable was detectable, whereas DINA in the same
   product showed considerable losses of up to 70\% compared to the
   original contents. These deficits were attributable to biodegradation
   rather than leaching. There was no equivalent release of plasticisers
   into the leachate. (C) 2001 Elsevier Science Ltd. All rights reserved.}},
