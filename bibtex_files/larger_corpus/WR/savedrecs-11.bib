@article{ ISI:000167033400030,
Author = {Hem, LJ and Efraimsen, H},
Title = {{Assimilable organic carbon in molecular weight fractions of natural
   organic matter}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{4}},
Pages = {{1106-1110}},
Month = {{MAR}},
Abstract = {{Assimilable organic carbon (AOC) in natural organic matter (NOM) has
   been measured in three Norwegian waters. The AOC measurement has been
   done on unfiltered and membrane-filtered (molecular weight (MW) cut-off
   of 1000 and 10,000) samples. While the colour in the waters is mainly
   related to the NOM with MW > 10,000, the AOC was mainly related to the
   NOM with MW < 1000. The fraction of the NOM with MW < 1000 corresponds
   to 16-38\% of the total organic carbon (TOC), and 4-9\% of the colour.
   This shows that a water treatment process removing the colour and a
   large part of the TOC in the water will not necessarily result in a
   reduction in the biofilm formation potential. (C) 2001 Elsevier Science
   Ltd. All rights reserved.}},
