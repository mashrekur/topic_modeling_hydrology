@article{ ISI:000170177300014,
Author = {Chu, W},
Title = {{Dye removal from textile dye wastewater using recycled alum sludge}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{13}},
Pages = {{3147-3152}},
Month = {{SEP}},
Abstract = {{The removal of dyes from textile dying wastewater by recycled alum
   sludge (RAS) generated by the coagulation process itself was studied and
   optimized. One hydrophobic and one hydrophilic dye were used as probes
   to examine the performance of this process. It was found that RAS is a
   good way of removing hydrophobic dye in wastewater, while simultaneously
   reducing the fresh alum dosage, of which one third of the fresh alum can
   be saved. The back-diffusion of residued dye from the recycling sludge
   is detected but is easily controlled as long as a small amount of fresh
   alum is added to the system. The use of RAS is not recommended for the
   removal of hydrophilic dyes, since the high solubility characteristics
   of such dyes can cause deterioration in the water quality during
   recycling. (C) 2001 Elsevier Science Ltd. All rights reserved.}},
