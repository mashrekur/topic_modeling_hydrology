@article{ ISI:000171413600018,
Author = {Vilanova, RM and Fernandez, P and Martinez, C and Grimalt, JO},
Title = {{Polycyclic aromatic hydrocarbons in remote mountain lake waters}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{16}},
Pages = {{3916-3926}},
Month = {{NOV}},
Abstract = {{Polycyclic aromatic hydrocarbons (PAH), including alkylated and sulfur
   derivatives. were identified and measured in the waters from three
   European remote mountain lakes during both ice-free and ice covered
   periods. The measured concentrations were in the same order in all three
   lakes (700-1100 pg.l). The PAH patterns in both dissolved and
   particulate water phases were dominated by the low molecular weight
   compounds (i.e. phenanthrene, fluoranthene and pyrene) in Lakes Redo
   (Pyrenees) and Gossenkolle (Alps). In contrast. the high molecular
   weight (HMW) compounds (i.e. chrysene+triphenylene, benzofluoranthenes,
   benzo{[}e]pyrene) were very significant in lake Ovre Neadalsvatn
   (Caledonian). These HMW PAH correspond to mixtures originating from high
   temperature combustion processes which have been photodegraded during
   long range atmospheric transport and parallel the PAH mixtures
