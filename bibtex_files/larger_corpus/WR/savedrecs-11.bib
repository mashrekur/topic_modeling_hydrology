@article{ ISI:000168954000015,
Author = {Ma, CW and Chu, W},
Title = {{Photodegradation mechanism and rate improvement of chlorinated aromatic
   dye in non-ionic surfactant solutions}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{10}},
Pages = {{2453-2459}},
Month = {{JUL}},
Abstract = {{A typical insoluble chlorinated aromatic dye (CAD), disperse red (DR),
   was used to explore the reaction mechanism and kinetics of
   photodegradation in non-ionic surfactant solutions. The use of an
   additional hydrogen source and photosensitizer is also studied to
   improve the decay rates. The decay rate of dye in surfactants depends on
   the K, of surfactants and their ability to offering an effective
   hydrogen source. The photodegradation of CAD can be divided into three
   stages: the initial lag stage, the fast degradation stage and the final
   retardation stage. The lag stage will vanish and the decay rates of dye
   can be greatly improved by 2.5-3.6 times after adding an additional
   hydrogen source (NaBH4) or photosensitizer (acetone) to the surfactant
   micellar solution. However, the use of an additional hydrogen Source or
   photosensitizer has dosage limitations in such applications. The
   photoreduction of DR is the main reaction mechanism, in which
   photodechlorination is observed first with the generation of HCl as the
   final product, then followed by photodecolorization by breaking the azo
   bond of the chromophore. (C) 2001 Elsevier Science Ltd. All rights
   reserved.}},
