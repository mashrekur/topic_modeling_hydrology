@article{ ISI:000169522600030,
Author = {Eljarrat, E and Caixach, J and Rivera, J},
Title = {{Evaluation of dioxin contamination in sewage sludge discharges on
   coastal sediments from Catalonia, Spain}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{11}},
Pages = {{2799-2803}},
Month = {{AUG}},
Abstract = {{The Fate of polychlorinated dibenzo-p-dioxins (PCDDs) and dibenzofurans
   (PCDFs) in sewage sludges after discharge into the sea was investigated,
   Sediment samples were analysed at a sewage sludge disposal site as well
   as in the surrounding areas. Moreover, eight sediment samples from two
   rivers in Catalonia and three sediment samples from Catalonian Coast
   were analysed to determine the background levels of contamination. Total
   international toxicity equivalent (I-TEQ) values for these sediments
   ranged from 0.42 to 8.01 pg/g. with a mean value of 4.15 pg/g and a
   median value of 3.69 pg/g. However, the I-TEQ values of sludge-treated
   areas were higher: 57.04 pg/g at the dumping site, and within a range of
   13.42-47.76 pg/g near this site. Thus, European sediment quality
   objectives were exceeded. The higher concentrations coincided with
   changes in the ratio between PCDD and PCDF levels, suggesting the
   influence of the sewage sludge on coastal sediments. (C) 2001 Elsevier
   Science Ltd. All rights reserved.}},
