@article{ ISI:000167033400022,
Author = {Chamarro, E and Marco, A and Esplugas, S},
Title = {{Use of Fenton reagent to improve organic chemical biodegradability}},
Journal = {{WATER RESEARCH}},
Year = {{2001}},
Volume = {{35}},
Number = {{4}},
Pages = {{1047-1051}},
Month = {{MAR}},
Abstract = {{Fenton reagent has been used to test the degradation of different
   organic compounds (formic acid, phenol, 4-chlorophenol,
   2,4-dichlorophenol and nitrobenzene) in aqueous solution. A
   stoichiometric coefficient for the Fenton reaction was found to be
   0.5mol of organic compound/mol of hydrogen peroxide, except for the
   formic acid where a value of approximately one was obtained (due to the
   direct formation of carbon dioxide). The treatment eliminates the toxic
   substances and increases the biodegradability of the treated water
   (measured as the ratio BOD5/COD). Biodegradability is attained when the
   initial compound is removed. (C) 2001 Elsevier Science Ltd. All rights
   reserved.}},
