@article{ ISI:A1993LM27800028,
Author = {HELLMUTH, KH and SIITARIKAUPPI, M and LINDBERG, A},
Title = {{STUDY OF POROSITY AND MIGRATION PATHWAYS IN CRYSTALLINE ROCK BY
   IMPREGNATION WITH C-14 POLYMETHYLMETHACRYLATE}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1993}},
Volume = {{13}},
Number = {{1-4}},
Pages = {{403-418}},
Month = {{JUN}},
Note = {{3RD INTERNATIONAL CONF ON CHEMISTRY AND MIGRATION OF ACTINIDES AND
   FISSION PRODUCTS ( MIGRATION 91 ), JEREZ FRONTERA, SPAIN, OCT 21-25,
   1991}},
Abstract = {{A new method is described, which gives quantitative information on
   levels and spatial distributions of porosity in the matrix of rock in
   the sub-mm- to cm-scale. Migration pathways can be identified and
   visualized. The method employs impregnation of dried rock samples by
   C-14-labelled methylmethacrylate (MMA), radiation polymerization,
   autoradiography and digital image processing. Infiltration and diffusion
   processes can be quenched by polymerization and the resulting profiles
   studied easily.}},
