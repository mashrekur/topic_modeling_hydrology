@article{ ISI:A1993LM27800013,
Author = {LEFEVRE, F and SARDIN, M and SCHWEICH, D},
Title = {{MIGRATION OF STRONTIUM IN CLAYEY AND CALCAREOUS SANDY SOIL -
   PRECIPITATION AND ION-EXCHANGE}},
