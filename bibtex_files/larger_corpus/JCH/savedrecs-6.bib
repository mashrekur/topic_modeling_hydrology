@article{ ISI:A1992JW11400009,
Author = {RUSIN, PA and SINCLAIR, NA and GERBA, CP and GERSHMAN, M},
Title = {{APPLICATION OF PHAGE TYPING TO THE IDENTIFICATION OF SOURCES OF
   GROUNDWATER CONTAMINATION}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1992}},
Volume = {{11}},
Number = {{1-2}},
Pages = {{173-188}},
Month = {{OCT}},
Abstract = {{Phage typing of Escherichia coli populations was used as a
   ``fingerprinting{''} tool to identify the source(s) of fecal coliform
   contamination of a drinking water well, PW-12. Group discriminate
   analysis was used to evaluate the data and deter-mine the relative
   distance of population centroids from the centroid of the PW-12
   population. The phage typing patterns were compared to serological
   results and correlations noted. Phage typing patterns were shown to be
   stable following 32-days incubation of E. coli isolates in garden soil,
   tap water and neutralized tertiary effluent, with and without the
   presence of autochthonous flora.}},
