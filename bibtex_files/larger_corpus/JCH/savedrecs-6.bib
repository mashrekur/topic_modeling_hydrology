@article{ ISI:A1992HP81300014,
Author = {TATSUMI, K and ICHIKAWA, H and WADA, S},
Title = {{FLAVIN-SENSITIZED PHOTOOXIDATION OF SUBSTITUTED PHENOLS IN NATURAL-WATER}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1992}},
Volume = {{9}},
