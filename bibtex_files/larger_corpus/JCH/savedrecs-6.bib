@article{ ISI:A1992HP84200004,
Author = {GRUNDL, T},
Title = {{REDOX INACTIVITY OF COLLOIDAL FERRIC OXYHYDROXIDE SOLIDS}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1992}},
Volume = {{9}},
Number = {{4}},
Pages = {{369-377}},
Month = {{APR}},
Abstract = {{A low-molecular-weight dialysis membrane was used to exclude colloidal
   ferric oxyhydroxide solids with molecular weights of > 6000-8000 daltons
   from a portion of an anaerobic ferric/ferrous chloride solution.  Redox
   potential measurements taken in the presence and absence of colloidal
   ferric solids agree within +/- 7 mV, indicating that ferric colloids of
   this size are redox inactive.  Experiments conducted under varying
   solid/aqueous ferric ratios also indicate that ferric solids do not
   contribute exchange current density and are therefore inactive.  In
   spite of tabulated exchange currents which indicate stable potentials
   require greater-than-or-equal-to 10(-5) M Fe3+, stable electrode
   response was seen in solutions as dilute as 10(-6) M Fe3+.  Differences
   in experimental conditions may cause this difference in observed
   results.}},
