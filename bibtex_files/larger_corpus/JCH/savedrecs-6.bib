@article{ ISI:A1991GU44500005,
Author = {FUJIKAWA, Y and FUKUI, M},
Title = {{VARIATIONS IN ADSORPTION MECHANISMS OF RADIOACTIVE COBALT AND CESIUM IN
   ROCKS}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1991}},
Volume = {{8}},
Number = {{2}},
Pages = {{177-195}},
Month = {{NOV}},
Abstract = {{Variations in adsorption mechanisms of Co-60 and Cs-137 in metamorphosed
   shale and chert. and granodiorite were examined under aerobic conditions
   in terms of different adsorption periods, salinity of solution and solid
   to liquid ratios. The mechanisms were investigated by means of selective
   extraction tests using:  (1) distilled water; (2) 1 N ammonium acetate
   (pH 7). (3) 0.1 M hydroxylamine hydrochloride (pH 3.5); and (4) 0.2 M
   ammonium oxalate + 0.2 M oxalate (pH 2.5).
   The increase in relative amounts of Cs-137 and Co-60 extracted by
   ammonium acetate was observed in the first 1-month period of adsorption.
   Then the conversion of the nuclides from ammonium acetate extractable to
   non-extractable was observed, which continued on the order of several
   years. It indicates that under aerobic conditions, remobilization of the
   nuclides once adsorbed to geologic media will become less probable as
   time elapses.
   Solid to liquid ratios and salinity of solution of the adsorption
   experiments did not influence the adsorption mechanisms of Co-60 and
   Cs-137.}},
