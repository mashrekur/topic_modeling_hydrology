@article{ ISI:A1992JW11400003,
Author = {LEROY, C and HULIN, JP and LENORMAND, R},
Title = {{TRACER DISPERSION IN STRATIFIED POROUS-MEDIA - INFLUENCE OF TRANSVERSE
   DISPERSION AND GRAVITY}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1992}},
Volume = {{11}},
Number = {{1-2}},
Pages = {{51-68}},
Month = {{OCT}},
Abstract = {{We have studied tracer dispersion in stratified media made of
   alternating layers of glass beads with different permeabilities. Both
   transmission experiments and echo experiments, in which the tracer is
   first injected into the sample and then pumped back through a detector,
   have been used. When there is no exchange between layers, the
   transmission dispersion curves give the permeability contrast, while the
   echo curves give the dispersivity of individual layers (namely their
   grain size if they are homogeneous). If the transverse transport between
   layers homogenizes the concentration across the sample, echo and
   transmission dispersivities are identical and related to the layer
   thickness and the transverse dispersivity. We observed the transition
   between both regimes as the flow velocity decreases in thin-layered
   samples. In samples with thicker layers, transverse tracer spreading
   requires more time; in addition, strong gravity effects associated to
   very small density variations with the tracer concentration were
   observed.}},
