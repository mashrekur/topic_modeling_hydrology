@article{ ISI:A1997XM20600027,
Author = {Yu, JW and Neretnieks, I},
Title = {{Theoretical evaluation of a technique for electrokinetic decontamination
   of soils}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1997}},
Volume = {{26}},
