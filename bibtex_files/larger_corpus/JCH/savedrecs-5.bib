@article{ ISI:000073047000004,
Author = {Stafford, P and Toran, L and McKay, L},
Title = {{Influence of fracture truncation on dispersion: A dual permeability
   model}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1998}},
Volume = {{30}},
Number = {{1-2}},
Pages = {{79-100}},
Month = {{MAR 31}},
Abstract = {{Simulations with a dual permeability model show that a few discrete
   fractures can have a major influence on plume geometry. A limited number
   of truncated fractures (1-4) within a permeable matrix can create nearly
   circular plumes, with about the same degree of spreading in the
   direction transverse to the average hydraulic gradient as in the
   longitudinal direction. By comparison, continuous fractures in the
   direction of flow tend to produce elongated plumes, similar to those
   typically seen in granular materials. Both types of plumes have been
   observed in tracer experiments in fractured porous media on the Oak
   Ridge Reservation. Understanding the influence of fracture geometry is
   important in planning field characterization and subsequent remediation
   in fractured porous media. (C) 1998 Elsevier Science B.V.}},
