@article{ ISI:A1997XM20600018,
Author = {Gutierrez, MG and Guimera, J and deLlano, AY and Benitez, AH and Humm, J
   and Saltink, M},
Title = {{Tracer test at El Berrocal site}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1997}},
Volume = {{26}},
