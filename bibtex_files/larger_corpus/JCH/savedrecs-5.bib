@article{ ISI:A1996TY36100011,
Author = {Banwart, S and Tullborg, EL and Pedersen, K and Gustafsson, E and
   Laaksoharju, M and Nilsson, AC and Wallin, B and Wikberg, P},
Title = {{Organic carbon oxidation induced by large-scale shallow water intrusion
   into a vertical fracture zone at the Aspo Hard Rock Laboratory (Sweden)}},
