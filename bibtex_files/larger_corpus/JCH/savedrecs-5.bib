@article{ ISI:A1995QF73300001,
Author = {SLEEP, BE},
Title = {{A METHOD OF CHARACTERISTICS MODEL FOR EQUATION OF STATE COMPOSITIONAL
   SIMULATION OF ORGANIC-COMPOUNDS IN GROUNDWATER}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1995}},
Volume = {{17}},
Number = {{3}},
Pages = {{189-212}},
Month = {{JAN}},
Abstract = {{A method of characteristics model for compositional simulation of
   groundwater contamination and remediation involving organic compounds is
   developed. The method of characteristics technique for two-phase,
   three-component systems is coupled to an algorithm for multiphase,
   multicomponent equilibrium using the non-random two-liquid equation of
   state. This model may be used for validating one-dimensional multiphase,
   multicomponent numerical models. It may also be used for preliminary
   analysis of processes such as infiltration of multicomponent mixtures of
   organic compounds into soils, and remediation processes such as alcohol
   and surfactant flooding. The model is applied to alcohol flooding for
   removal of 1, 1, 2-trichloroethane from groundwater and for water
   flushing of an acetone-trichloroethylene mixture.}},
