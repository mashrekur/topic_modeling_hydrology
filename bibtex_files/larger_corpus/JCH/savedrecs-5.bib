@article{ ISI:000078246700007,
Author = {Kozaki, T and Saito, N and Fujishima, A and Sato, S and Ohashi, H},
Title = {{Activation energy for diffusion of chloride ions in compacted sodium
   montmorillonite}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
