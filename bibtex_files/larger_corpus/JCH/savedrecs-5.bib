@article{ ISI:000078246700011,
Author = {Frieg, B and Alexander, WR and Dollinger, H and Buhler, C and Haag, P
   and Mori, A and Ota, K},
Title = {{In situ resin impregnation for investigating radionuclide retardation in
   fractured repository host rocks}},
