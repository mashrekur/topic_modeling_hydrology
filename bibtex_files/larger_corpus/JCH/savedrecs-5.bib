@article{ ISI:000078246700005,
Author = {Johnsson, H and Siitari-Kauppi, M and Skalberg, M and Tullborg, EL},
Title = {{Diffusion pathways in crystalline rock - examples from Aspo-diorite and
   fine-grained granite}},
