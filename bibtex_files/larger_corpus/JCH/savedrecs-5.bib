@article{ ISI:A1994PG77800004,
Author = {HO, CK and LIU, SW and UDELL, KS},
Title = {{PROPAGATION OF EVAPORATION AND CONDENSATION FRONTS DURING MULTICOMPONENT
   SOIL VAPOR EXTRACTION}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1994}},
Volume = {{16}},
Number = {{4}},
Pages = {{381-401}},
Month = {{SEP}},
Abstract = {{One-dimensional through-flow air venting of sand containing liquid
   hydrocarbon mixtures has been investigated to simulate soil vapor
   extraction processes in uniform soils. An isothermal transport model has
   been developed to predict the spatially and temporally varying
   composition of the liquid mixture within the contaminated region during
   the venting process. Experiments were performed to determine these
   changing in situ liquid concentrations in a packed column, and
   comparisons were made to the predicted values. Results showed that
   during through-flow venting, selective evaporation of the
   higher-volatility compounds led to the propagation of multiple
   evaporation fronts. These fronts corresponded to the location of
   vaporization of each compound, and the velocity of each front depended
   on the volatility of the compound. Thus, evaporation fronts of
   higher-volatility compounds propagated more quickly through the column
   than those of lower-volatility compounds for equal volumes of each
   compound initially emplaced into the liquid mixture. Condensation of
   lower-volatility compounds was also observed at the evaporation fronts
   of higher-volatility compounds, leading to a continuous accumulation of
   the lower-volatility compounds downstream of their evaporation fronts.}},
