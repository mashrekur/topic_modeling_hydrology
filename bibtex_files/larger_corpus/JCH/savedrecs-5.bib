@article{ ISI:000076449400009,
Author = {Griffioen, J},
Title = {{Suitability of the first-order mass transfer concept for describing
   cyclic diffusive mass transfer in stagnant zones}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
