@article{ ISI:000078246700006,
Author = {Yamaguchi, T and Nakayama, S},
Title = {{Diffusivity of U, Pu and Am carbonate complexes in a granite from Inada,
   Ibaraki, Japan studied by through diffusion}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1998}},
