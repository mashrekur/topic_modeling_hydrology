@article{ ISI:A1997XM20600029,
Author = {Smith, PA and Gautschi, A and Vomvoris, S and Zuidema, P and Mazurek, M},
Title = {{The development of a safety assessment model of the geosphere for a
   repository sited in the crystalline basement of northern Switzerland}},
