@article{ ISI:A1997XM20600016,
Author = {Marmier, N and Dumonceau, J and Fromage, F},
Title = {{Surface complexation modeling of Yb(III) sorption and desorption on
   hematite and alumina}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{1997}},
