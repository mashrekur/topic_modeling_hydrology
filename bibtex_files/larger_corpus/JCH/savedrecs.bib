@article{ ISI:000348878900001,
Author = {Chen, Hao and Gao, Bin and Yang, Liu-Yan and Ma, Lena Q.},
Title = {{Montmorillonite enhanced ciprofloxacin transport in saturated porous
   media with sorbed ciprofloxacin showing antibiotic activity}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2015}},
Volume = {{173}},
Pages = {{1-7}},
Month = {{FEB}},
Abstract = {{Antibiotic ciprofloxacin (CIP) is immobile in the subsurface but it has
   been frequently detected in the aquatic system. Therefore it is
   important to investigate the factors impacting CIP's mobilization in
   aquifer. Laboratory columns packed with sand were used to test
   colloid-facilitated UP transport by 1) using kaolinite or
   montmorillonite to mobilize presorbed-CIP in a column or 2)
   co-transporting with CIP by pre-mixing them before transport The
   Langmuir model showed that UP sorption by montmorillonite (23 g kg(-1))
   was 100 times more effective than sand or kaolinite. Even with strong
   CIP complexation ability to Fe/AI coating on sand surface,
   montrnorillonite promoted UP transport, but not kaolinite. All
   presorbed-CIP by sand was mobilized by montmorillonite after 3 pore
   volumes through co-transporting of CIP with montmorillonite. The
   majority of CIP was fixed onto the montmorillonite interlayer but still
   showed inhibition of bacteria growth. Our results suggested that
   montmorillonite with high UP sorption ability can act as a carrier to
   enhance CIP's mobility in aquifer. Published by Elsevier B.V.}},
