@article{ ISI:000453113400003,
Author = {Brooks, Michael C. and Wood, A. Lynn and Cho, Jaehyun and Williams,
   Christine A. P. and Brandon, William and Annable, Michael D.},
Title = {{Source strength functions from long-term monitoring data and spatially
   distributed mass discharge measurements}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
