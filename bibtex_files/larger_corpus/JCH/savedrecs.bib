@article{ ISI:000497598000005,
Author = {Svensson, Urban and Voutilainen, Mikko and Muuri, Eveliina and Ferry,
   Michel and Gylling, Bjorn},
Title = {{Modelling transport of reactive tracers in a heterogeneous crystalline
   rock matrix}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2019}},
Volume = {{227}},
Month = {{DEC}},
Abstract = {{A numerical reactive transport model for crystalline rocks is developed
   and evaluated. The model is based on mineral maps generated by X-ray
   micro computed tomography (X-mu CT); the maps used have a resolution of
   approximately 30 pm and the rock samples are on the cm scale. A
   computational grid for the intergranular space is generated and a
   micro-DFN (Discrete Fracture Network) model governs the grid properties.
   A particle tracking method (Time Domain Random Walk) is used for
   transport simulations. The basic concept of the model can now be
   formulated as follows; ``when a particle is close to a reactive mineral
   surface it has a certain probability to get sorbed during a certain time
   span. Once sorbed it will remain so a certain time{''}. The model
   requires a number of input parameters that represent the sorption
   properties of the reactive minerals. Attempts are made to relate the
   parameters to traditional distribution parameters. The model is
   evaluated by comparisons with recent laboratory experimental data. These
   experiments consider two rock types (veined gneiss and pegmatitic
   granite) and two radionuclides (cesium and barium). It is concluded that
   the new reactive transport model can simulate the experimental data in a
   consistent and realistic way.}},
