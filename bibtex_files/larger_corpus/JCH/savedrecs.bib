@article{ ISI:000383299400007,
Author = {Xiong, Qingrong and Baychev, Todor G. and Jivkov, Andrey P.},
Title = {{Review of pore network modelling of porous media: Experimental
   characterisations, network constructions and applications to reactive
   transport}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
