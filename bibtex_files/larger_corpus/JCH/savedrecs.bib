@article{ ISI:000438823400012,
Author = {Vesselinov, Velimir V. and Alexandrov, Boian S. and O'Malley, Daniel},
Title = {{Contaminant source identification using semi-supervised machine learning}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2018}},
Volume = {{212}},
Number = {{SI}},
