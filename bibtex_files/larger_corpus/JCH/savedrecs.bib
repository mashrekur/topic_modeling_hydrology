@article{ ISI:000438823400007,
Author = {Mondal, Pulin K. and Furbacher, Paul D. and Cui, Ziteng and Krol,
   Magdalena M. and Sleep, Brent E.},
Title = {{Transport of polymer stabilized nano-scale zero-valent iron in porous
   media}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2018}},
Volume = {{212}},
Number = {{SI}},
