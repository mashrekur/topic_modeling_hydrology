@article{ ISI:000383299400002,
Author = {Cohen, Gregory J. V. and Jousse, Florie and Luze, Nicolas and Hohener,
   Patrick and Atteia, Olivier},
Title = {{LNAPL source zone delineation using soil gases in a heterogeneous
   silty-sand aquifer}},
