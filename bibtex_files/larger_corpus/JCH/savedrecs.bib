@article{ ISI:000460827500009,
Author = {Bedekar, Vivek S. and Memari, Sama S. and Clement, T. Prabhakar},
Title = {{Investigation of transient freshwater storage in island aquifers}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
