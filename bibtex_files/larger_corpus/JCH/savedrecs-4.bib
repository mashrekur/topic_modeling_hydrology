@article{ ISI:000181510100030,
Author = {Cheng, HP and Li, MH and Li, S},
Title = {{A sensibility analysis of model selection in modeling the reactive
   transport of cesium in crushed granite}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2003}},
