@article{ ISI:000166844400021,
Author = {Smith, PA and Alexander, WR and Kickmaier, W and Ota, K and Frieg, B and
   McKinley, IG},
Title = {{Development and testing of radionuclide transport models for fractured
   rock: examples from the Nagra/JNC Radionuclide Migration Programme in
   the Grimsel Test Site, Switzerland}},
