@article{ ISI:000166844400023,
Author = {Xu, SL and Worman, A and Dverstorp, B},
Title = {{Heterogeneous matrix diffusion in crystalline rock - implications for
   geosphere retardation of migrating radionuclides}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2001}},
Volume = {{47}},
Number = {{2-4}},
Pages = {{365-378}},
Month = {{FEB}},
