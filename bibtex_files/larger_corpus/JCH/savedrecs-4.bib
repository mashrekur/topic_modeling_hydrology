@article{ ISI:000180412200004,
Author = {Shouakar-Stash, O and Frape, SK and Drimmie, RJ},
Title = {{Stable hydrogen, carbon and chlorine isotope measurements of selected
   chlorinated organic solvents}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2003}},
Volume = {{60}},
Number = {{3-4}},
Pages = {{211-228}},
Month = {{FEB}},
Abstract = {{Stable hydrogen isotopes of two chlorinated solvents, trichloroethylene
   (TCE) and 1,1,1-trichloroethane (TCA), provided by five different
   manufacturers, were determined and compared to their carbon and chlorine
   isotopic signatures. The isotope ratio for delta(2)H of different TCEs
   ranged between + 466.9parts per thousand and + 681.9parts per thousand,
   for delta(13)C between - 31.57parts per thousand and - 27.37parts per
   thousand, and for delta(37)Cl between -3.19parts per thousand and
   +3.90parts per thousand. In the case of the TCAs, the isotope ratio for
   delta(2)H ranged between - 23.1parts per thousand and + 15.1parts per
   thousand, for delta(13)C between - 27.39parts per thousand and -
   25.84parts per thousand, and for delta(37)Cl between - 3.54parts per
   thousand and + 1.39parts per thousand. As well, a column experiment was
   carried out to dechlorinate tetrachloroethylene (PCE) to TCE using iron.
   The dechlorination products have completely different hydrogen isotope
   ratios than the manufactured TCEs. Compared to the positive values of
   delta(2)H in manufactured TCEs (between + 466.9parts per thousand and +
   681.9parts per thousand), the dechlorinated products had a very depleted
   delta(2)H (less than - 300parts per thousand). This finding has strong
   implications for distinguishing dechlorination products (PCE to TCE)
   from manufactured TCE. In addition, the results of this study show the
   potential of combining H-2/H-1 analyses with C-13/C-12 and Cl-37/Cl-35
   for isotopic fingerprinting applications in organic contaminant
   hydrogeology. (C) 2002 Elsevier Science B.V. All rights reserved.}},
