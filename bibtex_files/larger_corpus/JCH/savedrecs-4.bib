@article{ ISI:000188597700006,
Author = {Worch, E},
Title = {{Modelling the solute transport under nonequilibrium conditions on the
   basis of mass transfer equations}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2004}},
Volume = {{68}},
Number = {{1-2}},
Pages = {{97-120}},
Month = {{JAN}},
Abstract = {{A solute transport model that describes nonequilibrium adsorption in
   soil/groundwater systems by mass transfer equations for film and
   intraparticle diffusion is presented. The model is useful in cases where
   breakthrough curve spreading cannot be explained by dispersion only. To
   evaluate its validity, the model was applied to several data sets from
   column experiments. The validity was also proved by a comparison with an
   analytical solution for the limiting case of predominating dispersion.
   Furthermore, a sensitivity analysis was performed to illustrate the
   influence of different process and sorption parameters (pore water
   velocity, intraparticle mass transfer coefficient, isotherm
   nonlinearity) on the shape of the calculated breakthrough curves. The
   application of the proposed model is discussed in comparison to the
   widely used dispersed flow/local equilibrium model, and a relationship
   between both models, which is based on a lumped parameter approach, is
   shown. (C) 2003 Elsevier B.V All rights reserved.}},
