@article{ ISI:000166844400007,
Author = {Kozaki, T and Inada, K and Sato, S and Ohashi, H},
Title = {{Diffusion mechanism of chloride ions in sodium montmorillonite}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2001}},
