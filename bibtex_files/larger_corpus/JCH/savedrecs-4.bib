@article{ ISI:000181510100025,
Author = {Ochs, M and Lothenbach, B and Shibata, M and Sato, H and Yui, M},
Title = {{Sensitivity analysis of radionuclide migration in compacted bentonite: a
   mechanistic model approach}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2003}},
Volume = {{61}},
Number = {{1-4}},
Pages = {{313-328}},
Month = {{MAR}},
Note = {{8th International Conference on Chemistry and Migration Behaviour of
   Actinides and Fission Products in the Geosphere (Migration 01), BREGENZ,
   AUSTRIA, SEP 16-21, 2001}},
Abstract = {{Mechanistic model calculations for the migration of Cs, Ra, Am and Pb in
   compacted bentonite have been carried out to evaluate sensitivities with
   respect to different parameter variations. A surface chemical
   speciation/electric double layer model is used to calculate: (i)
   porewater composition and radionuclide speciation in solution and at the
   bentonite surface, yielding the distribution of mobile and sorbed
   species and (ii) interaction of diffusing species with negatively
   charged pore walls to obtain diffusion parameters. The basic scenario
   considers the interaction of compacted bentonite with a fresh-type
   groundwater; variations include the presence of bentonite impurities and
   saline groundwater. It is shown that these scenarios result in
   significant variations of porewater composition that affect migration
   via three mechanisms that can partly compensate each other: (1) effects
   on sorption through radionuclide complexation in solution, and
   competition of major cations for surface sites; (2) changes in
   radionuclide solution speciation leading to different diffusing species
   under different conditions; (3) effects on diffusion through changes in
   the electric double layer properties of the clay pores as a function of
   ionic strength. (C) 2002 Elsevier Science B.V. All rights reserved.}},
