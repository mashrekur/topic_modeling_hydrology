@article{ ISI:000185855300004,
Author = {Schurmann, A and Schroth, MH and Saurer, M and Bernasconi, SM and Zeyer,
   J},
Title = {{Nitrate-consuming processes in a petroleum-contaminated aquifer
   quantified using push-pull tests combined with N-15 isotope and
   acetylene-inhibition methods}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2003}},
Volume = {{66}},
Number = {{1-2}},
Pages = {{59-77}},
Month = {{OCT}},
Abstract = {{Nitrate consumption in aquifers may result from several biogenic and
   abiotic processes such as denitrification, assimilatory NO3- reduction,
   dissimilatory NO3- reduction to ammonium (DNRA), or abiotic NO3- (or
   NO2-) reduction. The objectives of this study were to investigate the
   fate of NO3- in a petroleum-contaminated aquifer, and to assess the
   feasibility of using single-well push-pull tests (PPTs) in combination
   with N-15 isotope and C2H2 inhibition methods for the quantification of
   processes contributing to NOT consumption. Three consecutive PPTs were
   performed in a monitoring well of a heating oil-contaminated aquifer in
   Erlen, Switzerland. For each test, we injected 500 1 of test solution
   containing 0.5 mM Br- as conservative tracer and either 0.5 mM unlabeled
   NO3- or similar to 0.3 mM N-15-labeled NO3- as reactant. Test solutions
   were sparged during preparation and injection with either N-2, At or
   10\% C2H2 in Ar. After an initial incubation period of 1.5-3.2 h, we
   extracted the test solution/groundwater mixtures from the same location
   and measured concentrations of relevant species including Br-, NO3-,
   NO2-, N2O, N-2, and NH4+. In addition, we determined the N-15 contents
   of N-2, N2O, NH4+, and suspended biomass from N-15/N-14 isotope-ratio
   measurements. Average total test duration was 50.5 h. First-order rate
   coefficients (k) were computed from measured NO3- consumption, N-2-N-15
   production and N2O-N-15 production. From measured NO3- consumption we
   obtained nearly identical estimates of k for all PPTs with small 95\%
   confidence intervals, indicating good reproducibility and accuracy for
   the tests. Estimates of k from N-2-N-15 production and N2O-N-15
   production indicated that denitrification accounted for only 46-49\% of
   observed NO3- consumption. Production of N-2-N-15 in the presence Of
   C2H2 was observed during one of the tests, which may be an indicator for
   abiotic NO3- reduction. Moreover, N-15 isotope analyses confirmed
   occurrence of assimilatory NO3- reduction (0.58 at.\% N-15 in suspended
   biomass) and to a smaller extent DNRA (up to 4 at.\% N-15 in NH4+). Our
   results indicated that the combination of PPTs, N-15-isotope and C2H2
   inhibition methods provided improved information on denitrification as
   well as alternative fates of NO3- in this aquifer. (C) 2003 Elsevier
   Science B.V. All rights reserved.}},
