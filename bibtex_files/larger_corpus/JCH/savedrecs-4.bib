@article{ ISI:000169709100008,
Author = {Lee, JY and Cheon, JY and Lee, KK and Lee, SY and Lee, MH},
Title = {{Factors affecting the distribution of hydrocarbon contaminants and
   hydrogeochemical parameters in a shallow sand aquifer}},
