@article{ ISI:000088215500003,
Author = {Younger, PL},
Title = {{Predicting temporal changes in total iron concentrations in groundwaters
   flowing from abandoned deep mines: a first approximation}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
