@article{ ISI:000168212600003,
Author = {Reitsma, S and Dai, QL},
Title = {{Reaction-enhanced mass transfer and transport from non-aqueous phase
   liquid source zones}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
