@article{ ISI:000166844400014,
Author = {Prikryl, JD and Jain, A and Turner, DR and Pabalan, RT},
Title = {{Uranium(VI) sorption behavior on silicate mineral mixtures}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2001}},
