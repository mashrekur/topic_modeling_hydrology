@article{ ISI:000181510100018,
Author = {Kienzler, B and Vejmelka, P and Romer, J and Fanghanel, E and Jansson, M
   and Eriksen, TE and Wikberg, P},
Title = {{Swedish-German actinide migration experiment at ASPO hard rock
   laboratory}},
