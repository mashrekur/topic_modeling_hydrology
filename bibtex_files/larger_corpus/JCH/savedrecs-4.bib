@article{ ISI:000171780900008,
Author = {Lefebvre, R and Hockley, D and Smolensky, J and Lamontagne, A},
Title = {{Multiphase transfer processes in waste rock piles producing acid mine
   drainage 2. Applications of numerical simulation}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
