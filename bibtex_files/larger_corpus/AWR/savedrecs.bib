@article{ ISI:A1991HX22700004,
Author = {HUDSPETH, RT and GUENTHER, RB and ROLEY, KL and MCDOUGAL, WG},
Title = {{PARAMETER-IDENTIFICATION IN RADIAL FLOW}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1991}},
Volume = {{14}},
Number = {{5}},
Pages = {{240-251}},
Month = {{OCT}},
Abstract = {{The classical inverse parameter identification problem is solved using
   an indirect method. This method is based on the minimization of an
   objective function or error criterion consisting of three parts: 1)
   least-squares error of head residuals; 2) prior information of flow
   parameters; and 3) regularization. An adjoint equation is incorporated
   into the method to eliminate the need to differentiate the heads with
   respect to the parameters being identified, increasing the stability of
   the algorithm. This method is applied to a radial coordinate system used
   for describing an anisotropic confined aquifer. A variable implicit
   finite difference method is used for predicting hydraulic heads given a
   continuous set of flow parameters. The method is tested using synthetic
   data and is found to give estimates which are both stable and unique.}},
