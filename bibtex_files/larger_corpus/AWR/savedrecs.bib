@article{ ISI:A1994PF51500009,
Author = {BRUBAKER, KL and ENTEKHABI, D and EAGLESON, PS},
Title = {{A SIMPLE-MODEL OF THE HYDROLOGIC-CYCLE AND CLIMATE .2. OBSERVATIONAL
   EVIDENCE FOR THE PARAMETRIZATION OF ATMOSPHERIC HEAT AND MOISTURE
   DIVERGENCE}},
