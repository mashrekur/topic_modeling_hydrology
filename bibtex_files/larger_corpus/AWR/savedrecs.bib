@article{ ISI:A1997WC26300006,
Author = {Lee, CK and Mei, CC},
Title = {{Thermal consolidation in porous media by homogenization theory .1.
   Derivation of macroscale equations}},
Journal = {{ADVANCES IN WATER RESOURCES}},
