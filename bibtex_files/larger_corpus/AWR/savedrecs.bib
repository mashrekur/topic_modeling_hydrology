@article{ ISI:A1995RU01700002,
Author = {ODENWALD, B and STAMM, J and HERRLING, B},
Title = {{CONTINUOUS TRANSMISSIVITY TRANSITIONS FOR HORIZONTAL GROUNDWATER-FLOW
   MODELS}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1995}},
Volume = {{18}},
Number = {{5}},
Pages = {{257-265}},
Abstract = {{In the southern part of Germany, shallow and mostly unconfined valley
   aquifers are widespread. The differential equation describing
   groundwater flow and its boundary conditions varies for different flow
   situations due to the transient nature of the groundwater table. The
   authors present a continuous physical-mathematical formulation of
   two-dimensional horizontal groundwater flow, which allows for a
   simultaneous computation of saturated unconfined, partially unsaturated,
   or confined aquifers. This requires a physically sensible functional
   relationship between the transmissivity and the piezometric head. To
   numerically approximate the nonlinear parabolic differential equation
   describing groundwater flow, the finite element method is used in the
   spatial directions and a finite difference technique for the time
   derivation. Newton's method is applied to solve the resulting nonlinear
   algebraic equation system.}},
