@article{ ISI:A1997WD42600003,
Author = {Rajaram, H},
Title = {{Time and scale dependent effective retardation factors in heterogeneous
   aquifers}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1997}},
Volume = {{20}},
Number = {{4}},
Pages = {{217-230}},
Month = {{AUG}},
Abstract = {{The time and scale dependence of the retardation factor is examined for
   transport of a linearly adsorbing solute in an aquifer with spatially
   variable distribution coefficient (K-d). The variability of ln K-d and
   the logarithm of the hydraulic conductivity (ln K) are modeled as
   stationary random fields. Two definitions of the effective retardation
   factor are introduced: (i) the ratio of the ensemble average centroid
   velocities of a passive and retarded solute and (ii) the ratio of the
   ensemble average arrival time of a passive solute to that of a retarded
   solute. The effective retardation factor based on the first definition
   is shown to be time-dependent and that based on the second definition is
   shown to be distance (scale) dependent. Theoretical expressions are
   developed for computing the effective retardation factors based on a
   knowledge of statistical parameters of the In K and ln K-d fields.
   Illustrative computations are presented for one-dimensional and
   three-dimensional heterogeneous media. Copyright (C) 1996 Elsevier
   Science Ltd}},
