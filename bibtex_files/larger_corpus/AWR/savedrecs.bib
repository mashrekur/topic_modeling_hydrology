@article{ ISI:A1993LT08600001,
Author = {ELKADI, AI},
Title = {{CORE SAMPLING VERSUS FIELD-MEASURED DATA FOR MODELING DRAINAGE IN-FIELD
   SOILS - VALIDITY OF THE EQUIVALENT MEDIA APPROACH}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1993}},
Volume = {{16}},
Number = {{3}},
Pages = {{153-162}},
Abstract = {{Natural or field soils are characterized, in general, by the existence
   of macropores. Two approaches to modeling flow in such soils can be
   identified in the literature. The first describes the flow in two
   distinct domains: one in the soil and another in macropores, with
   appropriate interaction between the two systems. The second treats the
   whole system as a continuum with properties that are averaged over a
   representative control volume. This study is aimed at assessing the
   success of both field- and laboratory-based soil-data in representing
   the behavior of field soils under drainage, within a Darcian, continuum
   framework. Based on available data, three approaches to defining the
   hydraulic properties are adopted. The first two models represent closely
   data from undisturbed core-samples that were obtained independently from
   drainage measurements. The third is an approximate data model with
   parameters that are determined from field-measured tensiometric data
   during drainage. For each data model, available data are used to their
   fullest extent without any adjustment. The approximate data model
   represents best the behavior of the field soil under drainage
   conditions. The laboratory-based hydraulic functions, although estimated
   from undisturbed samples, are less successful in representing the
   bebavior of such soils. When compared to elaborated sampling and
   laboratory analyses, estimating the hydraulic properties from in-situ
   measured tensiometric data is not only more convenient, but can also be
   more accurate. The study concludes that the equivalent porous-media
   approach is valid for modeling drainage in well-drained field soils.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD, OXON, ENGLAND OX5 1GB}},
