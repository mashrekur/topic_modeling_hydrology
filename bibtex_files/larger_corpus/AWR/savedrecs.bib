@article{ ISI:A1994PX58200004,
Author = {GANAPATHY, R},
Title = {{FREE-CONVECTION FLOW-INDUCED BY A LINE SOURCE IN A SPARSELY PACKED
   POROUS-MEDIUM}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1994}},
Volume = {{17}},
Number = {{4}},
Pages = {{251-258}},
Abstract = {{A simple mathematical theory is proposed for the analysis of the free
   convection motion around a horizontal line source of thermal energy in a
   sparsely packed porous medium assuming the validity of the Brinkman
   model. The theory consists of retaining only the leading terms of the
   series expansions of the dependent variables in terms of the Rayleigh
   number and is valid in the limit of small Rayleigh number only. The heat
   generating rate is assumed to be not excessive. The evolution of the
   flow held is demonstrated by drawing the streamlines at various times.
   The results are delineated by comparing them with those of the Darcy
   flow model.}},
