@article{ ISI:A1992JK97600003,
Author = {DAVIS, PA and OLAGUE, NE and GOODRICH, MT},
Title = {{APPLICATION OF A VALIDATION STRATEGY TO DARCY EXPERIMENT}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1992}},
Volume = {{15}},
Number = {{3}},
Pages = {{175-180}},
Abstract = {{A strategy recently developed for use in an international study of
   ground-water flow and transport model validation is applied to Darcy's
   original experiment. The results of applying the validation strategy
   indicate that Darcy's law was not validated by Darcy's experiment.
   However, when the strategy is applied to a similar experiment conducted
   by Stearns using much smaller hydraulic gradients than Darcy, Darcy's
   law is found to be ``not invalid{''}. In addition, the authors propose
   that the adequacy of a model for a given purpose, at a given site, may
   be more important than model validity for making model-based decisions.}},
