@article{ ISI:A1996UT37900002,
Author = {Broadbridge, P and Edwards, MP and Kearton, JE},
Title = {{Closed-form solutions for unsaturated flow under variable flux boundary
   conditions}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1996}},
Volume = {{19}},
Number = {{4}},
Pages = {{207-213}},
Month = {{AUG}},
Abstract = {{It is shown that from any solution of the linear diffusion equation, we
   may construct a solution of a realistic form of the Richards equation
   for unsaturated flow. Compared to the usual direct linearization method,
   our inverse approach involves a quite different sequence of
   transformations. This opens the possibility of exact solutions with a
   wider variety of continuously varying flux boundary conditions.
   Closed-form solutions are presented for two examples. In these, the
   varying water flux boundary conditions resemble (i) the passage of a
   peaking storm and (ii) the continuous opening of a valve preceding a
   steady water supply. Unlike earlier more systematic approaches to this
   problem, our method does not require the numerical solution of an
   integral equation. Copyright (C) 1996 Elsevier Science Limited}},
