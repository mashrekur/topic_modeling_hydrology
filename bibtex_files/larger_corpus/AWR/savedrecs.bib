@article{ ISI:A1991FG15700003,
Author = {ALDAMA, AA and HARLEMAN, DRF},
Title = {{THE APPROXIMATION OF NONLINEARITIES IN THE FILTERED NAVIER-STOKES
   EQUATIONS}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1991}},
Volume = {{14}},
Number = {{1}},
Pages = {{15-30}},
Month = {{FEB}},
Abstract = {{Space and space-time filtering approaches for the numerical simulation
   of turbulent flow are described.  Space-time filtering is presented as
   an attractive option for geophysical applications in which the scales of
   interest are large, not only in space, but also in time.  It is argued
   that the space-time filtering procedure must rely on the use of
   non-integral approximations of the nonlinear terms that appear as a
   result of filtering the instantaneous advective terms in the
   Navier-Stokes equations.  Accordingly, the generalization of the Leonard
   approximation in the framework of space-time filtering is described.  In
   addition, an approximation for the cross terms is derived through
   theoretically justifiable arguments, by employing a Gaussian filter. 
   The mathematical nature of the Leonard and cross terms approximations is
   investigated in physical and Fourier space.  It is concluded that the
   series that give rise to both approximations have an asymptotic nature
   in a small parameter defined as the ratio of the smallest (numerically)
   resolvable scales and the dominant scales of the filtered velocity
   field.  It is also shown that both approximations are equivalent to
   approximating the Gaussian filter by an osculating paraboloid in Fourier
   space.  Finally, the filtering effect of numerical discretizations is
   examined.  Since space filtering is a particular case of space-time
   filtering, the conclusions obtained in this paper are also valid in the
   LES context.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD, OXON, ENGLAND OX5 1GB}},
