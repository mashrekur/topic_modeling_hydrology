@article{ ISI:000076182400005,
Author = {Hanna, S and Yeh, TCJ},
Title = {{Estimation of co-conditional moments of transmissivity, hydraulic head,
   and velocity fields}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1998}},
Volume = {{22}},
Number = {{1}},
Pages = {{87-95}},
Month = {{SEP 28}},
Abstract = {{An iterative co-conditional Monte Carlo simulation (IMCS) approach is
   developed. This approach derives co-conditional means and variances of
   transmissivity (T), head (phi), and Darcy's velocity (q), based on
   sparse measurements of T and phi in heterogeneous, confined aquifers
   under steady-state conditions. It employs the classical co-conditional
   Monte Carlo simulation technique (MCS) and a successive linear estimator
   that takes advantage of our prior knowledge of the covariances of T and
   phi and their cross-covariance. In each co-conditional simulation, a
   linear estimate of T is improved by solving the governing steady-state
   flow equation, and by updating residual covariance functions
   iteratively. These residual covariance functions consist of the
   covariance of T and phi and the cross-covariance function between T and
   phi. As a result, the non-linear relationship between T and phi is
   incorporated in the co-conditional realizations of T and phi. Once the T
   and phi fields are generated, a corresponding velocity field is also
   calculated. The average of the co-conditioned realizations of T, phi,
   and q yields the co-conditional mean fields. In turn, the co-conditional
   variances of T, phi, and q, which measure the reduction in uncertainty
   due to measurements of T and phi, are derived. Results of our numerical
   experiments show that the co-conditional means from IMCS for T and phi
   fields have smaller mean square errors (MSE) than those from a
   non-iterative Monte Carlo simulation (NIMCS). Finally, the
   co-conditional mean fields from IMCS are compared with the
   co-conditional effective fields from a direct approach developed by Yeh
   et nl. {[}Water Resources Research, 32(1), 85-92, 1996]. (C) 1998
   Elsevier Science Limited.}},
