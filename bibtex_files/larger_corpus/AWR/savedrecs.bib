@article{ ISI:A1992JF26600004,
Author = {MOHAMED, FA and GUENTHER, RB and HUDSPETH, RT and MCDOUGAL, WG},
Title = {{A DAMPED WAVE-EQUATION FOR GROUNDWATER-FLOW}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1992}},
Volume = {{15}},
Number = {{2}},
Pages = {{117-123}},
Abstract = {{A linear hyperbolic groundwater model (a damped wave equation) based on
   the unsteady Darcy equation is developed for pressure waves in a
   saturated, porous medium. The field equations in this formulation
   include the time-dependent fluid flux which is characterized by a small
   parameter referred to as thc relaxation time. The equations are solved
   analytically by standard integral transform techniques. Numerical
   results are presented to demonstrate the behavior of thc analytical
   solution. After an initial start-up time, the damped wave equation
   problem reduces to the classical diffusive problem. The motivation for
   using the hyperbolic model is that it allows an estimate of the
   transmissivity and storativity from the long-term response and the
   porosity based on the lag time of the head response.}},
