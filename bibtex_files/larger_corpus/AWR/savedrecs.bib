@article{ ISI:A1994QD69500004,
Author = {ZHANG, RD and HUANG, K and XIANG, J},
Title = {{SOLUTE MOVEMENT THROUGH HOMOGENEOUS AND HETEROGENEOUS SOIL COLUMNS}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1994}},
Volume = {{17}},
Number = {{5}},
Pages = {{317-324}},
Abstract = {{The classical convection-dispersion equation (CDE) and the stochastical
   convection-dispersion equation (SCDE) were utilized to study solute
   transport in homogeneous and heterogeneous porous media.  Extensive
   experimental data collected in 1 250-cm soil columns were used for
   testing validity of the CDE and SCDE.  Analyses of the experimental data
   showed that the observed dispersion coefficient increased with the
   travel distance for both homogeneous and heterogeneous soils.  The range
   of the dispersion coefficients was from 0.05 to 20 cm2 min-1 for the
   homogeneous soil, and from 3 to 200 cm2 min-1 for the heterogeneous
   soil.  A linear relationship between the dispersion coefficient and
   travel distance, assumed in the SCDE, described the scale-dependent
   dispersion coefficient reasonably well.  Therefore, the SCDE predicted
   solute transport in the porous media more accurately than the CDE for
   these examples.  In addition, the SCDE provided more consistent
   estimation of breakthrough curves than the CDE, using information
   obtained at a location either close to or far from the predicting place.}},
