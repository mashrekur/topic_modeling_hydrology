@article{ ISI:A1997WC26300007,
Author = {Lee, CK and Mei, CC},
Title = {{Thermal consolidation in porous media by homogenization theory .2.
   Calculation of effective coefficients}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1997}},
Volume = {{20}},
Number = {{2-3}},
Pages = {{145-156}},
Month = {{APR-JUN}},
Abstract = {{In Lee \& Mei(7) the macroscale equations for thermal consolidation in a
   periodic porous medium have been derived by homogenization theory. Here
   the macroscale coefficients are calculated for a cubic array of
   Wigner-Seitz grains. We first show that the elastic coefficient tensor
   of rank four has only three distinct constants and all the second rank
   tensor coefficients are isotropic. The canonical boundary cell problem
   for the elasticity tenser is then replaced by a variational principle,
   which is used as the basis for three-dimensional finite element
   computation. The dependence of the macroscale coefficients on the
   porosity is examined by varying an aspect ratio of the Wigner-Seitz
   cell. It is shown that the effective elastic coefficients and thermal
   modulus decrease with porosity whereas the pressure coefficient
   increases with porosity. Copyright (C) 1996 Elsevier Science Ltd}},
