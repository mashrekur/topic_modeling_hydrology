@article{ ISI:A1991GT63300003,
Author = {STRUPCZEWSKI, WG and ROMANOWICZ, RJ},
Title = {{INVARIANCE OF THE IUH 1ST CUMULANTS OF SIMPLIFIED LINEAR VENANT ST.
   MODELS}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1991}},
Volume = {{14}},
Number = {{4}},
Pages = {{175-182}},
Month = {{AUG}},
Abstract = {{In this paper an attempt is made towards the generalization and
   summarisation of up-to-date research work in the field of simplified
   flood routing models derived from the linearized St. Venant equation. 
   The case of a uniform wide-open channel with an arbitrary cross section
   and friction law is considered.  The characteristic equation for
   deriving the cumulants of the IUH is used, which is applicable only when
   one boundary condition is active only.  Introduced in the paper is a
   method for evaluation of cumulants which allows the substitution rules
   for the second-order derivatives of the linearized St. Venant equation
   under the condition of cumulants equivalence to be formulated. 
   Depending on the number of equivalent cumulants, three kinds of models
   originating from the St. Venant equation are specified.  The families of
   simplified models belonging to each class are derived using the
   substitution rules.  Examples of applications are given.}},
