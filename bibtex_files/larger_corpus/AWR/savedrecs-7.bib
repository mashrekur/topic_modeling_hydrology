@article{ ISI:000365623000019,
Author = {Huang, Cheng-Wei and Chu, Chia-Ren and Hsieh, Cheng-I and Palmroth, Sari
   and Katul, Gabriel G.},
Title = {{Wind-induced leaf transpiration}},
