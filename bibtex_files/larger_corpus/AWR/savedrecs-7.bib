@article{ ISI:000381529000028,
Author = {Yan, Hongxiang and Moradkhani, Hamid},
Title = {{Combined assimilation of streamflow and satellite soil moisture with the
   particle filter and geostatistical modeling}},
Journal = {{ADVANCES IN WATER RESOURCES}},
