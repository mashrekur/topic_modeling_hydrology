@article{ ISI:000374478700011,
Author = {Lo, Wei-Cheng and Sposito, Garrison and Lee, Jhe-Wei and Chu, Hsiuhua},
Title = {{One-dimensional consolidation in unsaturated soils under cyclic loading}},
