@article{ ISI:000365623500013,
Author = {Abaza, Mabrouk and Anctil, Francois and Fortin, Vincent and Turcotte,
   Richard},
Title = {{Exploration of sequential streamflow assimilation in snow dominated
   watersheds}},
Journal = {{ADVANCES IN WATER RESOURCES}},
