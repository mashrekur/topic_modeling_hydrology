@article{ ISI:000365623000002,
Author = {Caviedes-Voullieme, Daniel and Kesserwani, Georges},
Title = {{Benchmarking a multiresolution discontinuous Galerkin shallow water
   model: Implications for computational hydraulics}},
Journal = {{ADVANCES IN WATER RESOURCES}},
