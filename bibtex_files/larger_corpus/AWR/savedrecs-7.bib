@article{ ISI:000381529000016,
Author = {Mohammadmoradi, Peyman and Kantzas, Apostolos},
Title = {{Petrophysical characterization of porous media starting from
   micro-tomographic images}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2016}},
Volume = {{94}},
Pages = {{200-216}},
Month = {{AUG}},
Abstract = {{A quasi-static scheme based on pore space spatial statistics is
   presented to simulate pore-scale two-phase capillary-dominant
   displacement processes. The algorithm is coupled with computational
   fluid dynamics in order to evaluate saturation functions. Wettability
   heterogeneity in partial and fractional/mixed-wet media is implemented
   using a contact angle map. The simulation process is pixel-wised and
   performed directly on binary images. Bypassing and snap-off are tackled
   as non-wetting phase trapping mechanisms. Post-processing results
   include residual saturations, effective permeability and capillary
   pressure curves for drainage and imbibition scenarios. The primary
   advantages of the proposed workflow are eliminating pore space
   skeletisation/discretization, superior time efficiency and minimal
   numerical drawbacks when compared to other direct or network-based
   simulation techniques. (C) 2016 Elsevier Ltd. All rights reserved.}},
