@article{ ISI:000383299300020,
Author = {Arzilli, F. and Cilona, A. and Mancini, L. and Tondi, E.},
Title = {{Using synchrotron X-ray microtomography to characterize the pore network
   of reservoir rocks: A case study on carbonates}},
Journal = {{ADVANCES IN WATER RESOURCES}},
