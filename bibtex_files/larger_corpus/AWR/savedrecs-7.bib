@article{ ISI:000360192200030,
Author = {Martins, Ricardo and Leandro, Jorge and Djordjevic, Slobodan},
Title = {{A well balanced Roe scheme for the local inertial equations with an
   unstructured mesh}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2015}},
Volume = {{83}},
Pages = {{351-363}},
Month = {{SEP}},
Abstract = {{In this paper a finite volume method with an unstructured node based
   grid is developed in order to integrate the local inertial equations for
   flood modelling: the Gravity Wave Model (GWM). The GWM is based on the
   Shallow Water Equation (SWE) but neglects the convective term for
   improving the model computational speed. The numerical integration of
   the GWM equations is made using a first order in time and space
   well-balanced Roe Riemann Solver derived for the GWM set of equations.
   Several well-known SWE and urban flood model benchmark tests are used to
   verify the applicability of the GWM model. In addition analytical and
   numerical solutions of the SWE are also compared to the GWM. The results
   show a good agreement between the GWM and the SWE in all tested cases.
   (C) 2015 Elsevier Ltd. All rights reserved.}},
