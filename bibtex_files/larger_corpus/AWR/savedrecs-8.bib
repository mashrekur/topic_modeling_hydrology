@article{ ISI:000418262400030,
Author = {Laloy, Eric and Herault, Romain and Lee, John and Jacques, Diederik and
   Linde, Niklas},
Title = {{Inversion using a new low-dimensional representation of complex binary
   geological media based on a deep neural network}},
Journal = {{ADVANCES IN WATER RESOURCES}},
