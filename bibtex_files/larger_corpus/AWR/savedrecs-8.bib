@article{ ISI:000416037100023,
Author = {Yan, Zhifeng and Yang, Xiaofan and Li, Siliang and Hilpert, Markus},
Title = {{Two-relaxation-time lattice Boltzmann method and its application to
   advective-diffusive-reactive transport}},
