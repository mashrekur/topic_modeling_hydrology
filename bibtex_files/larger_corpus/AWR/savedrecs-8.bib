@article{ ISI:000407660800002,
Author = {Brutsaert, Wilfried},
Title = {{Global land surface evaporation trend during the past half century:
   Corroboration by Clausius-Clapeyron scaling}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2017}},
Volume = {{106}},
Pages = {{3-5}},
Month = {{AUG}},
Abstract = {{Analyses of satellite data mainly over the world's ocean surfaces have
   shown that during 1986-2006 global average values of atmospheric water
   vapor, precipitation and evaporation have increased at a relative rate
   of 0.0013 a(-1); this is roughly in accordance with the
   Clausius-Clapeyron equation for the average temperature trend during
   this period, and amounts to 0.065K(-1) at the average temperature of T =
   14 degrees C. Application of this concept over the world's land surfaces
   yields an average global evaporation trend during the past half century
   of around 0.4 to 0.5 mm a(-2); this confirms the values obtained in
   previous studies with totally different methods. (C) 2016 Elsevier Ltd.
   All rights reserved.}},
