@article{ ISI:000394631100003,
Author = {Xie, Chiyu and Raeini, Ali Q. and Wang, Yihang and Blunt, Martin J. and
   Wang, Moran},
Title = {{An improved pore-network model including viscous coupling effects using
   direct simulation by the lattice Boltzmann method}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2017}},
Volume = {{100}},
Pages = {{26-34}},
Month = {{FEB}},
Abstract = {{Viscous coupling during simultaneous flow of different fluid phases has
   a significant impact on their flow through porous media. In this work, a
   new multiscale strategy is proposed for multiphase flow in porous media.
   We use the lattice Boltzmann method (LBM) to simulate two-phase flow at
   pore scale and obtain empirical terms for the viscous coupling inside
   individual pores. The empirical coupling terms are then used in a
   pore-network model to efficiently simulate two-phase flow through porous
   media at core scale. It is shown that including viscous coupling leads
   to better predictions of relative permeability. (C) 2016 Elsevier Ltd.
   All rights reserved,}},
