@article{ ISI:000404307200010,
Author = {Zhao, Fang and Huai, Wenxin and Li, Dan},
Title = {{Numerical modeling of open channel flow with suspended canopy}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2017}},
Volume = {{105}},
Pages = {{132-143}},
Month = {{JUL}},
Abstract = {{Suspended canopies are common in aquatic environments. The mean flow
   velocity and Reynolds stress in the open channels with the suspended
   canopies were studied through laboratory experiments and numerical
   simulations. Numerical modeling was conducted with a modified version of
   the Delft3D-Flow model using a k-epsilon turbulence closure scheme. The
   resistance effects of the suspended canopy were modeled by adding a
   resistance term in the momentum equations of fluid flow, along with an
   addition production and dissipation term in the k and epsilon equations
   respectively. The numerical results for mean flow velocity and Reynolds
   stress agreed well with the experimental data of this study and the
   previously published data by Plew (2010.). The spatial evolution of the
   mixing layer near the bottom of the suspended canopy can be broadly
   separated into the diverging flow, developing, and fully developed
   zones. The thickness and density of the suspended canopy significantly
   influenced the flow characteristics. (C) 2017 Elsevier Ltd. All rights
   reserved.}},
