@article{ ISI:000410674200017,
Author = {Yeylaghi, Shahab and Moa, Belaid and Buckham, Bradley and Oshkai, Peter
   and Vasquez, Jose and Crawford, Curran},
Title = {{ISPH modelling of landslide generated waves for rigid and deformable
   slides in Newtonian and non-Newtonian reservoir fluids}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2017}},
Volume = {{107}},
Pages = {{212-232}},
Month = {{SEP}},
Abstract = {{A comprehensive modeling of landslide generated waves using an in-house
   parallel Incompressible Smoothed Particle Hydrodynamics (ISPH) code is
   presented in this paper. The study of landslide generated waves is
   challenging due to the involvement of several complex physical
   phenomena, such as slide-water interaction, turbulence and complex free
   surface profiles. A numerical tool that can efficiently calculate both
   slide motion, impact with the surface and the resulting wave is needed
   for ongoing study of these phenomena. Mesh-less numerical methods, such
   as Smoothed Particle Hydrodynamics (SPH), handle the slide motion and
   the complex free surface profile with ease. In this paper, an in-house
   parallel explicit ISPH code is used to simulate both subaerial and
   submarine landslides in 2D and in more realistic 3D applications. Both
   rigid and deformable slides are used to generate the impulsive waves. A
   landslide case is simulated where a slide falls into a non-Newtonian
   reservoir fluid (water-bentonite mixture). A new technique is also
   proposed to calculate the motion of a rigid slide on an inclined ramp
   implicitly, without using the prescribed motion in SPH. For all the test
   cases, results generated from the proposed ISPH method are compared with
   available experimental data and show good agreement. (C) 2017 Elsevier
   Ltd. All rights reserved.}},
