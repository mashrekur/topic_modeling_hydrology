@article{ ISI:000172958200002,
Author = {Gorenburg, IP and McLaughlin, D and Entekhabi, D},
Title = {{Scale-recursive assimilation of precipitation data}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2001}},
Volume = {{24}},
Number = {{9-10}},
Pages = {{941-953}},
Month = {{NOV-DEC}},
Abstract = {{This paper presents a new statistical method for assimilating
   precipitation data from different sensors operating over a range of
   scales. The technique is based on a scale-recursive estimation algorithm
   which is computationally efficient and able to account for the nested
   spatial structure of precipitation fields. The version of the algorithm
   described here relies on a static multiplicative cascade model which
   relates rainrates at different scales. Bayesian estimation techniques
   are used to condition rainrate estimates on measurements. The
   conditioning process is carried out recursively in two sweeps: first
   from fine to coarse scales and then from coarse to fine scales. The
   complete estimation algorithm is similar to a fixed interval smoother
   although it processes data over scale rather than time. We use this
   algorithm to assimilate radar and satellite microwave data collected
   during the tropical ocean-global atmosphere coupled ocean-atmosphere
   response experiment (TOGA-COARE). The resulting rainrate estimates
   reproduce withheld radar measurements to within the level of accuracy
   predicted by the assimilation algorithm. (C) 2001 Elsevier Science Ltd.
   All rights reserved.}},
