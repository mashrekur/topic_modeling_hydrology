@article{ ISI:000082752500007,
Author = {Holm, EJ and Langtangen, HP},
Title = {{A method for simulating sharp fluid interfaces in groundwater flow}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1999}},
Volume = {{23}},
Number = {{1}},
Pages = {{83-95}},
Month = {{SEP 15}},
Abstract = {{We present a numerical model for two-phase porous media flow, where the
   phases are separated by a sharp interface. The model is based on a
   unified pressure equation, and an advection equation for tracking a
   pseudo-concentration function. The zero-level set of this function
   defines the interface between the fluids. The finite element method is
   used for spatial discretization, with local grid refinements in the
   vicinity of the interface. Examples on applications involving moving
   interface and steady-state seepage problems are investigated. (C) 1999
   Elsevier Science Ltd. All rights reserved.}},
