@article{ ISI:000180478300005,
Author = {Mackay, DS and Ahl, DE and Ewers, BE and Samanta, S and Gower, ST and
   Burrows, SN},
Title = {{Physiological tradeoffs in the parameterization of a model of canopy
   transpiration}},
Journal = {{ADVANCES IN WATER RESOURCES}},
