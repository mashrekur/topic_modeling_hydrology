@article{ ISI:000166351600003,
Author = {Qualls, RJ and Yates, DN},
Title = {{Directional radiometric temperature profiles within a grass canopy}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2000}},
Volume = {{24}},
Number = {{2}},
Pages = {{145-155}},
Month = {{NOV}},
Abstract = {{The scalar roughness for sensible heat flux, Z(0h), which appears in
   Monin and Obukhov similarity theory has been shown to exhibit
   substantial variability. Brutsaert and Sugita (cf W. Brutsaert and M.
   Sugita, J. Atmos. Sci. 53 (1996) 209-16) derived an expression for Z(0h)
   to account for variability in Z0h as a result anisothermal vegetation
   skin temperature, which assumes an exponential profile of canopy skin
   temperature. We examined this assumption through directional radiometric
   measurements of temperature profiles in a relatively uniform grass
   canopy. The temperature gradient steepened monotonely throughout the
   morning and was modeled successfully with an exponential profile. The
   root-mean-square error (RMSE) between measured and modeled profiles was
   small and ranged between 0.038 degreesC and 0.530 degreesC. One of the
   most significant findings of this research is that the decay
   coefficient, b, of the exponential profile model was approximately
   constant with time for this canopy. This may provide a useful
   simplification in applying Brutsaert and Sugita's (loc. cit.)
   parameterization for z(0h). (C) 2000 Elsevier Science Ltd. All rights
   reserved.}},
