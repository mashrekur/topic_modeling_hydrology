@article{ ISI:000083465100002,
Author = {Keijzer, H and van Dijke, MIJ and van der Zee, SEATM},
Title = {{Analytical approximation to characterize the performance of in situ
   aquifer bioremediation}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{1999}},
Volume = {{23}},
Number = {{3}},
Pages = {{217-228}},
Month = {{NOV 1}},
Abstract = {{The performance of in situ bioremediation to remove organic contaminants
   from contaminated aquifers depends on the physical and biochemical
   parameters. We characterize the performance by the contaminant removal
   rate and the region where biodegradation occurs, the biologically active
   zone (BAZ). The numerical fronts obtained by one-dimensional in situ
   bioremediation modeling reveal a traveling wave behavior: fronts of
   microbial mass, organic contaminant and electron acceptor move with a
   constant velocity and constant front shape through the domain. Hence,
   only one front shape and a linear relation between the front position
   and time is found for each of the three compounds. We derive analytical
   approximations for the traveling wave front shape and front position
   that agree perfectly with the traveling wave behavior resulting from the
   bioremediation model. Using these analytical approximations, we
   determine the contaminant removal rate and the BAZ. Furthermore, we
   assess the influence of the physical and biochemical parameters on the
   performance of the in situ bioremediation technique. (C) 1999 Elsevier
   Science Ltd. All rights reserved.}},
