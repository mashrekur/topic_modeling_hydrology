@article{ ISI:000084682100008,
Author = {Wang, YQ and Hutter, K},
Title = {{Methods of substructuring in lake circulation dynamics}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2000}},
Volume = {{23}},
Number = {{4}},
Pages = {{399-425}},
Month = {{JAN 11}},
Abstract = {{A semi-implicit semi-spectral hydrodynamic primitive equation model is
   used in combination with a substructuring technique to study
   wind-induced motions and tracer diffusion in the homogeneous and
   stratified Lake Constance. An impulsively applied spatially uniform wind
   is applied in the long direction of the lake (305 degrees from True
   North) lasting indefinitely. Tracer mass is released at various
   locations of the free surface over a finite area for 24 h starling
   together with the imposed wind.
   We compute for a given wind and tracer scenario the flow and
   concentration fields for the entire lake using a coarse grid resolution.
   Within three different subregions, where improvements of the obtained
   results are sought, computations are repeated by using a finer grid and
   employing the results of the global computations at the open boundaries
   of the subregion. We demonstrate that this substructuring technique can
   be used: (i) to improve results where subgrid processes are significant
   as e.g., near shore, at early times in the neighbourhood of tracer
   sources and in complex geometric areas (bays); (ii) to better resolve
   and graphically display velocity and tracer concentration distributions
   on larger scales. We employ the technique for the homogeneous and the
   stratified Lake Constance. The technique is seen to be an economically
   efficient procedure in improving computational results when
   implementation of fine resolutions is not feasible. (C) 2000 Elsevier
   Science Ltd. All rights reserved.}},
