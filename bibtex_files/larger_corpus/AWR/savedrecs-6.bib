@article{ ISI:000327144300012,
Author = {Ata, Riadh and Pavan, Sara and Khelladi, Sofiane and Toro, Eleuterio F.},
Title = {{A Weighted Average Flux (WAF) scheme applied to shallow water equations
   for real-life applications}},
Journal = {{ADVANCES IN WATER RESOURCES}},
