@article{ ISI:000332392500002,
Author = {Naff, Richard L.},
Title = {{Application of the iterative solution method with Schur complement
   reduction to mixed finite elements based in a tetrahedral discretization}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2014}},
Volume = {{65}},
Pages = {{9-17}},
Month = {{MAR}},
Abstract = {{The iterative solution method for mixed finite element methods is
   applied to a 3-D domain partitioned with tetrahedral elements. For the
   particular discretization technique of first partitioning the domain
   with hexahedral cells, and then subsequently partitioning cells with
   five tetrahedral elements, a Schur complement decomposition is devised
   wherein the actual number of equations solved is reduced by 80\%.
   Although this Schur complement reduction requires a fair amount of
   computational overhead, its application within the iterative solution
   method can reduce overall solution time by about 44\%, depending on
   closure criterion and other factors. (C) 2014 Elsevier Ltd. All rights
   reserved.}},
