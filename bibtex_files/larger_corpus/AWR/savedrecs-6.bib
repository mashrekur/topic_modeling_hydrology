@article{ ISI:000333255600006,
Author = {Husain, Sarhang M. and Muhammed, Jowhar R. and Karunarathna, Harshinie
   U. and Reeve, Dominic E.},
Title = {{Investigation of pressure variations over stepped spillways using smooth
   particle hydrodynamics}},
