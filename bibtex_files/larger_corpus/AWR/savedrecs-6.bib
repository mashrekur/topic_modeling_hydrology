@article{ ISI:000323606500013,
Author = {Paul, George and Gowda, Prasanna H. and Prasad, P. V. Vara and Howell,
   Terry A. and Staggenborg, Scott A. and Neale, Christopher M. U.},
Title = {{Lysimetric evaluation of SEBAL using high resolution airborne imagery
   from BEAREX08}},
