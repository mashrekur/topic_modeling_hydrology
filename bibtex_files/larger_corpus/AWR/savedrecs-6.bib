@article{ ISI:000327540600013,
Author = {Hidalgo, Juan J. and MacMinn, Christopher W. and Juanes, Ruben},
Title = {{Dynamics of convective dissolution from a migrating current of carbon
   dioxide}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2013}},
Volume = {{62}},
