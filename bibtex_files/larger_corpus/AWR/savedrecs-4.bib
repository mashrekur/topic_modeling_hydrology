@article{ ISI:000243620000004,
Author = {Acharya, R. C. and van Dijke, M. I. J. and Sorbie, K. S. and Van der
   Zee, S. E. A. T. M. and Leijnse, A.},
Title = {{Quantification of longitudinal dispersion by upscaling Brownian motion
   of tracer displacement in a 3D pore-scale network model}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2007}},
Volume = {{30}},
