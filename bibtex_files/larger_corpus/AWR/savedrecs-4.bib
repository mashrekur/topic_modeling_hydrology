@article{ ISI:000266895000005,
Author = {Canestrelli, Alberto and Siviglia, Annunziato and Dumbser, Michael and
   Toro, Eleuterio F.},
Title = {{Well-balanced high-order centred schemes for non-conservative hyperbolic
   systems. Applications to shallow water equations with fixed and mobile
   bed}},
