@article{ ISI:000262026800015,
Author = {Ni, Chuen-Fa and Yeh, Tian-Chyi Jim},
Title = {{Stochastic inversion of pneumatic cross-hole tests and barometric
   pressure fluctuations in heterogeneous unsaturated formations}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2008}},
Volume = {{31}},
