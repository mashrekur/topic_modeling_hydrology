@article{ ISI:000247713700001,
Author = {Verde, Cristina and Visairo, Nancy and Gentil, Sylviane},
Title = {{Two leaks isolation in a pipeline by transient response}},
Journal = {{ADVANCES IN WATER RESOURCES}},
