@article{ ISI:000262589700008,
Author = {Moghaddamnia, A. and Gousheh, M. Ghafari and Piri, J. and Amin, S. and
   Han, D.},
Title = {{Evaporation estimation using artificial neural networks and adaptive
   neuro-fuzzy inference system techniques}},
Journal = {{ADVANCES IN WATER RESOURCES}},
