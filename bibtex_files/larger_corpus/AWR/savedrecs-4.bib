@article{ ISI:000253137000004,
Author = {Liu, Gang and Si, Bing C.},
Title = {{Analytical modeling of one-dimensional diffusion in layered systems with
   position-dependent diffusion coefficients}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2008}},
Volume = {{31}},
Number = {{2}},
Pages = {{251-268}},
Month = {{FEB}},
Abstract = {{Diffusion in stratified porous media is common in the natural
   environment. The objective of this study is to develop analytical
   solutions for describing the diffusion in layered porous media with a
   position-dependent diffusion coefficient within each layer. The
   orthogonal expansion technique was used to solve a one-dimensional
   multi-layer diffusion equation in which the diffusion coefficient is
   expressed as a segmented linear function of positions in the porous
   media. The behavior of the solutions is illustrated using several
   examples of a three-layer system, with constant diffusion coefficient
   alpha(1) in layer 1 (0 < x < l(1)), alpha(3) in layer 3(l(1) < x <
   l(3)), and a linearly position-dependent diffusion coefficient alpha(1)
   (1 + Delta(x - l(1))/(l(2) - l(1))) in the center layer (Delta =
   alpha(3)/alpha(1) - 1). Because of the asymmetry of the layered system,
   the diffusion and related concentration distributions are also
   asymmetrical. For a given Delta value, the smaller the value of (l(2) -
   l(1))/l(3), the more significant the accumulation of concentration in
   the middle transition zone (l(1) < x < l(2)), the sharper the change in
   the concentration profile of spatial distribution. Therefore, transition
   between two layers has significant effects on diffusion. (C) 2007
   Elsevier Ltd. All rights reserved.}},
