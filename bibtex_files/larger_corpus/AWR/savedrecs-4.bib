@article{ ISI:000253137000007,
Author = {Chung, Wei-Hao and Smith, James A.},
Title = {{Viscosity-excluded formulations of 2-D river flow with a new wetting and
   drying algorithm}},
Journal = {{ADVANCES IN WATER RESOURCES}},
