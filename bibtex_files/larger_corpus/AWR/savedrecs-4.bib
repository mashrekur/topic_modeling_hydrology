@article{ ISI:000247713700004,
Author = {Guo, Qiaona and Li, Hailong and Boufadel, Michel C. and Xia, Yuqiang and
   Li, Guohui},
Title = {{Tide-induced groundwater head fluctuation in coastal multi-layered
   aquifer systems with a submarine outlet-capping}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2007}},
Volume = {{30}},
Number = {{8}},
Pages = {{1746-1755}},
Month = {{AUG}},
Abstract = {{This paper considered the tide-induced head fluctuations in two coastal
   multi-layered aquifer systems. Model I comprises two semipermeable
   layers and a confined aquifer between them. Model II is a four-layered
   aquifer system including an unconfined aquifer, an upper semi-permeable
   layer, a confined aquifer and a lower semi-permeable layer. In each
   model, the submarine outlet of the confined aquifer is covered with a
   skin layer ({''}outlet-capping{''}). Analytical solutions of the two
   models are derived. In both models, leakages of the semi-permeable
   layers decrease the tidal head fluctuations. The outlet-capping reduces
   the aquifer's head fluctuation by a constant factor and shifts the phase
   by a positive constant. The solution to Model II explains the
   inconsistency between the relatively small lag time and the strong
   amplitude damping effect of the tidal head fluctuations reported by
   Trefty and Johnston {[}Ground Water 1998;36:427-33] near the Port
   Adelaide River, Australia. (c) 2007 Elsevier Ltd. All rights reserved.}},
