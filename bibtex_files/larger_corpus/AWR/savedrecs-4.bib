@article{ ISI:000262589700006,
Author = {Guinot, V. and Delenne, C. and Cappelaere, B.},
Title = {{An approximate Riemann solver for sensitivity equations with
   discontinuous solutions}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2009}},
Volume = {{32}},
Number = {{1}},
Pages = {{61-77}},
Month = {{JAN}},
Abstract = {{The sensitivity Of a model output (called a variable) to a parameter can
   be defined as the partial derivative of the variable with respect to the
   parameter. When the governing equations are not differentiable with
   respect to this parameter, problems arise in the numerical solution of
   the sensitivity equations, such as locally infinite values or
   instability. An approximate Riemann solver is thus proposed for direct
   sensitivity calculation for hyperbolic systems of conservation laws in
   the presence of discontinuous solutions. The proposed approach uses an
   extra source term in the form of a Dirac function to restore sensitivity
   balance across the shocks. It is valid for systems such as the Euler
   equations for gas dynamics or the shallow water equations for free
   surface flow. The method is first detailed and its application to the
   shallow water equations is proposed, with some test cases such as dike-
   or dam-break problems with or without source terms. An application to a
   two-dimensional flow problem illustrates the superiority of direct
   sensitivity calculation over the classical empirical approach. (C) 2008
   Elsevier Ltd. All rights reserved.}},
