@article{ ISI:000264512000014,
Author = {Zhang, H. W. and Fu, Z. D. and Wu, J. K.},
Title = {{Coupling multiscale finite element method for consolidation analysis of
   heterogeneous saturated porous media}},
Journal = {{ADVANCES IN WATER RESOURCES}},
