@article{ ISI:000262589700009,
Author = {Chalbaud, C. and Robin, M. and Lombard, J-M and Martin, F. and Egermann,
   P. and Bertin, H.},
Title = {{Interfacial tension measurements and wettability evaluation for
   geological CO2 storage}},
