@article{ ISI:000253019900015,
Author = {Vogel, H. -J. and Samouelian, A. and Ippisch, O.},
Title = {{Multi-step and two-step experiments in heterogeneous porous media to
   evaluate the relevance of dynamic effects}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2008}},
