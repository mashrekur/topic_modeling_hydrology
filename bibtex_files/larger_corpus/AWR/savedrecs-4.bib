@article{ ISI:000267676700002,
Author = {Diss, S. and Testud, J. and Lavabre, J. and Ribstein, P. and Moreau, E.
   and du Chatelet, J. Parent},
Title = {{Ability of a dual polarized X-band radar to estimate rainfall}},
