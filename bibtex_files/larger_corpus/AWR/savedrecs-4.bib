@article{ ISI:000246092800006,
Author = {Gotovac, H. and Andricevic, R. and Gotovac, B.},
Title = {{Multi-resolution adaptive modeling of groundwater flow and transport
   problems}},
Journal = {{ADVANCES IN WATER RESOURCES}},
