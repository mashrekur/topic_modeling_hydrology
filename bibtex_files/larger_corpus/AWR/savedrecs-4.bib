@article{ ISI:000253019900004,
Author = {Wen, Zhang and Huang, Guanhua and Zhan, Hongbin},
Title = {{An analytical solution for non-Darcian flow in a confined aquifer using
   the power law function}},
Journal = {{ADVANCES IN WATER RESOURCES}},
