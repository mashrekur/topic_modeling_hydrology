@article{ ISI:000269763400005,
Author = {Valdes-Parada, Francisco J. and Porter, Mark L. and Narayanaswamy,
   Karthik and Ford, Roseanne M. and Wood, Brian D.},
Title = {{Upscaling microbial chemotaxis in porous media}},
Journal = {{ADVANCES IN WATER RESOURCES}},
