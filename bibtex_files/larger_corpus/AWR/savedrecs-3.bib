@article{ ISI:000226625700006,
Author = {Miller, CT and Gray, WG},
Title = {{Thermodynamically constrained averaging theory approach for modeling
   flow and transport phenomena in porous medium systems: 2. Foundation}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2005}},
Volume = {{28}},
Number = {{2}},
Pages = {{181-202}},
Month = {{FEB}},
Abstract = {{This paper is the second in a series that details the thermodynamically
   constrained averaging theory (TCAT) approach for modeling flow and
   transport phenomena in porous medium systems. in this work, we provide
   the mathematical foundation upon which the theory is based. Elements of
   this foundation include definitions of mathematical properties of the
   systems of concern. previousiy available theorems needed to formulate
   models, and several theorems and corollaries, introduced and proven
   here. These tools are of use in producing complete, closed-form TCAT
   models for single- and multiple-fluid-phase porous medium systems.
   Future work in this series will rely and build upon the foundation laid
   in this work to detail the development of sets of closed models. (C)
   2004 Elsevier Ltd. All rights reserved.}},
