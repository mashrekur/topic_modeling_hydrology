@article{ ISI:000232328500006,
Author = {Jeng, DS and Seymour, BR and Barry, DA and Li, L and Parlange, JY},
Title = {{New approximation for free surface flow of groundwater: capillarity
   correction}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2005}},
Volume = {{28}},
Number = {{10}},
Pages = {{1032-1039}},
Month = {{OCT}},
Abstract = {{An existing capillarity correction for free surface groundwater flow as
   modelled by the Boussinesq equation is re-investigated. Existing
   solutions, based on the shallow flow expansion, have considered only the
   zeroth-order approximation. Here, a second-order capillarity correction
   to tide-induced watertable fluctuations in a coastal aquifer adjacent to
   a sloping beach is derived. A new definition of the capillarity
   correction is proposed for small capillary fringes, and a simplified
   solution is derived. Comparisons of the two models show that the
   simplified model can be used in most cases. The significant effects of
   higher-order capillarity corrections on tidal fluctuations in a sloping
   beach are also demonstrated. (c) 2004 Elsevier Ltd. All rights reserved.}},
