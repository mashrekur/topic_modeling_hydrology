@article{ ISI:000234313800019,
Author = {Eichinger, WE and Cooper, DI and Hipps, LE and Kustas, WP and Neale, CMU
   and Prueger, JH},
Title = {{Spatial and temporal variation in evapotranspiration using Raman lidar}},
Journal = {{ADVANCES IN WATER RESOURCES}},
