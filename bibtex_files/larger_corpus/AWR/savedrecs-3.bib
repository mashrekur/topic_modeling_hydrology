@article{ ISI:000240319900004,
Author = {Grimaldi, Salvatore and Serinaldi, Francesco},
Title = {{Asymmetric copula in multivariate flood frequency analysis}},
Journal = {{ADVANCES IN WATER RESOURCES}},
