@article{ ISI:000242783100012,
Author = {Ataie-Ashtiani, B. and Aghayi, M. M.},
Title = {{A note on benchmarking of numerical models for density dependent flow in
   porous media}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2006}},
Volume = {{29}},
Number = {{12}},
Pages = {{1918-1923}},
Month = {{DEC}},
Abstract = {{Verification of numerical models for density dependent flow in porous
   media (DDFPM) by the means of appropriate benchmark problems is a very
   important step in developing and using these models. Recently, Infinite
   Horizontal Box (IHB) problem was suggested as a possible benchmark
   problem for verification of DDFPM codes. IHB is based on
   Horton-Rogers-Lapwood (HRL) problem. Suitability of this problem for the
   benchmarking purpose has been investigated in this paper. It is shown
   that the wavelength of instabilities fails to be a proper criterion to
   be considered for this problem. However, the threshold of instability
   formation has been found to be appropriate for benchmarking purpose. (c)
   2006 Elsevier Ltd. All rights reserved.}},
