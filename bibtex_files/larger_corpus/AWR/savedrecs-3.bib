@article{ ISI:000236636600005,
Author = {Drecourt, JP and Madsen, H and Rosbjerg, D},
Title = {{Bias aware Kalman filters: Comparison and improvements}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2006}},
Volume = {{29}},
Number = {{5}},
Pages = {{707-718}},
Month = {{MAY}},
Abstract = {{This paper reviews two different approaches that have been proposed to
   tackle the problems of model bias with the Kalman filter: the use of a
   colored noise model and the implementation of a separate bias filter.
   Both filters are implemented with and without feedback of the bias into
   the model state. The colored noise filter formulation is extended to
   correct both time correlated and uncorrelated model error components. A
   more stable version of the separate filter without feedback is
   presented. The filters are implemented in an ensemble framework using
   Latin hypercube sampling. The techniques are illustrated on a simple
   one-dimensional groundwater problem. The results show that the presented
   filters outperform the standard Kalman filter and that the
   implementations with bias feedback work in more general conditions than
   the implementations without feedback. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
