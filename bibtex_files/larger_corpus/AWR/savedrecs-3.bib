@article{ ISI:000230250500007,
Author = {Kuby, MJ and Fagan, WF and ReVelle, CS and Graf, WL},
Title = {{A multiobjective optimization model for dam removal: an example trading
   off salmon passage with hydropower and water storage in the Willamette
   basin}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2005}},
Volume = {{28}},
Number = {{8}},
Pages = {{845-855}},
Month = {{AUG}},
Abstract = {{We introduce the use of systematic, combinatorial, multiobjective
   optimization models to analyse ecological-economic tradeoffs and to
   support complex decision-making associated with dam removal in a river
   system. The model's ecological objective enhances salmonid migration and
   spawning by maximizing drainage area reconnected to the sea. The
   economic objective minimizes loss of hydropower and storage capacity. We
   present a proof-of-concept demonstration for the Willamette River
   watershed (Oregon, USA). The case study shows a dramatic tradeoff in
   which removing twelve dams reconnects 52\% of the basin while
   sacrificing only 1.6\% of hydropower and water-storage capacity.
   Additional ecological gains, however, come with increasingly steeper
   economic costs. A second model incorporates existing fish-passage
   systems. Because of data limitations and model simplifications, these
   results are intended solely for the purpose of illustrating a novel
   application of multiobjective programming to dam-removal issues. Far
   more work would be needed to make policy-relevant recommendations.
   Nevertheless, this research suggests that the current practice of
   analysing dam-removal decisions on a dam-by-dam, basis be supplemented
   by evaluation on a river-system basis, trading off economic and
   ecological goals. (C) 2005 Elsevier Ltd. All rights reserved.}},
