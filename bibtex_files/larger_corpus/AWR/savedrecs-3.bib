@article{ ISI:000236636600002,
Author = {Farthing, MW and Kees, CE and Russell, TF and Miller, CT},
Title = {{An ELLAM approximation for advective-dispersive transport with nonlinear
   sorption}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2006}},
Volume = {{29}},
Number = {{5}},
Pages = {{657-675}},
Month = {{MAY}},
Abstract = {{We consider an Eulerian-Lagrangian localized adjoint method (ELLAM)
   applied to nonlinear model equations governing solute transport and
