@article{ ISI:000290929700006,
Author = {McKenna, Sean A. and Ray, Jaideep and Marzouk, Youssef and Waanders,
   Bart van Bloemen},
Title = {{Truncated multiGaussian fields and effective conductance of binary media}},
Journal = {{ADVANCES IN WATER RESOURCES}},
