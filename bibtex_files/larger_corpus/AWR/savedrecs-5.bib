@article{ ISI:000285812400008,
Author = {Jamali, Mirmosadegh and Haddadzadegan, Hamed},
Title = {{Effects of a sill on selective withdrawal through a point sink in a
   linearly stratified fluid}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2010}},
Volume = {{33}},
Number = {{12}},
Pages = {{1517-1523}},
Month = {{DEC}},
Abstract = {{Reservoir topography has significant effects on mechanism of selective
   withdrawal. We study selective withdrawal of a linearly stratified fluid
   through a point sink in a long rectangular reservoir with a sill at the
   bed experimentally and analytically. Experiments were conducted for
   various flow rates and sill heights to evaluate their effects on
   withdrawal layer in the inertial-buoyancy regime. Transition to the
   steady state is discussed in terms of dynamics of shear waves over the
   sill. The results show that in presence of a sill the withdrawal layer
   is thicker and higher in elevation. For flows controlled at the sill
   crest, the withdrawal layer thickness in the point-sink flow is almost
   equal to that in the similar line-sink flow. We propose an analytical
   relation for the withdrawal layer thickness in presence of a sill and
   confirm it by experiments. (C) 2010 Elsevier Ltd. All rights reserved.}},
