@article{ ISI:000286301500008,
Author = {Cardenas, M. Bayani},
Title = {{Lessons from and assessment of Boussinesq aquifer modeling of a large
   fluvial island in a dam-regulated river}},
Journal = {{ADVANCES IN WATER RESOURCES}},
