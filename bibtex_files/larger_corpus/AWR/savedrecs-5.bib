@article{ ISI:000289185500004,
Author = {Li, Liangping and Zhou, Haiyan and Jaime Gomez-Hernandez, J.},
Title = {{Transport upscaling using multi-rate mass transfer in three-dimensional
   highly heterogeneous porous media}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2011}},
Volume = {{34}},
Number = {{4}},
Pages = {{478-489}},
Month = {{APR}},
Abstract = {{A methodology for transport upscaling of three-dimensional highly
   heterogeneous formations is developed and demonstrated. The overall
   approach requires a prior hydraulic conductivity upscaling using an
   interblock-centered full-tensor Laplacian-with-skin method followed by
   transport upscaling. The coarse scale transport equation includes a
   multi-rate mass transfer term to compensate for the loss of
   heterogeneity inherent to all upscaling processes. The upscaling
   procedures for flow and transport are described in detail and then
   applied to a three-dimensional highly heterogeneous synthetic example.
   The proposed approach not only reproduces flow and transport at the
   coarse scale, but it also reproduces the uncertainty associated with the
   predictions as measured by the ensemble variability of the breakthrough
   curves. (C) 2011 Elsevier Ltd. All rights reserved.}},
