@article{ ISI:000286301500009,
Author = {Boano, Fulvio and Revelli, Roberto and Ridolfi, Luca},
Title = {{Effect of streamflow stochasticity on bedform-driven hyporheic exchange}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2010}},
Volume = {{33}},
Number = {{11, SI}},
