@article{ ISI:000276917400001,
Author = {Cullen, Erin and O'Carroll, Denis M. and Yanful, Ernest K. and Sleep,
   Brent},
Title = {{Simulation of the subsurface mobility of carbon nanoparticles at the
   field scale}},
