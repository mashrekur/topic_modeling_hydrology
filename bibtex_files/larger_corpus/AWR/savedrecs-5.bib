@article{ ISI:000289185500011,
Author = {Krol, Magdalena M. and Mumford, Kevin G. and Johnson, Richard L. and
   Sleep, Brent E.},
Title = {{Modeling discrete gas bubble formation and mobilization during
   subsurface heating of contaminated zones}},
Journal = {{ADVANCES IN WATER RESOURCES}},
Year = {{2011}},
Volume = {{34}},
Number = {{4}},
Pages = {{537-549}},
Month = {{APR}},
Abstract = {{During thermal remediation the increase in subsurface temperature can
   lead to bubble formation and mobilization. In order to investigate the
   effect of gas formation on resulting aqueous concentrations, a 2D finite
   difference flow and mass transport model was developed which
   incorporates a macroscopic invasion percolation (MIP) model to simulate
   bubble expansion and movement. The model was used to simulate three soil
   scenarios with different permeabilities and entry pressures at various
   operating temperatures and groundwater velocities. It was observed that
   discrete bubble formation occurred in all three soils, upward mobility
   being limited by lower temperatures and higher entry pressures. Bubble
   mobilization resulted in a different aqueous mass distribution than if
   no discrete gas formation was modeled, especially at higher
   temperatures. This was a result of bubbles moving upwards to cooler
   areas, then collapsing, and contaminating previously clean zones. The
   cooling effect also led to possible non-aqueous phase liquid (NAPL)
   formation which was not predicted using a model without discrete bubble
   formation. (C) 2011 Elsevier Ltd. All rights reserved.}},
