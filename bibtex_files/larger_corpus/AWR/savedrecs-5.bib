@article{ ISI:000311023300011,
Author = {Piotrowski, Adam P. and Napiorkowski, Jaroslaw J.},
Title = {{Product-Units neural networks for catchment runoff forecasting}},
Journal = {{ADVANCES IN WATER RESOURCES}},
