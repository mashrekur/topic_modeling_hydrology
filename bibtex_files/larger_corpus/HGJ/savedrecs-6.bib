@article{ ISI:000079766300013,
Author = {Tadolini, T and Spizzico, M},
Title = {{Relation between ``terra rossa{''} from the Apulia aquifer of Italy and
   the radon content of groundwater: Experimental results and their
   applicability to radon occurrence in the aquifer}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{1998}},
Volume = {{6}},
Number = {{3}},
Pages = {{450-454}},
Month = {{OCT}},
Abstract = {{The radon-222 (Rn-222) activity in groundwater of the Apulian karstic
   aquifer in southern Italy is as great as 500 Becquerel per liter (Bq/L)
   locally. Normal radium-226 (Ra-226) activity in the limestone and
   calcareous dolomites of the aquifer is not enough to explain such a high
   level. Laboratory investigations identified high Ra-226 activity in the
   ``terra rossa,{''} the residuum occupying fissures and cavities in the
   bedrock, and also the relation between (1) Ra-226-bearing bedrock and
   ``terra rossa{''} and (2) Rn-222 in water. The ``terra rossa{''} is the
   primary source of the radon in the groundwater. The experimental results
   show the need to characterize the ``terra rossa{''} of Apulia on the
   basis of Ra-226 activity and also to study the distribution and
   variations in Rn-222 activity over time in the aquifer.}},
