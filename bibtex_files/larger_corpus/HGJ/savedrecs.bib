@article{ ISI:000407923900023,
Author = {Erhardt, Ildiko and Otvos, Viktoria and Eross, Anita and Czauner,
   Brigitta and Simon, Szilvia and Madl-Szonyi, Judit},
Title = {{Hydraulic evaluation of the hypogenic karst area in Budapest (Hungary)}},
Journal = {{HYDROGEOLOGY JOURNAL}},
