@article{ ISI:000448697800028,
Author = {Li, Na and Wen, Zhang and Zhan, Hongbin and Zhu, Qi},
Title = {{The single-well test dilemma: the skin effect and variable-rate pumping
   perspective}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
Volume = {{26}},
