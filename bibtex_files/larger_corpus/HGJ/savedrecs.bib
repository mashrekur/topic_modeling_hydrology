@article{ ISI:000502730500008,
Author = {Setlur, Neeraja and Sharp, Jr., John M. and Hunt, Brian B.},
Title = {{Crystalline-rock aquifer system of the Llano Uplift, Central Texas, USA}},
Journal = {{HYDROGEOLOGY JOURNAL}},
