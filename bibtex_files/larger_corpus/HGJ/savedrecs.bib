@article{ ISI:000462179900026,
Author = {Zhang, Jun and Wang, Wenke and Wang, Xiaoyong and Yin, Lihe and Zhu,
   Lifeng and Sun, Fangqiang and Dong, Jiaqiu and Xie, Yueqing and
