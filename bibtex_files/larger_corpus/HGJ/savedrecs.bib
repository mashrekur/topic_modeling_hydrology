@article{ ISI:000423051600002,
Author = {Schwartz, Michael O.},
Title = {{The new Wallula CO2 project may revive the old Columbia River Basalt
   (western USA) nuclear-waste repository project}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
Volume = {{26}},
Number = {{1}},
Pages = {{3-6}},
Month = {{FEB}},
Abstract = {{A novel CO2 sequestration project at Wallula, Washington, USA, makes
   ample use of the geoscientific data collection of the old nuclear waste
   repository project at the Hanford Site nearby. Both projects target the
   Columbia River Basalt (CRB). The new publicity for the old project comes
   at a time when the approach to high-level nuclear waste disposal has
   undergone fundamental changes. The emphasis now is on a technical
   barrier that is chemically compatible with the host rock. In the ideal
   case, the waste container is in thermodynamic equilibrium with the
   host-rock groundwater regime. The CRB groundwater has what it takes to
   represent the ideal case.}},
