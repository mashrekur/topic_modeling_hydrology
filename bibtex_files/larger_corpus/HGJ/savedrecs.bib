@article{ ISI:000415195900009,
Author = {Sener, M. Furkan and Sener, Mehmet and Uysal, I. Tonguc},
Title = {{The evolution of the Cappadocia Geothermal Province, Anatolia (Turkey):
   geochemical and geochronological evidence}},
