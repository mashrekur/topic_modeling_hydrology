@article{ ISI:000448697800015,
Author = {Lu, Li and Wang, Zhe and Pei, Jianguo},
Title = {{Sources and contamination characteristics of PAHs in environmental media
   in a karst underground river system (southern China)}},
Journal = {{HYDROGEOLOGY JOURNAL}},
