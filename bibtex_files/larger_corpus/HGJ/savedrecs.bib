@article{ ISI:000439318900003,
Author = {Hao, Aibing and Zhang, Yilong and Zhang, Eryong and Li, Zhenghong and
   Yu, Juan and Wang, Huang and Yang, Jianfeng and Wang, Yao},
Title = {{Review: Groundwater resources and related environmental issues in China}},
