@article{ ISI:000458520300028,
Author = {Binder, Martin and Tritschler, Felix and Burghardt, Diana and Klotzsch,
   Stephan and Dietrich, Peter and Liedl, Rudolf and Haendel, Falk},
Title = {{Application of snowmelt as an active and inexpensive dual isotope
   groundwater tracer}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2019}},
Volume = {{27}},
Number = {{1}},
Pages = {{423-433}},
Month = {{FEB}},
Abstract = {{The use of snowmelt as an inexpensive multi-component tracer solution
   for active aquifer characterization is investigated, creating a valid
   alternative to existing artificial water isotope labelling using
   enriched deuterium oxide ((H2O)-H-2) and water-O-18 ((H2O)-O-18). The
   approach directly takes advantage of natural differences between
   groundwater and precipitation. It is shown, at laboratory-scale and
   small field-scale, that a direct injection of snowmelt into a porous
   medium allows for the tracing of water flow and, therefore, for the
   determination of transport parameters based on the stable isotope
   signatures (H-2 and O-18) and on the sum parameter electrical
   conductivity (EC). The differences in the isotope signature between the
   snowmelt and groundwater applied in this study were significant, with
   (H-2)=61.0 and (O-18)=8.2, while the EC difference was similar to 0.5
   mS/cm. Stable isotope breakthrough was observed to be almost congruent
   to sodium chloride (laboratory tracer experiment) and to uranine
   (field-scale push-drift-pull test), clearly supporting the assumption of
   conservative transport. A crosscheck of the isotope data in H-2-O-18
   plots revealed no significant biases in the tests. On the other hand,
   the snowmelt's EC breakthrough suffered from a slight retardation due to
   ion exchange and mineral reactions.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
