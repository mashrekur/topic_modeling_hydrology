@article{ ISI:000502730500014,
Author = {Xia, Xuemin and Zhou, Nianqing and Wang, Lichun and Li, Xianwen and
   Jiang, Simin},
Title = {{Identification of transient contaminant sources in aquifers through a
   surrogate model based on a modified self-organizing-maps algorithm}},
Journal = {{HYDROGEOLOGY JOURNAL}},
