@article{ ISI:000439318900005,
Author = {Dong, Weihong and Xie, Wei and Su, Xiaosi and Wen, Chuanlei and Cao,
   Zhipeng and Wan, Yuyu},
Title = {{Review: Micro-organic contaminants in groundwater in China}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
