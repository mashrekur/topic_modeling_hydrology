@article{ ISI:000423051600019,
Author = {Salehi, Saeid and Chizari, Mohammad and Sadighi, Hassan and Bijani,
   Masoud},
Title = {{Assessment of agricultural groundwater users in Iran: a cultural
   environmental bias}},
