@article{ ISI:000441944100009,
Author = {Liu, Yi and Jiao, Jiu Jimmy and Cheng, Ho Kwan},
Title = {{Tracing submarine groundwater discharge flux in Tolo Harbor, Hong Kong
   (China)}},
Journal = {{HYDROGEOLOGY JOURNAL}},
