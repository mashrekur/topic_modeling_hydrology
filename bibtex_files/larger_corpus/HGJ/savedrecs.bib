@article{ ISI:000441944100010,
Author = {Chung, Sang Yong and Senapathi, Venkatramanan and Sekar, Selvam and Kim,
   Tae Hyung},
Title = {{Time series analyses of hydrological parameter variations and their
   correlations at a coastal area in Busan, South Korea}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
Volume = {{26}},
