@article{ ISI:000430538700005,
Author = {Tobin, Benjamin W. and Springer, Abraham E. and Kreamer, David K. and
   Schenk, Edward},
Title = {{Review: The distribution, flow, and quality of Grand Canyon Springs,
   Arizona (USA)}},
Journal = {{HYDROGEOLOGY JOURNAL}},
