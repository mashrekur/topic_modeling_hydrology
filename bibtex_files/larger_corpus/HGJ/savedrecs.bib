@article{ ISI:000433536800008,
Author = {Cameron, Enrico and Pilla, Giorgio and Stella, Fabio A.},
Title = {{Application of statistical classification methods for predicting the
   acceptability of well-water quality}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
Volume = {{26}},
Number = {{4}},
Pages = {{1099-1115}},
Month = {{JUN}},
Abstract = {{The application of statistical classification methods is investigated-in
   comparison also to spatial interpolation methods-for predicting the
   acceptability of well-water quality in a situation where an effective
   quantitative model of the hydrogeological system under consideration
   cannot be developed. In the example area in northern Italy, in
   particular, the aquifer is locally affected by saline water and the
   concentration of chloride is the main indicator of both saltwater
   occurrence and groundwater quality. The goal is to predict if the
   chloride concentration in a water well will exceed the allowable
   concentration so that the water is unfit for the intended use. A
   statistical classification algorithm achieved the best predictive
   performances and the results of the study show that statistical
   classification methods provide further tools for dealing with
   groundwater quality problems concerning hydrogeological systems that are
   too difficult to describe analytically or to simulate effectively.}},
