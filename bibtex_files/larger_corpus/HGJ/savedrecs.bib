@article{ ISI:000423051600007,
Author = {Miche, Helene and Saracco, Ginette and Mayer, Adriano and Qarqori,
   Khaoula and Rouai, Mohamed and Dekayir, Abdelilah and Chalikakis,
   Konstantinos and Emblanch, Christophe},
Title = {{Hydrochemical constraints between the karst Tabular Middle Atlas Causses
   and the Sais basin (Morocco): implications of groundwater circulation}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
Volume = {{26}},
Number = {{1}},
Pages = {{71-87}},
Month = {{FEB}},
Abstract = {{The karst Tabular Middle Atlas Causses reservoir is the main
   drinking-water supply of Fez-Meknes region (Sa < s Basin) in Morocco.
   Recent analyses showed a decline in associated groundwater chemical
   quality and increased turbidity. To understand this hydrosystem, four
   surveys were undertaken during fall and spring, 2009-2011.
   Hydrogeochemical studies coupled with isotopic analyses (delta O-18,
   delta D and Rn-222) showed that the aquifers between the causses
   (mountains) and the Sa < s Basin are of Liassic origin and at the
   southern extremities are of Triassic origin. Five recharge zones of
   different altitudes have been defined, including two main mixing zones
   in the south. Deuterium excess results suggest local recharge, while a
   plot of delta O-18 versus delta D characterizes a confined aquifer in
   the eastern sector. Rn-222 results reveal areas of rapid exchanges with
   an upwelling time of less than 2 weeks. A schematic conceptual model is
   presented to explain the groundwater circulation system and the behavior
   of this karst system.}},
