@article{ ISI:000430538700020,
Author = {Jones, Brendon R. and Brouwers, Luke B. and Dippenaar, Matthys A.},
Title = {{Partially to fully saturated flow through smooth, clean, open fractures:
   qualitative experimental studies}},
Journal = {{HYDROGEOLOGY JOURNAL}},
