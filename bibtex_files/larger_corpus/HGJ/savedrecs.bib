@article{ ISI:000439318900023,
Author = {Liao, Fu and Wang, Guangcai and Shi, Zheming and Cheng, Guoqiang and
   Kong, Qingmin and Mu, Wenqing and Guo, Liang},
Title = {{Estimation of groundwater discharge and associated chemical fluxes into
   Poyang Lake, China: approaches using stable isotopes (delta D and delta
   O-18) and radon}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2018}},
Volume = {{26}},
Number = {{5, SI}},
Pages = {{1625-1638}},
Month = {{AUG}},
Abstract = {{Poyang Lake is the largest freshwater lake in China and is well known
   for its ecological and economic importance. Understanding the
   contribution of groundwater to Poyang Lake is important for the lake's
   protection and management. In this study, stable isotopes (delta D and
   delta O-18), Rn-222 measurements, and corresponding models (Rn-222 and
   O-18 mass balance models) were employed to evaluate the groundwater
   discharge and associated chemical inputs to Poyang Lake. The results
   showed that the distribution of delta O-18 in the lake water reflects
   the groundwater discharge into the lake. The groundwater discharge
   estimated using the Rn-222 mass balance model was in reasonable
   agreement with the groundwater discharge derived from the O-18 mass
   balance model. The Rn-222 mass balance model showed that the groundwater
   discharge rate was 24.18 +/- 6.85 mm/d with a groundwater discharge flux
   of (2.24 +/- 0.63) x 10(7) m(3)/d, which accounts for 6.52-11.14\% of
   river-water input in the Poyang Lake area. The groundwater discharge
   flux estimated using the O-18 mass balance model was 3.17 x 10(7)
   m(3)/d, and the average groundwater discharge rate was 26.62 mm/d. The
   estimated groundwater discharge was used to estimate the associated
   chemical fluxes. It was found that groundwater-derived heavy metals such
   as iron and manganese are potential threats to the lake ecological
   system because of their large inputs from groundwater discharge.}},
