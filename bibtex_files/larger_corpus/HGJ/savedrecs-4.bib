@article{ ISI:000243104100007,
Author = {Doctor, Daniel H. and Alexander, Jr., E. Calvin and Petric, Metka and
   Kogovsek, Janja and Urbanc, Janko and Lojen, Sonja and Stichler,
   Willibald},
Title = {{Quantification of karst aquifer discharge components during storm events
   through end-member mixing analysis using natural chemistry and stable
   isotopes as tracers}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2006}},
Volume = {{14}},
Number = {{7}},
Pages = {{1171-1191}},
Month = {{NOV}},
Abstract = {{Karst aquifer components that contribute to the discharge of a water
   supply well in the Classical Karst (Kras) region (Italy/Slovenia) were
   quantitatively estimated during storm events. Results show that water
   released from storage within the epikarst may comprise as much as
   two-thirds of conduit flow in a karst aquifer following rainfall.
   Principal components analysis (PCA) and end-member mixing analysis
   (EMMA) were perfort-ned using major ion chemistry and the stable
   isotopes of water (delta O-18, delta H-2) and of dissolved inorganic
   carbon (delta C-13(DIC)) to estimate mixing proportions among three
   sources: (1) allogenic river recharge, (2) autogenic recharge, and (3)
   an anthropogenic component stored within the epikarst. The sinking river
   most influences the chemical composition of the water-supply well under
   low-flow conditions; however, this proportion changes rapidly during
   recharge events.}},
