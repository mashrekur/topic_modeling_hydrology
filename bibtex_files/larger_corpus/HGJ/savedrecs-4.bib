@article{ ISI:000266088400014,
Author = {Houben, Georg and Tuennermeier, Torge and Eqrar, Naim and Himmelsbach,
   Thomas},
Title = {{Hydrogeology of the Kabul Basin (Afghanistan), part II: groundwater
   geochemistry}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2009}},
Volume = {{17}},
Number = {{4}},
Pages = {{935-948}},
Month = {{JUN}},
Abstract = {{Shallow groundwater is the main source for drinking water in Kabul,
   Afghanistan. It comes from a multitude of shallow hand-pumped wells
   spread over the whole city area. The groundwater is characterised by
   slightly oxic redox conditions. Interactions with aquifer carbonates
   lead to near-neutral pH and high degrees of hardness. The mostly
   negative water budget of the Kabul Basin is the result of strong
   evaporation which leads to an increase in salt and also of some
   undesirable constituents, e.g. borate. Several years of drought have
   aggravated this problem. The shallow groundwater in the city has
   received tremendous amounts of pollution due to a lack of proper waste
   disposal and sewage treatment. Common indicators are elevated
   concentrations of nutrients such as nitrate and faecal bacteria. The
   high infant mortality can at least partially be attributed to the
   insufficient water hygiene. Acid generated during the mineralisation of
   the wastewater is hidden due to the strong pH buffering capacity of the
   groundwater system. Redox and pH conditions preclude significant
   mobilisation of trace metals and metalloids.}},
