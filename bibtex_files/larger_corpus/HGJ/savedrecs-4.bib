@article{ ISI:000270980700016,
Author = {Hatipoglu, Zubeyde and Motz, Louis H. and Bayari, Celal Serdar},
Title = {{Characterization of the groundwater flow system in the hillside and
   coastal aquifers of the Mersin-Tarsus region (Turkey)}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2009}},
Volume = {{17}},
Number = {{7}},
Pages = {{1761-1778}},
Month = {{NOV}},
Abstract = {{In the region between Mersin and Tarsus cities, located along the
   Mediterranean Sea coast in southern Turkey, the demand for groundwater
   has increased dramatically as the available surface water supplies have
   already been developed. Fundamental information is required to
   characterize the existing groundwater system in this area in order to
   establish a sustainable groundwater-use policy. For this purpose,
   hydrochemical and environmental isotopic data were collected and
   integrated with available geological and hydrogeological information to
   develop a conceptual model of the system. Results, backed up mainly by
   depleted stable isotope composition and infinitesimal tritium content,
   suggest that most of the groundwater along the coastal zone is supplied
   by the neighboring mountain belt while local precipitation has also
   contributes to aquifer recharge. The validation of the conceptual
   perspective by a steady-state numerical groundwater flow model reveals
   that about 90\% of the recharge to the aquifer system is supplied by the
   deep flow of karstic groundwater fed from the Taurus Mountains.
   Monitoring of changes in the recharge regime of the mountain sector
   seems to be critical in establishing future groundwater use policies.}},
