@article{ ISI:000274456600020,
Author = {Banks, David and Frolik, Adam and Gzyl, Grzegorz and Rogoz, Marek},
Title = {{Modeling and monitoring of mine water rebound in an abandoned coal mine
   complex: Siersza Mine, Upper Silesian Coal Basin, Poland}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2010}},
Volume = {{18}},
Number = {{2}},
Pages = {{519-534}},
Month = {{MAR}},
Note = {{Conference on Mine Water Pollution and Remediation, Newcastle Univ,
   Newcastle, ENGLAND, OCT 24, 2007}},
Abstract = {{A variable-volume, head-dependent mine water filling model (MIFIM) has
   been utilized to simulate the post-abandonment flooding of the Siersza
   coal mine in the Upper Silesian Coal Basin of southern Poland. It is
   demonstrated that desaturated pore space in the aquifer adjacent to the
   mine comprises a significant component of the resaturable mine-related
   void. The model results are very sensitive to the value of this poorly
   constrained parameter. Nevertheless, the model successfully predicted
   the first appearance of mine water in an observation well and its
   subsequent rise. Despite this apparent success, it is concluded that
   such modeling approaches generally lack predictive power for mines in
   permeable, porous host rocks. As real monitoring data accumulate,
   however, such models can be calibrated and their utility increased.}},
