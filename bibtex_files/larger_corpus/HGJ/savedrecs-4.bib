@article{ ISI:000255032600005,
Author = {Gandy, Catherine J. and Younger, Paul L.},
Title = {{Predicting long-term contamination potential of perched groundwater in a
   mine-waste heap using a random-walk method}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2008}},
Volume = {{16}},
Number = {{3}},
