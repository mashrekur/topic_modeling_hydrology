Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Ahmed, AA (Reprint Author), Sohag Univ, Dept Geol, Fac Sci, Sohag, Egypt.
   Sohag Univ, Dept Geol, Fac Sci, Sohag, Egypt.}},
DOI = {{10.1007/s10040-009-0461-z}},
