
@article{ ISI:000274456600015,
Author = {Land, Lewis and Huff, G. F.},
Title = {{Multi-tracer investigation of groundwater residence time in a karstic
   aquifer: Bitter Lakes National Wildlife Refuge, New Mexico, USA}},
