@article{ ISI:000254184800011,
Author = {Li, Xiangquan and Zhang, Li and Hou, Xinwei},
Title = {{Use of hydrogeochemistry and environmental isotopes for evaluation of
   groundwater in Qingshuihe Basin, northwestern China}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2008}},
Volume = {{16}},
Number = {{2}},
Pages = {{335-348}},
Month = {{MAR}},
Abstract = {{Hydrogeochemistry and environmental isotope data were utilized to
   understand origin, geochemical evolution, hydraulic interconnection, and
   renewability of groundwater in Qingshuihe Basin, northwestern China.
   There are four types of groundwater: (1) shallow groundwater in the
   mountain front pluvial fans, originating from recent recharge by
   precipitation, (2) deep paleo-groundwater of the lower alluvial plains,
   which was formed long ago, (3) shallow groundwater in the lower alluvial
   plains, which has undergone evaporation during the recharge process, and
   (4) mixed groundwater (shallow and deep groundwater in the plain). The
   main water types are Na-HCO3, which dominates type (1), and Na-SO4,
   which dominates types (2) and (3). Geochemical evolution in the upper
   pluvial fans is mainly the result of CO2 gas dissolution, silicates
   weathering and cation exchange; in the lower alluvial plains, it is
   related to mineral dissolution. The evaporative enrichment only produces
   significant salinity increases in the shallow groundwater of the lower
   alluvial plains. Shallow groundwater age in the upper plain is 10 years
   or so, showing a strong renewability. Deep groundwater ages in the lower
   plain are more than 200 years, showing poor renewability. In the
   exploitation areas, the renewability of groundwater evidently increases
   and the circulation period is 70-100 years.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
