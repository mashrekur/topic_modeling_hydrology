@article{ ISI:000255032600011,
Author = {Forcada, Elena Gimenez and Evangelista, Ignacio Morell},
Title = {{Contributions of boron isotopes to understanding the hydrogeochemistry
   of the coastal detritic aquifer of Castellon Plain, Spain}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2008}},
Volume = {{16}},
Number = {{3}},
Pages = {{547-557}},
