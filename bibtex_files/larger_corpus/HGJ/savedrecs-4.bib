Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Lazear, GD (Reprint Author), 20508 Brimstone Rd, Cedaredge, CO 81413 USA.}},
DOI = {{10.1007/s10040-008-0381-3}},
ISSN = {{1431-2174}},
Keywords = {{Fractured rocks; Groundwater flow; Underpressure; Convection; USA}},
