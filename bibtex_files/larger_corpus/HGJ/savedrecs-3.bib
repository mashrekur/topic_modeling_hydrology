@article{ ISI:000314333600007,
Author = {Utting, Nicholas and Lauriol, Bernard and Mochnacz, Neil and
   Aeschbach-Hertig, Werner and Clark, Ian},
Title = {{Noble gas and isotope geochemistry in western Canadian Arctic
   watersheds: tracing groundwater recharge in permafrost terrain}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2013}},
Volume = {{21}},
Number = {{1}},
Pages = {{79-91}},
Month = {{FEB}},
Abstract = {{In Canada's western Arctic, perennial discharge from permafrost
   watersheds is the surface manifestation of active groundwater flow
   systems with features including the occurrence of year-round open water
   and the formation of icings, yet understanding the mechanisms of
   groundwater recharge and flow in periglacial environments remains
   enigmatic. Stable isotopes (delta O-18, delta D, delta C-13(DIC)), and
   noble gases have proved useful to study groundwater recharge and flow of
   groundwater which discharges along rivers in Canada's western Arctic. In
   these studies of six catchments, groundwater recharge was determined to
   be a mix of snowmelt and precipitation. All systems investigated show
   that groundwater has recharged through organic soils with elevated
   P-CO2, which suggests that recharge occurs largely during summer when
   biological activity is high. Noble gas concentrations show that the
   recharge temperature was between 0 and 5 A degrees C, which when
   considered in the context of discharge temperatures, suggests that there
   is no significant imbalance of energy flux into the subsurface.
   Groundwater circulation times were found to be up to 31 years for
   non-thermal waters using the (3) H-He-3 method.}},
