@article{ ISI:000319350700005,
Author = {Sakata, Yoshitaka and Ikeda, Ryuji},
Title = {{Depth dependence and exponential models of permeability in alluvial-fan
   gravel deposits}},
Journal = {{HYDROGEOLOGY JOURNAL}},
