@article{ ISI:000300584200008,
Author = {Person, Mark and Marksamer, Andee and Dugan, Brandon and Sauer, Peter E.
   and Brown, Kyle and Bish, David and Licht, Kathy J. and Willett, Mark},
Title = {{Use of a vertical delta O-18 profile to constrain hydraulic properties
   and recharge rates across a glacio-lacustrine unit, Nantucket Island,
   Massachusetts, USA}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2012}},
Volume = {{20}},
Number = {{2}},
Pages = {{325-336}},
Month = {{MAR}},
Abstract = {{A vertical delta O-18 profile was measured through a glacio-lacustrine
   confining unit on Nantucket Island (USA) in order to constrain vertical
   infiltration rates when Glacial Lake Nantucket was filled (17-19
   thousand years (ka) BP) and after the lake drained. Pore fluids
   throughout the deposit were found to have a nearly uniform delta O-18
   isotopic composition (-9.3 to -6.3aEuro degrees) similar to modern
   precipitation. The porosity (0.35), permeability (10(-15) m(2)), and
   solute diffusivity (2 x 10(-10) +/- 1 x 10(-10) m(2)/s) of these
   silt-dominated glacio-lacustrine deposits were measured in order to
   quantify flow rates. A paleohydrologic model was developed across
   Nantucket Island to reconstruct vertical flow rates through the
   glacio-lacustrine sediments between 19 and 0 ka BP. Results indicate
   that the vertical Darcy flux through this confining unit varied by a
   factor of about 16, from about 0.08 m/y during lake-level high stands
   (17-19 ka BP) to 0.005 m/y once the lake drained. Models of Holocene
   isotope transport through these lacustrine deposits suggest that all of
   the isotopically depleted (delta O-18 < -20aEuro degrees) glacial
   meltwater would have been flushed by advection in less than 200 years.
   These findings suggest that significant amounts of glacial meltwater
   (similar to 64 million m(3)/y over 2,000 years) could have recharged the
   continental shelf.}},
