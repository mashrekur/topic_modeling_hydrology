@article{ ISI:000291488900009,
Author = {Kang, Fengxin and Jin, Menggui and Qin, Pinrui},
Title = {{Sustainable yield of a karst aquifer system: a case study of Jinan
   springs in northern China}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2011}},
Volume = {{19}},
Number = {{4}},
Pages = {{851-863}},
Month = {{JUN}},
Abstract = {{Based on the long-term monitoring data of rainfall, groundwater levels,
   groundwater abstraction, spring flow rates and groundwater quality, an
   assessment has been undertaken of the sustainable yield of a karst
   aquifer system in Shandong Province, northern China, to maintain
   perennial outflow of the karst springs while meeting water demands. One
   of the fundamental indicators for sustainable yield of groundwater is
   identified as maximum allowable water-level drawdown. A regional
   three-dimensional finite-difference numerical model has been developed
   to optimize the schemes associated with well fields and their locations
   and sustainable yields, in the Jinan spring catchment and its adjacent
   karst groundwater catchments, with the aim of maintaining the water
   level higher than the allowable lowest water level of 27.5 m above sea
   level. Furthermore, measures necessary to move towards sustainable use
   of the karst groundwater are outlined, drawing on contingency plans of
   water-source replacement and artificial recharge, dual water supply
   (based in water quality), use of the spring waters themselves, and
   groundwater quality protection.}},
