@article{ ISI:000277943700014,
Author = {Mckenzie, Jeffrey M. and Mark, Bryan G. and Thompson, Lonnie G. and
   Schotterer, Ulrich and Lin, Ping-Nan},
Title = {{A hydrogeochemical survey of Kilimanjaro (Tanzania): implications for
   water sources and ages}},
Journal = {{HYDROGEOLOGY JOURNAL}},
