@article{ ISI:000297197600008,
Author = {Yin, Lihe and Hu, Guangcheng and Huang, Jinting and Wen, Dongguang and
   Dong, Jiaqiu and Wang, Xiaoyong and Li, Hongbo},
Title = {{Groundwater-recharge estimation in the Ordos Plateau, China: comparison
   of methods}},
