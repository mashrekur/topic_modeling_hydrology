@article{ ISI:000325803900014,
Author = {Massuel, S. and George, B. A. and Venot, J. -P. and Bharati, L. and
   Acharya, S.},
Title = {{Improving assessment of groundwater-resource sustainability with
   deterministic modelling: a case study of the semi-arid Musi sub-basin,
   South India}},
