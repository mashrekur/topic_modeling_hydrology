@article{ ISI:000284599200015,
Author = {Wojda, Piotr and Brouyere, Serge and Derouane, Johan and Dassargues,
   Alain},
Title = {{HydroCube: an entity-relationship hydrogeological data model}},
Journal = {{HYDROGEOLOGY JOURNAL}},
