@article{ ISI:000297197300011,
Author = {Singh, Prafull K. and Kumar, Suyash and Singh, U. C.},
Title = {{Groundwater resource evaluation in the Gwalior area, India, using
   satellite data: an integrated geomorphological and geophysical approach}},
Journal = {{HYDROGEOLOGY JOURNAL}},
Year = {{2011}},
Volume = {{19}},
Number = {{7}},
Pages = {{1421-1429}},
Month = {{NOV}},
Abstract = {{The ever increasing demand for freshwater and overexploitation of
   existing reserves has necessitated the search for new resources,
   particularly in hard-rock terrains where groundwater is a vital and sole
   source for drinking and other activities. A fast and cost effective way
   of groundwater exploration is the study and analysis of remote sensing
   data. In the present study, various geomorphic units in the
   drought-prone and hard-rock-dominated Gwalior area, India, were
   identified using satellite images and classified into four categories:
   poor, moderate, good and excellent groundwater prospect zones. The area
   was scanned through vertical electrical soundings for the corroboration
   of inferred categories. The borehole yield data corroborate the results.
   Geomorphological mapping through satellite images, coupled with
   electrical resistivity surveys, provided vital information about spatial
   and depth-wise variation of aquifers in the area. Groundwater prospect
   mapping gives a rational picture of subsurface water resources and is
   helpful in predictive groundwater resource management.}},
