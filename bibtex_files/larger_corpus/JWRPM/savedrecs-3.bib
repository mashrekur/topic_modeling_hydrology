@article{ ISI:000265285600008,
Author = {Fen, Chiu-Shia and Chan, Chencha and Cheng, Hsien-Chie},
Title = {{Assessing a Response Surface-Based Optimization Approach for Soil Vapor
   Extraction System Design}},
