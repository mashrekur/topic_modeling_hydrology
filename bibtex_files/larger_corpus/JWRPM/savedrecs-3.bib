@article{ ISI:A1996TL96800010,
Author = {Lefkoff, LJ and Kendall, DR},
Title = {{Yields from ground-water storage for California state water project}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{1996}},
Volume = {{122}},
Number = {{1}},
Pages = {{72-74}},
Month = {{JAN-FEB}},
Abstract = {{Yields from a proposed ground-water storage facility are evaluated using
   a nonlinear optimization model of the California State Water Project
   (SWP) and the Central Valley Project (CVP). Model constraints account
   for the major hydrologic, regulatory, and operational features of both
   projects, including mass continuity, facility capacities, regulatory
   standards, and delivery contracts. The model maximizes long-term SWP
   yields over a 70-year period, using a quarterly time step and assuming
   current facilities are in place. Maximum yields are recomputed for a
   second scenario that includes new ground-water storage in the North Las
   Posas Basin, located in Ventura County. Delivery increases associated
   with the new facility are computed as the difference in optimized
   project yields between the two model runs. Results indicate that the
   ground-water storage facility may increase the average annual yield of
   SWP by 6\%.}},
