@article{ ISI:000240297700003,
Author = {Chen, Chi-Chung and McCarl, Bruce A. and Williams, R. Lynn},
Title = {{Elevation dependent management of the Edwards aquifer: Linked
   mathematical and dynamic programming approach}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2006}},
Volume = {{132}},
Number = {{5}},
Pages = {{330-340}},
Month = {{SEP-OCT}},
Abstract = {{This study addresses the dynamic issue of optimum pumping and optimum
   target ending aquifer elevation. A stochastic dynamic programming (DP)
   approach is used to address this optimal elevation question. The
   analytical approach relies on the generation of stochastic state
   transition information from a stochastic nonlinear, static programming
   model of the aquifer. Methodologically, the model linkage allowed us to
   handle a large number of potential state variables by collapsing them
   into a simple ending elevation state variable in the DP.}},
