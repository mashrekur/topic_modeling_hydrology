@article{ ISI:000228628300007,
Author = {Sung, WD and Huang, X and Wei, IW},
Title = {{Treatment and distribution system effects on chloramine decay, pH,
   nitrification, and disinfection by-products: Case study}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2005}},
Volume = {{131}},
Number = {{3}},
Pages = {{201-207}},
