@article{ ISI:000255353000009,
Author = {Zarghami, M. and Ardakanian, R. and Memariani, A. and Szidarovszky, F.},
Title = {{Extended OWA operator for group decision making on water resources
   projects}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
