@article{ ISI:000228628300003,
Author = {Farmani, R and Walters, GA and Savic, DA},
Title = {{Trade-off between total cost and reliability for Anytown water
   distribution network}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2005}},
