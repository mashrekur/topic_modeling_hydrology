@article{ ISI:000387233400021,
Author = {Jiang, Peng and Yu, Zhongbo and Gautam, Mahesh R. and Acharya, Kumud},
Title = {{The Spatiotemporal Characteristics of Extreme Precipitation Events in
   the Western United States}},
Journal = {{WATER RESOURCES MANAGEMENT}},
