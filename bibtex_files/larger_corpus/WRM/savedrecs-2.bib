@article{ ISI:000387233400020,
Author = {Tsai, Wen-Ping and Chiang, Yen-Ming and Huang, Jun-Lin and Chang,
   Fi-John},
Title = {{Exploring the Mechanism of Surface and Ground Water through Data-Driven
   Techniques with Sensitivity Analysis for Water Resources Management}},
Journal = {{WATER RESOURCES MANAGEMENT}},
