@article{ ISI:000394288000004,
Author = {Iglesias, Ana and Sanchez, Berta and Garrote, Luis and Lopez, Ivan},
Title = {{Towards Adaptation to Climate Change: Water for Rice in the Coastal
   Wetlands of Donana, Southern Spain}},
