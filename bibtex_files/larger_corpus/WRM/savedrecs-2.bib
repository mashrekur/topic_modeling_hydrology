@article{ ISI:000387233400017,
Author = {Agathokleous, Agathoklis and Christodoulou, Symeon},
Title = {{Vulnerability of Urban Water Distribution Networks under Intermittent
   Water Supply Operations}},
Journal = {{WATER RESOURCES MANAGEMENT}},
