@article{ ISI:000414039900015,
Author = {Ali, Zuliqar and Hussain, Ijaz and Faisal, Muhammad and Nazir, Hafiza
   Mamona and Abd-el Moemen, Mitwali and Hussain, Tajammal and Shamsuddin,
   Sadaf},
Title = {{A Novel Multi-Scalar Drought Index for Monitoring Drought: the
   Standardized Precipitation Temperature Index}},
Journal = {{WATER RESOURCES MANAGEMENT}},
