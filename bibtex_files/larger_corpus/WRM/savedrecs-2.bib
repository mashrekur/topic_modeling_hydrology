@article{ ISI:000393580100020,
Author = {Papaevangelou, Vassiliki and Gikas, Georgios D. and Tsihrintzis,
   Vassilios A.},
Title = {{Effect of Operational and Design Parameters on Performance of
   Pilot-Scale Vertical Flow Constructed Wetlands Treating University
   Campus Wastewater}},
