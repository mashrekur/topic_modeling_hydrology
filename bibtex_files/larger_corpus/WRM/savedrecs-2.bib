@article{ ISI:000397923700005,
Author = {Nandi, Ipsita and Srivastava, Prashant K. and Shah, Kavita},
Title = {{Floodplain Mapping through Support Vector Machine and Optical/Infrared
   Images from Landsat 8 OLI/TIRS Sensors: Case Study from Varanasi}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2017}},
Volume = {{31}},
