@article{ ISI:000395000500014,
Author = {Li, Wei and Zhou, Jianzhong and Sun, Huaiwei and Feng, Kuaile and Zhang,
   Hairong and Tayyab, Muhammad},
Title = {{Impact of Distribution Type in Bayes Probability Flood Forecasting}},
Journal = {{WATER RESOURCES MANAGEMENT}},
