@article{ ISI:000419553800021,
Author = {Carmen Carnero, Maria and Gomez, Andres},
Title = {{Optimization of Maintenance in Production and Storage Systems for
   Domestic Water}},
Journal = {{WATER RESOURCES MANAGEMENT}},
