@article{ ISI:000398042800002,
Author = {Yan, Huyong and Wang, Guoyin and Wu, Di and Huang, Yu and Shang,
   Mingsheng and Xu, Jianjun and Shan, Kun and Shi, Xiaoyu and Dong,
   Jianhua and Feng, Lei and Zhou, Botian and Yuan, Ye and Zhao, Yufei},
Title = {{Water Bloom Precursor Analysis Based on Two Direction S-Rough Set}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2017}},
Volume = {{31}},
Number = {{5}},
Pages = {{1435-1456}},
Month = {{MAR}},
Abstract = {{In the field of water quality management, it is vital to determine the
   main precursory anomalies from the precursor of intricate water bloom in
   the context of a given area. In this paper, a water bloom precursor
   analysis method, based on two direction singular rough set, was
   proposed. This approach was produced on the basis of the different
   sections and pre-water bloom of water bloom precursor anomalies and
   characteristic of elements transferred in singular rough set. For
   testing the validity of two direction singular rough set application in
   water bloom precursor analysis, Xiangxi River, which is one of the
   typical tributaries of Three Gorges Reservoir in China, was selected as
   study area. The result showed that compared with other indexes, pH and
