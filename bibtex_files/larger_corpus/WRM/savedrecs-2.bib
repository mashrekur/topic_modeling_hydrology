@article{ ISI:000393580100017,
Author = {Tzabiras, J. and Vasiliades, L. and Sidiropoulos, P. and Loukas, A. and
   Mylopoulos, N.},
Title = {{Evaluation of Water Resources Management Strategies to Overturn Climate
   Change Impacts on Lake Karla Watershed}},
