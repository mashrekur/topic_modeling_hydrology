@article{ ISI:000403011300011,
Author = {Yuan, Liu and Zhou, Jianzhong and Mai, Zijun and Li, Yuanzheng},
Title = {{Random Fuzzy Optimization Model for Short-Term Hydropower Scheduling
   Considering Uncertainty of Power Load}},
Journal = {{WATER RESOURCES MANAGEMENT}},
