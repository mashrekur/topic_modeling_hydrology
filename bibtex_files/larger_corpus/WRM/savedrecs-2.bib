@article{ ISI:000422982300005,
Author = {Palomba, Francesca and Cesari, Giorgio and Pelillo, Remo and Petroselli,
   Andrea},
Title = {{An Empirical Model for River Ecological Management with Uncertainty
   Evaluation}},
Journal = {{WATER RESOURCES MANAGEMENT}},
