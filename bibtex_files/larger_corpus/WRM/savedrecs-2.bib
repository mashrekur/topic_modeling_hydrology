@article{ ISI:000393777100015,
Author = {Kang, Shinuk and Lee, Sangho and Kang, Taeuk},
Title = {{Development and Application of Storage-Zone Decision Method for
   Long-Term Reservoir Operation Using the Dynamically Dimensioned Search
   Algorithm}},
Journal = {{WATER RESOURCES MANAGEMENT}},
