@article{ ISI:000422982300001,
Author = {Perez-Sanchez, Modesto and Amparo Lopez-Jimenez, P. and Ramos, Helena M.},
Title = {{Modified Affinity Laws in Hydraulic Machines towards the Best Efficiency
   Line}},
