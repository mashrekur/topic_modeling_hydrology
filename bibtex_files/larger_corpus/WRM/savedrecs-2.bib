@article{ ISI:000407831200017,
Author = {Chadalawada, Jayashree and Havlicek, Vojtech and Babovic, Vladan},
Title = {{A Genetic Programming Approach to System Identification of
   Rainfall-Runoff Models}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2017}},
Volume = {{31}},
