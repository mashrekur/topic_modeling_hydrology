@article{ ISI:000394288000005,
Author = {Giannikopoulou, A. S. and Gad, F. K. and Kampragou, E. and
   Assimacopoulos, D.},
Title = {{Risk-Based Assessment of Drought Mitigation Options: the Case of Syros
   Island, Greece}},
