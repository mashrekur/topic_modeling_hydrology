@article{ ISI:000393580100018,
Author = {Christelis, Vasileios and Mantoglou, Aristotelis},
Title = {{Pumping Optimization of Coastal Aquifers Assisted by Adaptive
   Metamodelling Methods and Radial Basis Functions}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2016}},
Volume = {{30}},
