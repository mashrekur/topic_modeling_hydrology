@article{ ISI:000399025400012,
Author = {Gulbaz, Sezar and Kazezyilmaz-Alhan, Cevza Melek},
Title = {{Hydrological Model of LID with Rainfall-Watershed-Bioretention System}},
Journal = {{WATER RESOURCES MANAGEMENT}},
