@article{ ISI:000403791300005,
Author = {Garrote, Luis},
Title = {{Managing Water Resources to Adapt to Climate Change: Facing Uncertainty
   and Scarcity in a Changing Context}},
Journal = {{WATER RESOURCES MANAGEMENT}},
