@article{ ISI:000393777100013,
Author = {Seyam, Mohammed and Othman, Faridah and El-Shafie, Ahmed},
Title = {{RBFNN Versus Empirical Models for Lag Time Prediction in Tropical Humid
   Rivers}},
Journal = {{WATER RESOURCES MANAGEMENT}},
