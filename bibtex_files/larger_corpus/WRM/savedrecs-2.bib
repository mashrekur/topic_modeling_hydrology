@article{ ISI:000407831200008,
Author = {Aghababaei, Mohammad and Etemad-Shahidi, Amir and Jabbari, Ebrahim and
   Taghipour, Milad},
Title = {{Estimation of Transverse Mixing Coefficient in Straight and Meandering
   Streams}},
Journal = {{WATER RESOURCES MANAGEMENT}},
