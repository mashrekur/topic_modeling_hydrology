@article{ ISI:000406652400007,
Author = {Tabari, Hossein and Taye, Meron Teferi and Onyutha, Charles and Willems,
   Patrick},
Title = {{Decadal Analysis of River Flow Extremes Using Quantile-Based Approaches}},
