@article{ ISI:000399025400015,
Author = {Qamar, Muhammad Uzair and Azmat, Muhammad and Shahid, Muhammad Adnan and
   Ganora, Daniele and Ahmad, Shakil and Cheema, Muhammad Jehanzeb Masud
   and Faiz, Muhammad Abrar and Sarwar, Abid and Shafeeque, Muhammad and
   Khan, Muhammad Imran},
Title = {{Rainfall Extremes: a Novel Modeling Approach for Regionalization}},
