@article{ ISI:000398042800006,
Author = {Zhou, Han and Huang, Jiejun and Yuan, Yanbin},
Title = {{Analysis of the Spatial Characteristics of the Water Usage Patterns
   Based on ESDA-GIS: An Example of Hubei Province, China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
