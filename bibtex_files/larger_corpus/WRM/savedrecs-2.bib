@article{ ISI:000419553800004,
Author = {Fazlali, Ali and Shourian, Mojtaba},
Title = {{A Demand Management Based Crop and Irrigation Planning Using the
   Simulation-Optimization Approach}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2018}},
Volume = {{32}},
Number = {{1}},
Pages = {{67-81}},
Month = {{JAN}},
Abstract = {{Coupled simulation-optimization models are useful tools for solving
   optimum water allocation and crop planning problems. In this study, the
   optimum crops pattern in the Arayez plain in the Karkheh river basin in
   Iran is determined by integration of a network flow programming (NFP)
   based simulation model and the shuffle frog leaping optimization
   algorithm (SFLA) in the form of a simulation-optimization approach.
   MODSIM applies NFP for finding water allocations which by use of its
   customization ability, the benefit of water supply for the agricultural
   crops is calculated based on the agronomic equations. The objective
   function is to maximize the total net benefit gained from crops
   production where the decision variables which are the irrigation depths
   and the cultivation areas are optimized by SFLA. Results show that by
   use of the coupled SFLA-NFP model, the net benefit increases 12\%
   comparing the present situation in the plain. Also, the sensitivity
   analyses on effective parameters indicate that the potential maximum
   yield and the net price of the crops yield in the market have a direct
   impact on the crops optimum cultivation area.}},
Publisher = {{SPRINGER}},
Address = {{VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS}},
Type = {{Article}},
