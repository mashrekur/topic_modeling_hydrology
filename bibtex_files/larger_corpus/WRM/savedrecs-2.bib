@article{ ISI:000393580100010,
Author = {Long Duc Nguyen and Karimanzira, Divas and Rauschenbach, Thomas and
   Ribbe, Lars},
Title = {{Design and Application of an Adaptive Time Delay Model for Flow Routing
   in Prismatic Trapezoidal Geometry River Reach}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2016}},
Volume = {{30}},
