@article{ ISI:000425155400010,
Author = {Zarei, Abdol Rassoul},
Title = {{Evaluation of Drought Condition in Arid and Semi- Arid Regions, Using
   RDI Index}},
Journal = {{WATER RESOURCES MANAGEMENT}},
