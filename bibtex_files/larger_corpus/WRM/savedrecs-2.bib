@article{ ISI:000406652400009,
Author = {Gu, Wenquan and Shao, Dongguo and Tan, Xuezhi and Shu, Chen and Wu, Zhen},
Title = {{Simulation and Optimization of Multi-Reservoir Operation in Inter-Basin
   Water Transfer System}},
