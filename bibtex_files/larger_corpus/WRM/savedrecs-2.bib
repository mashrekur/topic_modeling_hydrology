@article{ ISI:000388183900008,
Author = {Xiang, Zheng and Chen, Xiaohong and Lian, Yanqing},
Title = {{Quantifying the Vulnerability of Surface Water Environment in Humid
   Areas Base on DEA Method}},
Journal = {{WATER RESOURCES MANAGEMENT}},
