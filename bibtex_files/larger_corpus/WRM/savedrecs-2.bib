@article{ ISI:000403791300008,
Author = {Harmancioglu, Nilgun B.},
Title = {{Overview of Water Policy Developments: Pre- and Post-2015 Development
   Agenda}},
Journal = {{WATER RESOURCES MANAGEMENT}},
