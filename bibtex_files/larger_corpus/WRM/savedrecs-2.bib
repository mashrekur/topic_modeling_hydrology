@article{ ISI:000401076000010,
Author = {Araral, Eduardo and Asquer, Alberto and Wang, Yahua},
Title = {{Regulatory Constructivism: Application of Q Methodology in Italy and
   China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2017}},
Volume = {{31}},
Number = {{8, SI}},
Pages = {{2497-2521}},
Month = {{JUN}},
Abstract = {{Conventional view holds that beliefs play an important role in the
   development of regulations but there is little evidence to support this
   claim. We use Comparative Q Methodology to systematically map out and
   compare the beliefs of public officers in China and Italy, two countries
   with contrasting sets of institutions but have both adopted similar
   ideas about integrated water resource management. We find some
   similarities and differences in the beliefs of public officers in both
   countries. In particular, we find that in both countries beliefs on the
   regulation of water utilities are diverse and fragmented on issues such
   as ownership structure of water utilities, how water infrastructure
   development should be funded, and how tariffs should be regulated. Our
   findings have two implications for theory, methods and practice. First,
   the Q methodology is a useful tool for systematically mapping out the
   beliefs of regulators and managers. Second, systematically mapping out
   beliefs will help facilitate the development of an alternative regime of
   regulation such as negotiated rule making. This alternative regime can
   provide substantial benefits such as more efficient rule making, more
   cost effective enforcement and compliance, and more equitable in terms
   of balancing the interests of stakeholders.}},
