@article{ ISI:000393777100020,
Author = {Haque, Md Mahmudul and de Souza, Amaury and Rahman, Ataur},
Title = {{Water Demand Modelling Using Independent Component Regression Technique}},
Journal = {{WATER RESOURCES MANAGEMENT}},
