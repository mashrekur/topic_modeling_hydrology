@article{ ISI:000411869000003,
Author = {Capponi, Caterina and Ferrante, Marco and Zecchin, Aaron C. and Gong,
   Jinzhe},
Title = {{Leak Detection in a Branched System by Inverse Transient Analysis with
   the Admittance Matrix Method}},
Journal = {{WATER RESOURCES MANAGEMENT}},
