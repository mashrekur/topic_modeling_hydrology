@article{ ISI:000414039900012,
Author = {Sari, Vanessa and dos Reis Castro, Nilza Maria and Pedrollo, Olavo
   Correa},
Title = {{Estimate of Suspended Sediment Concentration from Monitored Data of
   Turbidity and Water Level Using Artificial Neural Networks}},
Journal = {{WATER RESOURCES MANAGEMENT}},
