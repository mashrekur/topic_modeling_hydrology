@article{ ISI:000374593500014,
Author = {Izady, A. and Abdalla, O. and Sadeghi, M. and Majidi, M. and Karimi, A.
   and Chen, M.},
Title = {{A Novel Approach to Modeling Wastewater Evaporation Based on Dimensional
   Analysis}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2016}},
Volume = {{30}},
Number = {{8}},
Pages = {{2801-2814}},
Month = {{JUN}},
Abstract = {{Wastewater from municipal and industrial sources is becoming
   increasingly important in being reused, for example, for irrigation
   purposes. Wastewater is commonly stored in treatment lagoons in which
   evaporation is the main cause of water loss. Nonetheless, modeling
   wastewater evaporation (WWE) has received little attention. Driven by
   this knowledge gap, this study was performed to explore extent to which
   impurities affect water evaporation. A dimensional analysis was used to
   formulate WWE as a function of clear water evaporation (CWE), wastewater
   properties and climatic variables. We based our modeling approach on
   experimental data collected from the Neishaboor municipal wastewater
   treatment plant, Iran. As a result of this analysis, a multiplicative
   model to formulate WWE as a function of the influencing variables is
   proposed which indicated a reasonably well accuracy (RMSE = 1.09 mm) for
   the WWE estimation. Clear water evaporation indicated to be the most
   correlated variable in the model such that a constant coefficient can
   also be used to estimate WWE from CWE at the cost of losing accuracy
   only by 4.6 \%.}},
