@article{ ISI:000373395500004,
Author = {Sattar, Ahmed M. A. and Gharabaghi, B. and McBean, Edward A.},
Title = {{Prediction of Timing of Watermain Failure Using Gene Expression Models}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2016}},
Volume = {{30}},
