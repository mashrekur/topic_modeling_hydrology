@article{ ISI:000367856800025,
Author = {Soltani, Maryam and Kerachian, Reza and Nikoo, Mohammad Reza and Noory,
   Hamideh},
Title = {{A Conditional Value at Risk-Based Model for Planning Agricultural Water
   and Return Flow Allocation in River Systems}},
Journal = {{WATER RESOURCES MANAGEMENT}},
