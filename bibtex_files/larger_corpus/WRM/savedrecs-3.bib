@article{ ISI:000350668300016,
Author = {Spiller, Marc and McIntosh, Brian S. and Seaton, Roger A. F. and
   Jeffrey, Paul J.},
Title = {{Integrating Process and Factor Understanding of Environmental Innovation
   by Water Utilities}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2015}},
