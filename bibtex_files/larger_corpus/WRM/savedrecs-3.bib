@article{ ISI:000369520400006,
Author = {Mancosu, Noemi and Spano, Donatella and Orang, Morteza and Sarreshteh,
   Sara and Snyder, Richard L.},
Title = {{SIMETAW\# - a Model for Agricultural Water Demand Planning}},
Journal = {{WATER RESOURCES MANAGEMENT}},
