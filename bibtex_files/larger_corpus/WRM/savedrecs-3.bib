@article{ ISI:000362422100011,
Author = {Wu, Jiang and Zhou, Jianzhong and Chen, Lu and Ye, Lei},
Title = {{Coupling Forecast Methods of Multiple Rainfall-Runoff Models for
   Improving the Precision of Hydrological Forecasting}},
Journal = {{WATER RESOURCES MANAGEMENT}},
