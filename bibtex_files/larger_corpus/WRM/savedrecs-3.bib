@article{ ISI:000354220700018,
Author = {Gul, A.},
Title = {{Comparing Past and Present Hydrologic Aspects of a Mediterranean Wetland
   from a Sustainable Management Perspective}},
Journal = {{WATER RESOURCES MANAGEMENT}},
