@article{ ISI:000357345300022,
Author = {Shao, Dongguo and Li, Xudong and Gu, Wenquan},
Title = {{A Method for Temporary Water Scarcity Analysis in Humid Region Under
   Droughts Condition}},
Journal = {{WATER RESOURCES MANAGEMENT}},
