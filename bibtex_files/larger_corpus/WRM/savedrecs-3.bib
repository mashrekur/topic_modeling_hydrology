@article{ ISI:000367856800013,
Author = {Razurel, Pierre and Gorla, Lorenzo and Crouzy, Benoit and Perona, Paolo},
Title = {{Non-proportional Repartition Rules Optimize Environmental Flows and
   Energy Production}},
Journal = {{WATER RESOURCES MANAGEMENT}},
