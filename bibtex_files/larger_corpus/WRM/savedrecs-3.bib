@article{ ISI:000363259500019,
Author = {Bhuyan, M. K. and Kumar, Sanjay and Jena, Joygopal and Bhunya, P. K.},
Title = {{Flood Hydrograph with Synthetic Unit Hydrograph Routing}},
Journal = {{WATER RESOURCES MANAGEMENT}},
