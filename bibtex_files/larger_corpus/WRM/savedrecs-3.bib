@article{ ISI:000377452800014,
Author = {Minatour, Yasser and Bonakdari, Hossein and Aliakbarkhani, Zahra
   Shirmohammadi},
Title = {{Extension of Fuzzy Delphi AHP Based on Interval-Valued Fuzzy Sets and
   its Application in Water Resource Rating Problems}},
Journal = {{WATER RESOURCES MANAGEMENT}},
