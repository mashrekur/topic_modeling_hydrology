@article{ ISI:000352822700012,
Author = {Stefanovic, Nenad and Radojevic, Ivana and Ostojic, Aleksandar and
   Comic, Ljiljana and Topuzovic, Marina},
Title = {{Composite Web Information System for Management of Water Resources}},
