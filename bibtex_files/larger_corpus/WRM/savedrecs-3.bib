@article{ ISI:000360811200002,
Author = {Reshma, T. and Reddy, K. Venkata and Pratap, Deva and Ahmedi, Mehdi and
   Agilan, V.},
Title = {{Optimization of Calibration Parameters for an Event Based Watershed
   Model Using Genetic Algorithm}},
Journal = {{WATER RESOURCES MANAGEMENT}},
