@article{ ISI:000354220700015,
Author = {Rathnayake, Upaka S. and Tanyimboh, Tiku T.},
Title = {{Evolutionary Multi-Objective Optimal Control of Combined Sewer Overflows}},
Journal = {{WATER RESOURCES MANAGEMENT}},
