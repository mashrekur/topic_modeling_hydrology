@article{ ISI:000354220700001,
Author = {Mosaedi, Abolfazl and Abyaneh, Hamid Zare and Sough, Mohammad Ghabaei
   and Samadi, S. Zahra},
Title = {{Quantifying Changes in Reconnaissance Drought Index using
   Equiprobability Transformation Function}},
