@article{ ISI:000377452800017,
Author = {Field, Graeme and Tavrisov, German and Brown, Christopher and Harris,
   Alan and Kreidl, O. Patrick},
Title = {{Particle Filters to Estimate Properties of Confined Aquifers}},
