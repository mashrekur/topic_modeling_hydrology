@article{ ISI:000367856800012,
Author = {Jing, Zhang and Zhang, Lan-yue and Xie, Qin and Li, Yao and Deng,
   Shi-huai and Shen, Fei and Li, Yuan-wei and Xiao, Hong and Yang, Gang
   and Song, Chun},
Title = {{An Empirical Method to Investigate the Spatial and Temporal Distribution
   of Annual Average Groundwater Recharge Intensity-a Case Study in Grand
   River, Michigan, USA}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2016}},
