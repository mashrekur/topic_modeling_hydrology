@article{ ISI:000379183600010,
Author = {Kumar, Rajesh and Singh, Shaktiman and Kumar, Ramesh and Singh, Atar and
   Bhardwaj, Anshuman and Sam, Lydia and Randhawa, Surjeet Singh and Gupta,
   Akhilesh},
Title = {{Development of a Glacio-hydrological Model for Discharge and Mass
   Balance Reconstruction}},
