@article{ ISI:000367856800005,
Author = {Shafaei, Maryam and Kisi, Ozgur},
Title = {{Lake Level Forecasting Using Wavelet-SVR, Wavelet-ANFIS and Wavelet-ARMA
   Conjunction Models}},
Journal = {{WATER RESOURCES MANAGEMENT}},
