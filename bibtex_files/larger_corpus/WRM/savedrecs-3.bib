@article{ ISI:000349361200003,
Author = {Kaviani, S. and Hassanli, A. M. and Homayounfar, M.},
Title = {{Optimal Crop Water Allocation Based on Constraint-State Method and
   Nonnormal Stochastic Variable}},
Journal = {{WATER RESOURCES MANAGEMENT}},
