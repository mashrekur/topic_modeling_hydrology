@article{ ISI:000377452800016,
Author = {Suliman, Ali H. Ahmed and Katimon, Ayob and Darus, Intan Zaurah Mat and
   Shahid, Shamsuddin},
Title = {{TOPMODEL for Streamflow Simulation of a Tropical Catchment Using
   Different Resolutions of ASTER DEM: Optimization Through Response
   Surface Methodology}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2016}},
Volume = {{30}},
Number = {{9}},
Pages = {{3159-3173}},
Month = {{JUL}},
Abstract = {{The unavailability of proper hydrological data quality combined with the
   complexity of most physical based hydrologic models limits research on
   rainfall-runoff relationships, particularly in the tropics. In this
   paper, an attempt has been made to use different resolutions of DEM
   generated from freely available 30 m-based ASTER imagery as primary
   input to the topographically-based hydrological (TOPMODEL) model to
   simulate the runoff of a medium catchment located in the tropics.
   Response surface methodology (RSM) was applied to optimize the most
   sensitive parameters for streamflow simulation. DEM resolutions from 30
   to 300 m have been used to assess their effects on the topographic index
   distribution (TI) and TOPMODEL simulation. It is found that changing DEM
   resolutions reduces the TOPMODEL simulation performance as the
   resolutions are varied from 30 to 300 m. The study concluded that the
   ASTER 30 m DEM can be used for reasonable streamflow simulation of a
   data scarce tropical catchment compared with the resampled DEMs.}},
