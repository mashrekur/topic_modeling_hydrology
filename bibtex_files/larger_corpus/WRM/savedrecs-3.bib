@article{ ISI:000362422100016,
Author = {Thompson, Angela R. and Stotler, R. L. and Macpherson, G. L. and Liu, G.},
Title = {{Laboratory Study of Low-Flow Rates on Clogging Processes for Application
   to Small-Diameter Injection Wells}},
Journal = {{WATER RESOURCES MANAGEMENT}},
