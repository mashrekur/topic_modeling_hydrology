@article{ ISI:000350668300001,
Author = {Osei-Twumasi, A. and Falconer, R. A. and Bockelmann-Evans, B. N.},
Title = {{Experimental Studies on Water and Solute Transport Processes in a
   Hydraulic Model of the Severn Estuary, UK}},
