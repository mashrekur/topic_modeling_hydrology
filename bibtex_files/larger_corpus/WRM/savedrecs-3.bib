@article{ ISI:000373395000013,
Author = {Subagadis, Yohannes Hagos and Schuetze, Niels and Grundmann, Jens},
Title = {{A Fuzzy-Stochastic Modeling Approach for Multiple Criteria Decision
   Analysis of Coupled Groundwater-Agricultural Systems}},
Journal = {{WATER RESOURCES MANAGEMENT}},
