@article{ ISI:000369520400008,
Author = {Urquijo, Julia and De Stefano, Lucia},
Title = {{Perception of Drought and Local Responses by Farmers: A Perspective from
   the Jucar River Basin, Spain}},
Journal = {{WATER RESOURCES MANAGEMENT}},
