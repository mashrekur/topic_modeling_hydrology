@article{ ISI:000373395500003,
Author = {Alexakis, Dimitris and Tsihrintzis, Vassili. S. A. and Tsakiris, George
   and Gikas, Georgios D.},
Title = {{Suitability of Water Quality Indices for Application in Lakes in the
   Mediterranean}},
