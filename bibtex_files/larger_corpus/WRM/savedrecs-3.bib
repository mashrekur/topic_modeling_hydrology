@article{ ISI:000362422100015,
Author = {Lian, Jinjiao and Huang, Mingbin},
Title = {{Evapotranspiration Estimation for an Oasis Area in the Heihe River Basin
   Using Landsat-8 Images and the METRIC Model}},
Journal = {{WATER RESOURCES MANAGEMENT}},
