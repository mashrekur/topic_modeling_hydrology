@article{ ISI:000380699100018,
Author = {Carravetta, Armando and Fecarotta, Oreste and Golia, Umberto Maria and
   La Rocca, Michele and Martino, Riccardo and Padulano, Roberta and
   Tucciarelli, Tullio},
Title = {{Optimization of Osmotic Desalination Plants for Water Supply Networks}},
