@article{ ISI:000326082100010,
Author = {Cammalleri, C. and Ciraolo, G. and Minacapilli, M. and Rallo, G.},
Title = {{Evapotranspiration from an Olive Orchard using Remote Sensing-Based Dual
   Crop Coefficient Approach}},
Journal = {{WATER RESOURCES MANAGEMENT}},
