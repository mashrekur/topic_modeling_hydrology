@article{ ISI:000328856900003,
Author = {Joseph-Duran, Bernat and Jung, Michael N. and Ocampo-Martinez, Carlos
   and Sager, Sebastian and Cembrano, Gabriela},
Title = {{Minimization of Sewage Network Overflow}},
