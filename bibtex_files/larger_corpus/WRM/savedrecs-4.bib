Author = {Jaiswal, R. K. and Thomas, T. and Galkate, R. V. and Ghosh, N. C. and
   Lohani, A. K. and Kumar, Rakesh},
Title = {{Development of Geomorphology Based Regional Nash Model for Data Scares
   Central India Region}},
