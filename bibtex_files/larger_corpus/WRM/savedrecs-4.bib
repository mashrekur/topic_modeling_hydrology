@article{ ISI:000326082100013,
Author = {Liu, Yuyu and Sun, Caizhi and Xu, Shiguo},
Title = {{Eco-Efficiency Assessment of Water Systems in China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
