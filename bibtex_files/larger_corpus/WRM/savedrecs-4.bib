@article{ ISI:000324267000015,
Author = {Preziosi, E. and Del Bon, A. and Romano, E. and Petrangeli, A. B. and
   Casadei, S.},
Title = {{Vulnerability to Drought of a Complex Water Supply System. The Upper
   Tiber Basin Case Study (Central Italy)}},
Journal = {{WATER RESOURCES MANAGEMENT}},
