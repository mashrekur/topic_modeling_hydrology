@article{ ISI:000346417700005,
Author = {Nyeko, Martine},
Title = {{Hydrologic Modelling of Data Scarce Basin with SWAT Model: Capabilities
   and Limitations}},
Journal = {{WATER RESOURCES MANAGEMENT}},
