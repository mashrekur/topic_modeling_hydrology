@article{ ISI:000346416900005,
Author = {He, Zhenfang and Zhang, Yaonan and Guo, Qingchun and Zhao, Xueru},
Title = {{Comparative Study of Artificial Neural Networks and Wavelet Artificial
   Neural Networks for Groundwater Depth Data Forecasting with Various
   Curve Fractal Dimensions}},
