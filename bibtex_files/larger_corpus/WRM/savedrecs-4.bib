@article{ ISI:000328338300003,
Author = {Padula, Silvia and Harou, Julien J. and Papageorgiou, Lazaros G. and Ji,
   Yiming and Ahmad, Mohammad and Hepworth, Nigel},
Title = {{Least Economic Cost Regional Water Supply Planning - Optimising
   Infrastructure Investments and Demand Management for South East
   England's 17.6 Million People}},
Journal = {{WATER RESOURCES MANAGEMENT}},
