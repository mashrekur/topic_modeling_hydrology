@article{ ISI:000335510900010,
Author = {Teegavarapu, Ramesh S. V. and Simonovic, Slobodan P.},
Title = {{Simulation of Multiple Hydropower Reservoir Operations Using System
   Dynamics Approach}},
Journal = {{WATER RESOURCES MANAGEMENT}},
