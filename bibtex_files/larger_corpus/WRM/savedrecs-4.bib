@article{ ISI:000340533300018,
Author = {Kabir, Md Aynul and Dutta, Dushmanta and Hironaka, Sadayuki},
Title = {{Estimating Sediment Budget at a River Basin Scale Using a Process-Based
   Distributed Modelling Approach}},
