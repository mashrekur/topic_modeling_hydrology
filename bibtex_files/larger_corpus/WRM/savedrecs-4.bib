@article{ ISI:000329809700002,
Author = {Sahay, Rajeev Ranjan and Srivastava, Ayush},
Title = {{Predicting Monsoon Floods in Rivers Embedding Wavelet Transform, Genetic
   Algorithm and Neural Network}},
