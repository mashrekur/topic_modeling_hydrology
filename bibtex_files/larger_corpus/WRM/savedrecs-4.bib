@article{ ISI:000349361200013,
Author = {Liu, J. and Li, Y. P. and Huang, G. H. and Nie, S.},
Title = {{Development of a Fuzzy-Boundary Interval Programming Method for Water
   Quality Management Under Uncertainty}},
