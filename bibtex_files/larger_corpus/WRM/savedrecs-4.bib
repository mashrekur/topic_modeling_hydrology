@article{ ISI:000339927500024,
Author = {Sahoo, Bhabagrahi and Perumal, Muthiah and Moramarco, Tommaso and
   Barbetta, Silvia},
Title = {{Rating Curve Development at Ungauged River Sites using Variable
   Parameter Muskingum Discharge Routing Method}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2014}},
Volume = {{28}},
Number = {{11}},
Pages = {{3783-3800}},
Month = {{SEP}},
Abstract = {{A physically based simplified discharge routing method, namely, the
   variable parameter Muskingum discharge-hydrograph (VPMD) routing method,
   having the capability of estimating the stage hydrographs simultaneously
   in channels with floodplains is presented herein. The upstream discharge
   hydrograph is routed using this VPMD method in different two-stage
   symmetrical trapezoidal compound cross section channel reaches. The
   performance of the VPMD method is evaluated by numerical experiments
   using the benchmark MIKE11 hydrodynamic model and the field data of the
   Tiber River in central Italy. The proposed method is capable of
   accurately routing the discharge hydrographs, corresponding stage
   hydrographs and synthesizing the normal rating curves at any downstream
   ungauged river site which is not affected by any downstream effects.
   This study can be helpful for various planning and management of river
   water resources in both the diagnostic and prognostic modes.}},
