@article{ ISI:000331827700009,
Author = {Bolouri-Yazdeli, Y. and Bozorg-Haddad, Omid and Fallah-Mehdipour, E. and
   Marino, M. A.},
Title = {{Evaluation of Real-Time Operation Rules in Reservoir Systems Operation}},
