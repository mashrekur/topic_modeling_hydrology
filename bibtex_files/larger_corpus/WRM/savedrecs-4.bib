@article{ ISI:000347776900004,
Author = {Pu, Xiao and Cheng, Hongguang and Lu, Lu and Yang, Shengtian and Xie,
   Jing and Hao, Fanghua},
Title = {{Spatial Profiling and Assessing Dominance of Sources to Water Phosphorus
   Burden in a Shallow Lake}},
Journal = {{WATER RESOURCES MANAGEMENT}},
