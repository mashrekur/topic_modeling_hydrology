@article{ ISI:000346416900016,
Author = {Romano, Giulia and Salvati, Nicola and Guerrini, Andrea},
Title = {{Factors Affecting Water Utility Companies' Decision to Promote the
   Reduction of Household Water Consumption}},
Journal = {{WATER RESOURCES MANAGEMENT}},
