Address = {{VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Tessema, SM (Reprint Author), KTH Royal Inst Technol, Div Land \& Water Resources Engn, Stockholm, Sweden.
   Tessema, Selome M.; Mortberg, Ulla, KTH Royal Inst Technol, Div Land \& Water Resources Engn, Stockholm, Sweden.
   Lyon, Steve W., Stockholm Univ, S-10691 Stockholm, Sweden.
   Setegn, Shimelis G., Florida Int Univ, Dept Earth \& Environm, Miami, FL 33199 USA.}},
DOI = {{10.1007/s11269-014-0674-3}},
ISSN = {{0920-4741}},
EISSN = {{1573-1650}},
Keywords = {{Sustainable water use; Water balance components; SWAT; Retention
