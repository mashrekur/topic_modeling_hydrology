@article{ ISI:000346417700007,
Author = {Santl, Saso and Steinman, Franci},
Title = {{Hydropower Suitability Analysis on a Large Scale Level: Inclusion of a
   Calibration Phase to Support Determination of Model Parameters}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2015}},
Volume = {{29}},
