@article{ ISI:000338651100019,
Author = {Tabari, M. Mohammad Rezapour and Tavakoli, Shiva and Mari, Mohsen Mazak},
Title = {{Optimal Design of Concrete Canal Section for Minimizing Costs of Water
   Loss, Lining and Earthworks}},
Journal = {{WATER RESOURCES MANAGEMENT}},
