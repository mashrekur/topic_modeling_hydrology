@article{ ISI:000347776900005,
Author = {Liechti, T. Cohen and Matos, J. P. and Boillat, J. -L. and Schleiss, A.
   J.},
Title = {{Influence of Hydropower Development on Flow Regime in the Zambezi River
   Basin for Different Scenarios of Environmental Flows}},
