@article{ ISI:000349361000009,
Author = {Sanusi, Wahidah and Jemain, Abdul Aziz and Zin, Wan Zawiah Wan and
   Zahari, Marina},
Title = {{The Drought Characteristics Using the First-Order Homogeneous Markov
   Chain of Monthly Rainfall Data in Peninsular Malaysia}},
Journal = {{WATER RESOURCES MANAGEMENT}},
