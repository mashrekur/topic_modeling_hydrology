@article{ ISI:000349361000008,
Author = {Wang, Huiliang and Wu, Zening and Hu, Caihong},
Title = {{A Comprehensive Study of the Effect of Input Data on Hydrology and
   non-point Source Pollution Modeling}},
Journal = {{WATER RESOURCES MANAGEMENT}},
