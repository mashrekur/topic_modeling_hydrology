@article{ ISI:000324108800006,
Author = {Yang, Kan and Zheng, Jiao and Yang, Min and Zhou, Ran and Liu, Guoshuai},
Title = {{Adaptive Genetic Algorithm for Daily Optimal Operation of Cascade
   Reservoirs and its Improvement Strategy}},
Journal = {{WATER RESOURCES MANAGEMENT}},
