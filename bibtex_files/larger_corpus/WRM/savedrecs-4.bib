@article{ ISI:000329809700006,
Author = {Siew, Calvin and Tanyimboh, Tiku T. and Seyoum, Alemtsehay G.},
Title = {{Assessment of Penalty-Free Multi-Objective Evolutionary Optimization
   Approach for the Design and Rehabilitation of Water Distribution Systems}},
