@article{ ISI:000335510900002,
Author = {Khatibi, Rahman and Salmasi, Farzin and Ghorbani, Mohammad Ali and
   Asadi, Hakimeh},
Title = {{Modelling Energy Dissipation Over Stepped-gabion Weirs by Artificial
   Intelligence}},
