@article{ ISI:000335510900003,
Author = {He, Shawei and Hipel, Keith W. and Kilgour, D. Marc},
Title = {{Water Diversion Conflicts in China: A Hierarchical Perspective}},
Journal = {{WATER RESOURCES MANAGEMENT}},
