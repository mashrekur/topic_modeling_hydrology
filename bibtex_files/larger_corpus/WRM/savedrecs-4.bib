@article{ ISI:000349361000001,
Author = {Ziolkowska, Jadwiga R.},
Title = {{Is Desalination Affordable?-Regional Cost and Price Analysis}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2015}},
Volume = {{29}},
Number = {{5}},
Pages = {{1385-1397}},
Month = {{MAR}},
Abstract = {{Desalination has proven to be a reliable and efficient water supply
   option in many countries, especially in times of water scarcity.
   However, high desalination costs and high prices for desalinated water
   (twice or three times higher than those from traditional water sources)
   have been hindering an uptake and the development of desalination in
   many countries. Applied desalination technology, capital and operational
   costs, production capacity, water salinity are just a few factors
   determining the final cost of desalinated water that varies considerably
   between \$1.7-9.5/kgal (\$0.45-2.51/m(3)). The final prices for
   desalinated water and the related costs for local municipalities are
   among the most crucial determinants of the overall short- and long-term
   effectiveness of desalination processes. This paper provides an in-depth
   analysis on economics of desalination with country specific examples. It
   depicts a comprehensive picture of cost variability of desalinated water
   and points out challenges for cost-effective desalination in the future.}},
