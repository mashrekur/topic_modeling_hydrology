@article{ ISI:000303889100009,
Author = {Liu, An and Goonetilleke, Ashantha and Egodawatta, Prasanna},
Title = {{Inadequacy of Land Use and Impervious Area Fraction for Determining
   Urban Stormwater Quality}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2012}},
Volume = {{26}},
Number = {{8}},
Pages = {{2259-2265}},
Month = {{JUN}},
Abstract = {{Urban stormwater quality is multifaceted and the use of a limited number
   of factors to represent catchment characteristics may not be adequate to
   explain the complexity of water quality response to a rainfall event or
   site-to-site differences in stormwater quality modelling. This paper
   presents the outcomes of a research study which investigated the
   adequacy of using land use and impervious area fraction only, to
   represent catchment characteristics in urban stormwater quality
   modelling. The research outcomes confirmed the inadequacy of the use of
   these two parameters alone to represent urban catchment characteristics
   in stormwater quality prediction. Urban form also needs to be taken into
   consideration as it was found have an important impact on stormwater
   quality by influencing pollutant generation, build-up and wash-off.
   Urban form refers to characteristics related to an urban development
   such as road layout, spatial distribution of urban areas and urban
   design features.}},
