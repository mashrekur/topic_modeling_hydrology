@article{ ISI:000305350400015,
Author = {Wolfe, S. E.},
Title = {{Water Cognition and Cognitive Affective Mapping: Identifying Priority
   Clusters Within a Canadian Water Efficiency Community}},
Journal = {{WATER RESOURCES MANAGEMENT}},
