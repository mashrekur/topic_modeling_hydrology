@article{ ISI:000315264300009,
Author = {Fung, Fai and Watts, Glenn and Lopez, Ana and Orr, Harriet G. and New,
   Mark and Extence, Chris},
Title = {{Using Large Climate Ensembles to Plan for the Hydrological Impact of
   Climate Change in the Freshwater Environment}},
Journal = {{WATER RESOURCES MANAGEMENT}},
