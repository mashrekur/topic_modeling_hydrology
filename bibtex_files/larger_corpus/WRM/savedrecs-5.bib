@article{ ISI:000301847400007,
Author = {Lee, Seungho and Choi, Gye-Woon},
Title = {{Governance in a River Restoration Project in South Korea: The Case of
   Incheon}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2012}},
Volume = {{26}},
