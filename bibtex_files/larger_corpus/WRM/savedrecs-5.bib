@article{ ISI:000303889100013,
Author = {Takbiri, Zeinab and Afshar, Abbas},
Title = {{Multi-Objective Optimization of Fusegates System under Hydrologic
   Uncertainties}},
Journal = {{WATER RESOURCES MANAGEMENT}},
