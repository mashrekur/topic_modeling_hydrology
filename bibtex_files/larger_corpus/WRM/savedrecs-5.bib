@article{ ISI:000301747700011,
Author = {Pande, V. C. and Sharda, V. N. and Kurothe, R. S. and Sena, D. R. and
   Tiwari, S. P.},
Title = {{An Empirical Assessment of On-Farm Water Productivity using Groundwater
   in a Semi-Arid Indian Watershed}},
Journal = {{WATER RESOURCES MANAGEMENT}},
