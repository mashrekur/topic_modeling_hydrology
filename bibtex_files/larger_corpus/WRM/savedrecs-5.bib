@article{ ISI:000321573500009,
Author = {Narsimlu, Boini and Gosain, Ashvin K. and Chahar, Baghu R.},
Title = {{Assessment of Future Climate Change Impacts on Water Resources of Upper
   Sind River Basin, India Using SWAT Model}},
