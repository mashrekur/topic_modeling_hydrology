@article{ ISI:000301847400011,
Author = {Haenggi, Pascal and Weingartner, Rolf},
Title = {{Variations in Discharge Volumes for Hydropower Generation in Switzerland}},
Journal = {{WATER RESOURCES MANAGEMENT}},
