@article{ ISI:000301748000008,
Author = {Sherif, Mohsen and Kacimov, Anvar and Javadi, Akbar and Ebraheem, Abdel
   Azim},
Title = {{Modeling Groundwater Flow and Seawater Intrusion in the Coastal Aquifer
   of Wadi Ham, UAE}},
Journal = {{WATER RESOURCES MANAGEMENT}},
