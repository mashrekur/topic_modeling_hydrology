@article{ ISI:000318004800027,
Author = {Shinyie, Wendy Ling and Ismail, Noriszura and Jemain, Abdul Aziz},
Title = {{Semi-parametric Estimation for Selecting Optimal Threshold of Extreme
   Rainfall Events}},
