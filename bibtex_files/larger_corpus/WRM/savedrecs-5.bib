@article{ ISI:000312884600010,
Author = {Yang, Wei and Yang, Zhifeng},
Title = {{Development of a Long-term, Ecologically Oriented Dam Release Plan for
   the Lake Baiyangdian Sub-basin, Northern China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
