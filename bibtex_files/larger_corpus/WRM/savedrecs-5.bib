@article{ ISI:000307354300001,
Author = {Sun, Zhandong and Huang, Qun and Opp, Christian and Hennig, Thomas and
   Marold, Ulf},
Title = {{Impacts and Implications of Major Changes Caused by the Three Gorges Dam
   in the Middle Reaches of the Yangtze River, China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
