@article{ ISI:000307332000014,
Author = {Ganguli, Poulomi and Reddy, M. Janga},
Title = {{Risk Assessment of Droughts in Gujarat Using Bivariate Copulas}},
Journal = {{WATER RESOURCES MANAGEMENT}},
