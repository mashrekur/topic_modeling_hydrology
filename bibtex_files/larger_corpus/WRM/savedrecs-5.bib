@article{ ISI:000318621800016,
Author = {Sparks, Tim D. and Bockelmann-Evans, Bettina N. and Falconer, Roger A.},
Title = {{Development and Analytical Verification of an Integrated 2-D Surface
   Water-Groundwater Model}},
Journal = {{WATER RESOURCES MANAGEMENT}},
