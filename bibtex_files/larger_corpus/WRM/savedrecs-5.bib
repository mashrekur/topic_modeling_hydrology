@article{ ISI:000318004800030,
Author = {Abushandi, Eyad and Merkel, Broder},
Title = {{Modelling Rainfall Runoff Relations Using HEC-HMS and IHACRES for a
   Single Rain Event in an Arid Region of Jordan}},
Journal = {{WATER RESOURCES MANAGEMENT}},
