Author = {Gonzalez-Gomez, Francisco and Garcia-Rubio, Miguel A. and Alcala-Olid,
   Francisco and Isabel Ortega-Diaz, M.},
Title = {{Outsourcing and Efficiency in the Management of Rural Water Services}},
Journal = {{WATER RESOURCES MANAGEMENT}},
