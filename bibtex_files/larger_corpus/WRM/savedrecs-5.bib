@article{ ISI:000301847400017,
Author = {Panagopoulos, George P. and Bathrellos, George D. and Skilodimou,
   Hariklia D. and Martsouka, Faini A.},
Title = {{Mapping Urban Water Demands Using Multi-Criteria Analysis and GIS}},
Journal = {{WATER RESOURCES MANAGEMENT}},
