@article{ ISI:000315264300010,
Author = {Wade, Steven D. and Rance, Jemima and Reynard, Nick},
Title = {{The UK Climate Change Risk Assessment 2012: Assessing the Impacts on
   Water Resources to Inform Policy Makers}},
Journal = {{WATER RESOURCES MANAGEMENT}},
