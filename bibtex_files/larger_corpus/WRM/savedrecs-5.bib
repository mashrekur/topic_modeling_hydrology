@article{ ISI:000312731300008,
Author = {Tabari, Hossein and Nikbakht, Jaefar and Talaee, P. Hosseinzadeh},
Title = {{Hydrological Drought Assessment in Northwestern Iran Based on Streamflow
   Drought Index (SDI)}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2013}},
Volume = {{27}},
Number = {{1}},
Pages = {{137-151}},
Month = {{JAN}},
Abstract = {{Drought is a natural and worldwide phenomenon that occurs when water
   availability is significantly below normal levels during a significant
   period of time and cannot meet demand. This work focused on the
   hydrologic drought defined by the streamflow drought index (SDI) for
   overlapping periods of 3, 6, 9 and 12 months at 14 hydrometric stations
   in the northwest of Iran over the period 1975-2009. It was found that
   some of the streamflow volume series did not follow the normal
   distribution. The ability of the log-normal, exponential and uniform
   probability distributions was examined in order to choose the most
   suitable distribution, and the log-normal distribution was used to fit
   the long-term streamflow data. The results of the hydrological drought
   analysis based on the SDI showed that almost all the stations suffered
   from extreme droughts during the study period. Additionally, extreme
   droughts occurred most frequently in the last 12 years from 1997-1998 to
   2008-2009.}},
