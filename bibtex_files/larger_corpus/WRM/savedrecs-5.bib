@article{ ISI:000315265400005,
Author = {Wang, Xiqin and Zhang, Yuan and Zeng, Yong and Liu, Changming},
Title = {{Resolving Trans-jurisdictional Water Conflicts by the Nash Bargaining
   Method: A Case Study in Zhangweinan Canal Basin in North China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
