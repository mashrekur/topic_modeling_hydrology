@article{ ISI:000303889100006,
Author = {Tabari, Hossein and Nikbakht, Jaefar and Talaee, P. Hosseinzadeh},
Title = {{Identification of Trend in Reference Evapotranspiration Series with
   Serial Dependence in Iran}},
Journal = {{WATER RESOURCES MANAGEMENT}},
