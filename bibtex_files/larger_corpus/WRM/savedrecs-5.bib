@article{ ISI:000301748200011,
Author = {Bakken, Tor Haakon and Ruden, Fridtjof and Mangset, Lars Erik},
Title = {{Submarine Groundwater: A New Concept for the Supply of Drinking Water}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2012}},
Volume = {{26}},
Number = {{4}},
Pages = {{1015-1026}},
Month = {{MAR}},
Abstract = {{Submarine groundwater is water stored under the sea-bed. It might exist
   in large quantities and can have much lower salinity than the ambient
   sea-water. Therefore, it may be considered as a potential source of raw
   water for drinking water supply. This article assesses the feasibility
   of practical use of these resources. The hydro-geological potential and
   economical factors are discussed, and a technical concept for the
   withdrawal, transport and treatment of the water is presented. Based on
   these factors, we propose that submarine groundwater can be a potential
   substitute for desalination of sea water, in terms of comparable or
   lower cost potential. Furthermore, we expect energy efficiency gains and
   thus also higher environmental performance in terms of reduced
   CO2-emissions, compared to desalination of sea water based on fossil
   fuel. We foresee no major environmental, social or political barrier in
   utilising submarine groundwater as a new source of raw water.}},
