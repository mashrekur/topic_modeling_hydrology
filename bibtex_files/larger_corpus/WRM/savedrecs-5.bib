@article{ ISI:000304411800009,
Author = {Seckin, Neslihan and Guven, Aytac},
Title = {{Estimation of Peak Flood Discharges at Ungauged Sites Across Turkey}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2012}},
Volume = {{26}},
