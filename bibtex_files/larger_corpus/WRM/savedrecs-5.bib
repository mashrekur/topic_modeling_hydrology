@article{ ISI:000301747700005,
Author = {Liu, Songsong and Papageorgiou, Lazaros G. and Gikas, Petros},
Title = {{Integrated Management of Non-conventional Water Resources in Anhydrous
   Islands}},
Journal = {{WATER RESOURCES MANAGEMENT}},
