@article{ ISI:000318004800017,
Author = {Guo, Xuning and Hu, Tiesong and Wu, Conglin and Zhang, Tao and Lv,
   Yibing},
Title = {{Multi-Objective Optimization of the Proposed Multi-Reservoir Operating
   Policy Using Improved NSPSO}},
Journal = {{WATER RESOURCES MANAGEMENT}},
