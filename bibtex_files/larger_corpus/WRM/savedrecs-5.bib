@article{ ISI:000304411800003,
Author = {Angelidis, Panagiotis and Maris, Fotios and Kotsovinos, Nikos and
   Hrissanthou, Vlassios},
Title = {{Computation of Drought Index SPI with Alternative Distribution Functions}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2012}},
Volume = {{26}},
Number = {{9}},
Pages = {{2453-2473}},
Month = {{JUL}},
Abstract = {{The Standardized Precipitation Index (SPI) is widely used as drought
   meteorological index, to identify the duration and/or severity of a
   drought. The SPI is usually computed by fitting the gamma probability
   distribution to the observed precipitation data. In this work, the
   possibility to calculate SPI by fitting to the precipitation data the
   normal and the log-normal probability distributions was studied. For
   this purpose, 19 time series of monthly precipitation of 76 years were
   used, and the assumption that the gamma probability distribution would
   provide better representation of the precipitation data than log-normal
   and normal distributions, at various time scales (1, 3, 6, 12 and 24
   months) was tested. It is concluded that for SPI of 12 or 24 months, the
   log-normal or the normal probability distribution can be used for
   simplicity, instead of gamma, producing almost the same results.}},
