@article{ ISI:000485317700016,
Author = {Liu, Bin and Zhang, Feilian and Wan, Wuyi and Luo, Xiaowei},
Title = {{Multi-objective Decision-Making for the Ecological Operation of Built
   Reservoirs Based on the Improved Comprehensive Fuzzy Evaluation Method}},
