@article{ ISI:000469431400014,
Author = {Hrnjica, Bahrudin and Bonacci, Ognjen},
Title = {{Lake Level Prediction using Feed Forward and Recurrent Neural Networks}},
Journal = {{WATER RESOURCES MANAGEMENT}},
