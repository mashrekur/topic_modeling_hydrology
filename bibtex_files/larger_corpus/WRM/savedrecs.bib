@article{ ISI:000475892100008,
Author = {Li, Fang-Fang and Wang, Zhi-Yu and Zhao, Xiao and Xie, En and Qiu, Jun},
Title = {{Decomposition-ANN Methods for Long-Term Discharge Prediction Based on
   Fisher's Ordered Clustering with MESA}},
Journal = {{WATER RESOURCES MANAGEMENT}},
