@article{ ISI:000458693900005,
Author = {Shoaib, Muhammad and Shamseldin, Asaad Y. and Khan, Sher and Sultan,
   Muhammad and Ahmad, Fiaz and Sultan, Tahir and Dahri, Zakir Hussain and
   Ali, Irfan},
Title = {{Input Selection of Wavelet-Coupled Neural Network Models for
   Rainfall-Runoff Modelling}},
Journal = {{WATER RESOURCES MANAGEMENT}},
