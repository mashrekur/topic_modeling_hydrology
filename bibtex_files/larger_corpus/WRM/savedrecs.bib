@article{ ISI:000485317700004,
Author = {Gupta, Sushindra Kumar and Tyagi, Jaivir and Sharma, Gunwant and Jethoo,
   A. S. and Singh, P. K.},
Title = {{An Event-Based Sediment Yield and Runoff Modeling Using Soil Moisture
   Balance/Budgeting (SMB) Method}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2019}},
Volume = {{33}},
Number = {{11}},
Pages = {{3721-3741}},
Month = {{SEP}},
Abstract = {{The Soil Conservation Service Curve Number (SCS-CN) method is frequently
   used for the estimation of direct surface runoff depth from the small
   watersheds. Coupling the SCS-CN method with the Soil Moisture Balance
   (SMB) method, new simple 2-parameters rainfall-runoff model and
   3-parametrs rainfall-sediment yield models are derived for computation
   of runoff and sediment yield respectively. The proposed runoff (R2) and
   sediment yield (S2) models have been tested on a large set of
   rainfall-runoff and sediment yield data (98 storm events) obtained from
   twelve watersheds from different land use/land cover, soil and climatic
   conditions. The improved runoff (R2) and sediment yield (S2) models show
   superior results as compared to the existing Mishra et al. (S1) and
   original SCS-CN (R1) models. The results and analysis justify the use of
   the proposed models for field applications.}},
