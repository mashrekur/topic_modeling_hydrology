@article{ ISI:000430661500009,
Author = {Wu, Bingfang and Zeng, Hongwei and Yan, Nana and Zhang, Miao},
Title = {{Approach for Estimating Available Consumable Water for Human Activities
   in a River Basin}},
Journal = {{WATER RESOURCES MANAGEMENT}},
