@article{ ISI:000430661500010,
Author = {Sharafati, A. and Azamathulla, H. M.},
Title = {{Assessment of Dam Overtopping Reliability using SUFI Based Overtopping
   Threshold Curve}},
Journal = {{WATER RESOURCES MANAGEMENT}},
