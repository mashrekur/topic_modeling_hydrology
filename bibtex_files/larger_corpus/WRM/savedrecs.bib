@article{ ISI:000504320400004,
Author = {Yang, Jie and Chen, Xiaohong},
Title = {{Quantification of the Driving Factors of Water Use in the Productive
   Sector Change Using Various Decomposition Methods}},
Journal = {{WATER RESOURCES MANAGEMENT}},
