@article{ ISI:000445220800009,
Author = {Tan, Mou Leong and Ramli, Hilmi P. and Tam, Tze Huey},
Title = {{Effect of DEM Resolution, Source, Resampling Technique and Area
   Threshold on SWAT Outputs}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2018}},
