@article{ ISI:000430661500003,
Author = {Mladenovic-Ranisavljevic, Ivana I. and Takic, Lj. and Nikolic, D.},
Title = {{Water Quality Assessment Based on Combined Multi-Criteria
   Decision-Making Method with Index Method}},
Journal = {{WATER RESOURCES MANAGEMENT}},
