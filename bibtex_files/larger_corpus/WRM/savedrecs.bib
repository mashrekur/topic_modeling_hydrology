@article{ ISI:000427995100008,
Author = {Zhang, Duo and Martinez, Nicolas and Lindholm, Geir and Ratnaweera,
   Harsha},
Title = {{Manage Sewer In-Line Storage Control Using Hydraulic Model and Recurrent
   Neural Network}},
