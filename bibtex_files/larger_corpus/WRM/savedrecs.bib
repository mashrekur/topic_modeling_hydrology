@article{ ISI:000440115100014,
Author = {Levi, Eldad and Goldman, Mark and Tibor, Gideon and Herut, Barak},
Title = {{Delineation of Subsea Freshwater Extension by Marine Geoelectromagnetic
   Soundings (SE Mediterranean Sea)}},
