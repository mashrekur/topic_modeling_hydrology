@article{ ISI:000499554300002,
Author = {Barreiras, Nuno and Correia, Francisco Nunes and Matos, Rafaela Saldanha},
Title = {{Assessment of a Web-Based Water Information System Performance in the
   Context of Groundwater Governance}},
Journal = {{WATER RESOURCES MANAGEMENT}},
