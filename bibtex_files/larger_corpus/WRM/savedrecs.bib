@article{ ISI:000511600900004,
Author = {Tao, Hai and Keshtegar, Behrooz and Yaseen, Zaher Mundher},
Title = {{The Feasibility of Integrative Radial Basis M5Tree Predictive Model for
   River Suspended Sediment Load Simulation}},
Journal = {{WATER RESOURCES MANAGEMENT}},
