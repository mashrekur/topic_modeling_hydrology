@article{ ISI:000457803400001,
Author = {Li, Ping and Muenich, Rebecca L. and Chaubey, Indrajeet and Wei, Xiaomei},
Title = {{Evaluating Agricultural BMP Effectiveness in Improving Freshwater
   Provisioning Under Changing Climate}},
