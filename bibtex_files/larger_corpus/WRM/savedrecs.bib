@article{ ISI:000432147200006,
Author = {Jing, Xueer and Zhang, Shouhong and Zhang, Jianjun and Wang, Yujie and
   Wang, Yunqi and Yue, Tongjia},
Title = {{Analysis and Modelling of Stormwater Volume Control Performance of
   Rainwater Harvesting Systems in Four Climatic Zones of China}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2018}},
Volume = {{32}},
