@article{ ISI:000445220800020,
Author = {Yazdi, J. and Moghaddam, M. Sabbaghian and Saghafian, B.},
Title = {{Optimal Design of Check Dams in Mountainous Watersheds for Flood
   Mitigation}},
Journal = {{WATER RESOURCES MANAGEMENT}},
