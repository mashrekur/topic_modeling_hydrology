@article{ ISI:000433501600016,
Author = {Schuster-Wallace, Corinne J. and Murray, Steven J. and McBean, Edward A.},
Title = {{Integrating Social Dimensions into Flood Cost Forecasting}},
Journal = {{WATER RESOURCES MANAGEMENT}},
