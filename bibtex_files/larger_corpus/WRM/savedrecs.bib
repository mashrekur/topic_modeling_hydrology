@article{ ISI:000445220800018,
Author = {Bazargan, Jalal and Norouzi, Hadi},
Title = {{Investigation the Effect of Using Variable Values for the Parameters of
   the Linear Muskingum Method Using the Particle Swarm Algorithm (PSO)}},
