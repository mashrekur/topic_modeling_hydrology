@article{ ISI:000454718300007,
Author = {Risva, Konstantina and Nikolopoulos, Dionysios and Efstratiadis, Andreas
   and Nalbantis, Ioannis},
Title = {{A Framework for Dry Period Low Flow Forecasting in Mediterranean Streams}},
Journal = {{WATER RESOURCES MANAGEMENT}},
