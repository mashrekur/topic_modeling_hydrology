@article{ ISI:000457803400003,
Author = {Shahraki, Nadia and Marofi, Safar and Ghazanfari, Sadegh},
Title = {{Modeling of Daily Rainfall Extremes, Using a Semi-Parametric Pareto Tail
   Approach}},
