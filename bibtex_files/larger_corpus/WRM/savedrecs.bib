@article{ ISI:000492643700001,
Author = {Lu, Wei and Qin, Xiaosheng},
Title = {{An Integrated Fuzzy Simulation-Optimization Model for Supporting Low
   Impact Development Design under Uncertainty}},
Journal = {{WATER RESOURCES MANAGEMENT}},
