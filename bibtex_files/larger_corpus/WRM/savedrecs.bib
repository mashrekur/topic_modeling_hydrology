@article{ ISI:000511600900009,
Author = {Jiang, Zhiqiang and Tang, Zhengyang and Liu, Yi and Chen, Yuyun and
   Feng, Zhongkai and Xu, Yang and Zhang, Hairong},
Title = {{Area Moment and Error Based Forecasting Difficulty and its Application
   in Inflow Forecasting Level Evaluation}},
