@article{ ISI:000440830200004,
Author = {Sabzkouhi, Adell Moradi and Haghighi, Ali},
Title = {{Uncertainty Analysis of Transient Flow in Water Distribution Networks}},
Journal = {{WATER RESOURCES MANAGEMENT}},
