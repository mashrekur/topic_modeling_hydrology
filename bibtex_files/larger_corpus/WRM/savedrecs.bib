@article{ ISI:000469431400019,
Author = {Yuan, Wenlin and Liu, Meiqi and Wan, Fang},
Title = {{Calculation of Critical Rainfall for Small-Watershed Flash Floods Based
   on the HEC-HMS Hydrological Model}},
Journal = {{WATER RESOURCES MANAGEMENT}},
