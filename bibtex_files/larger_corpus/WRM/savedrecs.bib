@article{ ISI:000511600900011,
Author = {Weyne Rodrigues, Gustavo Paiva and Magalhaes Costa, Luis Henrique and
   Farias, Guilherme Marques and Holanda de Castro, Marco Aurelio},
Title = {{A Depth-First Search Algorithm for Optimizing the Gravity Pipe Networks
   Layout}},
Journal = {{WATER RESOURCES MANAGEMENT}},
