@article{ ISI:000469431400001,
Author = {Manouseli, Despina and Kayaga, S. M. and Kalawsky, R.},
Title = {{Evaluating the Effectiveness of Residential Water Efficiency Initiatives
   in England: Influencing Factors and Policy Implications}},
Journal = {{WATER RESOURCES MANAGEMENT}},
