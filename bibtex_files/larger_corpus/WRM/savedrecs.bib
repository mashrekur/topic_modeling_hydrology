@article{ ISI:000460836100019,
Author = {Ahbari, Abdellatif and Stour, Laila and Agoumi, Ali},
Title = {{Ability of the Performance Criteria to Assess and Compare Reservoir
   Management Approaches}},
Journal = {{WATER RESOURCES MANAGEMENT}},
