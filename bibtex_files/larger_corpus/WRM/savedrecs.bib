@article{ ISI:000499989400002,
Author = {Bahoosh, Sheyda and Bahoosh, Reza and Haghighi, Ali},
Title = {{Development of a Self-Adaptive Ant Colony Optimization for Designing
   Pipe Networks}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2019}},
Volume = {{33}},
