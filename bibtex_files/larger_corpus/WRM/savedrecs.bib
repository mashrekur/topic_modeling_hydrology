@article{ ISI:000511600900015,
Author = {Lee, Moon-Hwan and Bae, Deg-Hyo and Im, Eun-Soon},
Title = {{Effect of the Horizontal Resolution of Climate Simulations on the
   Hydrological Representation of Extreme Low and High Flows}},
Journal = {{WATER RESOURCES MANAGEMENT}},
