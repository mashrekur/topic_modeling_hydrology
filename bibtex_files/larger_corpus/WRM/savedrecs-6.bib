@article{ ISI:000287755500012,
Author = {Kostarelos, Konstantinos and Khan, Eakalak and Callipo, Nazzareno and
   Velasquez, Jennifer and Graves, Dave},
Title = {{Field Study of Catch Basin Inserts for the Removal of Pollutants from
   Urban Runoff}},
