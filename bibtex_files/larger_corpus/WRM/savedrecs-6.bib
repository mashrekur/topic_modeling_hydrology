@article{ ISI:000272870500002,
Author = {Zhang, Yan and Grant, Andrew and Sharma, Ashok and Chen, Donghui and
   Chen, Liang},
Title = {{Alternative Water Resources for Rural Residential Development in Western
   Australia}},
