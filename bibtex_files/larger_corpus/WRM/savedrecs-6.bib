@article{ ISI:000274438200010,
Author = {Cheng, Shin-jen and Lee, Cheng-feng and Lee, Ju-huang},
Title = {{Effects of Urbanization Factors on Model Parameters}},
Journal = {{WATER RESOURCES MANAGEMENT}},
