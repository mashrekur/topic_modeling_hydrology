@article{ ISI:000295518500013,
Author = {Molina, Jose-Luis and Luis Garcia-Arostegui, Jose and Bromley, John and
   Benavente, Jose},
Title = {{Integrated Assessment of the European WFD Implementation in Extremely
   Overexploited Aquifers Through Participatory Modelling}},
Journal = {{WATER RESOURCES MANAGEMENT}},
