@article{ ISI:000275453900009,
Author = {Guzha, Alphonce Chenjerayi and Hardy, T. B.},
Title = {{Application of the Distributed Hydrological Model, TOPNET, to the Big
   Darby Creek Watershed, Ohio, USA}},
Journal = {{WATER RESOURCES MANAGEMENT}},
