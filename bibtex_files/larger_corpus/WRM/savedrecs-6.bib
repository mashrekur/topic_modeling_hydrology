@article{ ISI:000280242500023,
Author = {Ozhan, Sueleyman and Gokbulak, Ferhat and Serengil, Yusuf and Ozcan,
   Mehmet},
Title = {{Evapotranspiration from a Mixed Deciduous Forest Ecosystem}},
