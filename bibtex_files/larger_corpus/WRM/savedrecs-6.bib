@article{ ISI:000275661500001,
Author = {Kumar, D. Nagesh and Baliarsingh, Falguni and Raju, K. Srinivasa},
Title = {{Optimal Reservoir Operation for Flood Control Using Folded Dynamic
   Programming}},
Journal = {{WATER RESOURCES MANAGEMENT}},
