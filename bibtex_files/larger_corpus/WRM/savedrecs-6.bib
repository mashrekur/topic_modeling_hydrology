@article{ ISI:000281742000011,
Author = {Liu, Chen-Wuing and Chou, Yen-Lu and Lin, Shien-Tsung and Lin, Gin-Jie
   and Jang, Cheng-Shin},
Title = {{Management of High Groundwater Level Aquifer in the Taipei Basin}},
Journal = {{WATER RESOURCES MANAGEMENT}},
