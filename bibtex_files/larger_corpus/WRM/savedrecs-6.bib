@article{ ISI:000281742000007,
Author = {Akkoyunlu, Atilla and Akiner, Muhammed Ernur},
Title = {{Feasibility Assessment of Data-Driven Models in Predicting Pollution
   Trends of Omerli Lake, Turkey}},
