@article{ ISI:000277419300010,
Author = {Masetti, Marco and Diolaiuti, Guglielmina and D'Agata, Carlo and
   Smiraglia, Claudio},
Title = {{Hydrological Characterization of an Ice-Contact Lake: Miage Lake (Monte
   Bianco, Italy)}},
