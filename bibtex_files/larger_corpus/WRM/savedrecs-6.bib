@article{ ISI:000278363900006,
Author = {Yang, Y. S. and Wang, L.},
Title = {{A Review of Modelling Tools for Implementation of the EU Water Framework
   Directive in Handling Diffuse Water Pollution}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2010}},
