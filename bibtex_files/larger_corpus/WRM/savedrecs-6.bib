@article{ ISI:000286324200008,
Author = {Afshar, Amin and Marino, Miguel A. and Saadatpour, Motahareh and Afshar,
   Abbas},
Title = {{Fuzzy TOPSIS Multi-Criteria Decision Analysis Applied to Karun
   Reservoirs System}},
