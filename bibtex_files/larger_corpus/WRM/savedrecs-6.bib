@article{ ISI:000280948700021,
Author = {Ghazavi, Reza and Vali, Abbasali and Eslamian, Saeid},
Title = {{Impact of Flood Spreading on Infiltration Rate and Soil Properties in an
   Arid Environment}},
Journal = {{WATER RESOURCES MANAGEMENT}},
