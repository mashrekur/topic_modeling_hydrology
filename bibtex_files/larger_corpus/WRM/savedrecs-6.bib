@article{ ISI:000281398500011,
Author = {Franceschini, Fiorenzo and Galetto, Maurizio and Turina, Elisa},
Title = {{Water and Sewage Service Quality: A Proposal of a New
   Multi-Questionnaire Monitoring Tool}},
