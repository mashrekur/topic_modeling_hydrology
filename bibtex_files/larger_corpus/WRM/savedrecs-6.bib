@article{ ISI:000272870500005,
Author = {Li, Lanhai and Xu, Honggang and Chen, Xi and Simonovic, S. P.},
Title = {{Streamflow Forecast and Reservoir Operation Performance Assessment Under
   Climate Change}},
Journal = {{WATER RESOURCES MANAGEMENT}},
