@article{ ISI:000293136700009,
Author = {Park, Suwan},
Title = {{Estimating the Timing of the Economical Replacement of Water Mains Based
   on the Predicted Pipe Break Times Using the Proportional Hazards Models}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2011}},
Volume = {{25}},
