@article{ ISI:000286324200002,
Author = {Liu, Xinyuan and Guo, Shenglian and Liu, Pan and Chen, Lu and Li, Xiang},
Title = {{Deriving Optimal Refill Rules for Multi-Purpose Reservoir Operation}},
Journal = {{WATER RESOURCES MANAGEMENT}},
