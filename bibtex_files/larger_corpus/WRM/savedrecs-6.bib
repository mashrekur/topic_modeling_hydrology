@article{ ISI:000280242500007,
Author = {Choudhari, Sumant A. and Raj, P. Anand},
Title = {{Multiobjective Multireservoir Operation in Fuzzy Environment}},
Journal = {{WATER RESOURCES MANAGEMENT}},
