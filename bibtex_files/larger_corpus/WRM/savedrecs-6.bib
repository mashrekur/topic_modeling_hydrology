@article{ ISI:000278363900005,
Author = {Petheram, Cuan and McMahon, Thomas A. and Peel, Murray C. and Smith,
   Chris J.},
Title = {{A Continental Scale Assessment of Australia's Potential for Irrigation}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2010}},
Volume = {{24}},
