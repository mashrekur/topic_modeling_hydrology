@article{ ISI:000296474900019,
Author = {Molina, Jose-Luis and Farmani, Raziyeh and Bromley, John},
Title = {{Aquifers Management through Evolutionary Bayesian Networks: The
   Altiplano Case Study (SE Spain)}},
Journal = {{WATER RESOURCES MANAGEMENT}},
