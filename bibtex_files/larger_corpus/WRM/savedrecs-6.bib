Title = {{Application of GWQI to Assess Effect of Land Use Change on Groundwater
   Quality in Lower Shiwaliks of Punjab: Remote Sensing and GIS Based
   Approach}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2011}},
Volume = {{25}},
