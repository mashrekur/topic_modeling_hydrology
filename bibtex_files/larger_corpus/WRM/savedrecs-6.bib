@article{ ISI:000269886500004,
Author = {Awad, Mohamad and Khawlie, Mohamad and Darwich, Talal},
Title = {{Web Based Meta-database and its Role in Improving Water Resources
   Management in the Mediterranean Basin}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2009}},
Volume = {{23}},
Number = {{13}},
Pages = {{2669-2680}},
Month = {{OCT}},
Abstract = {{Water is a very crucial natural resource in the world and because of its
   selective availability among different countries; it will be the most
   precious commodity in the future. In the Mediterranean basin one can
   find a mosaic of countries rich in water and others poor. However, the
