@article{ ISI:000293921700012,
Author = {Sedki, Akram and Ouazar, Driss},
Title = {{Simulation-Optimization Modeling for Sustainable Groundwater
   Development: A Moroccan Coastal Aquifer Case Study}},
Journal = {{WATER RESOURCES MANAGEMENT}},
