@article{ ISI:000269886500013,
Author = {Pachpute, J. S. and Tumbo, S. D. and Sally, H. and Mul, M. L.},
Title = {{Sustainability of Rainwater Harvesting Systems in Rural Catchment of
   Sub-Saharan Africa}},
