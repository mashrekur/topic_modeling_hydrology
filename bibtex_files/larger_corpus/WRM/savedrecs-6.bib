@article{ ISI:000277419300001,
Author = {Villholth, Karen Grothe and Rajasooriyar, Lorraine D.},
Title = {{Groundwater Resources and Management Challenges in Sri Lanka-an Overview}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2010}},
Volume = {{24}},
Number = {{8}},
Pages = {{1489-1513}},
Month = {{JUN}},
Abstract = {{This paper gives an overview of the geophysico-chemical groundwater
   conditions in Sri Lanka and the associated contemporary management
   challenges. Groundwater is extensively used in Sri Lanka today, for
   agriculture, domestic use and industry/tourism. Groundwater access,
   availability and vulnerability are governed by six major types of
   aquifer systems of which the most prevalent is the regolith aquifers in
   the central hard rock areas of the island. Uncontrolled groundwater use
   and contamination or natural poor quality are leading to access
   limitations and health concerns. The tsunami severely affected
   groundwater in the coastal areas and functioned as a wake-up call to
   further emphasize the importance of groundwater for life-supporting
   functions. Despite an emerging awareness, groundwater management is in
   its infancy, with the attitude of groundwater development still not
   converted into an approach of active management. The role of groundwater
   in achieving sustainable development and in the development of
   appropriate water management institutions needs to be highlighted and
   specifically addressed in policy discussions.}},
