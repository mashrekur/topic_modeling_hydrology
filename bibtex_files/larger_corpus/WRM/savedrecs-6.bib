@article{ ISI:000272870500006,
Author = {Gueldal, Veysel and Tongal, Hakan},
Title = {{Comparison of Recurrent Neural Network, Adaptive Neuro-Fuzzy Inference
   System and Stochastic Models in Egirdir Lake Level Forecasting}},
Journal = {{WATER RESOURCES MANAGEMENT}},
