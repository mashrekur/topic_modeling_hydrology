@article{ ISI:000293136700011,
Author = {Sadegh, Mojtaba and Kerachian, Reza},
Title = {{Water Resources Allocation Using Solution Concepts of Fuzzy Cooperative
   Games: Fuzzy Least Core and Fuzzy Weak Least Core}},
Journal = {{WATER RESOURCES MANAGEMENT}},
