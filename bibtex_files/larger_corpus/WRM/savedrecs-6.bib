@article{ ISI:000295518500011,
Author = {Vanham, Davy and Millinger, Stefanie and Pliessnig, Harald and Rauch,
   Wolfgang},
Title = {{Rasterised Water Demands: Methodology for Their Assessment and Possible
   Applications}},
