@article{ ISI:000288450300007,
Author = {Martin-Ortega, Julia and Giannoccaro, Giacomo and Berbel, Julio},
Title = {{Environmental and Resource Costs Under Water Scarcity Conditions: An
   Estimation in the Context of the European Water Framework Directive}},
Journal = {{WATER RESOURCES MANAGEMENT}},
