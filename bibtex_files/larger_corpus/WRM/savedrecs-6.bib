@article{ ISI:000275661500007,
Author = {Amit, R. K. and Ramachandran, Parthasarathy},
Title = {{A Fair Contract for Managing Water Scarcity}},
Journal = {{WATER RESOURCES MANAGEMENT}},
Year = {{2010}},
Volume = {{24}},
Number = {{6}},
Pages = {{1195-1209}},
Month = {{APR}},
Abstract = {{In public utilities, under supply constraints, fairness considerations
   lead to a market failure. This paper characterizes a two-period
   principal-agent contract for demand management, that mitigates this
   market failure in urban water systems. The contract is designed as an
   extensive form mechanism using subgame perfect Nash equilibrium (SPNE)
   as the solution concept. The contract is fair; and is shown to be
   economically efficient if, in case of deviation by the agent, the gain
   to the agent and the loss to the principal are small. It is shown that
   the assumption can be avoided in an infinite horizon contract.}},
