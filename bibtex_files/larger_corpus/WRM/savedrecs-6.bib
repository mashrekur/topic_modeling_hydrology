@article{ ISI:000290585700001,
Author = {Pena de Andrade, Jose Geraldo and Franco Barbosa, Paulo Sergio and Alves
   Souza, Luiz Carlos and Makino, Daniel Lucas},
Title = {{Interbasin Water Transfers: The Brazilian Experience and International
   Case Comparisons}},
Journal = {{WATER RESOURCES MANAGEMENT}},
