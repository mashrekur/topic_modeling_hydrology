@article{ ISI:000288450300008,
Author = {Montesinos, Pilar and Camacho, Emilio and Campos, Blanca and Antonio
   Rodriguez-Diaz, Juan},
Title = {{Analysis of Virtual Irrigation Water. Application to Water Resources
   Management in a Mediterranean River Basin}},
Journal = {{WATER RESOURCES MANAGEMENT}},
