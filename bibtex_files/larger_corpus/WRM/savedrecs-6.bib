@article{ ISI:000271989700005,
Author = {Al-Omari, Abbas and Al-Quraan, Saleh and Al-Salihi, Adnan and Abdulla,
   Fayez},
Title = {{A Water Management Support System for Amman Zarqa Basin in Jordan}},
Journal = {{WATER RESOURCES MANAGEMENT}},
