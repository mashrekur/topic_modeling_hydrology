@article{ ISI:000297663000001,
Author = {Whitfield, Paul H. and Spence, Christopher},
Title = {{Estimates of Canadian Pacific Coast runoff from observed streamflow data}},
Journal = {{JOURNAL OF HYDROLOGY}},
