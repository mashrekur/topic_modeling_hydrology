@article{ ISI:000288828500022,
Author = {De Michele, C. and Zenoni, E. and Pecora, S. and Rosso, R.},
Title = {{Analytical derivation of rain intensity-duration-area-frequency
   relationships from event maxima}},
Journal = {{JOURNAL OF HYDROLOGY}},
