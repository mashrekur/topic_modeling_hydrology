@article{ ISI:000287074300010,
Author = {Wang, Kang and Zhang, Renduo},
Title = {{Heterogeneous soil water flow and macropores described with combined
   tracers of dye and iodine}},
Journal = {{JOURNAL OF HYDROLOGY}},
