@article{ ISI:000292367700004,
Author = {Bormann, Helge and Pinter, Nicholas and Elfert, Simon},
Title = {{Hydrological signatures of flood trends on German rivers: Flood
   frequencies, flood heights and specific stages}},
Journal = {{JOURNAL OF HYDROLOGY}},
