@article{ ISI:000293429200003,
Author = {Hong, Sungwook and Shin, Inchul},
Title = {{A physically-based inversion algorithm for retrieving soil moisture in
   passive microwave remote sensing}},
Journal = {{JOURNAL OF HYDROLOGY}},
