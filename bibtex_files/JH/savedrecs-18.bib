@article{ ISI:000291068400002,
Author = {Rotter, B. E. and Barry, D. A. and Gerhard, J. I. and Small, J. S.},
Title = {{Modeling the effectiveness of U(VI) biomineralization in dual-porosity
   porous media}},
Journal = {{JOURNAL OF HYDROLOGY}},
