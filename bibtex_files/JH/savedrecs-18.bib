@article{ ISI:000286554000011,
Author = {Tabari, Hossein and Talaee, Parisa Hosseinzadeh},
Title = {{Temporal variability of precipitation over Iran: 1966-2005}},
Journal = {{JOURNAL OF HYDROLOGY}},
