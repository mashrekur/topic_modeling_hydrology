@article{ ISI:000286554000004,
Author = {Galleani, Lorenzo and Vigna, Bartolomeo and Banzato, Cinzia and Lo
   Russo, Stefano},
Title = {{Validation of a Vulnerability Estimator for Spring Protection Areas: The
   VESPA index}},
