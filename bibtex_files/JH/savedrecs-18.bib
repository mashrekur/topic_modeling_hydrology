@article{ ISI:000295067500016,
Author = {Ni, Chuen-Fa and Lin, Chi-Ping and Li, Shu-Guang and Liu, Chien-Jung},
Title = {{Efficient approximate spectral method to delineate stochastic well
   capture zones in nonstationary groundwater flow systems}},
Journal = {{JOURNAL OF HYDROLOGY}},
