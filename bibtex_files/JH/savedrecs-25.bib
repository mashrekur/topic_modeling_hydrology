@article{ ISI:000335706900022,
Author = {Worrall, F. and Howden, N. J. K. and Burt, T. P.},
Title = {{A method of estimating in-stream residence time of water in rivers}},
Journal = {{JOURNAL OF HYDROLOGY}},
