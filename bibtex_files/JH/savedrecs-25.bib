Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Pfannerstill, M (Reprint Author), Univ Kiel, Inst Nat Resource Conservat, Dept Hydrol \& Water Resources Management, Kiel, Germany.
   Pfannerstill, Matthias; Guse, Bjoern; Fohrer, Nicola, Univ Kiel, Inst Nat Resource Conservat, Dept Hydrol \& Water Resources Management, Kiel, Germany.}},
DOI = {{10.1016/j.jhydrol.2013.12.044}},
