@article{ ISI:000335274900038,
Author = {Li, Xing and Zhou, Yunxuan and Zhang, Lianpeng and Kuang, Runyuan},
Title = {{Shoreline change of Chongming Dongtan and response to river sediment
   load: A remote sensing assessment}},
Journal = {{JOURNAL OF HYDROLOGY}},
