@article{ ISI:000333138800009,
Author = {Adams, Kenneth D. and Sada, Donald W.},
Title = {{Surface water hydrology and geomorphic characterization of a playa lake
   system: Implications for monitoring the effects of climate change}},
Journal = {{JOURNAL OF HYDROLOGY}},
