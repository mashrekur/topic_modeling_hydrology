@article{ ISI:000335706900001,
Author = {Wang, Shusen and Huang, Jianliang and Li, Junhua and Rivera, Alfonso and
   McKenney, Daniel W. and Sheffield, Justin},
Title = {{Assessment of water budget for sixteen large drainage basins in Canada}},
