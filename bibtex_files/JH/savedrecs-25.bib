@article{ ISI:000331662800023,
Author = {Wang, Li and Wu, Joan Q. and Elliot, William J. and Fiedler, Fritz R.
   and Lapin, Sergey},
Title = {{Linear diffusion-wave channel routing using a discrete Hayami
   convolution method}},
Journal = {{JOURNAL OF HYDROLOGY}},
