@article{ ISI:000315008300010,
Author = {Soja, Gerhard and Zueger, Johann and Knoflacher, Markus and Kinner, Paul
   and Soja, Anna-Maria},
Title = {{Climate impacts on water balance of a shallow steppe lake in Eastern
   Austria (Lake Neusiedl)}},
