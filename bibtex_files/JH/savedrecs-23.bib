@article{ ISI:000313935200006,
Author = {Luo, Yi and Arnold, Jeff and Liu, Shiyin and Wang, Xiuying and Chen, Xi},
Title = {{Inclusion of glacier processes for distributed hydrological modeling at
   basin scale with application to a watershed in Tianshan Mountains,
   northwest China}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2013}},
Volume = {{477}},
Pages = {{72-85}},
Month = {{JAN 16}},
Abstract = {{In this paper we proposed: (1) an algorithm of glacier melt,
   sublimation/evaporation, accumulation, mass balance and retreat; (2) a
   dynamic Hydrological Response Unit approach for incorporating the
   algorithm into the Soil and Water Assessment Tool (SWAT) model; and (3)
   simulated the transient glacier retreat and its impacts on streamflow at
   basin scale. Application of the enhanced SWAT model in the Manas River
   Basin (MRB) in the Tianshan Mountains in northwest China, shows that the
   approach is viable as evidenced by a Nash-Sutcliff efficiency of 0.65
   and a percent bias of -3.7\% for daily streamflow and water balance,
   respectively. The results indicate that the glacier area decreased by
   11\% during the simulation period from 1961 to 1999, which is within the
   range of records from other glaciers. On average, glacier melt
   contributed 25\% to streamflow, although glacier area accounts for only
   14\% of the catchment drainage area in the MRB. Glacier melt was
   positively correlated to temperature change (R-2 = 0.70, statistical
   significance P < 0.001) and negatively correlated to precipitation (R-2
   = 0.20, statistical significance P < 0.005). The results indicate that
   glacier melt was more sensitive to temperature change than to
   precipitation change, implying that modeling the effects of climate
   change with increasing temperatures and decreasing precipitation should
   be further studied. (C) 2012 Elsevier B.V. All rights reserved.}},
