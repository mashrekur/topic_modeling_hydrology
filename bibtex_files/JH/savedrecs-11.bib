@article{ ISI:000240248300012,
Author = {Szilagyi, Jonef},
Title = {{Discrete state-space approximation of the continuous
   Kalinin-Milyukov-Nash cascade of noninteger storage elements}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
Volume = {{328}},
Number = {{1-2}},
Pages = {{132-140}},
Month = {{AUG 30}},
Abstract = {{A generalization of the discrete linear cascade model (DLCM), which is a
   state-space-formulated discretized version of the continuous
   Kalinin-Milyukov-Nash Cascade, is described for noninteger number of
   uniform storage elements. The generalized model was tested against
   numerically integrated values of the Saint-Venant equations and was
   found to yield improved model accuracy in comparison with the
   traditional uniform cascade of integer number of storage elements. The
   storage coefficient of the last reservoir can be specified independently
   of that of the rest of the cascades, which makes the present model
   better suited for flow routing over reaches where the target gauging
   station is found near sudden changes in channel properties. (c) 2005
   Elsevier B.V. All rights reserved.}},
