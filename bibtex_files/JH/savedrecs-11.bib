@article{ ISI:000239870900007,
Author = {Gaudio, Roberto and De Bartolo, Samuele G. and Primavera, Leonardo and
   Gabriele, Salvatore and Veltri, Massimo},
Title = {{Lithologic control on the multifractal spectrum of river networks}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
Volume = {{327}},
Number = {{3-4}},
Pages = {{365-375}},
Month = {{AUG 20}},
Abstract = {{Previous observations on some natural river networks showed the similar
   values of the support fractal dimension and the information entropy in
   basins having similar source rocks. In the present work, lithologic
   control on river network multifractality is investigated through the
   reconstruction of the spectra belonging to two classes of different
   lithology: the plutonic-metamorphic class and the coherent sedimentary
   one. Two computational methods of the generalised fractal dimensions and
   multifractal spectra are illustrated and applied to river networks of
   the Calabria region (Southern Italy). It is shown that an increment of
   both the fractal dimensions and the Lipschitz-Holder exponents of
   singularity occurs when passing from a coherent sedimentary to a
   plutonic-metamorphic river network. The importance of this finding
   emerges from the direct link between flood hydrographs and multifractal
   spectra, as expressed in the Multifractal Instantaneous Unit Hydrograph
   (MIUH): the increment of the multifractal parameters produces an
   increment of the flood peaks. (c) 2005 Elsevier B.V. All rights
   reserved.}},
