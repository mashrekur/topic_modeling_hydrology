@article{ ISI:000239870900001,
Author = {Chen, Ching-Nuo and Tsai, Chih-Heng and Tsai, Chang-Tai},
Title = {{Simulation of sediment yield from watershed by physiographic soil
   erosion-deposition model}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
Volume = {{327}},
Number = {{3-4}},
Pages = {{293-303}},
Month = {{AUG 20}},
Abstract = {{Runoff and sediment are two of the critical factors for watershed
   management. This study developed a physiographic soil erosion-deposition
   model (PSED) by application of GIS to a physiographic rainfall-runoff
   model. The PSED model was evaluated for Chin-Shui river watershed based
   upon rainfall events occurred from 1990 to 1996. Runoff discharges and
   the concentration of suspended sediment hydrographs were estimated
   first, followed by sediment yield. PSED was found to be able to estimate
   the sediment yield within reasonable range as compared to the observed
   data. (c) 2005 Elsevier B.V. All rights reserved.}},
