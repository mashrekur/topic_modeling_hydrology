@article{ ISI:000237768100014,
Author = {Hunt, A. G. and Grant, G. E. and Gupta, V. K.},
Title = {{Spatio-temporal scaling of channels in braided streams}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
Volume = {{322}},
Number = {{1-4}},
Pages = {{192-198}},
Month = {{MAY 15}},
Note = {{International Conference on Fractals in the Hydrosciences (Hydrofractals
   03), Ascona, SWITZERLAND, AUG, 2003}},
Abstract = {{The spatio-temporal scaling relationship for individual channels in
   braided streams is shown to be identical to the spatio-temporal scaling
   associated with constant Froude number, e.g. F-r = 1. A means to derive
   this relationship is developed from a new theory of sediment transport.
   The mechanism by which the F-r = 1 condition apparently governs the
   scaling seems to derive from the sensitivity of sediment transport to
   flow fluctuations when F-r = 1. The condition F-r = 1 is also given a
   theoretical basis using arguments from surface roughness. (c) 2005
   Elsevier B.V. All rights reserved.}},
