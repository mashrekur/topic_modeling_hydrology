@article{ ISI:000237768100005,
Author = {Kundu, Prasun K. and Bell, Thomas L.},
Title = {{Space-time scaling behavior of rain statistics in a stochastic
   fractional diffusion model}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
Volume = {{322}},
