@article{ ISI:000236448000013,
Author = {Hogue, TS and Gupta, H and Sorooshian, S},
Title = {{A `User-Friendly' approach to parameter estimation in hydrologic models}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
