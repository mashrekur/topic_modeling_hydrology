@article{ ISI:000242023700004,
Author = {Burbey, Thomas J.},
Title = {{Three-dimensional deformation and strain induced by municipal pumping,
   Part 2: Numerical analysis}},
Journal = {{JOURNAL OF HYDROLOGY}},
