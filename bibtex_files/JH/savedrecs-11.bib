@article{ ISI:000236448000005,
Author = {Oudin, L and Perrin, C and Mathevet, T and Andreassian, V and Michel, C},
Title = {{Impact of biased and randomly corrupted inputs on the efficiency and the
   parameters of watershed models}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2006}},
