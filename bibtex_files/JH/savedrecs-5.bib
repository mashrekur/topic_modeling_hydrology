@article{ ISI:000080031200011,
Author = {Kite, GW and Haberlandt, U},
Title = {{Atmospheric model data for macroscale hydrology}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1999}},
Volume = {{217}},
