@article{ ISI:000085225400016,
Author = {Vermulst, JAPH and De Lange, WJ},
Title = {{An analytic-based approach for coupling models for unsaturated and
   saturated groundwater flow at different scales}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1999}},
Volume = {{226}},
Number = {{3-4}},
Pages = {{262-273}},
Month = {{DEC 31}},
Note = {{International Conference on Analytic-Based Modeling of Groundwater Flow,
   NUNSPEET, NETHERLANDS, APR, 1997}},
Abstract = {{A model for unsaturated groundwater flow describes processes in much
   smaller space and time units than a model for regional saturated
   groundwater flow. Coupling of these models requires up- and downscaling.
   For this, an approach is presented that uses one and the same Cauchy
   boundary condition for both models bur at different scales. This
   boundary condition describes the lumped interaction between groundwater
   and surface waters. A GIS-interface is used to compute the parameters in
   the boundary conditions of both models based on a single database of
   properties in a consistent way. The models run sequentially while in
   between output data of one model are scaled up or down to input for the
   other model. The continuity of flow between both models is used as a
   check of the accuracy during the coupling. The approach has been tested
   and implemented in a GIS-based interface between two Dutch groundwater
   models for national water management. (C) 1999 Elsevier Science B.V. All
   rights reserved.}},
