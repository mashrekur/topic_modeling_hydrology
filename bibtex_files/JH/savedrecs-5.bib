@article{ ISI:000077609200011,
Author = {Motyka, J and Pulido-Bosch, A and Borczak, S and Gisbert, J},
Title = {{Matrix hydrogeological properties of Devonian carbonate rocks of Olkusz
   (Southern Poland)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1998}},
Volume = {{211}},
Number = {{1-4}},
Pages = {{140-150}},
Month = {{NOV}},
Abstract = {{The limestone matrix of the Olkusz region (S Poland) does not present
   good conditions for water transmission and storage, according to a
   laboratory study of 165 core samples from 4 boreholes. The mean values
   obtained for open porosity, specific yield and hydraulic conductivity
   for limestone were 0.015, 0.00006 and 1.94 x 10(-10) m/s, respectively;
   for dolomites they were 0.011, 0.0013 and 2.77 x 10(-10) m/s
   respectively. No clear relationship between hydraulic conductivity and
   depth was identified. Nevertheless, it was established that the matrix
   can retain small quantities of water, with the dolomite matrix
   exhibiting more favourable conditions for storing and transmitting
   water. (C) 1998 Elsevier Science Ireland Ltd. All rights reserved.}},
