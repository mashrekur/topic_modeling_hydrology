@article{ ISI:000079109300007,
Author = {Xiong, LH and Guo, SL},
Title = {{A two-parameter monthly water balance model and its application}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1999}},
Volume = {{216}},
Number = {{1-2}},
Pages = {{111-123}},
Month = {{MAR 8}},
Abstract = {{A two-parameter monthly water balance model is developed. The model is
   used to simulate the runoff of seventy subcatchments in the Dongjiang,
   Ganjiang and Hanjiang Basins in the south of China. Application results
   show that the model efficiencies are high in both the calibration and
   verification periods. Comparative study of monthly water balance models
   indicates that the proposed two-parameter model as well as a
   five-parameter model performs. It is suggested that this two-parameter
   model can be easily and efficiently incorporated in the water resources
   planning program and the climate impact studies to simulate monthly
   runoff conditions in the humid and semi-humid regions. (C) 1999 Elsevier
   Science B.V. All rights reserved.}},
