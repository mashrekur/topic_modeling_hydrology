@article{ ISI:000082977000002,
Author = {Seo, DJ and Breidenbach, JP and Johnson, ER},
Title = {{Real-time estimation of mean field bias in radar rainfall data}},
Journal = {{JOURNAL OF HYDROLOGY}},
