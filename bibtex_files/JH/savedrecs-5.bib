@article{ ISI:000076861100008,
Author = {Guglielmi, Y and Mudry, J and Blavoux, B},
Title = {{Estimation of the water balance of alluvial aquifers in region of high
   isotopic contrast: an example from southeastern France}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1998}},
Volume = {{210}},
Number = {{1-4}},
Pages = {{106-115}},
Month = {{SEP}},
Abstract = {{The much contrasted orographic and climatic characters of southeastern
   France serve to establish a hydrological balance of the porous aquifers
   in the region. Comparing a regional average gradient of O-18 content
   versus elevation, which was calculated on low-water period karst waters
   content, with a -10.5 parts per thousand average delta of Alpine rivers,
   an estimate of the percentages brought to these aquifers is proposed
   under the form of an abacus. It appears that the main groundwaters of
   the Var, the Durance and the Rhone are fed by an average of 20-30\% of
   Provencal underground contribution, and 70-80\% shallow contribution
   from the Alpine rivers. These figures reveal that local supplies range
   between 25 and 10\% of the average yearly discharge flowing in the
   porous and shallow aquifers. (C) 1998 Elsevier Science B.V. All rights
   reserved.}},
