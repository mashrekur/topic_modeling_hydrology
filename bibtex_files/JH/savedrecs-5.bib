@article{ ISI:000075985800009,
Author = {Salve, R},
Title = {{Near-surface wetting of a ponded basalt surface: observations using time
   domain reflectometry}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1998}},
Volume = {{208}},
Number = {{3-4}},
Pages = {{249-261}},
Month = {{JUL 31}},
Abstract = {{The changes in dielectric constant K-a during an infiltration test on a
   fractured basalt surface in south-eastern Idaho were monitored using
   time domain reflectometry (TDR). Because changes in K-a were caused
   largely by changing amounts of water in the formation and to a small
   extent by diurnal and seasonal temperature fluctuations, TDR served as a
   useful indicator of the moisture status in the formation. Our results
   indicate that wetting and drying were spatially variable within and
   immediately beyond a 7 x 8 m(2) pond. Various locations close to the
   surface responded to fast flow, whereas others showed evidence of poor
   hydraulic communication with the surface. Data collected during a
   rainfall event, which occurred a few days after pending ceased,
   indicated that this boundary condition evoked a similar hydrologic
   response to pending. This effort suggests that TDR is a useful tool for
   detecting moisture changes in fractured media. (C) 1998 Elsevier Science
   B.V. All rights reserved.}},
