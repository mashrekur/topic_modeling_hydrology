@article{ ISI:000080031200006,
Author = {Becker, A and Braun, P},
Title = {{Disaggregation, aggregation and spatial scaling in hydrological}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1999}},
Volume = {{217}},
Number = {{3-4}},
Pages = {{239-252}},
