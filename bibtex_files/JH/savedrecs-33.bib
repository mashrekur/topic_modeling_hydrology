@article{ ISI:000423641300066,
Author = {Mueller, H. and Haberlandt, U.},
Title = {{Temporal rainfall disaggregation using a multiplicative cascade model
   for spatial application in urban hydrology}},
Journal = {{JOURNAL OF HYDROLOGY}},
