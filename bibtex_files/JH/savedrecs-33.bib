@article{ ISI:000423641300014,
Author = {Ramulifho, P. A. and Rivers-Moore, N. A. and Dallas, H. F. and Foord, S.
   H.},
Title = {{A conceptual framework towards more holistic freshwater conservation
   planning through incorporation of stream connectivity and thermal
   vulnerability}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2018}},
