@article{ ISI:000430902000060,
Author = {Oudin, Ludovic and Salavati, Bahar and Furusho-Percot, Carina and
   Ribstein, Pierre and Saadi, Mohamed},
Title = {{Hydrological impacts of urbanization at the catchment scale}},
