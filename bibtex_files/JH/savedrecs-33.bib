@article{ ISI:000423641300097,
Author = {Cugerone, Katia and De Michele, Carlo and Ghezzi, Antonio and Gianelle,
   Vorne},
Title = {{Aerosol removal due to precipitation and wind forcings in Milan urban
   area}},
