@article{ ISI:000430902000051,
Author = {Han, Li and Huang, Minsheng and Ma, Minghai and Wei, Jinbao and Hu, Wei
   and Chouhan, Seema},
Title = {{Evaluating sources and processing of nonpoint source nitrate in a small
   suburban watershed in China}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2018}},
Volume = {{559}},
Pages = {{661-668}},
Month = {{APR}},
Abstract = {{Identifying nonpoint sources of nitrate has been a long-term challenge
   in mixed land-use watershed. In the present study, we combine dual
   nitrate isotope, runoff and stream water monitoring to elucidate the
   nonpoint nitrate sources across land use, and determine the relative
   importance of biogeochemical processes for nitrate export in a small
   suburban watershed, Longhongjian watershed, China. Our study suggested
   that NH4+ fertilizer, soil NH4+, litter fall and groundwater were the
   main nitrate sources in Longhongjian Stream. There were large changes in
   nitrate sources in response to season and land use. Runoff analysis
   illustrated that the tea plantation and forest areas contributed to a
   dominated proportion of the TN export. Spatial analysis illustrated that
   NO3- concentration was high in the tea plantation and forest areas, and
   delta N-15-NO3 and delta O-18-NO3 were enriched in the step ponds.
   Temporal analysis showed high NO3- level in spring, and nitrate isotopes
   were enriched in summer. Study as well showed that the step ponds played
   an important role in mitigating nitrate pollution. Nitrification and
   plant uptake were the significant biogeochemical processes contributing
   to the nitrogen transformation, and denitrification hardly occurred in
   the stream. (C) 2017 Published by Elsevier B.V.}},
