@article{ ISI:000328592300001,
Author = {Kato, Hiroaki and Onda, Yuichi and Nanko, Kazuki and Gomi, Takashi and
   Yamanaka, Tsutomu and Kawaguchi, Shimpei},
Title = {{Effect of canopy interception on spatial variability and isotopic
   composition of throughfall in Japanese cypress plantations}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2013}},
Volume = {{504}},
Pages = {{1-11}},
Month = {{NOV 11}},
Abstract = {{We conducted a field monitoring experiment examining throughfall in
   Japanese cypress plantations located in Tochigi Prefecture, eastern
   Japan. A set of 20 tipping-bucket rain gauges and throughfall collectors
   were placed in a lattice-like distribution throughout a 10 x 10-m
   experimental plot to investigate the effect of the forest canopy on the
   spatial variability of throughfall. The isotopic composition of
   throughfall and open rainfall were analyzed and compared for each
   rainfall event. A clear relationship between throughfall rate and the
   radial distance to the nearest tree trunk was observed when the
   influence of wind was negligible; however, such a systematic pattern of
   throughfall was not observed during rainfall events that occurred during
   windy conditions. The delta O-18 and delta D values in throughfall
   varied considerably, reflecting different rainwater flow paths in the
   saturated canopy. The analysis of stable isotope ratios in the
   throughfall and its comparison with isotopic composition of rainwater
   hitting the canopy can help elucidate the interception processes in the
   forest canopy. (C) 2013 Elsevier B.V. All rights reserved.}},
