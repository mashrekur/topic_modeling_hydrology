@article{ ISI:000323019800021,
Author = {Hu, Wei and Shao, Ming An and Hou, Mei Ting and She, Dong Li and Si,
   Bing Cheng},
Title = {{Mean soil water content estimation using measurements from time stable
   locations of adjacent or distant areas}},
