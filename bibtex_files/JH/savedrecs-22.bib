@article{ ISI:000330490700007,
Author = {Badrzadeh, Honey and Sarukkalige, Ranjan and Jayawardena, A. W.},
Title = {{Impact of multi-resolution analysis of artificial intelligence models
   inputs on multi-step ahead river flow forecasting}},
