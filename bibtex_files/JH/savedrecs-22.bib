@article{ ISI:000322293800009,
Author = {Gao, Lei and Connor, Jeff and Doble, Rebecca and Ali, Riasat and
   McFarlane, Don},
Title = {{Opportunity for pen-urban Perth groundwater trade}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2013}},
