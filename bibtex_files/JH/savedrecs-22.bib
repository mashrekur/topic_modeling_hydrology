@article{ ISI:000320897500003,
Author = {Gohari, Alireza and Eslamian, Saeid and Mirchi, Ali and Abedi-Koupaei,
   Jahangir and Bavani, Alireza Massah and Madani, Kaveh},
Title = {{Water transfer as a solution to water shortage: A fix that can Backfire}},
