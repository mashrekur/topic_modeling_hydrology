@article{ ISI:000320840500008,
Author = {Zhang, ShangHong and Xia, ZhongXi and Wang, TaiWei},
Title = {{A real-time interactive simulation framework for watershed decision
   making using numerical models and virtual environment}},
Journal = {{JOURNAL OF HYDROLOGY}},
