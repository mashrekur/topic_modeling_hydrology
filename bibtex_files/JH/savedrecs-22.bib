@article{ ISI:000323688800010,
Author = {Sordo-Ward, Alvaro and Garrote, Luis and Dolores Bejarano, M. and
   Castillo, Luis G.},
Title = {{Extreme flood abatement in large dams with gate-controlled spillways}},
