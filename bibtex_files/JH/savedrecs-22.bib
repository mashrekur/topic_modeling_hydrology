@article{ ISI:000321023900016,
Author = {Spank, Uwe and Schwaerzel, Kai and Renner, Maik and Moderow, Uta and
   Bernhofer, Christian},
Title = {{Effects of measurement uncertainties of meteorological data on estimates
   of site water balance components}},
Journal = {{JOURNAL OF HYDROLOGY}},
