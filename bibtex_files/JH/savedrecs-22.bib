@article{ ISI:000324562700015,
Author = {Jiang, Zhenjiao and Mariethoz, Gregoire and Taulis, Mauricio and Cox,
   Malcolm},
Title = {{Determination of vertical hydraulic conductivity of aquitards in a
   multilayered leaky system using water-level signals in adjacent aquifers}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2013}},
Volume = {{500}},
Pages = {{170-182}},
Month = {{SEP 13}},
Abstract = {{This paper presents a methodology for determining the vertical hydraulic
   conductivity (KO of an aquitard, in a multilayered leaky system, based
   on the harmonic analysis of arbitrary water-level fluctuations in
   aquifers. As a result, K-v of the aquitard is expressed as a function of
   the phase-shift of water-level signals measured in the two adjacent
   aquifers. Based on this expression, we propose a robust method to
   calculate K-v,, by employing linear regression analysis of logarithm
   transformed frequencies and phases. The frequencies, where the K-v is
   calculated, are identified by coherence analysis. The proposed methods
   are validated by a synthetic case study and are then applied to the
   Westbourne and Birkhead aquitards, which form part of a five-layered
   leaky system in the Eromanga Basin, Australia. (C) 2013 Elsevier B.V.
   All rights reserved.}},
