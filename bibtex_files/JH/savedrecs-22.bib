@article{ ISI:000328806400018,
Author = {Niu, Jun and Sivakumar, Bellie and Chen, Ji},
Title = {{Impacts of increased CO2 on the hydrologic response over the Xijiang
   (West River) basin, South China}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2013}},
Volume = {{505}},
Pages = {{218-227}},
Month = {{NOV 15}},
Abstract = {{Increases in atmospheric carbon dioxide (CO2) concentration levels may
   affect terrestrial hydrologic processes through changes in
   evapotranspiration. This study assesses the hydrologic response to the
   elevated CO2 levels for the Xijiang (West River) basin in South China by
   applying the Variable Infiltration Capacity (VIC) model. The standard
   VIC model is modified by incorporating a mechanistic description of
   vegetation-type specific responses of both stomatal resistance and leaf
   area increase based on plant physiological studies. The dynamic
   (monthly) rises of CO2 concentration are introduced to estimate the
   historical impacts of increased CO2 over the past two decades
   (1991-2010). The cropland is identified as the most important
   contributor in the response of streamfiow increase for the region. For a
   doubled atmospheric CO2 concentration scenario (660 ppm), the streamflow
   might rise about 3\% (i.e. 21.8 mm/yr) under the baseline climatic
   conditions. Decreases in evapotranspiration and associated streamflow
   increases in response to ambient CO2 exposures are expected to
   episodically increase the frequency and severity of flood and affect
   flow-dependent aquatic biota in forested watersheds. (C) 2013 Elsevier
   B.V. All rights reserved.}},
