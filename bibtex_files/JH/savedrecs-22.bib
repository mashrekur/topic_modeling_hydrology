@article{ ISI:000317537400025,
Author = {Lhomme, Jean Paul and Montes, Carlo and Jacob, Frederic and Prevot,
   Laurent},
Title = {{Evaporation from multi-component canopies: Generalized formulations}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2013}},
Volume = {{486}},
Pages = {{315-320}},
Month = {{APR 12}},
Abstract = {{A general formulation is derived for expressing the evaporation from
   multi-component canopies representing mixed-plant communities. It
   generalizes to n components the formulations introduced by Lhomme et al.
   (Bound-Layer Meteorol. 144, 243-262, 2012) for two and three components
   models. The new formulation is an alternative and simpler form of the
   equations previously developed by Wallace (Q. J. R. Meteorol. Soc. 123,
   1885-1905, 1997). It is sounder, in the sense that: (i) it accounts for
   the impact of stomatal characteristics (hypostomatous or amphistomatous)
   on evaporation expression; (ii) it allows one to eliminate the confusing
   ``Penman-Monteith{''} type terms of Wallace's formulation (which do no
   not represent the individual component evaporations). Additionally, the
   steps of the mathematical demonstration are clearly specified, which was
   not the case for Wallace's formulation. In this way, a general
