@article{ ISI:000327903400018,
Author = {Homa, Elizabeth S. and Brown, Casey and McGarigal, Kevin and Compton,
   Bradley W. and Jackson, Scott D.},
Title = {{Estimating hydrologic alteration from basin characteristics in
   Massachusetts}},
