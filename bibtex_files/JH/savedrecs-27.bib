@article{ ISI:000348879900021,
Author = {Ojha, Richa and Prakash, Arun and Corradini, Corrado and Morbidelli,
   Renato and Govindaraju, Rao S.},
Title = {{Temporal moment analysis for stochastic-advective vertical solute
   transport in heterogeneous unsaturated soils}},
