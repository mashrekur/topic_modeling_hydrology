@article{ ISI:000355885600064,
Author = {Bradford, Scott A. and Headd, Brendan and Arye, Gilboa and Simunek, Jiri},
Title = {{Transport of E-coli D21g with runoff water under different solution
   chemistry conditions and surface slopes}},
