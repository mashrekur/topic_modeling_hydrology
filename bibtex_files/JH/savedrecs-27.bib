@article{ ISI:000354503300032,
Author = {Liu, Haifei and Wang, Hongda and Liu, Shu and Hu, Changwei and Ding, Yu
   and Zhang, Jie},
Title = {{Lattice Boltzmann method for the Saint-Venant equations}},
