@article{ ISI:000354503300025,
Author = {Schiperski, Ferry and Zirlewagen, Johannes and Hillebrand, Olav and
   Licha, Tobias and Scheytt, Traugott},
Title = {{Preliminary results on the dynamics of particles and their size
   distribution at a karst spring during a snowmelt event}},
