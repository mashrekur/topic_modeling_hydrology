@article{ ISI:000355885600016,
Author = {Oikonomidis, D. and Dimogianni, S. and Kazakis, N. and Voudouris, K.},
Title = {{A GIS/Remote Sensing-based methodology for groundwater potentiality
   assessment in Tirnavos area, Greece}},
