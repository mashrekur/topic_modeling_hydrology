@article{ ISI:000354503300043,
Author = {Shi, Liangsheng and Song, Xuehang and Tong, Juxiu and Zhu, Yan and
   Zhang, Qiuru},
Title = {{Impacts of different types of measurements on estimating unsaturated
   flow parameters}},
