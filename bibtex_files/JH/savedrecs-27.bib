@article{ ISI:000354503300053,
Author = {Wang, Weiguang and Xing, Wanqiu and Shao, Quanxi},
Title = {{How large are uncertainties in future projection of reference
   evapotranspiration through different approaches?}},
Journal = {{JOURNAL OF HYDROLOGY}},
