
@article{ ISI:000262789900013,
Author = {Wondzell, Steven M. and LaNier, Justin and Haggerty, Roy},
Title = {{Evaluation of alternative groundwater flow models for simulating
   hyporheic exchange in a small mountain stream}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
