@article{ ISI:000300759100018,
Author = {Suriya, S. and Mudgal, B. V.},
Title = {{Impact of urbanization on flooding: The Thirusoolam sub watershed - A
   case study}},
Journal = {{JOURNAL OF HYDROLOGY}},
