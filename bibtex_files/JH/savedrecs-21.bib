@article{ ISI:000300759100004,
Author = {Georgakakos, A. P. and Yao, H. and Kistenmacher, M. and Georgakakos, K.
   P. and Graham, N. E. and Cheng, F-Y and Spencer, C. and Shamir, E.},
Title = {{Value of adaptive water resources management in Northern California
   under climatic variability and change: Reservoir management}},
Journal = {{JOURNAL OF HYDROLOGY}},
