@article{ ISI:000300759100014,
Author = {Xu, Xu and Huang, Guanhua and Zhan, Hongbin and Qu, Zhongyi and Huang,
   Quanzhong},
Title = {{Integration of SWAP and MODFLOW-2000 for modeling groundwater dynamics
   in shallow water table areas}},
