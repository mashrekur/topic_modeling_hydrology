@article{ ISI:000071296200010,
Author = {Ridolfi, L and Sordo, S},
Title = {{Hydrodynamic dispersion in an artesian aquifer during flow to a
   partially penetrating well}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1997}},
Volume = {{201}},
Number = {{1-4}},
Pages = {{183-210}},
Month = {{DEC 20}},
Abstract = {{This study deals with the dispersive behaviour of a passive pollutant in
   an artesian aquifer, in which the three-dimensional flow field caused by
   a partially penetrating well is superimposed on a basic uniform
   groundwater flow.
   In order to describe the dispersion phenomenon, the convective aspect is
   analysed first. An original semi-analytical solution is proposed,
   involving a simplifying assumption entirely plausible from an
   engineering point of view, which enables us to obtain simple-to-use
   mathematical expressions of the potential and the corresponding flow
   field. The analyses developed to verify the correctness of the proposed
   solution are also presented, and diagrams are obtained which describe
   the principal dimensions of the convective catchment section of the well
   in relation to the geometric and hydraulic quantities governing the
   phenomenon.
   Subsequently, the dispersive aspect is described by means of the random
   walk method. This method provides us with a three-dimensional
   mathematical scheme, easily implemented on a computer, to analyse the
   particular dispersive phenomenon under examination.
   Two examples are also developed which illustrate the potential studies
   realisable with this scheme, in this case the description of the time
   evolution of a cloud or a pollutant plume and the influence of the
   position of the pollutant source in relation to the convective flow. (C)
   1997 Elsevier Science B.V.}},
