@article{ ISI:A1997XG47100005,
Author = {Dolman, AJ and Blyth, EM},
Title = {{Patch scale aggregation of heterogeneous land surface cover for
   mesoscale meteorological models}},
Journal = {{JOURNAL OF HYDROLOGY}},
