@article{ ISI:A1997XP62800016,
Author = {Makhuvha, T and Pegram, G and Sparks, R and Zucchini, W},
Title = {{Patching rainfall data using regression methods .1. Best subset
   selection, EM and pseudo-EM methods: theory}},
