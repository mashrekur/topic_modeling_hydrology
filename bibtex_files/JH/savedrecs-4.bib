@article{ ISI:A1997XG47100007,
Author = {Noilhan, J and Lacarrere, P and Dolman, AJ and Blyth, EM},
Title = {{Defining area-average parameters in meteorological models for land
   surfaces with mesoscale heterogeneity}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1997}},
