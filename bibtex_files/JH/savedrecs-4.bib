@article{ ISI:A1997XM20300011,
Author = {Haktanir, T},
Title = {{Self-determined probability-weighted moments method and its application
   to various distributions}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1997}},
Volume = {{194}},
Number = {{1-4}},
Pages = {{180-200}},
Month = {{JUL}},
Abstract = {{It is proposed that the non-exceedence probability of an element in a
   sample series needed for estimation of a probability-weighted moment
   (PWM) be computed using the distribution itself instead of a plotting
   position formula (PPF). Numerical values for the parameters of the
   distribution can be determined in such a way that they will satisfy the
   formulae relating the parameters to the PWMs when the PWMs are initially
   estimated using the probabilities computed by the distribution with
   those parameters. This approach eliminates usage of any PPF and enables
   a more rigorous estimation of the PWMs in the method of PWMs.
   Formulation and solution algorithms of the proposed method for some
   distributions used in flood frequency analysis are presented with
   numerical examples.}},
