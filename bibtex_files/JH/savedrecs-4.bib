@article{ ISI:A1997XP62800005,
Author = {Brandsma, T and Buishand, TA},
Title = {{Statistical linkage of daily precipitation in Switzerland to atmospheric
   circulation and temperature}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1997}},
Volume = {{198}},
Number = {{1-4}},
Pages = {{98-123}},
Month = {{NOV}},
Abstract = {{The daily precipitation of Bern, Neuchatel and Payerne in Switzerland is
   statistically linked to atmospheric circulation and temperature. For ail
   three stations, there is a marked increase of the mean precipitation
   amount with increasing temperature for wet days with temperatures
   between -5 and 20 degrees C. The amount of precipitation is also
   controlled by the direction and strength of the atmospheric flow. To
   lake these dependencies into account, the daily precipitation amounts
   are modelled as a function of temperature and strength of the flow for
   three categories of flow direction. Both parametric and nonparametric
   techniques from the statistical literature on generalized linear models
   are applied, The nonparametric technique is a helpful tool for the
   selection and evaluation of parametric models. The non-linear effects of
   temperature and strength of the flow on the amount of precipitation are
   described by natural cubic splines and piecewise linear functions. The
   use of the modelled relationships for climate-change scenario production
   is discussed. (C) 1997 Elsevier Science B.V.}},
