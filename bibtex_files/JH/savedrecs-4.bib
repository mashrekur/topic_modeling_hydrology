@article{ ISI:A1997XM70600014,
Author = {Lee, KK and Leap, DI},
Title = {{Simulation of a free-surface and seepage face using boundary-fitted
   coordinate system method}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1997}},
Volume = {{196}},
Number = {{1-4}},
Pages = {{297-309}},
Month = {{SEP}},
Abstract = {{The boundary-fitted coordinate (BFC) system method is applied to
   simulate steady groundwater seepage with a free-surface and seepage face
   using the finite-difference method. The BFC system method eliminates the
   difficulty of fitting finite-difference grids to a changeable
   free-surface which is not known a priori but will be obtained as part,
   of a solution, Also, grid generation with this approach is simpler than
   with the finite-element method. At each iterative sweep, the changeable
   free-surface becomes a part of the boundary-fitted grid lines, making
   boundary condition implementation easy and accurate, An example problem
   demonstrating the simulation procedure and numerical results compares
   very well with the analytical solution. (C) 1997 Elsevier Science B.V.}},
