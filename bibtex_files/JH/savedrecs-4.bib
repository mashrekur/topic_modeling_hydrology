@article{ ISI:A1997XA78800050,
Author = {Dolman, AJ and Gash, JHC and Goutorbe, JP and Kerr, Y and Lebel, T and
   Prince, SD and Stricker, JNM},
Title = {{The role of the land surface in Sahelian climate: HAPEX-Sahel results
   and future research needs}},
