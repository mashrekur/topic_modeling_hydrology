@article{ ISI:A1997WC90700001,
Author = {Xiang, JN and Chen, L},
Title = {{Evaluation of solutions for deep constant-head boreholes}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1997}},
Volume = {{187}},
Number = {{3-4}},
Pages = {{265-281}},
Month = {{JAN}},
Abstract = {{Estimation of the field saturated hydraulic conductivity (K-fs) is
   essential for ground-water management and for numerical modeling of
   ground-water flow and contaminant transport. The constant-head borehole
   test can be used to measure K-fs of deep soils. For a deep borehole of
   H/alpha = 10-500, where H is the water height in the borehole and alpha
   is the radius, the accuracy of the estimated K-fs varies depending on
   the shape factor associated with H/alpha. Many analytical solutions have
   been obtained on the basis of approximate boundary conditions available
   to evaluate the shape factor. To find the best solution that provides an
   accurate shape factor, the accuracy of the different solutions was
   evaluated using a series of numerical simulations that satisfy the
   pressure boundary condition of the borehole test. Our comparison, on the
   basis of the analytical and numerical solutions, shows that for
   homogeneous soils the K-fs calculated by Xiang's solution (Xiang, 1993,
   Eos Trans. Am. Geophys. Union, 74(43): 285) and that by Reynolds et al.
   (1983, Soil Sci, 136: 250-268) are almost identical to the numerical
   results. Xiang's solution, however, was found to be more accurate for
   evaluating the flow distribution than was that of Reynolds et al.}},
