
@article{ ISI:000272862000001,
Author = {Getirana, Augusto C. V. and Bonnet, Marie-Paule and Calmant, Stephane
   and Roux, Emmanuel and Rotunno Filho, Otto C. and Mansur, Webe J.},
Title = {{Hydrological monitoring of poorly gauged basins based on rainfall-runoff
   modeling and spatial altimetry}},
