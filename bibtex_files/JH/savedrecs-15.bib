@article{ ISI:000262789900005,
Author = {Feitosa-Felizzola, J. and Chiron, S.},
Title = {{Occurrence and distribution of selected antibiotics in a small
   Mediterranean stream (Arc River, Southern France)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
Volume = {{364}},
Number = {{1-2}},
Pages = {{50-57}},
Month = {{JAN 15}},
Abstract = {{The occurrence and distribution of nine selected antibiotics were
   investigated in a small Mediterranean stream (Arc River, Southern
   France) during the winter and the spring season. Carbamazepine and
   clarithromycin were the most frequently detected compounds in water.
   Ctarithromycin was determined at high mu g/L Levels in river water
   during the winter season due to the lack of dilution by background water
   while clarithromycin and azithromycin were ubiquitous in sediment. These
   contamination levels were at one order of magnitude higher than those
   reported in large European rivers. It could be shown that clarithromycin
   can be regarded as a priority antibiotic to be monitored in river waters
   mainly because this chemical showed high persistence in water and
   underwent stow dissipation processes in sediment. (C) 2008 Elsevier B.V.
   All rights reserved.}},
