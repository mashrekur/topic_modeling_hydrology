@article{ ISI:000269851000018,
Author = {Lopez-Moreno, J. I. and Goyette, S. and Beniston, M.},
Title = {{Impact of climate change on snowpack in the Pyrenees: Horizontal spatial
   variability and vertical gradients}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
