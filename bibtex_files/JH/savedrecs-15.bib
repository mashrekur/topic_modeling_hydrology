@article{ ISI:000265424700004,
Author = {Gurmessa, T. Kebede and Bardossy, A.},
Title = {{A principal component regression approach to simulate the bed-evolution
   of reservoirs}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
Volume = {{368}},
Number = {{1-4}},
Pages = {{30-41}},
Month = {{APR 30}},
Abstract = {{Long-term simulation of reservoir sedimentation suffers from process
   complexity, lack of data, as well as high computational cost. This work
   presents an efficient data-driven modeling approach to simulate the
   spatio-temporal dynamics of bed-evolution reservoirs using principal
   components regression. The daily bed-evolution of a validated numerical
   model was used as an input. The first four principal components
   contributed to some 90\% of the total variance of bed-evolution.
   Multiple linear regression between the eigenvectors of the first four
   principal components with the inflow discharge, suspended sediment
   concentration, and differential discharge was able to reconstruct the
   spatio-temporal bed-evolution. Predictions with similar initial
   morphological condition performed reasonably. The work is a step forward
   to advance the assimilation of numerical and data-driven approaches in
   modeling long-term sedimentation of reservoirs. (c) 2009 Elsevier B.V.
   All rights reserved.}},
