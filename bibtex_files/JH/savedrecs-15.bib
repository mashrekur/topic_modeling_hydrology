@article{ ISI:000272367900003,
Author = {Long, Andrew J. and Gilcrease, Patrick C.},
Title = {{A one-dimensional heat-transport model for conduit flow in karst
   aquifers}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
Volume = {{378}},
Number = {{3-4}},
