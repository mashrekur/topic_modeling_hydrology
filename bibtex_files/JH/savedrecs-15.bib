@article{ ISI:000263575600015,
Author = {Hsu, Bing-Mu and Chen, Chien-Hsien and Wan, Min-Tao and Chang, Po-Jen
   and Fan, Cheng-Wei},
Title = {{Detection and identification of enteroviruses from various drinking
   water sources in Taiwan}},
