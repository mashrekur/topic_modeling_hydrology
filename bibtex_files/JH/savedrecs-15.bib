@article{ ISI:000266644800014,
Author = {Li, Hongxia and Zhang, Yongqiang and Chiew, Francis H. S. and Xu, Shiguo},
Title = {{Predicting runoff in ungauged catchments by using Xinanjiang model with
   MODIS leaf area index}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
Volume = {{370}},
Number = {{1-4}},
Pages = {{155-162}},
Month = {{MAY 30}},
Abstract = {{Vegetation processes are seldom considered in lumped conceptual
   rainfall-runoff (RR) models although they have significant impacts on
   runoff via the control of evapotranspiration. This paper incorporates
   the remotely-sensed the moderate resolution imaging spectrometer mounted
   on the polar-orbiting terra satellite-leaf area index (MODIS-LAI) data
   into Xinanjiang rainfall-runoff model and assesses the model performance
   on 210 catchments in south-east Australia. The results show that the
   inclusion of LAI data improves both the model calibration results as
   well as the daily runoff prediction in ungauged catchments. It is likely
   that more significant improvements to the model structure to integrate
   the remotely-sensed vegetation and other data can further reduce the
   uncertainty in runoff prediction in ungauged catchments. Crown Copyright
   (c) 2009 Published by Elsevier B.V. All rights reserved.}},
