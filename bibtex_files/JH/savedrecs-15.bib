@article{ ISI:000267334800010,
Author = {Aksoy, Hafzullah and Kurt, Ilker and Eris, Ebru},
Title = {{Filtered smoothed minima baseflow separation method}},
Journal = {{JOURNAL OF HYDROLOGY}},
