@article{ ISI:000269851000013,
Author = {Tong, Xiao-Juan and Li, Jun and Yu, Qiang and Qin, Zhong},
Title = {{Ecosystem water use efficiency in an irrigated cropland in the North
   China Plain}},
Journal = {{JOURNAL OF HYDROLOGY}},
