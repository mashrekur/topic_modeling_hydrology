@article{ ISI:000268074900006,
Author = {Snelder, Ton H. and Lamouroux, Nicolas and Leathwick, John R. and Pella,
   Herve and Sauquet, Eric and Shankar, Ude},
Title = {{Predictive mapping of the natural flow regimes of France}},
Journal = {{JOURNAL OF HYDROLOGY}},
