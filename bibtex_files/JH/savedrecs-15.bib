@article{ ISI:000264503500009,
Author = {Parisopouios, G. A. and Malakou, M. and Giamouri, M.},
Title = {{Evaluation of lake level control using objective indicators: The case of
   Micro Prespa}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2009}},
Volume = {{367}},
Number = {{1-2}},
Pages = {{86-92}},
Month = {{MAR 30}},
Abstract = {{Three performance indicators allowing an objective post-evaluation of
   implementation of lake Micro Prespa level management provisions are
   presented. Indicator 11 measures the success in keeping level within
   predetermined elevation amplitude by plotting the level time series and
   adding all outlying surfaces. Indicator 12 quantifies the degree of
   convergence of lake level to a goal elevation by taking the square root
   of the sum of squares of all daily deviations from goals. Indicator 13
   quantifies the degree of establishment favorable fish spawning
   conditions by counting all potentially productive days for a water
   temperature depended time period. All indicators developed can also be
   applied to other lakes under similar level management plans. (C) 2009
   Elsevier B.V. All rights reserved.}},
