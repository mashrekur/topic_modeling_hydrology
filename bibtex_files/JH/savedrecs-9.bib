@article{ ISI:000221147300001,
Author = {Andreassian, V},
Title = {{Waters and forests: from historical controversy to scientific debate}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2004}},
Volume = {{291}},
Number = {{1-2}},
Pages = {{1-27}},
Month = {{MAY 31}},
Abstract = {{This article presents an historical perspective of the controversy
   concerning the hydrological impact of forests, and shows how a mostly
   romantic and emotional confrontation finally evolved into a scientific
   debate. We first analyze the historical evolution of ideas, starting
   with the views of Pliny the Elder in the first century AD and ending
   with the debate on the `Eaux et Forets' in France in the 19th century.
   Then, we give an up-to-date overview of the paired-watershed experiments
   conducted throughout 20th century, and identify some research issues
   that should help forest hydrology science to move forward in the 21st
   century. (C) 2004 Elsevier B.V. All rights reserved.}},
