@article{ ISI:000220936500006,
Author = {Panno, SV and Kelly, WR},
Title = {{Nitrate and herbicide loading in two groundwater basins of Illinois'
   sinkhole plain}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2004}},
Volume = {{290}},
Number = {{3-4}},
Pages = {{229-242}},
Month = {{MAY 25}},
Abstract = {{This investigation was designed to estimate the mass loading of nitrate
   (NO3-) and herbicides in spring water discharging from groundwater
   basins in an agriculturally dominated, mantled karst terrain. The
   loading was normalized to land use and NO3- and herbicide losses were
   compared to estimated losses in other agricultural areas of the
   Midwestern USA. Our study area consisted of two large karst springs that
   drain two adjoining groundwater basins (total area of 37.7 km(2)) in
   southwestern Illinois' sinkhole plain. USA. The springs and stream that
   they form were monitored for almost 2 years. Nitrate-nitrogen (NO3-N)
   concentrations at three monitoring sites were almost always above the
   background concentration (1.9 mg/l). NO3-N concentrations at the two
   springs ranged from 1.08 to 6.08 with a median concentration of 3.61
   mg/l. Atrazine and alachlor concentrations ranged from <0.01 to 34
   \&mu;g/l and <0.01 to 0.98 mug/l, respectively, with median
   concentrations of 0.48 and 0.12 mug/l. respectively. Approximately
   100,000 kg/yr of NO3-N, 39 kg/yr of atrazine, and 2.8 kg/yr of alachlor
   were discharged from the two springs. Slightly more than half of the
   discharged NO3- came from background sources and most of the 3 remainder
   probably came from fertilizer. This represents a 21-31\% loss of
   fertilizer N from the groundwater basins. The pesticide losses were
   3.8-5.8\% of the applied atrazine, and 0.05-0.08\% of the applied
   alachlor. The loss of atrazine adsorbed to the suspended solid fraction
   was about 2 k-/yr, only about 5\% of the total mass of atrazine
   discharged from the springs. (C) 2004, Elsevier B.V. All rights
   reserved.}},
