@article{ ISI:000221556400003,
Author = {Izbicki, JA and Stamos, CL and Nishikawa, T and Martin, P},
Title = {{Comparison of ground-water flow model particle-tracking results and
   isotopic data in the Mojave River ground-water basin, southern
   California, USA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2004}},
Volume = {{292}},
Number = {{1-4}},
Pages = {{30-47}},
Month = {{JUN 15}},
Abstract = {{Flow-path and time-of-travel results for the Mojave River ground-water
   basin, southern California, calculated using the ground-water flow model
   MODFLOW and particle-tracking model MODPATH were similar to flow path
   and time-of-travel interpretations derived from delta-deuterium and
   carbon-14 data. Model and isotopic data both show short flow paths and
   young ground-water ages throughout the floodplain aquifer along most the
   Mojave River. Longer flow paths and older ground-water ages as great as
   10,000 years before present were measured and simulated in the
   floodplain aquifer near the Mojave Valley. Model and isotopic data also
   show movement of water between the floodplain and regional aquifer and
   subsequent discharge of water from the river to dry lakes in some areas.
   It was not possible to simulate the isotopic composition of ground-water
   in the regional aquifer away from the front of the San Gabriel and San
   Bernardino Mountains-because recharge in these areas does not occur
   under the present-day climatic conditions used for calibration of the
   model. Published by Elsevier B.V.}},
