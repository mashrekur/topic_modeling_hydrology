@article{ ISI:000220324000004,
Author = {Orlandi, A and Ortolani, A and Meneguzzo, F and Levizzani, V and
   Torricella, F and Turk, FJ},
Title = {{Rainfall assimilation in RAMS by means of the Kuo parameterisation
   inversion: method and preliminary results}},
