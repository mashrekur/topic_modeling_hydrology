@article{ ISI:000221147300008,
Author = {Shu, C and Burn, DH},
Title = {{Homogeneous pooling group delineation for flood frequency analysis using
   a fuzzy expert system with genetic enhancement}},
Journal = {{JOURNAL OF HYDROLOGY}},
