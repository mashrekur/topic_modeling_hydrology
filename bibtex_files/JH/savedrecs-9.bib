@article{ ISI:000221556400002,
Author = {Manglik, A and Rai, SN and Singh, VS},
Title = {{Modelling of aquifer response to time varying recharge and pumping from
   multiple basins and wells}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2004}},
Volume = {{292}},
Number = {{1-4}},
Pages = {{23-29}},
Month = {{JUN 15}},
Abstract = {{This paper deals with the development of an analytical solution of
   two-dimensional linearized Boussinesq equation to predict water table
   variation in response to time varying recharge and pumping from any
   number of basins and wells. The rates of time varying recharge and
   pumping are approximated by series of line elements of different lengths
   and slopes depending on the nature of variation of the recharge and/or
   pumping rates. Advantage of this approach is that any complex nature of
   recharge and pumping rates for any number of basins and wells and for
   any number of these operations can be approximated with more accuracy.
   Application of the solution in the prediction of water table variation
   is demonstrated with the help of an example AM problem. Also, the
   results of the analytical solution are compared with those obtained from
   MODFLOW. Both results are found to be in good agreement. (C) 2004
   Elsevier B.V. All rights reserved.}},
