@article{ ISI:000247196600003,
Author = {McVicar, Tim R. and Van Niel, Thomas G. and Li, LingTao and Hutchinson,
   Michael F. and Mu, XingMin and Liu, ZhiHong},
Title = {{Spatially distributing monthly reference evapotranspiration and pan
   evaporation considering topographic influences}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2007}},
Volume = {{338}},
Number = {{3-4}},
