@article{ ISI:000245530400012,
Author = {Li, Zhongwei and Zhang, You-Kuan},
Title = {{Quantifying fractal dynamics of groundwater systems with detrended
   fluctuation analysis}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2007}},
Volume = {{336}},
Number = {{1-2}},
Pages = {{139-146}},
Month = {{MAR 30}},
Abstract = {{Groundwater system is a complex dynamic system characterized by
   nonstationary input (recharge), output (base flow), and response
   (groundwater Levels). In this paper, scaling properties of the observed
   nonstationary groundwater levels, streamflow, and base flow are
   quantified with detrended fluctuation analysis (DFA) method. With the
   results obtained with DFA, we further verify that fluctuations of
   groundwater levels may indeed be fractional Brownian motion and that
   fractal scaling may be universal since fluctuations of groundwater
   Levels are affected by various processes over multiple time scales. We
   also found that there may exist at least two crossovers in fractal.
   scaling of groundwater level fluctuations: one between a few days and
   10.0 days and the other between a few months to one year. Variations of
   groundwater levels are persistent (scaling exponent alpha > 1.50) at
   small time scales (less than a few to 10.0 days), anti-persistent (1.00
   > alpha > 1.50) at intermediate time scales (between 10.0 days and a few
   months), and 1/f noise (alpha congruent to 1.00) at Large time scales
   (more than a few months). In general, the values of alpha or temporal
   correlations decrease as time scale increases, but increase from
   rainfall to streamflow, to groundwater level, and to base flow due to
   the dampening effect by the land surface, soils, and aquifers. (c) 2007
   Elsevier B.V. All rights reserved.}},
