@article{ ISI:000248166500010,
Author = {Song, Zhiyao and Li, Ling and Kong, Jun and Zhang, Honggui},
Title = {{A new analytical solution of tidal water table fluctuations in a coastal
   unconfined aquifer}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2007}},
Volume = {{340}},
Number = {{3-4}},
Pages = {{256-260}},
Month = {{JUL 15}},
Abstract = {{This paper presents a new perturbation solution of the non-linear
   Boussinesq equation for one-dimensional tidal groundwater-flow in a
   coastal unconfined aquifer. Built upon the work of Parlange et at.
   {[}Parlange, J.-Y., Stagnitti, F., Starr, J. L., Braddock, R. D., 1984.
   Free-surface flow in porous media and periodic solution of the
   shallow-flow approximation, J. Hydrol., 70, 251-263], the solution
   adopts a new perturbation parameter that is by definition less than
   unit, and thus is applicable to a wider range of physical conditions
   within the constraint of the Boussinesq approximation. This approach
   avoids a secular term in the third-order perturbation equation of
   Parlange et at. (1984), enabling the derivation of the third- and
   higher-order solutions. In comparison with a numerical ({''}exact{''})
   solution, the new perturbation solution is shown to be slightly more
   accurate than that of Parlange et at. (1984) with the second-order
   approximation. The obtained third-order solution exhibits considerable
   improvement in accuracy. In relatively simple analytical forms, the
   present perturbation solution will help to understand better the
   non-linear characteristics of tidal water table fluctuations as modeled
   by the non-linear Boussinesq equation in coastal unconfined aquifers.
   (C) 2007 Elsevier B.V. All rights reserved.}},
