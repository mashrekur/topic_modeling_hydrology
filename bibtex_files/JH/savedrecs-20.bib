@article{ ISI:000308060100009,
Author = {Chen, Jui-Sheng and Liu, Chen-Wuing and Liang, Ching-Ping and Lai,
   Keng-Hsin},
Title = {{Generalized analytical solutions to sequentially coupled multi-species
   advective-dispersive transport equations in a finite domain subject to
   an arbitrary time-dependent source boundary condition}},
