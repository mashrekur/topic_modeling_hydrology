@article{ ISI:000310415200022,
Author = {Hong, Jian-Hao and Goyal, Manish Kumar and Chiew, Yee-Meng and Chua,
   Lloyd H. C.},
Title = {{Predicting time-dependent pier scour depth with support vector
   regression}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2012}},
Volume = {{468}},
Pages = {{241-248}},
Month = {{OCT 25}},
Abstract = {{The temporal variation of local pier scour depth is very complex,
   especially for cases where the bed comprises a sediment mixture. Many
   semi-empirical models have been proposed to predict the time-dependent
   local pier scour depth. In this paper, an alternative approach, the
   support vector regression method (SVR) is used to estimate the temporal
   variation of pier-scour depth with non-uniform sediments under
   clear-water conditions. Based on dimensional analyses, the temporal
   variation of scour depth was modeled as a function of seven
   dimensionless input parameters, namely flow shallowness (y/D-p),
   sediment coarseness (D-p/d(50)), densimetric Froude number (F-d), the
   difference between the actual and critical densimetric Froude number
   (F-d - F-d beta), geometric standard deviation of the sediment particle
   size distribution (sigma(g)), pier Froude number (U/root gD(p))and one
   of the following three dimensionless time scales (T-1 = t/t(R1), T-2 =
   t/t(R2) and T-3 = t/t(R3)). The SVR model not only estimates the
   time-dependent scour depth more accurately than conventional regression
   models, but also provides results that are consistent with the physics
   of the scouring process. (C) 2012 Elsevier B.V. All rights reserved.}},
