@article{ ISI:000309783700030,
Author = {Wagner, Paul D. and Fiener, Peter and Wilken, Florian and Kumar, Shamita
   and Schneider, Karl},
Title = {{Comparison and evaluation of spatial interpolation schemes for daily
   rainfall in data scarce regions}},
