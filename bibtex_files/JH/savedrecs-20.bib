@article{ ISI:000310415200016,
Author = {Wong, C. I. and Mahler, B. J. and Musgrove, M. and Banner, J. L.},
Title = {{Changes in sources and storage in a karst aquifer during a transition
   from drought to wet conditions}},
Journal = {{JOURNAL OF HYDROLOGY}},
