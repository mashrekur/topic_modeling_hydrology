@article{ ISI:000280024700022,
Author = {Hsu, Ming-Hsi and Lin, Shu-Horng and Fu, Jin-Cheng and Chung, Shih-Feng
   and Chen, Albert S.},
Title = {{Longitudinal stage profiles forecasting in rivers for flash floods}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2010}},
Volume = {{388}},
Number = {{3-4}},
Pages = {{426-437}},
Month = {{JUL 15}},
Abstract = {{A flash flood routing model with artificial neural networks predictions
   was developed for stage profiles forecasting. The artificial neural
   network models were used to predict the 1-3 h lead time river stages at
   gauge stations along a river. The predictions were taken as interior
   boundaries in the flash flood routing model for the forecast of
   longitudinal stage profiles, including the un-gauged sites of a whole
   river. The flash flood routing model was based on the dynamic wave
   equations with discretization processes of the four-point finite
   difference method. Five typhoon events were applied to calibrate the
   rainfall-stage model and the other three events were simulated to verify
   the model's capability. The results revealed that the flash flood river
   routing model conjunction with artificial neural networks can provide
   accurate river stages for flood forecasting. (C) 2010 Elsevier B.V. All
   rights reserved.}},
