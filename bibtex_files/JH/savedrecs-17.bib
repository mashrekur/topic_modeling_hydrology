@article{ ISI:000280976600006,
Author = {Abebe, Nibret A. and Ogden, Fred L. and Pradhan, Nawa R.},
Title = {{Sensitivity and uncertainty analysis of the conceptual HBV
   rainfall-runoff model: Implications for parameter estimation}},
Journal = {{JOURNAL OF HYDROLOGY}},
