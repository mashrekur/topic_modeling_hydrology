@article{ ISI:000279568100006,
Author = {Chang, Fi-John and Kao, Li-shan and Kuo, Yi-Ming and Liu, Chen-Wuing},
Title = {{Artificial neural networks for estimating regional arsenic
   concentrations in a blackfoot disease area in Taiwan}},
Journal = {{JOURNAL OF HYDROLOGY}},
