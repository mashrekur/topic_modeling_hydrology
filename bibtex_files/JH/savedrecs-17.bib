@article{ ISI:000274841000005,
Author = {Janetti, Emanuela Bianchi and Riva, Monica and Straface, Salvatore and
   Guadagnini, Alberto},
Title = {{Stochastic characterization of the Montalto Uffugo research site (Italy)
   by geostatistical inversion of moment equations of groundwater flow}},
