@article{ ISI:000280976600016,
Author = {Drake, Jennifer and Bradford, Andrea and Joy, Doug},
Title = {{Application of HEC-RAS 4.0 temperature model to estimate groundwater
   contributions to Swan Creek, Ontario, Canada}},
Journal = {{JOURNAL OF HYDROLOGY}},
