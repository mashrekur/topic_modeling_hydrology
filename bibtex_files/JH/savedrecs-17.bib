@article{ ISI:000276532700016,
Author = {Ruediger, Christoph and Western, Andrew W. and Walker, Jeffrey P. and
   Smith, Adam B. and Kalma, Jetse D. and Willgoose, Garry R.},
Title = {{Towards a general equation for frequency domain reflectometers}},
