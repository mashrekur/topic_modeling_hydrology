@article{ ISI:000282395600002,
Author = {Bertrand, Guillaume and Celle-Jeanton, Helene and Huneau, Frederic and
   Loock, Sebastien and Renac, Christophe},
Title = {{Identification of different groundwater flowpaths within volcanic
   aquifers using natural tracers for the evaluation of the influence of
   lava flows morphology (Argnat basin, Chaine des Puys, France)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2010}},
Volume = {{391}},
Number = {{3-4}},
Pages = {{223-234}},
Month = {{SEP 24}},
Abstract = {{Hydrochemical and stable isotopic (H-2, O-18) data were used to
   characterize the groundwater flow and major chemical features within a
   complex fractured volcanic aquifer system, the Argnat basin, which is
   located in the Chaine des Puys (French Massif Central). From 10 sampling
   points, the study of the transfer into the saturated zone from upstream
   to downstream, given the geological context and topography, allows to
   estimate the role of supply from high and low altitudes to the recharge
   processes. This work shows the existence of different types of supply
   between pahoehoe and a'a flows. Therefore, the morphology of volcanic
   flows impacts the chemical and isotopic signatures of groundwater,
   enhancing or reducing the influence of the unsaturated zone on the
   pathways of infiltrated water. Pahoehoe flows imply horizontal water
   flows of low discharge at the top of the lava whereas a'a flows seems to
   be much more heterogeneous and locally able to promote the existence of
   perched water bodies and further vertical circulations. Taking into
   account these two types of behaviour, a conceptual scheme of the
   functioning of this heterogeneous environment is proposed, which will
   help towards a sustainable management of volcanic aquifers in relation
   with the European Union Groundwater Directive (2006/118/CE) (2006). (c)
   2010 Elsevier B.V. All rights reserved.}},
