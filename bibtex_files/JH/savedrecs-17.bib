@article{ ISI:000284814900016,
Author = {Viglione, Alberto and Chirico, Giovanni Battista and Woods, Ross and
   Bloschl, Gunter},
Title = {{Generalised synthesis of space-time variability in flood response An
   analytical framework}},
Journal = {{JOURNAL OF HYDROLOGY}},
