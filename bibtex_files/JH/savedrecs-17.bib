@article{ ISI:000284343400018,
Author = {Asadi-Aghbolaghi, Mandi and Seyyedian, Hamid},
Title = {{An analytical solution for groundwater flow to a vertical well in a
   triangle-shaped aquifer}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2010}},
Volume = {{393}},
Number = {{3-4}},
Pages = {{341-348}},
Month = {{NOV 8}},
Abstract = {{The problem of drawdown and determination of velocity field near a fully
   penetrating pumping well which is located in a confined aquifer is
   investigated. An isosceles triangular domain defining the geometry of
   the aquifer is considered in this paper. The combination of streams
   with/without impervious layer forms the boundaries of the domain of
   interest. The concept of image well is employed to formulate the
   problem. The method of Green's Function together with two dimensional
   Fourier series expansion is used to obtain the closed-form solution for
   the problem. The isopotential lines and corresponding velocity field are
   presented for a particular position of pumping well. Also, the water
   depletion from interfacing streams is analytically calculated for
   different positions of pumping well, and the results are plotted. The
   plots can be effectively used in water resources management and
   environmental engineering. (C) 2010 Elsevier B.V. All rights reserved.}},
