@article{ ISI:000280976600009,
Author = {Luo, Yi and Sophocleous, Marios},
Title = {{Seasonal groundwater contribution to crop-water use assessed with
   lysimeter observations and model simulations}},
Journal = {{JOURNAL OF HYDROLOGY}},
