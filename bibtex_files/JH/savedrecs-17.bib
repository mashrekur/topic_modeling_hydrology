@article{ ISI:000275769000012,
Author = {Camporeale, Carlo and Ridolfi, Luca},
Title = {{Interplay among river meandering, discharge stochasticity and riparian
   vegetation}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2010}},
Volume = {{382}},
Number = {{1-4}},
Pages = {{138-144}},
Month = {{MAR 1}},
Abstract = {{River sinuosity has in impact on riparian vegetation through different
   processes The obvious one is the increment of the length of the riparian
   corridor due to meandering-induced river elongation However, additional
   more subtle aspects occur, ascribable to topographic changes induced by
   meandering dynamics and to their impact oil the stochastic structure of
   the river stage time series It follows that the increment in vegetation
   biomass is not simply equal to the increment in the river length In this
   work, we investigate this complex picture using suitable
   bio-geomorphological models through the study of ail exemplifying case
   This approach can Suggest some novel tools for design the restoration of
   riparian corridors (C) 2009 Elsevier B.V. All rights reserved}},
