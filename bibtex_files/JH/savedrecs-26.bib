@article{ ISI:000365050600020,
Author = {Sikorska, A. E. and Del Giudice, D. and Banasik, K. and Rieckermann, J.},
Title = {{The value of streamflow data in improving TSS predictions - Bayesian
   multi-objective calibration}},
