@article{ ISI:000364249000021,
Author = {Li, Jie and Pang, Zhonghe and Froehlich, Klaus and Huang, Tianming and
   Kong, Yanlong and Song, Wenhui and Yun, Hongxiang},
Title = {{Paleo-environment from isotopes and hydrochemistry of groundwater in
   East Junggar Basin, Northwest China}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2015}},
Volume = {{529}},
Number = {{2, SI}},
Pages = {{650-661}},
Month = {{OCT}},
Abstract = {{Isotopic and geochemical evidence of paleoclimatic conditions from the
   Pleistocene (similar to 30ka BP) has been obtained from groundwater in a
   non-monsoon area in the East Junggar Basin, NW China. The major ion
   chemistry of groundwater is controlled by some processes including ion
   exchange in clay minerals and dissolution of gypsum as well as other
   evaporites in the desert groundwater, while carbonate minerals seem to
   be essentially absent in the aquifers. The water chemistry, oxygen and
   deuterium, deuterium excess, and stable carbon isotopes of groundwater
   show distinctive excursions inferred to be related to environmental
   variations such as air temperature and land cover change. Shallow
   groundwater bears an isotopic signature similar to that of the mean
   annual rainfall in Urumqi, suggesting recharge under warm climate
   conditions in the Holocene. Deep groundwater in the central part of the
   basin is characterized by depleted heavy isotopes, lower deuterium
   excess and low C-14 content, indicating lower air temperature in the
   late Pleistocene, after correction for altitude effect. Old groundwater
   with enriched C-13 indicates the higher proportion of C4 species in the
   late Pleistocene. (C) 2015 Elsevier B.V. All rights reserved.}},
