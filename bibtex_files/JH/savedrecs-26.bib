@article{ ISI:000366769800008,
Author = {Antonio Luque-Espinar, Juan and Navas, Natalia and Chica-Olmo, Mario and
   Cantarero-Malagon, Samuel and Chica-Rivas, Lucia},
Title = {{Seasonal occurrence and distribution of a group of ECs in the water
   resources of Granada city metropolitan areas (South of Spain): Pollution
