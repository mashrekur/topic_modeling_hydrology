@article{ ISI:000364248800009,
Author = {Garcia-Marin, A. P. and Estevez, J. and Medina-Cobo, M. T. and
   Ayuso-Munoz, J. L.},
Title = {{Delimiting homogeneous regions using the multifractal properties of
   validated rainfall data series}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2015}},
Volume = {{529}},
Number = {{1}},
Pages = {{106-119}},
Month = {{OCT}},
Abstract = {{In this work, a regional frequency analysis of extreme annual rainfall
   data in Malaga (Southern Spain) has been performed. Rainfall records
   have been validated, applying various quality control tests as a
   pre-requisite before their use, ensuring their reliability and
   discarding anomalous data. For grouping the stations into potential
   homogeneous regions, the multifractal properties of daily rainfall data
   series recorded at 72 locations have been studied. The scaling of the
   rainfall moments has been analyzed and the empirical moments scaling
   exponent functions have been obtained. The corresponding multifractal
   values have been used to group stations into regions, resulting some of
   them homogeneous. (C) 2015 Elsevier B.V. All rights reserved.}},
