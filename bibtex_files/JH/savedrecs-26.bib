@article{ ISI:000355894700011,
Author = {Mallya, Ganeshchandra and Tripathi, Shivam and Govindaraju, Rao S.},
Title = {{Probabilistic drought classification using gamma mixture models}},
Journal = {{JOURNAL OF HYDROLOGY}},
