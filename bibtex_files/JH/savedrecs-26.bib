@article{ ISI:000364249500082,
Author = {Marti, Pau and Zarzo, Manuel and Vanderlinden, Karl and Girona, Joan},
Title = {{Parametric expressions for the adjusted Hargreaves coefficient in
   Eastern Spain}},
