@article{ ISI:000358629100083,
Author = {Abderrezzak, Kamal El Kadi and Ata, Riadh and Zaoui, Fabrice},
Title = {{One-dimensional numerical modelling of solute transport in streams: The
   role of longitudinal dispersion coefficient}},
