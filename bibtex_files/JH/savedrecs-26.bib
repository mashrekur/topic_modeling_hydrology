@article{ ISI:000364249500011,
Author = {Shao, Qi and Weatherley, Dion and Huang, Longbin and Baumgartl, Thomas},
Title = {{RunCA: A cellular automata model for simulating surface runoff at
   different scales}},
Journal = {{JOURNAL OF HYDROLOGY}},
