@article{ ISI:000358968200062,
Author = {Yu, Songyan and Xu, Zongxue and Liu, Xingcai and Dou, Tongwen and Xu,
   Chen},
Title = {{Identifying and validating freshwater ecoregions in Jinan City, China}},
