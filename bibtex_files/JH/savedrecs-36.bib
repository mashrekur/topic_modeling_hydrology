@article{ ISI:000455694400085,
Author = {Policelli, Frederick and Hubbard, Alfred and Jung, Hahn Chul and
   Zaitchik, Ben and Ichoku, Charles},
Title = {{A predictive model for Lake Chad total surface water area using remotely
   sensed and modeled hydrological and meteorological parameters and
   multivariate regression analysis}},
