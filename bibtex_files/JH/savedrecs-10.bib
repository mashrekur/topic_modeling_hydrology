@article{ ISI:000226243200003,
Author = {Wilcox, C and Huertos, ML},
Title = {{A simple, rapid method for mapping bathymetry of small wetland basins}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2005}},
Volume = {{301}},
Number = {{1-4}},
Pages = {{29-36}},
Month = {{JAN 20}},
Abstract = {{Many tools exist for determining and mapping the bathymetry and
   topography of aquatic systems. such as freshwater wetlands. However,
   these tools often require time-consuming survey work to produce accurate
   maps. In particular, the large quantity of data necessary may be
   prohibitive for projects where determining bathymetry is not a central
   focus. but instead a necessary step in achieving some other goal. We
   present a method to produce bathymetric surface maps with a minimum
   amount of effort using global positioning system receiver and laser
   transit survey data. We also demonstrate that this method is
   surprisingly accurate, given the small amount of data we use to generate
   the bathymetry maps. (C) 2004 Elsevier B.V. All fights reserved.}},
