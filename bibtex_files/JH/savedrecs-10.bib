@article{ ISI:000233981000018,
Author = {Cronin, AA and Barth, JAC and Elliot, T and Kalin, RM},
Title = {{Recharge velocity and geochemical evolution for the Permo-Triassic
   Sherwood Sandstone, Northern Ireland}},
Journal = {{JOURNAL OF HYDROLOGY}},
