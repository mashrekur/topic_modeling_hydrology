Language = {{English}},
Affiliation = {{Schlather, M (Reprint Author), Helmut Schmidt Univ, Fachbereich WOW, Postfach 700822, D-22008 Hemburg, Germany.
   Helmut Schmidt Univ, Fachbereich WOW, D-22008 Hemburg, Germany.
   Univ Bayreuth, Abt Bodenphys, D-95440 Bayreuth, Germany.}},
DOI = {{10.1016/j.jhydrol.2004.12.011}},
ISSN = {{0022-1694}},
EISSN = {{1879-2707}},
