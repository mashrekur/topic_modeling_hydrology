@article{ ISI:000229808300013,
Author = {Valeo, C and Skone, SH and Ho, CLI and Poon, SKM and Shrestha, SM},
Title = {{Estimating snow evaporation with GPS derived precipitable water vapour}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2005}},
Volume = {{307}},
