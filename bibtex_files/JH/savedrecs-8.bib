@article{ ISI:000185691900003,
Author = {Rose, CW and Yu, B and Hogarth, WL and Okom, AEA and Ghadiri, H},
Title = {{Sediment deposition from flow at low gradients into a buffer strip - a
   critical test of re-entrainment theory}},
