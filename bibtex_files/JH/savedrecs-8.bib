@article{ ISI:000185401100004,
Author = {Mendoza, GF and Steenhuis, TS and Walter, MT and Parlange, JY},
Title = {{Estimating basin-wide hydraulic parameters of a semi-arid mountainous
   watershed by recession-flow analysis}},
Journal = {{JOURNAL OF HYDROLOGY}},
