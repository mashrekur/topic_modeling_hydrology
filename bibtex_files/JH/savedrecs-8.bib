@article{ ISI:000187723100011,
Author = {Gautam, MR and Watanabe, K and Saegusa, H},
Title = {{Analysis of hydraulic pressure fluctuation in deep geologic formations
   in Tono area, Japan using artificial neural networks}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2003}},
Volume = {{284}},
Number = {{1-4}},
Pages = {{174-192}},
Month = {{DEC 22}},
Abstract = {{In this study, the concept and possible uses of artificial neural
   network (ANN) for mapping hydraulic pressures within boreholes are
   presented. ANN models were developed using monitored hydraulic pressure
   data over a certain period representing near-natural conditions to map
   the fluctuating patterns of hydraulic pressure in deep geologic
   formations in the Tono area, Japan. The ANN models developed captured
   the natural effects due to barometric pressure, earth tides, and other
   factors. It was also observed that geologic structures such as the
   Tsukiyoshi Fault divided the groundwater flow domains in the area. The
   formulated models that satisfactorily captured those natural effects
   were then used to evaluate the effect of pumping over a different
   period. (C) 2003 Elsevier B.V. All rights reserved.}},
