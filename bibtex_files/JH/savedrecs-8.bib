@article{ ISI:000181147900004,
Author = {Vervoort, RW and Cattle, SR},
Title = {{Linking hydraulic conductivity and tortuosity parameters to pore space
   geometry and pore-size distribution}},
Journal = {{JOURNAL OF HYDROLOGY}},
