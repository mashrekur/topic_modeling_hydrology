@article{ ISI:000165937500016,
Author = {Sharma, A and Luk, KC and Cordery, I and Lall, U},
Title = {{Seasonal to interannual rainfall probabilistic forecasts for improved
   water supply management: Part 2 - Predictor identification of quarterly
   rainfall using ocean-atmosphere information}},
Journal = {{JOURNAL OF HYDROLOGY}},
