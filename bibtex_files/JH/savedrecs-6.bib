@article{ ISI:000085378000002,
Author = {Arnold, JG and Muttiah, RS and Srinivasan, R and Allen, PM},
Title = {{Regional estimation of base flow and groundwater recharge in the Upper
   Mississippi river basin}},
Journal = {{JOURNAL OF HYDROLOGY}},
