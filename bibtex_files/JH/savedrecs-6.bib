@article{ ISI:000087736400025,
Author = {Ritsema, CJ and Dekker, LW},
Title = {{Preferential flow in water repellent sandy soils: principles and
   modeling implications}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2000}},
Volume = {{231}},
