@article{ ISI:000169711200008,
Author = {Strupczewski, WG and Singh, VP and Feluch, W},
Title = {{Non-stationary approach to at-site flood frequency modelling I. Maximum
   likelihood estimation}},
Journal = {{JOURNAL OF HYDROLOGY}},
