@article{ ISI:000089200700004,
Author = {Szekely, F and Senay, Y and Al-Rashed, M and Al-Sumait, A and Al-Awadi,
   E},
Title = {{Computer simulation of the hydraulic impact of water well fields in
   Kuwait}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2000}},
Volume = {{235}},
Number = {{3-4}},
Pages = {{205-220}},
Month = {{AUG 31}},
Abstract = {{This research paper discusses the development of a computer software
   program for numerical modeling of ground water how for the State of
   Kuwait. The software has been used to create a calibrated model system
   to simulate the drawdown induced by well field operation in the system
   of four leaky aquifers in Kuwait. The software includes the effects of
   discontinuous layering and multiple screens in many wells. A four-stage
   zooming technique is applied to magnify the flow patterns from the
   regional model through local and field models to final well site models.
   Manual calibration is performed to match the measured and simulated
   drawdown data over the operational period between 1945 and 1994.
   Drawdown prediction was performed for the period 1995-2020. (C) 2000
   Published by Elsevier Science B.V.}},
