@article{ ISI:000087736400017,
Author = {Robichaud, PR and Hungerford, RD},
Title = {{Water repellency by laboratory burning of four northern Rocky Mountain
   forest soils}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2000}},
Volume = {{231}},
