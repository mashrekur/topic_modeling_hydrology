@article{ ISI:000168909900001,
Author = {Yue, S and Ouarda, TBMJ and Bobee, B},
Title = {{A review of bivariate gamma distributions for hydrological application}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2001}},
Volume = {{246}},
Number = {{1-4}},
Pages = {{1-18}},
Month = {{JUN 1}},
Abstract = {{A univariate gamma distribution is one of the most commonly adopted
   statistical distributions in hydrological frequency analysis. A
   bivariate gamma distribution constructed from specified gamma marginals
   may be useful for representing joint probabilistic properties of
   multivariate hydrological events such as floods and storms. This article
   presents a review of various bivariate gamma distribution models that
   are constructed from gamma marginals. Advantages and limitations of each
   of these models are pointed out. Applicability of a few bigamma
   distributions whose gamma marginal distributions have different scale
   and shape parameters is investigated. The dependence of these models is
   directly or indirectly measured via the Pearson's product-moment
   correlation coefficient. The scale and shape parameters of the models
   are estimated from their marginal distributions by the method of
   moments. Results indicate that these bigamma distribution models will be
   useful for describing the joint probability distribution of two
   correlated random variables with gamma marginals. (C) 2001 Elsevier
   Science Ltd All rights reserved.}},
