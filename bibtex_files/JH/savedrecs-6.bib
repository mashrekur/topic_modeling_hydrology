@article{ ISI:000165525000005,
Author = {Aboal, JR and Jimenez, MS and Morales, D and Gil, P},
Title = {{Effects of thinning on throughfall in Canary Islands pine forest - the
   role of fog}},
Journal = {{JOURNAL OF HYDROLOGY}},
