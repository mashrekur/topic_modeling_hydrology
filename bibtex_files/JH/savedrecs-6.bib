@article{ ISI:000087736400012,
Author = {Doerr, SH and Thomas, AD},
Title = {{The role of soil moisture in controlling water repellency: new evidence
   from forest soils in Portugal}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2000}},
Volume = {{231}},
Number = {{SI}},
Pages = {{134-147}},
