@article{ ISI:000087736400007,
Author = {Wang, Z and Wu, L and Wu, QJ},
Title = {{Water-entry value as an alternative indicator of soil water-repellency
   and wettability}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2000}},
Volume = {{231}},
