@article{ ISI:000347589500040,
Author = {Cardenas, M. Bayani and Kanarek, Michael R.},
Title = {{Soil moisture variation and dynamics across a wildfire burn boundary in
   a loblolly pine (Pinus taeda) forest}},
Journal = {{JOURNAL OF HYDROLOGY}},
