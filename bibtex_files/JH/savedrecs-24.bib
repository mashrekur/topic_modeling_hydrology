@article{ ISI:000347589600036,
Author = {Xia, Lu and Zheng, Xilai and Shao, Haibing and Xin, Jia and Peng, Tao},
Title = {{Influences of environmental factors on bacterial extracellular polymeric
   substances production in porous media}},
