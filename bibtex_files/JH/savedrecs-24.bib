@article{ ISI:000347589600054,
Author = {Liao, Zijie and Osenbrueck, Karsten and Cirpka, Olaf A.},
Title = {{Non-stationary nonparametric inference of river-to-groundwater
   travel-time distributions}},
Journal = {{JOURNAL OF HYDROLOGY}},
