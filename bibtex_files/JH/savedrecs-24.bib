@article{ ISI:000347589500064,
Author = {Gyasi-Agyei, Yeboah and Pegram, Geoffrey},
Title = {{Interpolation of daily rainfall networks using simulated radar fields
   for realistic hydrological modelling of spatial rain field ensembles}},
