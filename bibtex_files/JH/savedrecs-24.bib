@article{ ISI:000347589500001,
Author = {Kottegoda, N. T. and Natale, L. and Raiteri, E.},
Title = {{Monte Carlo Simulation of rainfall hyetographs for analysis and design}},
Journal = {{JOURNAL OF HYDROLOGY}},
