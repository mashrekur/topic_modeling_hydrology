Author = {Jackson, Tracie R. and Apte, Sourabh V. and Haggerty, Roy},
Title = {{Effect of multiple lateral cavities on stream solute transport under
   non-Fickian conditions and at the Fickian asymptote}},
Journal = {{JOURNAL OF HYDROLOGY}},
