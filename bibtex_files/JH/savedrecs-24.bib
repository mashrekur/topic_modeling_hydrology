@article{ ISI:000347589500101,
Author = {Ezquerro, Pablo and Herrera, Gerardo and Marchamalo, Miguel and Tomas,
   Roberto and Bejar-Pizarro, Marta and Martinez, Ruben},
Title = {{A quasi-elastic aquifer deformational behavior: Madrid aquifer case
   study}},
