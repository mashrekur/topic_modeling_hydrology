@article{ ISI:000347589500052,
Author = {Coopersmith, Evan J. and Cosh, Michael H. and Daughtry, Craig S. T.},
Title = {{Field-scale moisture estimates using COSMOS sensors: A validation study
   with temporary networks and Leaf-Area-Indices}},
Journal = {{JOURNAL OF HYDROLOGY}},
