@article{ ISI:000347018100057,
Author = {Martinelli, G. and Chahoud, A. and Dadomo, A. and Fava, A.},
Title = {{Isotopic features of Emilia-Romagna region (North Italy) groundwaters:
   Environmental and climatological implications}},
Journal = {{JOURNAL OF HYDROLOGY}},
