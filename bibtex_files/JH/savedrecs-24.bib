@article{ ISI:000347589500072,
Author = {Szolgayova, E. and Parajka, J. and Bloeschl, G. and Bucher, C.},
Title = {{Long term variability of the Danube River flow and its relation to
   precipitation and air temperature}},
Journal = {{JOURNAL OF HYDROLOGY}},
