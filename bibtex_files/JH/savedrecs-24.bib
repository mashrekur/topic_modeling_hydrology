@article{ ISI:000347589500063,
Author = {Zhou, Xinyao and Bi, Shaojie and Yang, Yonghui and Tian, Fei and Ren,
   Dandan},
Title = {{Comparison of ET estimations by the three-temperature model, SEBAL model
   and eddy covariance observations}},
Journal = {{JOURNAL OF HYDROLOGY}},
