@article{ ISI:000347589600018,
Author = {Tao, Yumeng and Duan, Qingyun and Ye, Aizhong and Gong, Wei and Di,
   Zhenhua and Xiao, Mu and Hsu, Kuolin},
Title = {{An evaluation of post-processed TIGGE multimodel ensemble precipitation
   forecast in the Huai river basin}},
Journal = {{JOURNAL OF HYDROLOGY}},
