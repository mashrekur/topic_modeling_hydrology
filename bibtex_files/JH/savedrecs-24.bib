@article{ ISI:000347589500019,
Author = {Longo, Sandro and Di Federico, Vittorio},
Title = {{Axisymmetric gravity currents within porous media: First order solution
   and experimental validation}},
Journal = {{JOURNAL OF HYDROLOGY}},
