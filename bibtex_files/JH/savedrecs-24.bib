@article{ ISI:000347018100027,
Author = {Liwata, Pauliina and Hanninen, Pekka and Okkonen, Jarkko and Sutinen,
   Raimo},
Title = {{Time-stability of soil water through boreal (60-68 degrees N) gradient}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2014}},
Volume = {{519}},
Number = {{B}},
Pages = {{1584-1593}},
Month = {{NOV 27}},
Abstract = {{The time-stability concept suggests that the spatial pattern of the soil
   water content does not change with time when the observations are ranked
   according to the magnitude of the soil water. The time-stability holds
   for soils in agricultural fields and forest compartments, yet broader
   scale effects of boreal climate on time-stability are poorly understood.
   We studied the spatial persistence of soil water through the boreal
   climatic gradient (60-68 degrees N) on the Baltic Shield using
   multi-year (2002-2010) automatic monitoring at sites (up to 18 in
   number) with diverse soil texture and land use. We found that the soils
   exhibited significant time-stability, Spearman's rank correlation
   coefficients r(s) = 0.94-0.95, p < 0.01, for the whole climatic
   gradient. This applied to the soil depth of 0-100 cm and the growing
   seasons when a soil temperature threshold value of +3.2 degrees C was
   used. Our observation helps in estimation and modeling the effects of
   climate change along the boreal biogeographical zones. (C) 2014 Elsevier
   B.V. All rights reserved.}},
