@article{ ISI:000371940900028,
Author = {Luo, Jing and Huai, Wenxin and Wang, Ping},
Title = {{Contaminant transport in a three-zone wetland: Dispersion and ecological
   degradation}},
Journal = {{JOURNAL OF HYDROLOGY}},
