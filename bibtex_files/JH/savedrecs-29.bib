@article{ ISI:000370086200024,
Author = {Revuelto, Jesus and Vionnet, Vincent and Lopez-Moreno, Juan-Ignacio and
   Lafaysse, Matthieu and Morin, Samuel},
Title = {{Combining snowpack modeling and terrestrial laser scanner observations
   improves the simulation of small scale snow dynamics}},
