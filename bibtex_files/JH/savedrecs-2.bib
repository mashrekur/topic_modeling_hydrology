@article{ ISI:A1994NR26700003,
Author = {BANGOY, LM and DROGUE, C},
Title = {{ANALYSIS OF INTERMITTENT PUMPING TESTS IN FISSURED FRACTAL AQUIFERS -
   THEORY AND APPLICATIONS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{158}},
Number = {{1-2}},
Pages = {{47-59}},
Month = {{JUN 1}},
Abstract = {{A new formulation of water flow equations for fissured media allows the
   analysis of drawdowns caused by intermittent pumping which consist of a
   series of pumping cycles with variable rates and durations. Simulation
   of the evolution of dynamic levels in the pumping well and in
   observation wells, by superimposing the effects of irregular pumping
   over 89 days (125 pumping cycles) is satisfactory. The study of the
   variations in the average pumping rate of each cycle allows analysis of
   the periodic phenomena which occur almost weekly at the lowest water
   level.}},
