@article{ ISI:A1994QB26800007,
Author = {MALEK, E},
Title = {{CALIBRATION OF THE PENMAN WIND FUNCTION USING THE BOWEN-RATIO
   ENERGY-BALANCE METHOD}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{163}},
Number = {{3-4}},
Pages = {{289-298}},
Month = {{DEC 15}},
Abstract = {{The Bowen ratio energy balance (BREB) method was used to calibrate the
   wind function constants (W-f = a(w) + b(w)u(2)) in the aerodynamic term
   of the Penman equation for computation of the hourly and 24 h
   alfalfa-reference evapotranspiration (E(tr)) at an irrigated semi-arid
   valley in northern Utah during the 1991-1992 growing season. Statistical
   analysis of the 28 non-rainy days (when the conditions of
   alfalfa-reference evapotranspiration were met) showed that the hourly
