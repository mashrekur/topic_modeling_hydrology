@article{ ISI:A1993LX80600008,
Author = {MOHARRAM, SH and GOSAIN, AK and KAPOOR, PN},
Title = {{A COMPARATIVE-STUDY FOR THE ESTIMATORS OF THE GENERALIZED PARETO
   DISTRIBUTION}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{150}},
Number = {{1}},
Pages = {{169-185}},
Month = {{SEP}},
Abstract = {{Various estimation methods for the three-parameter case of Generalized
   Pareto distribution are given, including an alternative method based on
   least square (LS). Modified formulae for computing estimators are
   provided. The performances of these methods are compared by Monte Carlo
   simulation. It is found that the LS method has generally a lower root
   mean square error (RMSE) than that obtained using other methods. The LS
   method also performs best in terms of BIAS when the shape parameter is
   greater than zero (c > 0), while the probability weighted moments (PWM)
   method performs best when c < 0.}},
