@article{ ISI:A1994NU68100007,
Author = {LEE, SI},
Title = {{VALIDATION OF GEOSTATISTICAL MODELS USING THE FILLIBEN TEST OF
   ORTHONORMAL RESIDUALS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{158}},
Number = {{3-4}},
Pages = {{319-332}},
Month = {{JUN 15}},
Abstract = {{In geostatistical estimation, a model developed to describe the
   structure of the spatial function must be tested against field data to
   check its validity. Orthonormal residuals as a diagnostic tool have
   advantages over conventional standardized residuals in that they are
   easy to calculate and the determination of the sampling distributions of
   statistics based on them is computationally efficient. In this paper,
   the applicability of a normality test known as the Filliben test is
   studied which checks the hypothesis that orthonormal residuals have a
   normal distribution when the model is consistent with the available
   data. For a set of hydraulic head measurements, two different models of
   the structure of the head field were tested. Result indicated that the
   Filliben test agrees with other validation tests and thus can be used
   for diagnosing model adequacy.}},
