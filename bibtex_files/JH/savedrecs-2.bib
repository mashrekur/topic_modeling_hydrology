@article{ ISI:A1994NU68100008,
Author = {GOVINDARAJU, RS and KAVVAS, ML},
Title = {{A SPECTRAL APPROACH FOR ANALYZING THE RILL STRUCTURE OVER HILLSLOPES .1.
   DEVELOPMENT OF STOCHASTIC-THEORY}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{158}},
Number = {{3-4}},
Pages = {{333-347}},
Month = {{JUN 15}},
Abstract = {{The rill structure over the soil surface plays an important role in
   determining the runoff and sediment loss from hillslopes. The
   microtopography is often complicated, and representation of the
   hillslope surface on a rill-by-rill basis is uneconomical and
   impractical. A spectral approach combined with a stochastic theory is
   proposed for the rill profile. The theory develops appropriate
   probability distribution laws for rill amplitudes and rill periods
   (interrill areas) under the assumption that the spectrum of the rill
   profile is narrow banded. This study establishes a foundation for future
   studies dealing with parameter estimation, and determining the global
   roughness and erodibility for runoff and sediment discharge predictions
   over hillslopes.}},
