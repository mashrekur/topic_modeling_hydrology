@article{ ISI:A1994NL36500002,
Author = {DWYER, IJ and REED, DW},
Title = {{EFFECTIVE FRACTAL DIMENSION AND CORRECTIONS TO THE MEAN OF ANNUAL MAXIMA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{157}},
Number = {{1-4}},
Pages = {{13-34}},
Month = {{MAY}},
Abstract = {{Given a time series of daily rainfall totals, it is recognized that the
   maximum daily value is, in general, lower than the true 24 h maximum.
   Thus, annual maximum series under-represent the extreme events, and
   correction factors are therefore required to convert them to their 24 h
   counterparts. The phenomenon is generalized and examined by reference to
   UK time series for rainfall, wind speed and air temperature, resulting
   in mean correction factors of 1.167, 1.099 and 1.036, respectively. For
