@article{ ISI:A1993KQ06300015,
Author = {EBISEMIJU, FS},
Title = {{THE PLANIMETRIC AND GEOMETRIC-PROPERTIES OF THE CHANNEL BENDS OF
   LOW-ENERGY STREAMS IN A FORESTED HUMID TROPICAL ENVIRONMENT,
   SOUTHWESTERN NIGERIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{142}},
Number = {{1-4}},
Pages = {{319-335}},
Month = {{FEB}},
Abstract = {{A detailed statistical analysis of the morphometry of channel bends
   along the low-energy River Elemi in southwestern Nigeria confirms the
   inherent tendency in rivers to develop sinuous channels with
   asymmetrical bends and compound loops. However, the channel bends differ
   in many respects from those widely reported in the literature. First,
   the parameters have low values because of the characteristic low energy
   to resistance ratio of the intermittent streams. Second, cross-sectional
   and planform properties vary downstream in a non-systematic manner,
   owing to the greater effectiveness of site-specific rather than areal
   factors in controlling the morphology of channel bends in the area.
   Third, the interrelations of bend properties are extremely weak, and the
   patterns of correlations also are different. All these findings indicate
   that the channel bends of the River Elemi are neither true meanders nor
   equilibrium forms. It is doubtful, therefore, if the observed relations
   are of any use in palaeoenvironmental reconstruction.}},
