@article{ ISI:A1993MQ12700006,
Author = {RUSHTON, KR and SALMON, S},
Title = {{SIGNIFICANCE OF VERTICAL FLOW-THROUGH LOW-CONDUCTIVITY ZONES IN
   BROMSGROVE SANDSTONE AQUIFER}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{152}},
Number = {{1-4}},
Pages = {{131-152}},
Month = {{DEC}},
Abstract = {{When water passes vertically through low-conductivity zones in aquifers,
   significant vertical head gradients occur. The importance of such
   gradients is identified in a study of the Bromsgrove Sandstone Aquifer
   Unit. A mathematical model incorporating the low-conductivity layers has
   been developed to explain the changed conditions from 1900, when there
   were extensive spring flows, to the present day, when deep boreholes
   intercept much of the groundwater flow. The mathematical model also
   helps in understanding the changing pumped drawdowns and water table
   elevations.}},
