@article{ ISI:A1994NY36300023,
Author = {DUCK, RW and MCMANUS, J},
Title = {{A LONG-TERM ESTIMATE OF BEDLOAD AND SUSPENDED SEDIMENT YIELD DERIVED
   FROM RESERVOIR DEPOSITS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
