@article{ ISI:A1993LX80600004,
Author = {YU, YS and ZOU, SM and WHITTEMORE, D},
Title = {{NONPARAMETRIC TREND ANALYSIS OF WATER-QUALITY DATA OF RIVERS IN KANSAS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{150}},
Number = {{1}},
Pages = {{61-80}},
Month = {{SEP}},
Abstract = {{Surface water quality data for 15 sampling stations in the Arkansas,
   Verdigris, Neosho, and Walnut river basins inside the state of Kansas
   were analyzed to detect trends (or lack of trends) in 17 major
   constituents by using four different non-parametric methods. The results
   show that concentrations of specific conductance, total dissolved
   solids, calcium, total hardness, sodium, potassium, alkalinity, sulfate,
   chloride, total phosphorus, ammonia plus organic nitrogen, and suspended
   sediment generally have downward trends. Some of the downward trends are
   related to increases in discharge, while others could be caused by
   decreases in pollution sources. Homogeneity tests show that both
   station-wide trends and basin-wide trends are non-homogeneous.}},
