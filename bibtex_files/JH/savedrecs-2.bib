@article{ ISI:A1993LD07000011,
Author = {BLACKIE, JR and SIMPSON, TKM},
Title = {{CLIMATIC VARIABILITY WITHIN THE BALQUHIDDER CATCHMENTS AND ITS EFFECT ON
