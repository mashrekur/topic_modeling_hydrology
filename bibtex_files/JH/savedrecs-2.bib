@article{ ISI:A1993LP48300004,
Author = {VANCLOOSTER, M and MALLANTS, D and DIELS, J and FEYEN, J},
Title = {{DETERMINING LOCAL-SCALE SOLUTE TRANSPORT PARAMETERS USING TIME-DOMAIN
   REFLECTOMETRY (TDR)}},
