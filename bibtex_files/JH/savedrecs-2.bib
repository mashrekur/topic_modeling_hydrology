@article{ ISI:A1993LD07000010,
Author = {ROBSON, AJ and WHITEHEAD, PG and JOHNSON, RC},
Title = {{AN APPLICATION OF A PHYSICALLY BASED SEMIDISTRIBUTED MODEL TO THE
   BALQUHIDDER CATCHMENTS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{145}},
Number = {{3-4}},
Pages = {{357-370}},
Month = {{MAY 15}},
Abstract = {{A physically based semi-distributed model, TOPMODEL, is applied to the
   two catchments at Balquhidder. The model uses a topographic index which
   highlights hydrologically significant areas within the catchments. The
   model is used to simulate runoff and to compare the behaviour of the two
   catchments. The results indicate that a large proportion of stream water
   is generated from saturated contributing areas (source areas); the
   Monachyle has higher contributions of water of this type. The results
   may also indicate that the hydrology of the Monachyle catchment has
   changed following agricultural improvement by increased drainage; a
   damped subsurface response is observed.}},
