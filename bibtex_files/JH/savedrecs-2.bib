@article{ ISI:A1994NZ41400006,
Author = {LIN, YD and LYE, LM},
Title = {{MODELING LONG-TERM DEPENDENCE BASED ON CUMULATIVE DEPARTURES OF ANNUAL
   FLOW SERIES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{160}},
Number = {{1-4}},
Pages = {{105-121}},
Month = {{AUG}},
Abstract = {{This paper investigates the suitability of Sen's approach in modelling
   hydrologic time series and in particular the generation of synthetic
   flow series. It was found that several problems exist with the method as
   proposed. These include too many parameters in the model, difficulties
   in modelling skewed series, and finding a suitable stochastic model for
   the residuals between the original and the fitted cumulative departure
   curves. On the other hand, it has been found that Sen's method is
   effective in preserving the Hurst phenomenon and is especially suited
   for modelling time series with a relatively high Hurst coefficient but
   low lag-one serial correlation coefficient. Also, with some
   modifications to the suggested procedure some of the stated problems can
   be somewhat overcome.}},
