@article{ ISI:A1993MC28100003,
Author = {BONELL, M},
Title = {{PROGRESS IN THE UNDERSTANDING OF RUNOFF GENERATION DYNAMICS IN FORESTS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{150}},
