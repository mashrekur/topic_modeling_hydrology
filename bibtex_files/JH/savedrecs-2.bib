@article{ ISI:A1994MZ15700015,
Author = {BISHOP, PK and SMALLEY, PC and EMERY, D and DICKSON, JAD},
Title = {{STRONTIUM ISOTOPES AS INDICATORS OF THE DISSOLVING PHASE IN A CARBONATE
   AQUIFER - IMPLICATIONS FOR C-14 DATING OF GROUNDWATER}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{154}},
Number = {{1-4}},
Pages = {{301-321}},
Month = {{FEB}},
Abstract = {{The calculation of model C-14 ages for groundwater often relies on a
   correction being made for the incongruent dissolution of aquifer
   carbonate. Correction equations are particularly sensitive, in certain
   ranges, to variation in the deltaC-13 value input for dissolving
   carbonate. This study uses a combined Sr-87/Sr-86 and deltaC-13 approach
   to define the source and mean deltaC-13 value of dissolving carbonate in
   the Lincolnshire Limestone aquifer of eastern England. This deltaC-13
   value is most appropriately used in C-14 correction equations and yields
   the most potentially accurate groundwater model ages.}},
