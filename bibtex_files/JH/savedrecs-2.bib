@article{ ISI:A1994NL36500003,
Author = {WILSON, GB and ANDREWS, JN and BATH, AH},
Title = {{THE NITROGEN ISOTOPE COMPOSITION OF GROUNDWATER NITRATES FROM THE EAST
   MIDLANDS TRIASSIC SANDSTONE AQUIFER, ENGLAND}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{157}},
Number = {{1-4}},
Pages = {{35-46}},
Month = {{MAY}},
Abstract = {{The N-15/N-14 composition of nitrates dissolved in groundwaters from 23
   abstraction sites in the East Midlands Sherwood Sandstone are reported.
   Dissolved N2/Ar ratios are, with two exceptions, consistent with
   atmospheric input and show that denitrification does not occur
   throughout most Of the aquifer, The nitrogen isotope composition of
   nitrates dissolved in the groundwaters can be shown not to have been
   mixed or fractionated to any significant extent within the aquifer. The
   observed (deltaN(NO3))-N-15 isotopic variations are attributed to four
   nitrate sources, all of which are associated with agricultural land use.}},
