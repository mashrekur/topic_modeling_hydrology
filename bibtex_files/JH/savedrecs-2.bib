@article{ ISI:A1993LE96400002,
Author = {WICKS, CM and GROVES, CG},
Title = {{ACIDIC MINE DRAINAGE IN CARBONATE TERRAINS - GEOCHEMICAL PROCESSES AND
   RATES OF CALCITE DISSOLUTION}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{146}},
Number = {{1-4}},
Pages = {{13-27}},
Month = {{JUN 1}},
Abstract = {{In situ rates of calcite dissolution were measured by weight loss of
   single crystals placed in an East Tennessee stream that is affected by
   acidic mine drainage. The dissolution rates ranged from 2.6 x 10(-8) to
   7.9 x 10(-8) mmol cm-2 s-1 over the entire pH range of 3.6-7.7. Calcite
   dissolution rates were several orders of magnitude slower than observed
   in laboratory solutions of CO2-HCl-H2O-CaCO3 and appeared to be
   inhibited by dissolved iron and possibly other solutes in the stream.}},
