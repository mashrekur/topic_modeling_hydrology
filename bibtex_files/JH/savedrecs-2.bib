@article{ ISI:A1994NR26700005,
Author = {BROWN, R and NEWCOMB, PD and CHEUNGLEE, J and RYALL, G},
Title = {{DEVELOPMENT AND EVALUATION OF THE FORECAST STEP OF THE FRONTIERS
   SHORT-TERM PRECIPITATION FORECASTING SYSTEM}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{158}},
Number = {{1-2}},
Pages = {{79-105}},
Month = {{JUN 1}},
Abstract = {{The development of the forecast step of the FRONTIERS precipitation
   nowcasting system is described. In particular, an option to advect the
   precipitation field with forecast winds has been introduced. Routine
   evaluations indicate a consistent improvement in forecast accuracy,
   relative to persistence, following the upgrade. Forecast accumulations
   are evaluated by comparison with raingauge data in two regions of the
   UK. In the North West, they are compared with heavy rainfall warnings
   issued operationally. The operational forecasts were significant
   overestimates (average bias +250\%) but were issued more reliably than
   were the FRONTIERS forecasts, which were much closer to ground truth for
   the same cases (average bias -29\%). Comparison with a dense gauge
   network in the South East shows that the accuracy of the forecasts
   increases with increasing accumulation period and catchment size.}},
