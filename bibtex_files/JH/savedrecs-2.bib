@article{ ISI:A1994NA50600010,
Author = {LIANG, GC and OCONNOR, KM and KACHROO, RK},
Title = {{A MULTIPLE-INPUT SINGLE-OUTPUT VARIABLE GAIN FACTOR MODEL}},
