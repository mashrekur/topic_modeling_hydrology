@article{ ISI:A1994MV75700005,
Author = {BERTOLI, L and MOISELLO, U},
Title = {{A REGIONAL MODEL FOR ESTIMATING THE PROBABILITY-DISTRIBUTION OF ROUTED
   PEAK DISCHARGES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{153}},
Number = {{1-4}},
Pages = {{103-138}},
Month = {{JAN}},
Abstract = {{A regional model is proposed for estimating the probability distribution
   of routed peak discharges. The model is composed of two parts: the
   distribution of the annual maxima of peak discharges Q(p) and the
   conditional joint distribution, given the peak discharge q(p), of three
   of the four parameters (peak discharge q(p), baseflow discharge q(b),
   storm runoff v and time to peak t(p)) assumed to represent a flood event
   hydrograph. The model is based on some simplifications and
   approximations. Analysis of the model behaviour in flood routing
   simulations, however, suggests that it may be considered reasonably
   accurate, at least in the range of accuracy accepted for design
   purposes.}},
