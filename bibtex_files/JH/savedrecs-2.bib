@article{ ISI:A1993LE96400013,
Author = {CULF, AD},
Title = {{THE POTENTIAL FOR ESTIMATING REGIONAL SENSIBLE HEAT-FLUX FROM CONVECTIVE
   BOUNDARY-LAYER GROWTH}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{146}},
Number = {{1-4}},
Pages = {{235-244}},
Month = {{JUN 1}},
Abstract = {{It is shown how a simple model, which has been used successfully by many
   authors to describe the growth of the convective mixed layer, can, in
   theory, be used in reverse to give an estimate of regional sensible heat
   flux. The reversed model is tested using a dataset of frequent
   radiosonde ascents and measured surface sensible heat flux. The results
   of the comparison are encouraging; improved methods, involving
   continuous monitoring of boundary layer height and temperature, will
   give better results.}},
