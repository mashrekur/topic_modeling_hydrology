@article{ ISI:A1994NA50600012,
Author = {BACCHI, B and BECCIU, G and KOTTEGODA, NT},
Title = {{BIVARIATE EXPONENTIAL MODEL APPLIED TO INTENSITIES AND DURATIONS OF
   EXTREME RAINFALL}},
