@article{ ISI:A1994NY36300008,
Author = {LENSING, HJ and VOGT, M and HERRLING, B},
Title = {{MODELING OF BIOLOGICALLY MEDIATED REDOX PROCESSES IN THE SUBSURFACE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
