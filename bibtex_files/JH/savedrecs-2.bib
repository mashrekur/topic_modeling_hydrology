@article{ ISI:A1994ND64500006,
Author = {POLCHER, J and LAVAL, K},
Title = {{THE IMPACT OF AFRICAN AND AMAZONIAN DEFORESTATION ON TROPICAL CLIMATE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{155}},
Number = {{3-4}},
Pages = {{389-405}},
Month = {{MAR 15}},
Note = {{16th General Assembly of the European-Geophysical-Society: Mesoscale
   Hydrology and General Circulation Models, WIESBADEN, GERMANY, APR 22-26,
   1991}},
Organization = {{EUROPEAN GEOPHYS SOC}},
Abstract = {{This paper presents results of a deforestation experiment carried out
   with the general circulation model of the Laboratoire de Meteorologie
   Dynamique coupled to the Schematisation des Echanges Hydriques a
   l'Intertace entre le Biosphere et l'Atmosphere (SECHIBA) vegetation
   model. An integration of 1 year was carried out in which the tropical
   forests of Amazonia, Africa and Indonesia were replaced by grassland. It
   was found that the model produces a reduction in evaporation and an
   increase in soil temperature. In contrast to similar experiments, a
   significant increase in moisture convergence was obtained; this increase
   is attributed to an enhancement of the convective activity of the
