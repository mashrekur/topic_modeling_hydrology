@article{ ISI:A1994NL36500012,
Author = {BELLON, A and ZAWADZKI, I},
Title = {{FORECASTING OF HOURLY ACCUMULATIONS OF PRECIPITATION BY OPTIMAL
   EXTRAPOLATION OF RADAR MAPS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{157}},
Number = {{1-4}},
Pages = {{211-233}},
Month = {{MAY}},
Abstract = {{The forecast of radar rainfall rate maps is optimized when the maps to
   be forecast are averaged over an area A = L2 such that L = kT(lambda)
   where L is in kilometres, T is the forecast period in minutes, and k and
   lambda are empirical coefficients that are usually restricted to 1.0
   less-than-or-equal-to k less-than-or-equal-to 1.3 and 0.7
   less-than-or-equal-to lambda less-than-or-equal-to 0.8. Application of
   this relationship to the generation of forecast 1 h rainfall
   accumulation maps has consistently reduced r.m.s. errors by at least
   10\% when compared with those derived with the originally unsmoothed
   data.}},
