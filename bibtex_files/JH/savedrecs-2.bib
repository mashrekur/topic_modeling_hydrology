@article{ ISI:A1994NA50600006,
Author = {DISKIN, MH},
Title = {{A RATIONAL ROUTING ELEMENT FOR WATERSHED CELL MODELS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{155}},
Number = {{1-2}},
Pages = {{93-101}},
Month = {{MAR 1}},
Abstract = {{Cell models consist of a finite number of cell units, each representing
   a certain portion of the watershed. The cells are interconnected in a
   tree-like structure simulating the main drainage pattern in the
   watershed. Each model cell unit is composed of three elements-one for
   converting rainfall excess on the cell area into surface runoff at the
   cell outlet, one to convert channel inflow to the cell into channel
   outflow, and an element that adds the outputs of the other two elements,
   which are in parallel, to produce the total outflow from the cell unit.
   Conventional channel routing methods used for channel inflow-outflow
   conversion have deficiencies which make them difficult to apply for cell
   models. A new, simple, routing element is presented and its properties
   are discussed. The new routing method is described as `rational' because
   it overcomes the deficiencies of other routing methods.}},
