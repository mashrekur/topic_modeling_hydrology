@article{ ISI:A1994QB26800005,
Author = {ROBINS, NS and SMEDLEY, PL},
Title = {{HYDROGEOLOGY AND HYDROGEOCHEMISTRY OF A SMALL, HARD-ROCK ISLAND - THE
   HEAVILY STRESSED AQUIFER OF JERSEY}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{163}},
Number = {{3-4}},
Pages = {{249-269}},
Month = {{DEC 15}},
Abstract = {{The fractured basement aquifer of Jersey provides 30\% of the total
   water needs of the island plus baseflow to surface catchment storage. A
   3-year field study has attempted to describe the groundwater resources
   in terms of quantity and quality. Borehole yields are typically less
   than 11 s(-1) the island-wide estimate of transmissivity is 3 m(3)
   day(-1) and effective aquifer thickness is 30-40 m although deeper
   circulation occurs in selected fracture systems. It is estimated that
   much of the renewable resource is exploited. Groundwaters are
   predominantly derived from recent recharge, most being oxidising with
   measurable tritium contents. There is widespread pollution from
   agricultural nutrients but natural denitrification is apparent in some
   areas.}},
