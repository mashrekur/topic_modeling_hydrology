@article{ ISI:A1994NL36500011,
Author = {ONOF, C and WHEATER, HS and ISHAM, V},
Title = {{NOTE ON THE ANALYTICAL EXPRESSION OF THE INTEREVENT TIME CHARACTERISTICS
   FOR BARTLETT-LEWIS TYPE RAINFALL MODELS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{157}},
Number = {{1-4}},
Pages = {{197-210}},
Month = {{MAY}},
Abstract = {{The random parameter Bartlett-Lewis rectangular pulses model has been
   shown to be an effective tool in reproducing the features of the
   rainfall process at various time-scales, including features of the
   time-series of events. The current paper yields exact expressions of
   these features, i.e. the distribution of the inter-event time, the
   number of events per month and the average duration of rainfall events.
   Equations are given for both this model and the simpler Bartlett-Lewis
   rectangular pulses model. These equations are applied to the case of
   rainfall data from Birmingham (UK) to provide optimal parameters for the
   random parameter model.}},
