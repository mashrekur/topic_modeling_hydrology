@article{ ISI:A1993LQ11800004,
Author = {CHAKRAPANI, GJ and SUBRAMANIAN, V},
Title = {{RATES OF EROSION AND SEDIMENTATION IN THE MAHANADI RIVER BASIN, INDIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{149}},
Number = {{1-4}},
Pages = {{39-48}},
Month = {{AUG}},
Abstract = {{The Mahanadi river basin extends over an area of 1.42 x 10(5) km2 and
   discharges about 15.74 x 10(6) t of sediment to the Bay of Bengal. The
   geology of the basin is mostly characterised by Precambrian rocks of the
   Eastern Ghats, rocks associated with the Gondwana supercontinent and
   recent littoral deposits. Sampling was done to determine chemical
   erosion, physical erosion and sedimentation rate in the basin. Carbonate
   rock weathering is the dominant control on the chemical composition of
   the river water. There is a spatial and temporal variation in water and
   sediment flow. The chemical erosion rate for individual elements at
   different sites has been established. Sedimentation rate deter-mined by
   the Pb-210 isotope method in core samples gives rates of 5.08 to 20.39
   mm year-1. This high sedimentation rate is attributed to local
   anthropogenic activities carried out in the basin.}},
