@article{ ISI:A1994NY36300014,
Author = {FERGUSON, RI and TRUDGILL, ST and BALL, J},
Title = {{MIXING AND UPTAKE OF SOLUTES IN CATCHMENTS - MODEL DEVELOPMENT}},
