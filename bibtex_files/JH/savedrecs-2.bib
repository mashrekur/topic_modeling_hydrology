@article{ ISI:A1993MP97200007,
Author = {LAPCEVIC, PA and NOVAKOWSKI, KS and PAILLET, FL},
Title = {{ANALYSIS OF FLOW IN AN OBSERVATION WELL INTERSECTING A SINGLE FRACTURE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{151}},
Number = {{2-4}},
Pages = {{229-239}},
Month = {{NOV}},
Abstract = {{A semi-analytical model is developed to determine transmissivity and
   storativity from the interpretation of transient flow in an observation
   well due to pumping in a source well where the two wells are connected
   by a single fracture. Flow rate can be determined using a heat-pulse
   flowmeter located above the intersection of the fracture in the
   observation well. The results of a field experiment were interpreted
   using the new model and compared with drawdown data from the same test.
   Good agreement between the transmissivity estimates was observed whereas
   estimates of storativity were found to be better determined from the
   analysis of flow rate.}},
