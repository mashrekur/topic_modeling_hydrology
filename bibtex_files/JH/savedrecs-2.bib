@article{ ISI:A1993MQ12700008,
Author = {MATSUBAYASHI, U and VELASQUEZ, GT and TAKAGI, F},
Title = {{HYDROGRAPH SEPARATION AND FLOW-ANALYSIS BY SPECIFIC ELECTRICAL
   CONDUCTANCE OF WATER}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{152}},
Number = {{1-4}},
Pages = {{179-199}},
Month = {{DEC}},
Abstract = {{The use of the specific electrical conductance of water to separate flow
   hydrographs into event (new) and pre-event (old) waters has been studied
   through the analysis of background events and flow process phenomena in
   a study basin. Runoff ratio analysis shows that background events, such
   as periods of no rainfall or no flow, influence the percentage of old
   water occurring in the hydrograph. Comparison of one result with a
   separation using deltaO-18 as tracer shows that the separation model of
   Matsubayashi et al. (1991) produces reasonable results. The analysis of
   the conductance-discharge (C-Q) relationship showed that there were fast
   flows in a majority of the wet season hydrographs.}},
