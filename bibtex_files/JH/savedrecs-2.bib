@article{ ISI:A1993LD07000005,
Author = {ROBERTS, G and FRANCE, M and JOHNSON, RC and LAW, JT},
Title = {{THE ANALYSIS OF REMOTELY-SENSED IMAGES OF THE BALQUHIDDER CATCHMENTS FOR
   ESTIMATION OF PERCENTAGES OF LAND COVER TYPES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{145}},
Number = {{3-4}},
Pages = {{259-265}},
Month = {{MAY 15}},
Abstract = {{Remotely sensed images taken with both aircraft and satellite-borne
   sensors have been analysed to estimate the percentage cover of the
   vegetative types in the Balquhidder catchments. Detailed ground surveys
   have been carried out to provide `ground truth' training areas for the
   image classifications. Good agreement was obtained between the extents
   of different vegetation cover as given by the ground survey and analysis
   of the images. The value of remotely sensed images for vegetation
   classification for water balance, and other purposes, in difficult
   terrain is demonstrated.}},
