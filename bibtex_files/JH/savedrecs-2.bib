@article{ ISI:A1994NU68100009,
Author = {GOVINDARAJU, RS and KAVVAS, ML},
Title = {{A SPECTRAL APPROACH FOR ANALYZING THE RILL STRUCTURE OVER HILLSLOPES .2.
   APPLICATION}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{158}},
Number = {{3-4}},
Pages = {{349-362}},
Month = {{JUN 15}},
Abstract = {{This paper deals with examining the bandwidth of the spectral density
   functions of rill profiles from transect measurements at three
   cross-slope locations over decomposed granite hillslopes in Northern
   California. Parameter estimation for the probabilistic description of
   rill heights under the narrow-banded spectrum assumption was then
   considered. The theoretical predictions of the rill structure agree
   fairly well with-the corresponding observed results. Estimation of
   extreme-valued rills and significant rill heights is addressed to
   quantify the severity of the rilled hillslope surface. It is concluded
   that the spectral approach is a valid method for quantifying the rill
   structure for use in physical models of water flow and sediment
   transport over rilled hillslope surfaces.}},
