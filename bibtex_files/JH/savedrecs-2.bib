@article{ ISI:A1994MZ15700002,
Author = {SKJELKVALE, BL and WRIGHT, RF},
Title = {{RESPONSE OF RUNOFF CHEMISTRY TO EXPERIMENTAL ADDITION OF LIF TO A
   PRISTINE CATCHMENT AT SOGNDAL, NORWAY}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{154}},
Number = {{1-4}},
Pages = {{19-34}},
Month = {{FEB}},
Abstract = {{Fluoride is a strong acid anion and a constituent of acid rain. Fluoride
   concentrations are elevated in surface waters in acidified areas in
   Norway. In July 1991, 100 muequiv l-1 fluoride in 15 mm artificial
   precipitation were added to a 0.38 ha pristine catchment at Sogndal,
   western Norway, to investigate fluoride mobility and its effects on
   runoff chemistry, in particular aluminium. Runoff chemistry responded
   with increased levels of Li and F, increased pH and concentrations of
   Al, but generally lower concentrations of the other major ions. About
   1.4\% of the added Li and 0.2\% of the added F left the catchment in
   runoff during the 14 days after treatment. The concurrent increase in pH
   and inorganic Al suggests anion exchange between the added F and OH
   adsorbed on the soil. The results further indicate that F may be
   mobilized by acidification, with subsequent complexation with inorganic
   Al.}},
