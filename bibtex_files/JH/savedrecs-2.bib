@article{ ISI:A1994ND64500002,
Author = {ROWNTREE, PR and LEAN, J},
Title = {{VALIDATION OF HYDROLOGICAL SCHEMES FOR CLIMATE MODELS AGAINST CATCHMENT
   DATA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1994}},
Volume = {{155}},
Number = {{3-4}},
Pages = {{301-323}},
Month = {{MAR 15}},
Note = {{16th General Assembly of the European-Geophysical-Society: Mesoscale
   Hydrology and General Circulation Models, WIESBADEN, GERMANY, APR 22-26,
   1991}},
Organization = {{EUROPEAN GEOPHYS SOC}},
Abstract = {{The effects of two hydrological parametrization schemes have been
   assessed  in the framework of a one-dimensional (column) version of such
   a model by comparing the results with observed data for catchments in
   southern England. The schemes, for use in global circulation models,
   were the standard one from the UK Meteorological Office model and the
   ARNO or Nanjing model. The ARNO scheme is designed to represent the
   heterogeneity of the hydrological properties over the area, whereas the
   standard scheme has hydrological characteristics based on data for
   single points. The standard scheme gave deficient runoff through most of
   the year, particularly for the summer and autumn. The ARNO scheme was
   found to be capable, with careful choice of model parameters, of
   simulating the main seasonal variation in runoff for the catchments
   considered. The principal residual problem is that runoff is deficient
   in summer. This probably reflects the difficulty of representing a
   complex hydrological system with one soil moisture variable. In
   particular, to simulate runoff correctly, groundwater storage needs to
   be modelled. The results also indicated possible improvements in other
   aspects of the model including the specification of stomatal resistance
   and the inclusion of aerosols.}},
