@article{ ISI:A1993KV20000008,
Author = {KELLY, WE and BOGARDI, I},
Title = {{DETERMINING AQUIFER PARAMETERS FROM THE IMPULSE-RESPONSE TO PUMPING}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{144}},
Number = {{1-4}},
Pages = {{155-164}},
Month = {{APR}},
Abstract = {{The impulse response of non-leaky and leaky confined aquifers is used to
   determine aquifer properties. The impulse response is determined by
   differentiating the response observed from a constant rate pumping test.
   A linearized form of the impulse response is then fitted using
   least-squares regression to determine values for transmissivity, storage
   coefficient, and leakance. This approach appears to be best suited for a
   preliminary analysis. Estimated values can then be refined or checked by
   calculating complete time-draw-down responses. Advantages of the impulse
   method include its simplicity and that there is no requirement for
   starting values.}},
