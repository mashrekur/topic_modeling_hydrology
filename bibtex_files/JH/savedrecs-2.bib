@article{ ISI:A1993LK25600007,
Author = {LUNGU, EM},
Title = {{DETECTION OF CHANGES IN RAINFALL AND RUNOFF PATTERNS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{147}},
Number = {{1-4}},
Pages = {{153-167}},
Month = {{JUN 15}},
Abstract = {{This study had two main objectives. First, a simple model has been
   assumed for time series consisting of trend, periodic, autoregressive
   and random residual components, which have been used to search for
   significant changes in the components of a number of runoff time series.
   Second, the potential of topological conjugacy to identify changes in
   the dynamics of a system has been investigated. It is shown that the
