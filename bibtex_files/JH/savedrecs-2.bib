@article{ ISI:A1993MC28100014,
Author = {ADAMS, MB and EDWARDS, PJ and WOOD, F and KOCHENDERFER, JN},
Title = {{ARTIFICIAL WATERSHED ACIDIFICATION ON THE FERNOW EXPERIMENTAL FOREST,
   USA}},
