@article{ ISI:A1993LK25600005,
Author = {SZYMKIEWICZ, R},
Title = {{SOLUTION OF THE INVERSE PROBLEM FOR THE SAINT-VENANT EQUATIONS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1993}},
Volume = {{147}},
Number = {{1-4}},
Pages = {{105-120}},
Month = {{JUN 15}},
Abstract = {{This paper presents the formulation of the inverse problem for the Saint
   Venant equations. It appears that this problem can be posed only for
   subcritical channel flow. The equations of unsteady flow are integrated
   in the domain 0 less-than-or-equal-to t less-than-or-equal-to T and x
   less-than-or-equal-to 0 and solved using an implicit four-point
   difference scheme. The effect of the weighting parameters of this scheme
   on the results is discussed. The proposed approach is illustrated by
   numerical experiments for arbitrarily chosen data. The results confirm
   the possibility of solution of an inverse flow routing problem.}},
