@article{ ISI:000392767000036,
Author = {Bhanja, Soumendra N. and Rodell, Matthew and Li, Bailing and Saha,
   Dipankar and Mukherjee, Abhijit},
Title = {{Spatio-temporal variability of groundwater storage in India}},
