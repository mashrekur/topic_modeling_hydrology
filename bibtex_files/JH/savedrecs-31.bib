@article{ ISI:000398871100041,
Author = {Kim, Hanbeen and Kim, Sooyoung and Shin, Hongjoon and Heo, Jun-Haeng},
Title = {{Appropriate model selection methods for nonstationary generalized
   extreme value models}},
