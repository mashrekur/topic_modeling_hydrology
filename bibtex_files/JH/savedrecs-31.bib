@article{ ISI:000395607700040,
Author = {Libera, Arianna and de Barros, Felipe P. J. and Guadagnini, Alberto},
Title = {{Influence of pumping operational schedule on solute concentrations at a
   well in randomly heterogeneous aquifers}},
Journal = {{JOURNAL OF HYDROLOGY}},
