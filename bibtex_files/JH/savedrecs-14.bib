@article{ ISI:000252562500014,
Author = {Johnson, Andrew C. and Juergens, Monika D. and Williams, Richard J. and
   Kuemmerer, Klaus and Kortenkamp, Andreas and Sumpter, John P.},
Title = {{Do cytotoxic chemotherapy drugs discharged into rivers pose a risk to
   the environment and human health? An overview and UK case study}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2008}},
Volume = {{348}},
Number = {{1-2}},
Pages = {{167-175}},
Month = {{JAN 1}},
Abstract = {{This opinion paper assesses for the UK, whether the current use of
   cytotoxic drugs, one of the most toxic pharmaceuticals in common use,
   could pose a risk to aquatic organisms and to humans through water
   recycling. A water quality model was set up for one of these drugs,
   5-fluorouracil, in the Aire and Calder catchment in Northern UK. The
   study predicts 5-50 ng/L concentrations for long stretches of this
   catchment under tow flow conditions. Due to their mode of action,
   practically all. eukaryotic organisms are vulnerable to damage, with
   teratogenicity being the greatest concern at such levels. However, it is
   unclear to what extent the predicted low concentrations would affect
   flora and fauna in receiving waters but there may be an additive effect
   of a mixture of cytotoxic drugs which should be taken into account. The
   exposure of the pregnant mother, or more specifically her foetus, to
   these drugs via drinking water should be minimised. Current drinking
   water purification technology gives grounds for optimism on removal of
   these compounds, but no appropriate data exist yet. (C) 2007 Elsevier
   B.V. All rights reserved.}},
