Author = {Gribovszki, Zoltan and Kalicz, Peter and Szilagyi, Jozsef and Kucsara,
   Mihaly},
Title = {{Riparian zone evapotranspiration estimation from diurnal groundwater
   level fluctuations}},
Journal = {{JOURNAL OF HYDROLOGY}},
