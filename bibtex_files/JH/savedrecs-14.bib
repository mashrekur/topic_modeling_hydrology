@article{ ISI:000254773800009,
Author = {Martinez, Christopher J. and Campbell, Kenneth L. and Annable, Michael
   D. and Kiker, Gregory A.},
Title = {{An object-oriented hydrologic model for humid, shallow water-table
   environments}},
Journal = {{JOURNAL OF HYDROLOGY}},
