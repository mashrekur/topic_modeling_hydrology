@article{ ISI:000254095100010,
Author = {Ceballos-Barbancho, Antonio and Moran-Tejeda, Enrique and Angel
   Luengo-Ugidos, Miguel and Manuel Llorente-Pinto, Jose},
Title = {{Water resources and environmental change in a Mediterranean environment:
   The south-west sector of the Duero river basin (Spain)}},
