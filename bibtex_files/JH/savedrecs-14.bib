@article{ ISI:000254095100012,
Author = {Bormann, Helge},
Title = {{Sensitivity of a soil-vegetation-atmosphere-transfer scheme to input
   data resolution and data classification}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{2008}},
Volume = {{351}},
