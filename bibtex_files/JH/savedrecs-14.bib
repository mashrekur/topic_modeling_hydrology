@article{ ISI:000254095100002,
Author = {Harman, Ciaran and Stewardson, Michael and DeRose, Ronald},
Title = {{Variability and uncertainty in reach bankfull hydraulic geometry}},
Journal = {{JOURNAL OF HYDROLOGY}},
