@article{ ISI:A1992KF74500004,
Author = {RAMESH, R and SARIN, MM},
Title = {{STABLE ISOTOPE STUDY OF THE GANGA (GANGES) RIVER SYSTEM}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{139}},
Number = {{1-4}},
Pages = {{49-62}},
Month = {{NOV}},
Abstract = {{Stable hydrogen (deltaD) and oxygen (deltaO-18) isotope ratios of the
   Ganga river system are reported for the first time. High-altitude
   streams (Ganga headwaters) show a deltaD-deltaO-18 relationship close to
   that of the global meteoric water line. Samples from the lowland regions
   show a significant effect of evaporation, indicated by a reduced slope
   (of approximately six) in the deltaD-deltaO-18 plot. There is a
   progressive enrichment of the delta values downstream in the Ganga. The
   delta values of the Ganga headwaters show a change of -0.19 parts per
   thousand (deltaO-18) and -1.6 parts per thousand (deltaD) per 100 m
   increase in altitude (the `altitude effect'), which is shown by a simple
   theoretical calculation to be half that in the local precipitation.}},
