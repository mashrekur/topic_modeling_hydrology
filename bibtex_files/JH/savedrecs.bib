@article{ ISI:A1992HQ00400009,
Author = {STONE, WJ},
Title = {{PALEOHYDROLOGIC IMPLICATIONS OF SOME DEEP SOILWATER CHLORIDE PROFILES,
   MURRAY BASIN, SOUTH-AUSTRALIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{132}},
Number = {{1-4}},
Pages = {{201-223}},
Month = {{MAR}},
Abstract = {{A chloride mass-balance approach to estimating soilwater ages and
   paleorecharge rates was tested in an area where considerable
   paleoclimatic data were available for comparison. Application of the
   method to data from four cores of the unsaturated zone taken in the
   western Murray Basin showed that the soil water at the base of the
   chloride build-up or bulge associated with the root zone of the native
   mallee vegetation was emplaced during the last glacial interval (at
   least 20 000 years BP). Furthermore, recharge rates at this time were
   very low. These observations agree with independent paleoclimate
   evidence that suggest that this time was colder, windier and drier.
   Results are consistent with the notion that the root-zone chloride
   bulge, so typical of the Murray Mallee region, developed in response to
   aridity that peaked during the last full glacial interval in Australia.
   The chloride method is a useful tool in paleohydrology, especially when
   used in concert with other evidence.}},
