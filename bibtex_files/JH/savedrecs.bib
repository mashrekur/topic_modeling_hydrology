@article{ ISI:A1991HH09700002,
Author = {BOERS, TM and VANDERMOLEN, WH and EPPINK, LAAJ and BENASHER, J},
Title = {{EFFECT OF THE THOMSON WEIR AND WEIRBOX ON THE MEASUREMENT OF FLOW-RATES
   FROM MICRO-CATCHMENTS AND RUNOFF PLOTS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{128}},
Number = {{1-4}},
Pages = {{29-39}},
Month = {{NOV}},
Abstract = {{This paper deals with the effect of the Thomson weir and weirbox on
   measured recession curves. Small discharges can be measured accurately
   with a Thomson weir. The variation of the flow rate entering the weirbox
   changes the storage in the box, which thus gives a time lag in the
   measured discharge. Three cases of inflow were investigated: zero
   inflow, exponential recession inflow, and linear recession inflow, as
   predicted by the Zarmi model. Exponential and linear recession were
   described by differential equations, solved numerically through a
   predictor-corrector method. In most cases the time lag is less than 1
   min but at very low discharges the time lag can become important. The
   results presented indicate that the modifying effects of the weirboxes
   on the outflows from these catchments are not sufficient to explain the
   differences between the observed exponential recessions and the linear
   recessions predicted by the Zarmi model.}},
