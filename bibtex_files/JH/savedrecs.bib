@article{ ISI:A1992JW46700016,
Author = {PHILIP, JR},
Title = {{FALLING-HEAD PONDED INFILTRATION WITH EVAPORATION}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{138}},
Number = {{3-4}},
Pages = {{591-598}},
Month = {{OCT}},
Abstract = {{The problem of falling-head ponded infiltration with evaporation is
   solved using the Green-Ampt (or delta-function) model. A significant
   parameter is epsilon/KBAR, the ratio of evaporation rate to mean
   hydraulic conductivity in the wetted region. An illustrative example
   with epsilon/KBAR = 1.633 gives a time for disappearance of water from
   the soil surface only 45.6\% of that for the case epsilon/KBAR = 0. When
   epsilon/KBAR = 1.633, only 57.8\% of the water infiltrates, with 42.2\%
   lost by evaporation.}},
