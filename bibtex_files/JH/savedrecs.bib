@article{ ISI:A1992HW41200004,
Author = {ZHANG, WZ},
Title = {{TRANSIENT GROUNDWATER-FLOW IN AN AQUIFER AQUITARD SYSTEM IN RESPONSE TO
   WATER LEVEL CHANGES IN RIVERS OR CANALS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{133}},
Number = {{3-4}},
Pages = {{233-257}},
Month = {{MAY}},
Abstract = {{Previous authors have studied unsteady flow in aquifer-aquitard systems
   with negligible storativity in the confined aquifer layer. Formulae have
   been developed for groundwater level in the aquifer (or aquitard) and no
   adequate tables and graphs have been provided. In this paper, solutions
   for transient flow in an aquifer-aquitard system that consider
   storativity in a confined layer in response to both abrupt and uniform
   water level changes, as well as steady seepage rate from rivers (or
   canals), are developed. Analytical formulae for groundwater levels both
   in the aquifer and aquitard, and for seepage rate and total amount of
   water percolated from rivers, have been derived. To facilitate
   computation, graphs for the special functions involved in the proposed
   equations have been prepared.}},
