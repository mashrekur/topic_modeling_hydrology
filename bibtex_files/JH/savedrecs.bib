@article{ ISI:A1991HH09700009,
Author = {KITE, GW},
Title = {{A WATERSHED MODEL USING SATELLITE DATA APPLIED TO A MOUNTAIN BASIN IN
   CANADA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{128}},
Number = {{1-4}},
Pages = {{157-169}},
Month = {{NOV}},
Abstract = {{A simple watershed model has been developed which uses Landsat and
   National Oceanic and Atmospheric Administration (NOAA) data to simulate
   basin runoff on a daily basis. The paper describes the application of
   this model to the Kootenay Basin in the Canadian Rocky Mountains and
   compares the results obtained using satellite data with those obtained
   using only ground-based data. Snow cover data from NOAA/Advanced Very
   High Resolution Radiometer (AVHRR) improved the model performance but
   cloud cover data were not found to be helpful in improving estimates of
   precipitation input.}},
