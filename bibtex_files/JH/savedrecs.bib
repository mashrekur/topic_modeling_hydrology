@article{ ISI:A1992HW41200010,
Author = {PAPAMICHAIL, DM and PAPAZAFIRIOU, ZG},
Title = {{MULTIPLE INPUT SINGLE OUTPUT LINEAR FUNCTIONAL MODELS FOR RIVER FLOW
   ROUTING}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{133}},
Number = {{3-4}},
Pages = {{365-377}},
Month = {{MAY}},
Abstract = {{A multiple input-single output linear functional model for river flow
   routing is developed. The linear kernels, corresponding to each input
   series, are approximated by expansions in Meixner functions,
   unit-weighted orthonormal polynomials and Chebyshev polynomials. The
   memory length of the system and the order of polynomial expansion of the
   linear kernels are determined by trial and error. The cross-correlation
   between each input and the output provided a short-cut method of memory
   length determination.
   A two input-single output linear functional model was applied to predict
   daily flow of the Aliakmon river in Greece, using daily flows at points
   further up the main stream and on a tributary. The results were
   satisfactory. The predictions of daily output for real-time forecasting
   were further improved by fitting autoregressive moving average models to
   the residual sequences.}},
