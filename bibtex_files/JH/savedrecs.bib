@article{ ISI:A1991EY07500018,
Author = {CHOUBEY, VK and SUBRAMANIAN, V},
Title = {{SPECTRAL RESPONSE OF SUSPENDED SEDIMENTS IN WATER UNDER CONTROLLED
   CONDITIONS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{122}},
Number = {{1-4}},
Pages = {{301-308}},
Month = {{JAN}},
Abstract = {{A laboratory experiment using a spectral radiometer has determined the
   spectral characteristics of suspended sediment in water under controlled
   conditions in four wavelength bands (0.45-0.9-mu-m) as a function of
   type and concentration of sediment.  The pattern of reflectance of
   natural black and brown sediments from a small catchment in central
   India has been determined, as has the feasible limit of concentration of
   sediments that can be measured in the four wavelength bands used by the
   Indian Remote Sensing Satellite LISS-1.  The spectral response of
   sediment-laden water is more affected by particle size than by the
   concentration of suspended sediment; the spectral response depends upon
   the colour, type, size and mineral composition of the sediments.  When
   the concentration of sediment exceeds 90 or 140 mgl-1 (for black and
   brown sediments respectively), the respective spectral responses become
   poorer.}},
