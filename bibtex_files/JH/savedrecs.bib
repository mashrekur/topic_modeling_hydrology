@article{ ISI:A1992HB82500020,
Author = {WOOD, WL and CALVER, A},
Title = {{INITIAL CONDITIONS FOR HILLSLOPE HYDROLOGY MODELING}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{130}},
Number = {{1-4}},
Pages = {{379-397}},
Month = {{JAN}},
Abstract = {{The initial conditions of slope water in the finite element modelling of
   hillslope hydrology are investigated from mathematical and hydrological
   standpoints. Finite element modelling of both saturated and unsaturated
   Darcian flow is considered, and examples are drawn from a particular
   model, the Institute of Hydrology Distributed Model, version 4. In the
   frequent case of insufficient field data, it is suggested that suitable
   initial conditions include inference from slope discharges using the
   Dupuit-Forchheimer and Lynch methods, or the use of an approximately
   steady baseflow which can be achieved simply. In the latter case the
   hydrological run-in period can be considerably in excess of that
   necessary for solely numerical reasons. It is essential to use a
   suitable finite element mesh, particularly near the bottom of the slope
   where the discharge is calculated.}},
