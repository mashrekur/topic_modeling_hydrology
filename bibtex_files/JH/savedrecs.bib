@article{ ISI:A1992JF93100005,
Author = {PRICE, JS},
Title = {{BLAKET BOG IN NEWFOUNDLAND .1. THE OCCURRENCE AND ACCUMULATION OF
   FOG-WATER DEPOSITS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{135}},
Number = {{1-4}},
Pages = {{87-101}},
Month = {{JUL}},
Abstract = {{Fog deposition on to a blanket bog near Cape Race, Newfoundland, was
   measured with a fog collector and a lysimeter. Fog is most frequently
   produced by warm southwesterly air flow over cold offshore water,
   although cooler northeasterly air is also important. Between 21 June and
   14 July 1989 fog deposition was 50\% of rainfall, averaging 1.8 mm
   day-1, with a maximum daily deposition of 10.2 mm. Evaporation was also
   restricted during fog and rain events; evaporation on days with fog or
   rain averaged < 1.1 mm, compared with 2.5 mm when it was not foggy or
   raining. The addition of water to the surface by fog, combined with low
   evaporation rates may be critical to the development of blanket bog in
   this area.}},
