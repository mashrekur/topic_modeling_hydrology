@article{ ISI:A1992JD69900018,
Author = {BULLOCK, A},
Title = {{THE ROLE OF DAMBOS IN DETERMINING RIVER FLOW REGIMES IN ZIMBABWE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{134}},
Number = {{1-4}},
Pages = {{349-372}},
Month = {{JUN}},
Abstract = {{Analysis of the nature and significance of the influence of dambos upon
   river flows in Zimbabwe is undertaken on a database of up to 110 gauged
   catchments containing different dambo densities, ranging from 1 to 63\%,
   and other catchment characteristics. It is concluded that dambos are
   indiscriminatory factors in determining the volume, persistence and
   variability of annual runoff. Dambos do not maintain dry-season low
   flows, as is widely believed, and reduce low flows where they occur in
   association with regolith with significant base flow components. The
   impact of dambos on flood magnitude, variability and frequency is
   insignificant, with the possible exception of extreme events.}},
