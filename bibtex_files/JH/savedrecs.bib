@article{ ISI:A1992JF93100010,
Author = {SAVENIJE, HHG and PAGES, J},
Title = {{HYPERSALINITY - A DRAMATIC CHANGE IN THE HYDROLOGY OF SAHELIAN ESTUARIES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{135}},
Number = {{1-4}},
Pages = {{157-174}},
Month = {{JUL}},
Abstract = {{Hypersalinity occurs in estuaries where evaporation exceeds rainfall and
   runoff. Generally, hypersaline estuaries occur in arid areas and are
   often used for salt farming. Dramatic ecological changes take place when
   a normal estuary tums hypersaline. This occurred in the Casamance during
   the Sahelian drought of the early 1980s. This paper describes the
   ecological changes that took place and it presents a model which
   describes the processes involved.}},
