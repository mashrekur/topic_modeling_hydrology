@article{ ISI:A1992JD69900010,
Author = {RAO, AR and HSIEH, CH},
Title = {{ACCURACY OF BASIN-WIDE FORECASTS OF STREAMFLOWS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{134}},
Number = {{1-4}},
Pages = {{151-171}},
Month = {{JUN}},
Abstract = {{Estimates of accuracy of streamflow forecasts obtained by using the data
   from all the stations in a basin are examined. The technique used is
   based on empirical orthogonal functions of the data. The accuracy of
   streamflow forecasts based only on previous streamflows or on previous
   rainfall are investigated. The artificial increases in forecast accuracy
   which are brought about by considering longer lags are examined. The
   results indicate that the forecast accuracies are significant for all
   the time scales considered.}},
