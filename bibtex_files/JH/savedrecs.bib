@article{ ISI:A1992HJ04700005,
Author = {BJERG, PL and CHRISTENSEN, TH},
Title = {{SPATIAL AND TEMPORAL SMALL-SCALE VARIATION IN GROUNDWATER QUALITY OF A
   SHALLOW SANDY AQUIFER}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{131}},
Number = {{1-4}},
Pages = {{133-149}},
Month = {{FEB}},
Abstract = {{The groundwater quality of a shallow unconfined sandy aquifer has been
   characterized for pH, alkalinity, chloride, nitrate, sulfate, calcium,
   magnesium, sodium and potassium in terms of vertical and horizontal
   variations (350 groundwater samples).  The test area is located within a
   farmland lot.  The geology of the area described on the basis of 31
   sediment cores appears relatively homogeneous.  Large vertical and
   horizontal variations were observed.  The vertical variations are
   strongly affected by the deviating composition of the agricultural
   infiltration water.  The horizontal variations show very limited
   correlation ranges (less than 10 m) and large variations are found over
   a few metres.  The temporal variations observed over a period of 15
   months were modest.  The observed large variations should be taken into
   account when designing groundwater sampling and monitoring.}},
