@article{ ISI:A1991FU77000008,
Author = {MUKHERJEE, D and MANSOUR, N},
Title = {{A BIVARIATE STOCHASTIC APPROACH TO ESTIMATE RELIABILITY OF A
   PUMPED-STORAGE RESERVOIR}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{125}},
Number = {{3-4}},
Pages = {{293-310}},
Month = {{JUL}},
Abstract = {{The reliability of water supply from a single pumped storage reservoir
   fed by a combination of a natural flow and as pumped flow from a second
   source river is studied.
   Methods are presented to estimate actual monthly pumped flows, given a
   set of constraints imposed by the daily fluctuations in the source flow.
   It is found that a parameter called the `fitting parameter' plays the
   most important role in the identification of the minimum monthly source
   flow necessary in the source for a maximum pumped flow to occur. 
   Further, the low-flow exceedance probabilities are obtained by direct
   integration of the joint probability density function of monthly natural
   and pumped flows:  this takes account of the effect of correlation of
   the two inflows, which have different characteristics.  The results are
   compared with a previous study.}},
