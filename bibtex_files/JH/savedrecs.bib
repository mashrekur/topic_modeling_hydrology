@article{ ISI:A1992JT54700015,
Author = {LU, LH and STEDINGER, JR},
Title = {{SAMPLING VARIANCE OF NORMALIZED GEV PWM QUANTILE ESTIMATORS AND A
   REGIONAL HOMOGENEITY TEST}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{138}},
Number = {{1-2}},
Pages = {{223-245}},
Month = {{SEP}},
Abstract = {{For a three-parameter generalized extreme value (GEV) distribution the
   asymptotic variance of probability weighted moments (PWM) quantile
   estimators has been derived previously. This paper extends these results
   to obtain the asymptotic variance of normalized GEV/PWM quantile
   estimators, which are the at-site quantile estimators divided by the
   sample mean. Monte Carlo simulations provide correction factors for use
   with small samples. Normalized 10-year flood quantile estimators and
   their sample variances are used to construct a regional homogeneity test
   for GEV/PWM index-flood analyses. The new test performed better than the
   R-statistic test proposed before.}},
