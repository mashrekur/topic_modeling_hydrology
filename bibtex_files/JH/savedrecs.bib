@article{ ISI:A1992JD69900009,
Author = {REVFEIM, KJA},
Title = {{DOMINANT EVENTS IN EXTREME RAINFALL RECORDS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{134}},
Number = {{1-4}},
Pages = {{143-149}},
Month = {{JUN}},
Abstract = {{A maximum rainfall over a short interval will sometimes be greater than
   the maximum persisting over a longer interval. Data from this situation
   are usually taken as point values in the analysis of extremes but these
   maxima are interpretable as upper bounds for the longer interval.
   Physically meaningful parameters of a model of rainfall extremes can
   still be estimated from data including upper bounds, by the method of
   maximum likelihood, with little loss of precision compared with the
   analysis of point valued data only. The benefits of recognizing these
   upper bounds are an improved ordering of the estimated average number of
   rainfall events with increasing event duration.}},
