@article{ ISI:A1992JL36800007,
Author = {GIACOMIN, A and TRUCCHI, P},
Title = {{RAINFALL INTERCEPTION IN A BEECH COPPICE (ACQUERINO, ITALY)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{137}},
Number = {{1-4}},
Pages = {{141-147}},
Month = {{AUG 15}},
Abstract = {{In an experimental plot comprising a beech coppice situated in the
   Acquerino forest near Florence (Italy), measurements of precipitation
   (P), throughfall (T) and stemflow (S) were made over a period of 12
   years (1978-1989). The rainfall has been sorted into classes and the
   interception values have been calculated. For each class, percentage
   values of T, S and I have been determined. In this classification, two
   periods have been distinguished according to the leaf index of the
   canopy cover: the first, called `uncovered', has a leaf index ranging
   from 0 to 80\% of the maximum; the second, called `covered', has an
   index superior to 80\%. The different values of P, T, S and I are
   explained for each period. The variations of the parameters are shown
   for two sets of events characterized by different height, duration and
   intensity.}},
