@article{ ISI:A1991FH58900011,
Author = {MISHRA, BK and MISRA, C},
Title = {{LEACHING OF AMMONIUM-NITRATE UNDER FIELD CONDITIONS - STUDIES ON
   KINETICS OF NITRIFICATION AND NITRATE REDUCTION IN AN ULTISOL PROFILE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{124}},
Number = {{1-2}},
Pages = {{185-195}},
Month = {{APR}},
Abstract = {{The simultaneous occurrence of nitrification and nitrate reduction was
   studied by leaching solution pulses of NH4NO3 through 9 cm long columns
   and natural field profile of Bhubaneswar sandy loam soil (Ultisol). 
   Employing known analytical solutions of transport equations relevant to
   conditions induced for the leaching experiments, the nitrate reduction
   rate constant, k2, was estimated to be 0.072h-1 at 28-degrees-C based on
   the breakthrough curve (BTC) of NO3-(-)N stemming from the soil column
   through which Ca(NO3)2 was leached.  Using this value of k2, the value
   of nitrification rate constant, k1 was estimated to be 0.06h-1 by curve
   fitting the NO3-(-)N BTC derived from NH4NO3 leaching in an equivalent
   soil column.  Using values of k2 close to that derived from the
   laboratory column studies, k1 was evaluated to be 0.1h-1 and 0.04h-1 for
   0-30 and 0-50 cm soil layers, respectively, under field situations.}},
