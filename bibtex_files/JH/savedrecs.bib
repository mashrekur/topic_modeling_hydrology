@article{ ISI:A1991HH09700018,
Author = {SHARMA, ML and TANIGUCHI, M},
Title = {{MOVEMENT OF A NONREACTIVE SOLUTE TRACER DURING STEADY AND INTERMITTENT
   LEACHING}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{128}},
Number = {{1-4}},
Pages = {{323-334}},
Month = {{NOV}},
Abstract = {{Laboratory experiments were conducted using two soils (Bassendean sand
   and Collie loam) in the unsaturated state to investigate the effect of
   the mode of water application (steady state versus intermittent) on the
   leaching of a pulse of conservative tracer (bromide). With the same
   amount of water applied for leaching, the downward movement of the
   solute peak was found to be significantly different under the two
   conditions. This difference could be ascribed to different effective
   soilwater contents during leaching as well as to different modes of
   water application. The comparisons made at similar average soilwater
   content showed that the movement of the solute peak was much faster
   under the steady state than the intermittent application. These
   intermittent effects were probably caused by hysteresis, and were more
   pronounced in the loam than the sand. The implications of these results
