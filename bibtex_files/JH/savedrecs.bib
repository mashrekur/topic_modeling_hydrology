@article{ ISI:A1991FR89800003,
Author = {SEN, Z},
Title = {{ON THE PROBABILITY OF THE LONGEST RUN LENGTH IN AN INDEPENDENT SERIES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{125}},
Number = {{1-2}},
Pages = {{37-46}},
Month = {{JUN}},
Abstract = {{Various problems concerning the analysis of droughts lead to the problem
   of finding the probability distribution function (PDF), of the longest
   run lengths in a series of finite length.  Unfortunately, the exact PDF
   of such runs is not available in the literature.  This paper is
   concerned with the derivation of the exact PDF of the longest run length
   to be expected in an independent series.  In the derivation of the PDF,
   different definitions of runs encountered in the hydrology literature
   have been considered.  The analytical derivations are solved numerically
   on digital computers and the results are presented in forms of
   cumulative PDF and the expected maximum run length changes with the
   sample lengths.  The application of the methodology developed herein is
   presented for three annual flow series.}},
