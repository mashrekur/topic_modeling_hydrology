@article{ ISI:A1991FH58900002,
Author = {SOULIOS, G},
Title = {{CONTRIBUTION TO THE STUDY OF THE RECESSION CURVES OF KARSTIC SPRINGS -
   EXAMPLES FROM GREECE}},
