@article{ ISI:A1992JF93100018,
Author = {KACHROO, RK and NATALE, L},
Title = {{NONLINEAR MODELING OF THE RAINFALL RUNOFF TRANSFORMATION}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{135}},
Number = {{1-4}},
Pages = {{341-369}},
Month = {{JUL}},
Abstract = {{This work is concerned with investigating the existence of a systematic
   relationship between the residuals of a simple linear rainfall-runoff
   model and the observed discharge as evidence of non-linearity. A
   modification to the simple linear model is proposed, based on the
   evidence of non-linearity obtained when the model is applied to five
   catchments. The results of applying the modified model are found to be
   satisfactory. A simple calibration procedure for the modified model is
   suggested. The suitability of the proportion of variance accounted for
   in the verification period, as a model evaluation criterion, is
   discussed and it is concluded that the criterion is unsuitable for use
   within the verification periods. Even for a consistent model, the values
   of R2 can vary greatly if the calibration period is unusually wet and
   the verification period unusually dry.}},
