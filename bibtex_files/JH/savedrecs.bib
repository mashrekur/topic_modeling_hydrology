@article{ ISI:A1991HH09700014,
Author = {SKLASH, MG and MWANGI, MP},
Title = {{AN ISOTOPIC STUDY OF GROUNDWATER SUPPLIES IN THE EASTERN PROVINCE OF
   KENYA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{128}},
Number = {{1-4}},
Pages = {{257-275}},
Month = {{NOV}},
Abstract = {{Although the Eastern Province of Kenya is arid to semi-arid with limited
   surface water resources, little is known about the groundwater
   resources. We interpreted environmental-isotope (oxygen-18, deuterium,
   tritium) concentrations in selected groundwater supplies, streams, and
   springs to determine where, when, and how recharge occurs. The ground
   water in the Eastern Province is recharged from rainfall, which, except
   for selected highland areas, has undergone significant evaporation
   during the precipitation process. Ground water recharges directly from
   rainfall in the more humid highland areas whereas in drier lowland
   areas, recharge is from ephemeral streams and possibly by lateral
   groundwater flow from high areas. Based on a comparison of deuterium and
   chloride concentrations, groundwater recharge appears to be rapid with
   little further evaporation. Ground water with post 1953 recharge is
   common in the dry northeast but surprisingly rare in the humid highlands
   in the south.}},
