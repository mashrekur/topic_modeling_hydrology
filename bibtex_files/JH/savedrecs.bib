@article{ ISI:A1992JL36800009,
Author = {BOONSTRA, J},
Title = {{AQUIFER TESTS WITH PARTIALLY PENETRATING WELLS - THEORY AND PRACTICE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{137}},
Number = {{1-4}},
Pages = {{165-179}},
Month = {{AUG 15}},
Abstract = {{Two solutions to the problem of groundwater flow to partially
   penetrating wells are presented. The first solution is the well-known
   and widely used Hantush modification of the Theis method. The second is
   based on the author's recent work; it consists of a matching procedure
   between the drawdowns observed in the field and the theoretical
   drawdowns found from the analysis. This solution is only possible when
   specially developed software is used.
   Both solutions are described and compared with a theoretical data set.
   The comparison reveals important sensitivities in the estimate of the
   thickness of the tested aquifer. The author is of the opinion that the
   application of the Theis-Hantush method may lead to a serious
   overestimate of the aquifer thickness. These sensitivities are also
   illustrated using data from a field test in Pakistan.}},
