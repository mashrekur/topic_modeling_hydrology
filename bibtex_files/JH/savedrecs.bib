@article{ ISI:A1991FR89800004,
Author = {SEN, Z},
Title = {{NOTE ON THE CYCLIC FEATURES IN CUMULATIVE DEPARTURES OF ANNUAL FLOW
   SERIES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{125}},
Number = {{1-2}},
Pages = {{47-54}},
Month = {{JUN}},
Abstract = {{Harmonic analysis of the cumulative departures of annual flow series
   from their sample mean values have been performed in detail.  The main
   purpose was to obtain a mathematical model of the cumulative departures
   that would preserve the Hurst phenomenon.  As a result of the analysis
   it has been observed that in the cumulative departures there exist
   strong periodicities with the slowest cycles having periods equal to the
   total sample length.  The cyclic features account for more than 95\% of
   the variability in the Hurst coefficients.  An alternative to direct
   simulation of annual sequences for preserving the Hurst phenomenon is
   suggested in this paper.  First the cumulative departures should be
   simulated and then the annual flow series should be obtained by
   successive differencing of the generated cumulative departures.}},
