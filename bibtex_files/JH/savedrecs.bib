@article{ ISI:A1992HQ00400010,
Author = {RUSHTON, KR and OWEN, M and TOMLINSON, LM},
Title = {{THE WATER-RESOURCES OF THE GREAT OOLITE AQUIFER IN THE THAMES BASIN, UK}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{132}},
Number = {{1-4}},
Pages = {{225-248}},
Month = {{MAR}},
Abstract = {{The conflicting requirements of using ground water for maintaining river
   flows or providing water supply by direct pumping is examined for the
   Middle Jurassic limestone aquifers of the Cotswolds. The hydrology and
   hydrogeology of the Great Oolite aquifer are reviewed to identify the
   important flow mechanisms and ascertain the aquifer's resources. A
   mathematical model of the aquifer is described which reproduces the
   historical response and then is used to examine the viability of target
   abstractions and their effect on stream flows.}},
