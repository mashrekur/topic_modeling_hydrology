@article{ ISI:A1992JT54700010,
Author = {SZILAGYI, J},
Title = {{WHY CAN THE WEIGHTING PARAMETER OF THE MUSKINGUM CHANNEL ROUTING METHOD
   BE NEGATIVE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{138}},
Number = {{1-2}},
Pages = {{145-151}},
Month = {{SEP}},
Abstract = {{For a typical river reach, observations often show that the peak value
   of the outflow is lower than the synchronous value of the inflow. This
   is caused by the adoption of simplified rating curves or/and the
   discrete nature of water stage measurements. In this situation, the most
   accurate estimation of storage in the reach, using the conventional
   Muskingum method, is when the weighting parameter theta is negative.}},
