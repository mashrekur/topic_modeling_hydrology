@article{ ISI:A1991GN04500018,
Author = {NEAL, C and ROBSON, AJ and HALL, RL and RYLAND, G and CONWAY, T and
   NEAL, M},
Title = {{HYDROLOGICAL IMPACTS OF HARDWOOD PLANTATION IN LOWLAND BRITAIN -
   PRELIMINARY FINDINGS ON INTERCEPTION AT A FOREST EDGE, BLACK WOOD,
   HAMPSHIRE, SOUTHERN ENGLAND}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{127}},
Number = {{1-4}},
Pages = {{349-365}},
Month = {{OCT}},
Abstract = {{Throughfall, stemflow and net rainfall data are presented for a network
   of collectors located in a beech plantation in southern England.
   Statistical analysis of the results shows: (a) average net rainfall is
   independent of distance from the forest edge (20-350 m); (b)
   interception loss averages about 12\% in both the leafed and unleafed
   periods; (c) stemflow increases as a proportion of net rainfall from 2\%
   in the leafed period to 6\% in the unleafed period; (d) a block design
   approach is important for assessing interception losses: localised
   rainfall inputs are observed; systematic differences are seen between
   similar localities.}},
