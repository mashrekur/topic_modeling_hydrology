@article{ ISI:A1991FU77000003,
Author = {WHEATER, HS and ONOF, C and BUTLER, AP and HAMILTON, GS},
Title = {{A MULTIVARIATE SPATIAL-TEMPORAL MODEL OF RAINFALL IN SOUTHWEST
   SAUDI-ARABIA .2. REGIONAL-ANALYSIS AND LONG-TERM PERFORMANCE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{125}},
Number = {{3-4}},
Pages = {{201-220}},
Month = {{JUL}},
Abstract = {{In the preceding paper a stochastic multivariate model of the spatial
   distribution of hourly rainfall, developed for five basins in southwest
   Saudi Arabia, is described and tested with respect to 2 years of
   detailed spatial data.  In this paper, long-term performance is
   evaluated in the context of regional analysis, as, in general, long-term
   gauges lie outside the study basins.  The calibration data are shown to
   have lower than average rainday occurrence and the model is empirically
   adjusted.  Extreme value characteristics of hourly rainfall are
   reasonably reproduced in the regional context.  Some under-estimation of
   daily rainfall is shown to be associated with a small but distinct
   population of very high occurrences.}},
