@article{ ISI:A1992JT54700009,
Author = {PIONKE, HB and DEWALLE, DR},
Title = {{INTRASTORM AND INTERSTORM O-18 TRENDS FOR SELECTED RAINSTORMS IN
   PENNSYLVANIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{138}},
Number = {{1-2}},
Pages = {{131-143}},
Month = {{SEP}},
Abstract = {{Rainfall was collected in central Pennsylvania from 33 storms during
   autumn 1989 and spring 1990. The variability of O-18 content among 33
   storms and within 10 storms is described and also examined in terms of
   weather type (air mass source and frontal activity) and storm
   characteristics (rainfall amount, intensity and duration). From storm to
   storm, the O-18 content expressed as deltaO-18 varied greatly (+2.79 to
   -24.65 parts per thousand), but showed greatest variability for the
   smaller rainstorms with low intensities. Also, O-18 depletion was
   greatest for storms of Atlantic (-10.9 parts-per thousand) vs. Gulf
   (-6.2 parts per thousand) origin. Within a storm, the O-18 content
   varied greatly, but usually was depleted as the storm continued. For one
   storm, the deltaO-18 decreased from -4.00 to -20.00 parts per thousand
   over a 10 h span of continuous rainfall. The most rapid changes in
   deltaO-18 (2-6 parts per thousand h-1) within a storm occurred mostly
   during storm intervals characterized by rapidly increasing rainfall
   intensities associated with moderate to high rainfall rates (more than 5
   mm h-1). When these bursts occurred within longer storms, the most rapid
   changes in O-18 content were associated with the higher rainfall
   intensities.}},
