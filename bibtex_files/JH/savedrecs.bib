@article{ ISI:A1992JD69900002,
Author = {KITCHEN, M and BLACKALL, RM},
Title = {{REPRESENTATIVENESS ERRORS IN COMPARISONS BETWEEN RADAR AND GAUGE
   MEASUREMENTS OF RAINFALL}},
