@article{ ISI:A1991GZ66000011,
Author = {CALVACHE, ML and PULIDOBOSCH, A},
Title = {{SALTWATER INTRUSION INTO A SMALL COASTAL AQUIFER (RIO VERDE, ALMUNECAR,
   SOUTHERN SPAIN)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1991}},
Volume = {{129}},
Number = {{1-4}},
Pages = {{195-213}},
Month = {{DEC}},
Abstract = {{A mathematical simulation of the contact zone between fresh water and
   salt water in the Rio Verde aquifer via two models leads to a
   satisfactory understanding of the development over time of a saline
   wedge. One of the two models is a finite-element model, taking both
   fluids to be immiscible; the other is a finite-difference model, which
   considers the zone as being a single phase of variable density. Whereas
   the latter, mass-transport model, gives more accurate information about
   the composition of the mixing zone, the sharp interface model, although
   somewhat less precise, gives a 3-D picture of the freshwater-saltwater
   contact. The application of both models leads to the conclusion that the
   saline wedge is gradually encroaching inland.}},
