@article{ ISI:A1992JJ21700001,
Author = {HAKTANIR, T},
Title = {{COMPARISON OF VARIOUS FLOOD FREQUENCY-DISTRIBUTIONS USING ANNUAL FLOOD
   PEAKS DATA OF RIVERS IN ANATOLIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{136}},
Number = {{1-4}},
Pages = {{1-31}},
Month = {{AUG}},
Abstract = {{The two-parameter log-normal, three-parameter log-normal, smemax,
   two-step-power, log-Boughton, Gumbel, general extreme-value, Pearson-3,
   log-Pearson-3, log-logistic, and Wakeby distributions were applied to
   the annual flood peaks series greater-than-or-equal-to 30 observations
   of 45 unregulated streams in Anatolia. The parameters of most of these
   distributions were estimated by methods of. (1) moments; (2)
   maximum-likelihood; (3) probability-weighted moments. Methods of
