@article{ ISI:A1992JT54700005,
Author = {BANGOY, LM and BIDAUX, P and DROGUE, C and PLEGAT, R and PISTRE, S},
Title = {{A NEW METHOD OF CHARACTERIZING FISSURED MEDIA BY PUMPING TESTS WITH
   OBSERVATION WELLS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1992}},
Volume = {{138}},
Number = {{1-2}},
Pages = {{77-88}},
Month = {{SEP}},
Abstract = {{An analogy has been defined between the relation s(r,t) = At(N) and the
   elementary solution derived from the general flow equation describing
   fluid flow in fissured media. Assuming a non-integer flow dimension,
   that analogy can be used to characterize a fissured medium by parameters
   that are similar to those describing a porous reservoir, if at least two
   observation wells are available, and if flow involves a sufficient
   number of fractures. The flow system behaves as a fractal object. With
   observation wells located along axes in several directions, anisotropy
   can be characterized. Two field applications illustrate the proposed
   method.}},
