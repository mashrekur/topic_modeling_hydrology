@article{ ISI:A1995TG71300017,
Author = {GOVINDARAJU, RS},
Title = {{NONDIMENSIONAL ANALYSIS OF A PHYSICALLY-BASED RAINFALL-RUNOFF-EROSION
   MODEL OVER STEEP SLOPES}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{173}},
Number = {{1-4}},
Pages = {{327-341}},
Month = {{DEC}},
Abstract = {{A non-dimensional analysis is undertaken to study the
   rainfall-runoff-esosion process over steep slopes. This analysis leads
   to a reduction in the number of parameters required for modeling erosion
   by overland flow. Two important parameters are identified: the
   dimensionless erodibility sigma{*} denoting the average erodibility over
   the whole slope, and G which partitions the sediment transport dynamics
   into detachment, deposition and equilibrium regimes. Analytical
   solutions are presented for the non-dimensional equations. It is
   observed that G remains practically time-invariant but has a marked
   spatial variation along the slope. The dimensionless sediment discharge
   along the slope shows strong dependence on both sigma{*} and G. Some
   asymptotic results are also discussed along with practical applications
   of this work.}},
