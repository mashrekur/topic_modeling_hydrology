@article{ ISI:A1996UB59900018,
Author = {Mackay, R and Cooper, TA and Metcalfe, AV and OConnell, PE},
Title = {{Contaminant transport in heterogeneous porous media: A case study .2.
   Stochastic modelling}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{175}},
Number = {{1-4}},
Pages = {{429-452}},
Month = {{FEB}},
Abstract = {{A stochastic modelling approach has been used to assess the uncertainty
   in predictions of contaminant migration in geological media surrounding
   a hypothetical buried waste repository in Bedfordshire, UK. In Part 1 of
   this two-part paper, the physical setting and the development of the
   conceptual and numerical models for groundwater flow and transport are
   described. In this part, the development of the stochastic modelling
   approach and the analysis of the set of Monte Carlo experiments used to
   characterise the uncertainty in the migration pathways are presented.
   Eleven Monte Carlo experiments have been performed. Nine have been
   undertaken to evaluate the effect of uncertainty in the parameters of
   the geostatistical model of hydraulic conductivity of the principal
   aquifer, and two have been performed to examine the influence of the
   location of the repository release zone. The study has demonstrated the
   use of the stochastic modelling approach in probabilistic risk
   assessments bf the potential hazards arising from the burial of wastes.}},
