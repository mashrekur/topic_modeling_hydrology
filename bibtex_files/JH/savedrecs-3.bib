@article{ ISI:A1996UU52600014,
Author = {Jeong, GD and Rao, AR},
Title = {{Chaos characteristics of tree ring series}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{182}},
Number = {{1-4}},
Pages = {{239-257}},
Month = {{JUL}},
Abstract = {{The climatic characteristics of an area are reflected in the growth of
   the trees. Consequently, the characteristics of tree rings are studied
   to determine the long-term climatic behavior of a region. In the present
   study, 13 tree ring series from the Salt and Verde River basins in
   Arizona are analyzed to determine their chaos characteristics. Two
   methods of analysis are used in the study: the Grassberger-Procaccia
   algorithm and the method proposed by Scargle. Neither indicated any
   evidence of chaotic behavior in these tree ring series. The series are
   also successfully modeled by a second-order stationary stochastic
   process. These results confirm the earlier result of Grassberger, who
   analyzed a much longer tree ring series and found no evidence of chaotic
   behavior.}},
