@article{ ISI:A1996VX29500017,
Author = {Guo, SL and Kachroo, RK and Mngodo, RJ},
Title = {{Nonparametric kernel estimation of low flow quantiles}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{185}},
Number = {{1-4}},
Pages = {{335-348}},
Month = {{NOV 1}},
Abstract = {{A nonparametric kernel estimation model is proposed and developed for
   estimating low flow quantiles. Based on annual minimum low flow data and
   Monte Carlo simulation tests, the descriptive and predictive ability of
   the proposed model is compared with that of Weibull models. The results
   indicate that it has small bias and root mean square error in low flow
   quantile estimates. Application of the model to data from the Blue Nile
   has shown that the nonparametric approach is a viable alternative to the
   Weibull models.}},
