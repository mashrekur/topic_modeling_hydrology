Title = {{An investigation of rooting depth of the semiarid shrub Retama
   sphaerocarpa (L) Boiss by labelling of ground water with a chemical
   tracer}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{177}},
Number = {{1-2}},
Pages = {{23-31}},
Month = {{MAR 15}},
Abstract = {{(1) Water containing lithium chloride as a chemical tracer was applied
   to the bottom of two survey wells 16 and 28 m deep and the concentration
   of Li+ in young cladodes of Retama sphaerocarpa shrubs growing around
   the wells was monitored for 9 days after the subterranean application.
