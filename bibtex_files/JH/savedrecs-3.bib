@article{ ISI:A1995RP73600001,
Author = {HARIA, AH},
Title = {{HYDROLOGICAL AND ENVIRONMENTAL-IMPACT OF EARTHWORM DEPLETION BY THE
   NEW-ZEALAND FLATWORM (ARTIOPOSTHIA-TRIANGULATA)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{171}},
Number = {{1-2}},
Pages = {{1-3}},
Month = {{SEP}},
Abstract = {{The predation of earthworms by the New Zealand flatworm is an
   environmental threat that may have consequences beyond the scope of
   merely reducing earthworm populations in the UK. The role of earthworms
   in developing soil structure is substantial and the effect of structural
   degradation on hydrological processes, following earthworm eradication,
   may therefore be major, with a resulting increase in flood risk from
   river systems and decrease in agricultural productivity of economic
   concern.}},
