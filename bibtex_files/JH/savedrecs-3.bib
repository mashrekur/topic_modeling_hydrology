@article{ ISI:A1995QQ22700002,
Author = {YADAV, GS},
Title = {{RELATING HYDRAULIC AND GEOELECTRIC PARAMETERS OF THE JAYANT AQUIFER,
   INDIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{167}},
Number = {{1-4}},
Pages = {{23-38}},
Month = {{MAY}},
Abstract = {{Geoelectrical soundings with Schlumberger arrays were carried out close
   to nine pumping-test sites in the Jayant project, a part of the
   Singrauli Coalfields, Madhya Pradesh, India, to relate geoelectric
   parameters to hydraulic parameters, such as hydraulic conductivity and
   transmissivity. The interpretation of the sounding data was carried out
   using partial curve matching and the final model parameters were
   obtained using the computer program AIMRESI. Water samples collected
   from different pumping-test sites were analyzed to obtain water
   resistivity. The relationship between transmissivity and normalized
   aquifer resistivity is discussed in this paper. Normalized aquifer
   resistivity is a very good predictor for transmissivity in this aquifer.}},
