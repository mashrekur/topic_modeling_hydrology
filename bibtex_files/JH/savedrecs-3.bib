@article{ ISI:A1995RH48000006,
Author = {GASH, JHC and LLOYD, CR and LACHAUD, G},
Title = {{ESTIMATING SPARSE FOREST RAINFALL INTERCEPTION WITH AN ANALYTICAL MODEL}},
