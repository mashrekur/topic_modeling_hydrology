@article{ ISI:A1995RD35200015,
Author = {LINDROTH, A and CIENCIALA, E},
Title = {{MEASURING WATER-USE EFFICIENCY OF EUCALYPT TRESS WITH CHAMBERS AND
   MICROMETEOROLOGICAL TECHNIQUES - COMMENT}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{169}},
Number = {{1-4}},
Pages = {{281-283}},
Month = {{JUL}},
Abstract = {{In a recent paper on water use efficiency (WUE) of Eucalypt trees, a
   large difference was found between WUE measured in a whole tree chamber
   and by micrometeorological methods, respectively. The former method gave
   up to 50\% higher WUE on clear days. The authors attributed this to a
   changed radiation regime inside the chamber and concluded that chambers
   were unsuitable for such studies. In this paper we point out the
   importance of considering also the dependence of vapour pressure deficit
   on WUE. Because of WUE's sensitivity to vapour pressure deficit, as
   illustrated by examples from several species, it is practically
   meaningless to make comparisons without compensating for differences in
   vapour pressure deficit. In our opinion, chamber can still play an
   important role in water relation and productivity studies.}},
