@article{ ISI:A1996VZ63400003,
Author = {Pohll, GM and Warwick, JJ and Tyler, SW},
Title = {{Coupled surface-subsurface hydrologic model of a nuclear subsidence
   crater at the Nevada test site}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{186}},
Number = {{1-4}},
Pages = {{43-62}},
Month = {{NOV 15}},
Abstract = {{A series of computer models describing overland flow, pending and
   infiltration was dynamically linked to simulate the movement of water
   during seepage beneath a nuclear subsidence feature. The coupled
   surface-subsurface hydrologic model contains a terrain analysis module,
   an overland bow simulator using the kinematic wave equation and a
   numerical Richards' equation solver to simulate moisture migration
   within the vadose zone. Simulations resulted in successful
   approximations to measured surface and subsurface data. The surface
   water overland flow component was able to reach an agreement between
   observed and simulated water levels following a precipitation event.
   Longterm simulations underpredicted the volumetric water contents and
   depth of moisture fronts. The flux (Darcian velocity) at 10 m below the
   surface was temporally stable with a temporal mean value of 0.75 m
   year(-1), which is in agreement with an earlier estimate of 0.60 m
   year(-1). The mean flux at 5 m below the surface was 1.4 m year(-1).
   Long-term simulations suggest that significant recharge does exist
   beneath surface collapse structures owing to periodic pending.}},
