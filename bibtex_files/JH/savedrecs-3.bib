@article{ ISI:A1995QE03800017,
Author = {TALLAKSEN, LM},
Title = {{A REVIEW OF BASEFLOW RECESSION ANALYSIS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{165}},
Number = {{1-4}},
Pages = {{349-370}},
Month = {{FEB}},
Abstract = {{Recession analysis is a well-known tool in hydrological analysis. Its
   application, however, poses many methodical questions, and throughout
   the literature numerous solutions have been sought. The quantification
   of the recession curve involves the selection of an analytical
   expression, derivation of a characteristic recession and optimization of
   the recession parameters. A major problem is the high variability
   encountered in the recession behaviour of individual segments. The
   segments represent different stages in the out-Bow process, and a
   physically based short-term or seasonal influence on the recession rate
   adds to the problem of deriving a characteristic recession. This paper
   discusses these elements of recression analysis and reviews different
   ways of characterizing the baseflow recession rate.}},
