@article{ ISI:A1996UB59900017,
Author = {Franchini, M and OConnell, PE},
Title = {{An analysis of the dynamic component of the geomorphologic instantaneous
   unit hydrograph}},
Journal = {{JOURNAL OF HYDROLOGY}},
