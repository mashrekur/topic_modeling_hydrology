@article{ ISI:A1996UB78600016,
Author = {Garfias, J and Verrette, JL and Antiguedad, I and Llamas, J},
Title = {{A reliability analysis of a hybrid rainfall-runoff model in the Bolivian
   highlands}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{176}},
Number = {{1-4}},
Pages = {{269-283}},
Month = {{MAR 1}},
Abstract = {{The sensitivity of the output to the parameter values of a hybrid model
   (HEC-1 model) with one stochastic component added is considered in the
   context of the Bolivian highlands. First-order and Monte Carlo analysis
   techniques were used to establish the influence of each parameter in the
   model. The greatest contribution to uncertainty in the model estimation
   corresponds to the initial loss (IL) and the uniform loss (CL)
   parameters. However, it is important to recognize that large variation
   in output may also be caused by differences in the spatial distribution
   of precipitation (a low-density network). Other parameters of the model
   structure (TC and (S-R)) and the parameters defining the concentration
   of flow had less effect. If it were possible to estimate the net
   rainfall with greater precision the concentration parameters could
   easily be adjusted to meet the requirements of accurate representation
   of peak flows.}},
