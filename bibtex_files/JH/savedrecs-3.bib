@article{ ISI:A1995QM04400010,
Author = {CLAUSSEN, M},
Title = {{ESTIMATION OF REGIONAL HEAT AND MOISTURE FLUXES IN HOMOGENEOUS TERRAIN
   WITH BLUFF ROUGHNESS ELEMENTS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
