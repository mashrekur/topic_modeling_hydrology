@article{ ISI:A1996TR93400001,
Author = {Mahesha, A and Nagaraja, SH},
Title = {{Effect of natural recharge on sea water intrusion in coastal aquifers}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{174}},
Number = {{3-4}},
Pages = {{211-220}},
Month = {{JAN 15}},
Abstract = {{Parametric studies were carried out to investigate the effect of uniform
   recharge on sea water intrusion into coastal aquifers using a
   one-dimensional finite element model. For this purpose, uniform recharge
   over the entire length of intrusion for a wide range of practical
   aquifers is considered. Useful relationships have been established
   between the interface motion and the intensity and duration of uniform
   recharge. These relationships can be used to estimate the repulsion of
   sea water intrusion owing to rainfall. This repulsion under favourable
   conditions is estimated to reach a maximum of about 5\%. The effect of
   uneven distribution of rainfall is demonstrated through an example,
   Movement of the interface is also tracked after the cessation of
   recharge.}},
