@article{ ISI:A1996VX29500014,
Author = {Tan, BQ and OConnor, KM},
Title = {{Application of an empirical infiltration equation in the SMAR conceptual
   model}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{185}},
Number = {{1-4}},
Pages = {{275-295}},
Month = {{NOV 1}},
Abstract = {{An empirical equation, which relates the infiltration rate to the actual
   soil moisture content, is proposed and incorporated in the Soil Moisture
   Accounting and Routing (SMAR) model to demonstrate one application of
   that equation. A modified SMAR model (SMARY) is developed and tested on
   four catchments with different climatic conditions. The results show
   that the SMARY model performs better than the SMAR model in terms of
   R(2), mean square error and relative error. The modified model provides
   a more rational interpretation of the physical process of infiltration
   and also provides a tool for determining the actual infiltration rate
   under specific soil moisture conditions.}},
