@article{ ISI:A1996VZ77300005,
Author = {Puente, CE},
Title = {{A new approach to hydrologic modeling: Derived distributions revisited}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{187}},
Number = {{1-2}},
Pages = {{65-80}},
Month = {{DEC}},
Note = {{International Conference on Fractals in Hydrology, ISCHIA, ITALY, 1993}},
Abstract = {{A fractal geometric procedure to model hydrologic (geophysical)
   phenomena is introduced. The method consists of using derived
   distributions, obtained by transforming arbitrary multinomial
   multifractal measures via fractal interpolating functions, to represent
   observed hydrologic phenomena. Specific applications include modeling
   either along a line (in space or in time) or over the plane (in space).
   It is shown via simulations that the new procedure leads to realistic
   descriptions of hydrologic patterns, and provides a very parsimonious
   and deterministic explanation of seemingly random phenomena. It is
   argued that the new method may lead to an alternative description of the
   dynamics of hydrologic (geophysical) phenomena. Examples dealing with
   rainfall and transport of pollutants in porous media illustrate the
   ideas.}},
