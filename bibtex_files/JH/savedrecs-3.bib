@article{ ISI:A1996UN80700005,
Author = {Leonardi, V and Arthaud, F and Grillot, JC and Avetissian, V and
   Bochnaghian, P},
Title = {{Modelling of a fractured basaltic aquifer with respect to geological
   setting, and climatic and hydraulic conditions: The case of perched
   basalts at Garni (Armenia)}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{179}},
Number = {{1-4}},
Pages = {{87-109}},
Month = {{MAY 1}},
Abstract = {{The underground water flows in volcanic rocks of a continental seismic
   area (Armenia) are analysed using three approaches: (1) the structure of
   the basaltic reservoir; (2) the features of the hydrogeological domains
   involved and their structural limits; (3) the hydraulic behaviour of the
   system, deduced from spring outflows, measured every 3 days for 4 years.
   According to these data, and without piezometric data, it is possible to
   compute the circulations in the basaltic reservoirs according to the
   diffusivity equation, The results of this computing exercise are
   compared with the measured values. The good fit obtained between
   computed and measured values suggests that this model could be used in
   similar cases.}},
