@article{ ISI:A1995TE18300004,
Author = {MAGESAN, GN and WHITE, RE and SCOTTER, DR},
Title = {{THE INFLUENCE OF FLOW-RATE ON THE CONCENTRATION OF INDIGENOUS AND
   APPLIED SOLUTES IN MOLE-PIPE DRAIN EFFLUENT}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{172}},
Number = {{1-4}},
Pages = {{23-30}},
Month = {{NOV}},
Abstract = {{Two mole and pipe drained fields (labelled A and B) of Tokomaru silt
   loam soil in New Zealand, each 1250 m(2) in area, were used in the
   experiment. For field A, sodium bromide was applied (200 kg Br ha(-1)),
   and nitrate was taken to be the indigenous solute. For field B, urea
   (120 kg N ha(-1)) was the applied solute, and chloride was the
   indigenous solute. Drainage from the mole and pipe systems was measured
   and sampled using V-notch weirs, with continuous flow recorders and
   proportional samplers. Measurements were made over one drainage season
   (June to August) and the drainage analysed for these solutes.
   The results showed that early in the drainage season the concentration
   of applied solute increased with increasing flow rate, while the
   indigenous solute concentration decreased with increasing flow rate,
   apparently due to preferential flow. However, over time, applied and
   indigenous solutes were depleted to such an extent that the chemographs
   of both solutes showed a similar form. The longer the time interval
   between the application of a solute (Br or urea) and a drainage event,
   the less clear was the distinction between an applied (introduced)
   solute and a resident (indigenous) solute.}},
