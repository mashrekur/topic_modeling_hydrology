@article{ ISI:A1995QM04400006,
Author = {KRAMM, G and DLUGI, R and LENSCHOW, DH},
Title = {{A REEVALUATION OF THE WEBB CORRECTION USING DENSITY-WEIGHTED AVERAGES}},
Journal = {{JOURNAL OF HYDROLOGY}},
