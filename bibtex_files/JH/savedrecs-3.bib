@article{ ISI:A1995RD35200013,
Author = {OSIENSKY, JL and DONALDSON, PR},
Title = {{ELECTRICAL FLOW-THROUGH AN AQUIFER FOR CONTAMINANT SOURCE LEAK DETECTION
   AND DELINEATION OF PLUME EVOLUTION}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{169}},
Number = {{1-4}},
Pages = {{243-263}},
Month = {{JUL}},
Abstract = {{A surface, electrical geophysical method first suggested in 1920 holds
   great promise as a tool to detect contaminant source leaks and to
   delineate evolving contaminant plumes in suitable hydrogeologic
   environments. This tool, known as the mise-a-la-masse method, involves
   the measurement of electrical current how through the earth materials
   under investigation. Realization of the direct analogy between ground
   water flow and the flow of electricity through conductive media aids in
   the interpretation of electrical potential data derived by a modified
   mise-a-la-masse method presented herein.
   A modified mise-a-la-masse method was used to track the evolution of a
   conductive tracer plume over a period of 155 h. Voltage differences
   between 171 stationary, surface electrodes and a reference electrode
   were monitored over time to map the evolving tracer plume. For this
   particular experiment, the method proved to be about as sensitive as a
   portable, electrical conductivity meter to detect changes in the
   specific conductance of the ground water.}},
