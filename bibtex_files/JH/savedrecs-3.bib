@article{ ISI:A1995QM04400011,
Author = {CLAUSSEN, M},
Title = {{FLUX AGGREGATION AT LARGE SCALES - ON THE LIMITS OF VALIDITY OF THE
   CONCEPT OF BLENDING HEIGHT}},
