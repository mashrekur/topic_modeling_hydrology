@article{ ISI:A1995RH48000001,
Author = {TATTARI, S and IKONEN, JP and SUCKSDORFF, Y},
Title = {{A COMPARISON OF EVAPOTRANSPIRATION ABOVE A BARLEY FIELD-BASED ON QUALITY
   TESTED BOWEN-RATIO DATA AND DEARDORFF MODELING}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{170}},
Number = {{1-4}},
Pages = {{1-14}},
Month = {{AUG}},
Abstract = {{Assessment of the quality of evapotranspiration data is essential if
   databases valid for modelling purposes are to be constructed. In this
   study a quality control technique was applied to test the validity of
   long-term evapotranspiration data. Approximately 53\% of daytime data
   proved to be reliable. Unreliable data are caused not only by weather
   conditions, such as unfavourable wind direction and rain events, but
   also by the condensation of water inside the air intake tubing and
   filters of the instruments. The Deardorff model was applied to calculate
   evapotranspiration. The appropriateness of the parametrization used to
   obtain the leaf area index and the stomatal resistance was evaluated for
   a barley field located in southern Finland. The equation of Noilhan and
   Planton to calculate r(s) proved to be the best. The discrepancies
   between the measurements and the modelled data were assessed by critical
   examination of the measurements.}},
