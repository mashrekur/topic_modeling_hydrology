@article{ ISI:A1996UJ14400020,
Author = {Diskin, MH and Nazimov, N},
Title = {{Ponding time and infiltration capacity variation during steady rainfall}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{178}},
Number = {{1-4}},
Pages = {{369-380}},
Month = {{APR 15}},
Abstract = {{The variation of the infiltration capacity at a given location during
   steady rainfall having an intensity smaller than the initial
   infiltration capacity of the soil is investigated. Two distinct time
   periods are considered: one prior to pending, when the rainfall
   intensity is smaller than the infiltration capacity, and the second,
   which begins at the initiation of pending, when the infiltration
   capacity becomes smaller than the intensity of the steady rainfall. The
   duration of the first period depends on the intensity of the steady
   rainfall, as well as on the initial value of the infiltration capacity.
   If the initial infiltration capacity is at its highest value, the
   duration is defined as the steady rainfall pending time or, in short,
   the `ponding time' for the specified conditions. Estimates of the
   pending time and the variation in the infiltration capacity depend also
   on the model adopted to describe the infiltration process. The model
   used in this case is a simple two-element and three-parameter model
   described recently by Diskin and Nazimov (J. Hydrol., 172: 313-330,
   1995). Results obtained with this model are compared with literature
   values of pending time for a number of soil types. The agreement is
   reasonably good, leading to the conclusion that the proposed simple
   model is valid for describing the infiltration process.}},
