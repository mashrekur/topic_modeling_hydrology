@article{ ISI:A1996UP76700019,
Author = {Lloyd, EH},
Title = {{On the integral equation of the continuous-state stochastic reservoir
   with seasonally varying autocorrelated inflows}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{180}},
Number = {{1-4}},
Pages = {{351-360}},
Month = {{MAY 15}},
Abstract = {{The paper deals with continuous-state reservoirs in discrete time, in
   terms of the (continuous) Moran transform U-t = Z(t) + X(t) of the
   (discontinuous) storage levels Z(t). In the non-seasonal case, if the
   inflows X,have a first-order Markov structure, the modified storage
   levels U, have a second-order Markov structure. It is shown that, for
   each value of u, the limit p(u,v) as t --> infinity of the joint density
   p(t+1) (u,v) of U-t+1 at u and U-t at v satisfies a certain non-standard
   first-order linear integral equation of the form
   p(u, v) = (0) integral(infinity) a(u, v, w)p(v, z)dz
   or, equivalently, a standard second-order equation of the form
   p(u, v) = (0) integral(0)(infinity) integral(infinity) a(u, v, z)a(v, z
   w)p(z, w)dzdw
   In the seasonal case, with two seasons, the corresponding bivariate
   density p(u, v) for consecutive seasons satisfies a second-order
   integral equation of the form
   p(u, v) = (0) integral(0)(infinity) integral(infinity) A(u, v, w, z)p(z,
   w)dzdw
   the kernel being a function of the season-to-season inflow transition
   densities.}},
