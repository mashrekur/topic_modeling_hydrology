@article{ ISI:A1995RP73600006,
Author = {VANDERMOLEN, WH and TORFS, PJJF and DELIMA, JLMP},
Title = {{WATER DEPTHS AT THE UPPER BOUNDARY FOR OVERLAND-FLOW ON SMALL GRADIENTS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{171}},
Number = {{1-2}},
Pages = {{93-102}},
Month = {{SEP}},
Abstract = {{In Hortonian overland flow caused by rainfall on a sloping plane, the
   usual presumption is that water depth is zero at the upper end of the
   slope. It is clear that this is impossible at zero slope or for very
   slight gradients. A numerical method was used to calculate this water
   depth in the final equilibrium (steady-state) situation. Depth appears
   to be finite but negligible at slopes of more than 0.5\%. Laboratory
   experiments, conducted on an impermeable plane surface with a rainfall
   simulator, confirmed the numerical results.}},
