@article{ ISI:A1995RD35200009,
Author = {OWEN, CR},
Title = {{WATER-BUDGET AND FLOW PATTERNS IN AN URBAN WETLAND}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{169}},
Number = {{1-4}},
Pages = {{171-187}},
Month = {{JUL}},
Abstract = {{The hydrologic budget of a streamside urban peatland in south central
   Wisconsin, USA, was constructed, and the patterns of groundwater and
   surface water flow were described over a 2 year period. Precipitation
   and surface flow comprised over 95\% of the inputs to the wetland, while
   evapotranspiration was the major output. Evapotranspiration was
   significantly lower in cattail-dominated sections of the wetland. Low
   piezometric heads in and below the aquitard underneath the peatland
   created weak downward gradients from the wetland to the aquifer below.
   Within the peat itself, weak upward or discharge gradients were
   dominant. Three major patterns of horizontal shallow groundwater flow
   were seen, but shallow gradients and low hydraulic conductivities
   prevented large inputs or outputs of water through these pathways. The
   wetland has a large capacity for flood storage, but it did not make
   significant contributions to groundwater supplies or to streamflow
   during the time of this study. Error analysis revealed enormous
   uncertainties in many of the components of the budget.}},
