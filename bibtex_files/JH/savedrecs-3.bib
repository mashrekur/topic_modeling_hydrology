@article{ ISI:A1995QQ22700003,
Author = {ARNELL, NW},
Title = {{GRID MAPPING OF RIVER DISCHARGE}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{167}},
Number = {{1-4}},
Pages = {{39-56}},
Month = {{MAY}},
Abstract = {{This paper summarises methods for creating maps of river discharge, and
   evaluates them using river flow data from Europe. Most published maps
   are isoline maps, but choropleth maps are more appropriate for showing
   the spatial distribution of river flow. Five approaches to producing
   grid choropleth maps can be identified. The first averages the data from
   all the small catchments in a grid cell; this overestimates runoff. The
   second involves statistical interpolation around small catchments, but
   it,is difficult to account for ah the details of spatial variability in
   a statistical interpolation. The third method uses empirical
   relationships to estimate runoff from gridded climate data; the results,
   however, depend on the climate data used. The fourth approach uses a
   water balance model to estimate runoff from gridded climate data; it
   also is dependent on the input climate data. A final approach produces a
   weighted cell average based on catchment runoff maps, and this was shown
   to produce reasonable estimates of gridded runoff.}},
