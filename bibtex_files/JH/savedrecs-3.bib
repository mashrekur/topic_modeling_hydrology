@article{ ISI:A1995QY98900005,
Author = {PADILLA, A and PULIDOBOSCH, A},
Title = {{STUDY OF HYDROGRAPHS OF KARSTIC AQUIFERS BY MEANS OF CORRELATION AND
   CROSS-SPECTRAL ANALYSIS}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{168}},
Number = {{1-4}},
Pages = {{73-89}},
Month = {{JUN}},
Abstract = {{Correlation and cross-spectral analysis can be applied to the study of
   karstic aquifers to characterize the transformations in these systems
   between the input function (precipitation) and the output function
   (discharge). The parameters that can be deduced are the response time,
   the distinction between quickflow, intermediate flow and baseflow, and
   the mean delay. This method offers quantifiable and objective criteria
   for differentiation and comparisons of karstic aquifers.}},
