@article{ ISI:A1995QM04400012,
Author = {MICHELS, BI and JOCHUM, AM},
Title = {{HEAT AND MOISTURE FLUX PROFILES IN A REGION WITH INHOMOGENEOUS SURFACE
   EVAPORATION}},
Journal = {{JOURNAL OF HYDROLOGY}},
