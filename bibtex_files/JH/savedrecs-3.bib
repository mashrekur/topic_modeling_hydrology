@article{ ISI:A1996VG72300003,
Author = {Jansson, MB},
Title = {{Estimating a sediment rating curve of the Reventazon river at Palomo
   using logged mean loads within discharge classes}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1996}},
Volume = {{183}},
Number = {{3-4}},
Pages = {{227-241}},
Month = {{SEP}},
Abstract = {{The paper describes a new technique for developing a sediment rating
   curve. The new technique involves stratification of data into
   discharge-based classes, the means of which are used to fit a rating
   curve. This procedure adapts the rating curve close to the mean loads
   and eliminates to some extent the `retransformation bias' problem which
   complicates the rating curve technique. The procedure is demonstrated on
   data collected on the Reventazon river at Palomo, Costa Rica.}},
