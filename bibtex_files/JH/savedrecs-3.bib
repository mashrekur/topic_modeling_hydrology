@article{ ISI:A1995QQ22700020,
Author = {BARTARYA, SK and BHATTACHARYA, SK and RAMESH, R and SOMAYAJULA, BLK},
Title = {{DELTA-O-18 AND DELTA-D SYSTEMATICS IN THE SURFICIAL WATERS OF THE GAULA
   RIVER CATCHMENT-AREA, KUMAUN HIMALAYA, INDIA}},
Journal = {{JOURNAL OF HYDROLOGY}},
Year = {{1995}},
Volume = {{167}},
Number = {{1-4}},
Pages = {{369-379}},
Month = {{MAY}},
Abstract = {{The delta(18)O and delta D measurements of surface waters (streams,
   springs, lakes and precipitation) from the Gaula river catchment area of
   Kumaun Himalaya are reported. The delta D and delta(18)O values are
   positively correlated, as has been the case with waters from several
   other regions. The lake waters are found to be enriched in O-18 and D
   compared with the neighbouring regions owing to evaporation. Estimates
   of mean total evaporation during the dry season range from 10 to 25\%.
   Spring water samples collected from various altitudes (from 915 m to
   2150 m) display variations in values showing an altitude effect of -0.14
   parts per thousand per 100 m elevation.}},
