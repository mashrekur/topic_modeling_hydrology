@article{ ISI:000166267800020,
Author = {Cimen, M},
Title = {{A simple solution for drawdown calculation in a large-diameter well}},
Journal = {{GROUND WATER}},
Year = {{2001}},
Volume = {{39}},
Number = {{1}},
Pages = {{144-147}},
Month = {{JAN-FEB}},
Abstract = {{Large-diameter wells are used extensively in developing countries,
   especially in unconsolidated alluvial deposits or fissured rocks of
   shallow depths. Principally, they have large storage volume ready for
   exploitation by direct pumping. A simple calculation procedure for
   drawdown in a large-diameter well has been proposed. This is a new
   method that involves the use of the independent variable transform. The
   governing equation can be used easily for the evaluation of drawdowns in
   both pumping and observation wells. Type curves obtained by the
   procedure developed in this paper are compared with those already
   available in ground water literature. The accordance of the derived type
