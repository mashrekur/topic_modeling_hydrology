@article{ ISI:000080051100012,
Author = {Swamee, PK and Tyagi, A and Shandilya, VK},
Title = {{Optimal configuration of a wellfield}},
Journal = {{GROUND WATER}},
Year = {{1999}},
Volume = {{37}},
Number = {{3}},
Pages = {{382-386}},
Month = {{MAY-JUN}},
Abstract = {{Often in urban and rural water supply, the major source of water is an
   aquifer, An aquifer can be fully exploited by development of a
   wellfield. Considering well interference, individual wells have to be
   located in such a manner so that adjacent wells are at a distance of
   radius of minimum influence. Presently, as no optimal configuration is
   available, the wells are located by judgment. In this paper the optimal
   configurations of wellfields have been obtained by considering the
   minimum cost and the maximum reliability aspects for transporting the
   water to the center of the wellfield for its onward transmission to the
   treatment works.}},
