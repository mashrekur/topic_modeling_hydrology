@article{ ISI:000076864400005,
Author = {Abbaspour, KC and Schulin, R and van Genuchten, MT and Schlappi, E},
Title = {{Procedures for uncertainty analyses applied to a landfill leachate plume}},
Journal = {{GROUND WATER}},
Year = {{1998}},
Volume = {{36}},
Number = {{6}},
Pages = {{874-883}},
Month = {{NOV-DEC}},
Abstract = {{Environmental data often have features that are distinct from data in
   other branches of science. These features include spatial and/or
   temporal auto-correlation, natural heterogeneity, measurement errors,
   small sample sizes, and simultaneous existence of different types and
   qualities of data. Realistic environmental modeling requires simulation
   procedures that account for all of these features. In this study, a
   model of uncertainty analysis, BUDA, is used to account for the noted
   features and provide a unified framework for quantification,
   propagation, and reduction of uncertainty, The BUDA model is used to
   analyze the development of a chloride plume around an old landfill to
   the year 2020, This article describes the different components of BUDA
   as they relate to the landfill application.}},
