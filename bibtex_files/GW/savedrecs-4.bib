@article{ ISI:A1997YE24700012,
Author = {Banton, O and Delay, F and Porel, G},
Title = {{A new time domain random walk method for solute transport in 1-D
   heterogeneous media}},
Journal = {{GROUND WATER}},
Year = {{1997}},
Volume = {{35}},
Number = {{6}},
Pages = {{1008-1013}},
Month = {{NOV-DEC}},
Abstract = {{A new method to simulate solute transport in 1-D heterogeneous media is
   presented. This time domain random walk method (TDRW), similar in
   concept to the classical random walk method, calculates the arrival time
   of a particle cloud at a given location (directly providing the solute
   breakthrough curve). The main advantage of the method is that the
   restrictions on the space increments and the time steps which exist with
   the finite differences and random walk methods are avoided. In a
   homogeneous zone, the breakthrough curve (ETC) can be calculated
   directly at a given distance using a few hundred particles or directly
   at the boundary of the zone. comparisons with analytical solutions and
   with the classical random walk method show the reliability of this
   method. The velocity and dispersivity calculated from the simulated
   results agree within two percent with the values used as input in the
   model. For contrasted heterogeneous media, the random walk can generate
   high numerical dispersion, while the time domain approach does not.}},
