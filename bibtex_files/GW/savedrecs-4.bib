@article{ ISI:A1997XV14000022,
Author = {Onysko, SJ and McNearny, RL},
Title = {{GIBBTEQ: A MINTEQA2 thermodynamic data error detection program}},
Journal = {{GROUND WATER}},
Year = {{1997}},
Volume = {{35}},
Number = {{5}},
Pages = {{912-914}},
Month = {{SEP-OCT}},
Abstract = {{The U.S. Environmental Protection Agency geochemical computer code
   MINTEQA2 contains over 1,500 aqueous species entries with
   stoichiometric, thermodynamic, and gram formula weight data. GIBBTEQ is
   a LOTUS 123 program that scans MINTEQA2 data base entries and directly
   identifies errors in stoichiometry, species charge, and formula weight.
   GIBBTEQ indirectly facilitates identification of errors of equilibrium
   and enthalpy data in MINTEQA2 data base entries by printing those
   entries in readable format, Anomalies of equilibrium constant, standard
   free energy and enthalpy of mineral species, and redox potential (for
   electron-containing reactions) are amenable to user detection in the
   program output. MINTEQA2 errors of various types are quantified from
   GIBBTEQ results. Program availability inquiries can be sent via e-mail
   to gibbteq@mines.utah.edu.}},
