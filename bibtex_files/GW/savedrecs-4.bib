@article{ ISI:000089456900019,
Author = {Thomas, JM and McKay, WA and Cole, E and Landmeyer, JE and Bradley, PM},
Title = {{The fate of haloacetic acids and trihalomethanes in an aquifer storage
   and recovery program, Las Vegas, Nevada}},
