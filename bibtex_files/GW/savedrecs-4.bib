@article{ ISI:000079083500023,
Author = {Taylor, KC and Minor, TB and Chesley, MM and Matanawi, K},
Title = {{Cost effectiveness of well site selection methods in a fractured aquifer}},
Journal = {{GROUND WATER}},
Year = {{1999}},
Volume = {{37}},
Number = {{2}},
Pages = {{271-274}},
Month = {{MAR-APR}},
Abstract = {{In regions where ground water flow is restricted to sparse, narrow
   fractures, productive and nonproductive drilling sites frequently are
   separated by only a few tens of meters. Drilling success rates are low
   in these areas because favorable drilling sites occur infrequently and
   are difficult to identify. The drilling success rate can be increased
   when imagery and surface geophysics are incorporated into the site
   selection method. We present a method to evaluate the economic benefit
   of various site selection methods and show that the use of surface
   geophysics and aerial photography can reduce drilling costs by
   approximately a factor of two in an area with a low drilling success
   rate that is characterized by narrow fracture zones covered with several
   meters of weathered clays and thick vegetation. The benefits are smaller
   and less certain when social considerations are a strong factor in site
   selection.}},
