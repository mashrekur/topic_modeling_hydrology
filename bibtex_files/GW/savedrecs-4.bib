@article{ ISI:000079083500011,
Author = {Gieske, A},
Title = {{Flow to a well in a water table aquifer: Speeding up the computation of
   type curves for low beta values}},
Journal = {{GROUND WATER}},
Year = {{1999}},
Volume = {{37}},
Number = {{2}},
Pages = {{171-174}},
Month = {{MAR-APR}},
Abstract = {{A study is made of the computer implementation of a recently developed
   Laplace transform solution (Moench 1996) for flow to a web in a water
   table aquifer, Some improvements are suggested to increase the speed of
   computation, especially for low beta values. The first is to calculate
   the type curves at points which are spaced a factor of two apart, thus
   allowing more efficient use of the Stehfest inversion algorithm. An
   improved numerical method to determine the roots of the equation xtanx =
   alpha is suggested. Finally, more efficient determination of the number
   of terms in the required series summation may lead to significant
   reduction in computing time.}},
