@article{ ISI:000298733600010,
Author = {Werner, Adrian D. and Ward, James D. and Morgan, Leanne K. and Simmons,
   Craig T. and Robinson, Neville I. and Teubner, Michael D.},
Title = {{Vulnerability Indicators of Sea Water Intrusion}},
