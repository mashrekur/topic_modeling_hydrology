@article{ ISI:000285751700005,
Author = {Bech, Tina and Dalsgaard, Anders and Jacobsen, Ole Stig and Jacobsen,
   Carsten Suhr},
Title = {{Leaching of Salmonella enterica in Clay Columns Comparing Two Manure
   Application Methods}},
Journal = {{GROUND WATER}},
Year = {{2011}},
