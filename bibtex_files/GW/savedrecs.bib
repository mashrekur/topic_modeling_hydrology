@article{ ISI:000277123100012,
Author = {Hunt, Randall J. and Luchette, Joseph and Schreuder, Willem A. and
   Rumbaugh, James O. and Doherty, John and Tonkin, Matthew J. and
   Rumbaugh, Douglas B.},
Title = {{Using a Cloud to Replenish Parched Groundwater Modeling Efforts}},
Journal = {{GROUND WATER}},
Year = {{2010}},
Volume = {{48}},
Number = {{3}},
Pages = {{360-365}},
Month = {{MAY-JUN}},
Abstract = {{Groundwater models can be improved by introduction of additional
   parameter flexibility and simultaneous use of soft-knowledge. However,
   these sophisticated approaches have high computational requirements.
   Cloud computing provides unprecedented access to computing power via the
   Internet to facilitate the use of these techniques. A modeler can
   create, launch, and terminate ``virtual{''} computers as needed, paying
   by the hour, and save machine images for future use. Such cost-effective
   and flexible computing power empowers groundwater modelers to routinely
   perform model calibration and uncertainty analysis in ways not
   previously possible.}},
