@article{ ISI:000287584100013,
Author = {Schillig, P. C. and Devlin, J. F. and Roberts, J. A. and Tsoflias, G. P.
   and McGlashan, M. A.},
Title = {{Transient Heterogeneity in an Aquifer Undergoing Bioremediation of
   Hydrocarbons}},
Journal = {{GROUND WATER}},
Year = {{2011}},
Volume = {{49}},
Number = {{2}},
Pages = {{184-196}},
Month = {{MAR-APR}},
Abstract = {{Localized, transient heterogeneity was studied in a sand aquifer
   undergoing benzene, toluene, ethylbenzene, and xylene bioremediation
   using a novel array of multilevel, in situ point velocity probes (PVPs).
   The experiment was conducted within a sheet-pile alleyway to maintain a
   constant average flow direction through time. The PVPs measured changes
   in groundwater velocity direction and magnitude at the centimeter scale,
   making them ideal to monitor small-scale changes in hydraulic
   conductivity (K). Velocities were shown to vary nonuniformly by up to a
   factor of 3 when a source of oxygen was established down-gradient of the
   petroleum spill. In spite of these local variations, the average
   groundwater velocity within the 7 m x 20 m sheet-piled test area only
   varied within +/- 25\%. The nonuniform nature of the velocity variations
   across the gate indicated that the changes were not due solely to
   seasonal hydraulic gradient fluctuations. At the conclusion of the
   experiment, microbial biomass levels in the aquifer sediments was
   approximately 1 order of magnitude higher in the oxygen-amended portion
   of the aquifer than at the edge of the plume or in locations up-gradient
   of the source. These data suggest that the transient velocities
   resulted, at least in part, from enhanced biological activity that
   caused transient heterogeneities in the porous medium.}},
