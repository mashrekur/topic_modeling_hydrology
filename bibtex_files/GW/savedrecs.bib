@article{ ISI:000274941500015,
Author = {Taylor, Richard and Tindimugaya, Callist and Barker, John and Macdonald,
   David and Kulabako, Robinah},
Title = {{Convergent Radial Tracing of Viral and Solute Transport in Gneiss
   Saprolite}},
