@article{ ISI:000323889900010,
Author = {Kessler, Timo Christian and Comunian, Alessandro and Oriani, Fabio and
   Renard, Philippe and Nilsson, Bertel and Klint, Knud Erik and Bjerg,
   Poul Logstrup},
Title = {{Modeling Fine-Scale Geological Heterogeneity-Examples of Sand Lenses in
   Tills}},
