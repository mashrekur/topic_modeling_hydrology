@article{ ISI:000326972100011,
Author = {Jeanne, Pierre and Guglielmi, Yves and Cappa, Frederic},
Title = {{Hydromechanical Heterogeneities of a Mature Fault Zone: Impacts on Fluid
   Flow}},
