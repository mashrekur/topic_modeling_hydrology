@article{ ISI:000270900900021,
Author = {Dong, Yanhui and Li, Guomin},
Title = {{A Parallel PCG Solver for MODFLOW}},
Journal = {{GROUND WATER}},
Year = {{2009}},
Volume = {{47}},
Number = {{6}},
Pages = {{845-850}},
Month = {{NOV-DEC}},
Abstract = {{In order to simulate large-scale ground water flow problems more
   efficiently with MODFLOW, the OpenMP programming paradigm was used to
   parallelize the preconditioned conjugate-gradient (PCG) solver with in
   this study. Incremental parallelization, the significant advantage
   supported by OpenMP on a shared-memory computer, made the solver transit
   to a parallel program smoothly one block of code at a time. The parallel
   PCG solver, suitable for both MODFLOW-2000 and MODFLOW-2005, is verified
   using an 8-processor computer. Both the impact of compilers and
   different model domain sizes were considered in the numerical
   experiments. Based on the timing results, execution times using the
   parallel PCG solver are typically about 1.40 to 5.31 times faster than
   those using the serial one. In addition, the simulation results are the
   exact same as the original PCG solver, because the majority of serial
   codes were not changed. It is worth noting that this parallelizing
   approach reduces cost in terms of software maintenance because only a
   single source PCG solver code needs to be maintained in the MODFLOW
   source tree.}},
