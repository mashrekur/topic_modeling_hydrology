@article{ ISI:000287584100023,
Author = {Lu, Xiaohui and Jin, Menggui and van Genuchten, Martinus Th. and Wang,
   Bingguo},
Title = {{Groundwater Recharge at Five Representative Sites in the Hebei Plain,
   China}},
Journal = {{GROUND WATER}},
Year = {{2011}},
Volume = {{49}},
Number = {{2}},
Pages = {{286-294}},
Month = {{MAR-APR}},
Abstract = {{Accurate estimates of groundwater recharge are essential for effective
   management of groundwater, especially when supplies are limited such as
   in many arid and semiarid areas. In the Hebei Plain, China, water
   shortage is increasingly restricting socioeconomic development,
   especially for agriculture, which heavily relies on groundwater. Human
   activities have greatly changed groundwater recharge there during the
   past several decades. To obtain better estimates of recharge in the
   plain, five representative sites were selected to investigate the
   effects of irrigation and water table depth on groundwater recharge. At
   each site, a one-dimensional unsaturated flow model (Hydrus-1D) was
   calibrated using field data of climate, soil moisture, and groundwater
   levels. A sensitivity analysis of evapotranspirative fluxes and various
   soil hydraulic parameters confirmed that fine-textured surface soils
   generally generate less recharge. Model calculations showed that
   recharge on average is about 175 mm/year in the piedmont plain to the
   west, and 133 mm/year in both the central alluvial and lacustrine plains
   and the coastal plain to the east. Temporal and spatial variations in
   the recharge processes were significant in response to rainfall and
   irrigation. Peak time-lags between infiltration (rainfall plus
   irrigation) and recharge were 18 to 35 days in the piedmont plain and 3
   to 5 days in the central alluvial and lacustrine plains, but only 1 or 2
   days in the coastal plain. This implies that different time-lags
   corresponding to different water table depths must be considered when
   estimating or modeling groundwater recharge.}},
