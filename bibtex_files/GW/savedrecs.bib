@article{ ISI:000279082600012,
Author = {Hill, Melissa E. and Stewart, Mark T. and Martin, Angel},
Title = {{Evaluation of the MODFLOW-2005 Conduit Flow Process}},
Journal = {{GROUND WATER}},
