@article{ ISI:000287584100016,
Author = {Qi, Yongqiang and Donahoe, Rona J.},
Title = {{Modeling Arsenic Sorption in the Subsurface with a Dual-Site Model}},
Journal = {{GROUND WATER}},
Year = {{2011}},
Volume = {{49}},
