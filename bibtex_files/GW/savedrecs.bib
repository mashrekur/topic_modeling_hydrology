Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Seiler, RL (Reprint Author), US Geol Survey, 2730 N Deer Run Rd, Carson City, NV 89701 USA.
   US Geol Survey, Carson City, NV 89701 USA.}},
