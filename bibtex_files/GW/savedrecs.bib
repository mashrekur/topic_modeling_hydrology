@article{ ISI:000294737800009,
Author = {Scheibe, Timothy D. and Hubbard, Susan S. and Onstott, Tullis C. and
   DeFlaun, Mary F.},
Title = {{Lessons Learned from Bacterial Transport Research at the South Oyster
   Site}},
