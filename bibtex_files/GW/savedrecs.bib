@article{ ISI:000292104400016,
Author = {El-Kadi, Aly I. and Plummer, L. Niel and Aggarwal, Pradeep},
Title = {{NETPATH-WIN: An Interactive User Version of the Mass-Balance Model,
   NETPATH}},
