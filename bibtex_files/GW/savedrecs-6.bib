@article{ ISI:A1993LW31500008,
Author = {SINGH, VS and GUPTA, CP},
Title = {{THE SIGNIFICANCE OF THE EXTENT OF RECOVERY IN INTERPRETING AQUIFER
   PUMPING TEST DATA FROM LARGE-DIAMETER WELLS}},
Journal = {{GROUND WATER}},
Year = {{1993}},
Volume = {{31}},
Number = {{5}},
Pages = {{753-755}},
Month = {{SEP-OCT}},
Abstract = {{To estimate aquifer parameters from pumping test data obtained from
   large diameter wells, one must consider the recovery data. This is
   particularly important for low permeability formations. Observations on
   the recovery should be continued to the maximum possible extent for an
   accurate estimation of these parameters. Analysis of data from a few
   representative case studies for granitic and basaltic rocks are
   presented here to establish a criterion to decide how long observations
   should be taken during recovery in the well.}},
