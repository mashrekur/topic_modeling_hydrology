@article{ ISI:A1992JM11100012,
Author = {SCOTT, DM},
Title = {{AN EVALUATION OF FLOW NET ANALYSIS FOR AQUIFER IDENTIFICATION}},
Journal = {{GROUND WATER}},
Year = {{1992}},
Volume = {{30}},
Number = {{5}},
Pages = {{755-764}},
Month = {{SEP-OCT}},
Abstract = {{A computer-assisted flow net analysis technique was developed and
   evaluated using a hypothetical heterogeneous aquifer with spatially
   variable recharge. Flow nets were derived and analyzed for cases with
   differing piezometric observation density and accuracy. Using a
   criterion to determine a best approximation to the observed piezometric
   surface, the transmissivity distributions implied by fitted surfaces
   were compared with the known true transmissivities. Where an optimum
   approximation to the piezometric surface was clearly defined, the
   resulting computed transmissivities compared favorably with the known
   values. With sparse or inaccurate observations, ft became increasingly
   difficult to identify an optimum solution. ne experiments illustrate
   some of the difficulties inherent in the ground-water inverse problem
   and the flow net analysis method. Application of the method to real
   field cases has yet to be tested.}},
