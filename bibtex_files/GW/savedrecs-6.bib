@article{ ISI:A1991EQ83900009,
Author = {KATZ, BG and CHOQUETTE, AF},
Title = {{AQUEOUS GEOCHEMISTRY OF THE SAND-AND-GRAVEL AQUIFER, NORTHWEST FLORIDA}},
Journal = {{GROUND WATER}},
Year = {{1991}},
Volume = {{29}},
