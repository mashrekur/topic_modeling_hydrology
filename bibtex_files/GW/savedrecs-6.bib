@article{ ISI:A1992HT01300009,
Author = {MOORE, GK},
Title = {{HYDROGRAPH ANALYSIS IN A FRACTURED ROCK TERRANE}},
Journal = {{GROUND WATER}},
Year = {{1992}},
Volume = {{30}},
Number = {{3}},
Pages = {{390-395}},
Month = {{MAY-JUN}},
Abstract = {{A typical watershed near Oak Ridge, Tennessee is underlaid by a
   stormflow zone, which approximately corresponds with the root zone of
   vegetation, and by a ground-water zone.  Analysis of the streamflow
   hydrograph and water-level hydrographs during the nongrowing season
   shows that discharge from the stormflow zone constitutes nearly all
   streamflow after the end of overland runoff but that discharge from the
   ground-water zone is dominant after about 8-10 d (days) of recession. 
   Simple equations that express the relationship between streamflow
   recession and storage depletion can be used to calculate average values
   of specific yield, transmissivity, and hydraulic conductivity in both
   the stormflow and ground-water zones.  The calculated values are
   reasonable and are nearly the same as those obtained by other methods.}},
Publisher = {{GROUND WATER PUBLISHING CO}},
Address = {{601 DEMPSEY RD, WESTERVILLE, OH 43081}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{MOORE, GK (Reprint Author), OAK RIDGE NATL LAB,DIV CELL BIOPHYS,POB 2008,OAK RIDGE,TN 37831, USA.
