@article{ ISI:A1991FA88300015,
Author = {BABAJIMOPOULOS, C},
Title = {{A DOUGLAS-JONES PREDICTOR-CORRECTOR PROGRAM FOR SIMULATING
   ONE-DIMENSIONAL UNSATURATED FLOW IN SOIL}},
Journal = {{GROUND WATER}},
Year = {{1991}},
Volume = {{29}},
Number = {{2}},
Pages = {{267-270}},
Month = {{MAR-APR}},
Abstract = {{A fully documented program to represent one-dimensional unsaturated flow
   in soil is described.  The program is based on a Douglas-Jones
   finite-difference implicit method to solve the Richards equation.  An
   implicit linearization scheme is used to estimate the hydraulic
   conductivity and specific moisture capacity functions.  Predicted
   moisture content profiles compared with two Galerkin finite-element
   solutions and field observations on a Panoche clay loam soil show very
   good agreement.}},
