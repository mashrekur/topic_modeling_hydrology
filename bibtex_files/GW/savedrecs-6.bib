@article{ ISI:A1993LL09600002,
Author = {XUE, YQ and WU, J and LIU, PM and WANG, JJ and JIANG, QB and SHI, HW},
Title = {{SEA-WATER INTRUSION IN THE COASTAL AREA OF LAIZHOU BAY, CHINA .1.
