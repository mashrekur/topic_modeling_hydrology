@article{ ISI:A1991GD50900011,
Author = {GIBBONS, RD},
Title = {{SOME ADDITIONAL NONPARAMETRIC PREDICTION LIMITS FOR GROUNDWATER
   DETECTION MONITORING AT WASTE-DISPOSAL FACILITIES}},
Journal = {{GROUND WATER}},
Year = {{1991}},
Volume = {{29}},
Number = {{5}},
Pages = {{729-736}},
Month = {{SEP-OCT}},
Abstract = {{In a previous paper Gibbons (1990) derived nonparametric prediction
   limits that are applicable to ground-water monitoring applications.  The
   nonparametric prediction limit was defined as the maximum of n
   historical measurements (e.g., taken from upgradient wells), denoted
   X(max, n), and was designed to include one of m future measurements in
   each of k monitoring wells.  In this way, an owner/operator who failed
   an initial sample but passed any one of m - 1 resamples would be
   considered in compliance.  In this paper, these limits are modified so
   that m - 1 out of m future measurements in each of k monitoring wells
   are included.  In this way, the owner/operator is required to pass all m
   - 1 resamples in order to demonstrate compliance in the event of an
   initial failure.  The paper provides a detailed tabulation of confidence
   levels corresponding to selections of n (4-100), k (1-100), and m (2-4).
   Power characteristics of the nonparametric approach relative to the
   parametric alternative are presented for cases in which the assumed
   distribution is correct and incorrect.}},
