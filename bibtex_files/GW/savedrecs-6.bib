@article{ ISI:A1991FA88300009,
Author = {HENSEL, BR and KEEFER, DA and GRIFFIN, RA and BERG, RC},
Title = {{NUMERICAL ASSESSMENT OF A LANDFILL COMPLIANCE LIMIT}},
Journal = {{GROUND WATER}},
Year = {{1991}},
Volume = {{29}},
Number = {{2}},
Pages = {{218-224}},
Month = {{MAR-APR}},
Abstract = {{The PLASM and Random Walk ground-water flow and contaminant transport
   models were used to assess the potential impact of various proposed
   regulatory compliance distances on landfill siting.  Contaminant
   transport modeling was performed for 16 generalized geological sequences
   representative of hydrogeological conditions over an estimated 90 to 95
   percent of Illinois.  Results of this modeling indicate that about 50
   percent of the state would be hydrogeologically suitable for landfilling
   of nonhazardous wastes if the compliance distance was 100 feet.  With a
   compliance distance of 500 feet, about 55 percent of the state would be
   hydrogeologically suitable.  This work demonstrates the utility of
   computer modeling in the development of regulations governing landfill
   siting.}},
