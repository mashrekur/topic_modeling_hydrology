@article{ ISI:A1991GN14600007,
Author = {EHTESHAMI, M and PERALTA, RC and EISELE, H and DEER, H and TINDALL, T},
Title = {{ASSESSING PESTICIDE CONTAMINATION TO GROUND-WATER - A RAPID APPROACH}},
Journal = {{GROUND WATER}},
Year = {{1991}},
Volume = {{29}},
Number = {{6}},
Pages = {{862-868}},
Month = {{NOV-DEC}},
Abstract = {{A methodology has been developed for identifying hazardous
   pesticides/site combinations threatening ground-water contamination.
   Screening methodologies are required to determine which locations and
   pesticides now in use should receive the greatest attention to safeguard
   the public health. The presented method uses a hazard to ground-water
   hydrogeological screening model (DRASTIC) and employs a one-dimensional
   pesticide transport model (CMLS). The method is an efficient and
   practical technique to identify where particular combinations of
   pesticides, water management practices, soils, and geology result in the
   greatest potential hazard to ground-water contamination. Use of the
   presented approach can reduce sampling needs and expense.}},
