@article{ ISI:A1993MF85000018,
Author = {ELNAQA, A and ABUZEID, N},
Title = {{A PROGRAM OF FREQUENCY-ANALYSIS USING GUMBELS METHOD}},
Journal = {{GROUND WATER}},
