@article{ ISI:A1991GN14600001,
Author = {NEWMAN, BD and FUENTES, HR and POLZER, WL},
Title = {{AN EVALUATION OF LITHIUM SORPTION ISOTHERMS AND THEIR APPLICATION TO
   GROUNDWATER TRANSPORT}},
