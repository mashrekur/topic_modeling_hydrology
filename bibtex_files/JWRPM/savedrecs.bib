@article{ ISI:000425621200006,
Author = {Mozenter, Zachary D. and Yates, Andrew J. and Schnier, Kurt E. and
   Hughes, Jeffrey A. and Characklis, Gregory W.},
Title = {{Understanding Water Utility Attitudes toward Water Transfers and Risk:
   Pretest Results}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2018}},
Volume = {{144}},
Number = {{4}},
Month = {{APR}},
Abstract = {{Water transfers between utilities can be a cost-effective substitute for
   supply infrastructure expansions when seeking to reduce vulnerability to
   drought. Water utility employees may choose not to use water transfers,
   however, if they are risk averse, have nonmonetary preferences against
   transfers, or are uninformed about the benefits of transfers. A pretest
   is conducted for two surveys that address these issues. The results
   indicate that, on average, employees are indeed risk averse and prefer
   supply expansions to transfers, but they become more receptive to
   transfers after learning more about them. In addition, there is
   considerable heterogeneity in beliefs; hence, a subset of water
   utilities may be inclined to use water transfers.}},
