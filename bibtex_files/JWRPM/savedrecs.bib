@article{ ISI:000416710900010,
Author = {Sapin, Joseph R. and Saito, Laurel and Dai, Arthur and Rajagopalan,
   Balaji and Hanna, R. Blair and Kauneckis, Derek},
Title = {{Demonstration of Integrated Reservoir Operations and Extreme
   Hydroclimate Modeling of Water Temperatures for Fish Sustainability
   below Shasta Lake}},
