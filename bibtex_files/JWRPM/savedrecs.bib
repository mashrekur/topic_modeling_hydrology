@article{ ISI:000416710900004,
Author = {Witt, Adam and Magee, Timothy and Stewart, Kevin and Hadjerioua, Boualem
   and Neumann, David and Zagona, Edith and Politano, Marcela},
Title = {{Development and Implementation of an Optimization Model for Hydropower
   and Total Dissolved Gas in the Mid-Columbia River System}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
