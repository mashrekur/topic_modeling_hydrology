@article{ ISI:000447255900001,
Author = {Zhou, Zuhao and Jia, Yangwen and Qiu, Yaqin and Liu, Jiajia and Wang,
   Hao and Xu, Chong-Yu and Li, Jia and Liu, Lin},
Title = {{Simulation of Dualistic Hydrological Processes Affected by Intensive
   Human Activities Based on Distributed Hydrological Model}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2018}},
Volume = {{144}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Affected by intensive human activities, basin hydrologic systems show
   characteristics of a dualistic structure. Thus, simulation of
   hydrological processes needs to consider the social water cycle system
   to obtain a more accurate result. This study uses a dualistic water
   cycle simulation system to simulate the hydrological processes affected
   by intensive human activities, which comprises the natural water cycle
   system and social water cycle system. The social water cycle system
   includes the agricultural water cycle system, industrial water cycle
   system, domestic water cycle system, and cross-regional allocation
   system. As part of the dualistic water cycle simulation system, an
   integrated dualistic hydrological model is developed which couples a
   distributed hydrological simulation module with a water resource
   allocation module. The integrated modeling approach is applied to the
   Haihe River basin. The results show that the model performance can be
   improved when considering coupled simulation of natural and social water
   cycle systems. This model can also be used to investigate the
   evolutionary relationships among the natural and social water cycle
   systems in the study region, and to assess the regional water resources
   in other basins affected by intensive human activities. (C) 2018
   American Society of Civil Engineers.}},
