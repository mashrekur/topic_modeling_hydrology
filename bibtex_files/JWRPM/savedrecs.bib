@article{ ISI:000441686300004,
Author = {Maheshwari, Abhilasha and Abokifa, Ahmed A. and Gudi, Ravindra D. and
   Biswas, Pratim},
Title = {{Coordinated Decentralization-Based Optimization of Disinfectant Dosing
   in Large-Scale Water Distribution Networks}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
