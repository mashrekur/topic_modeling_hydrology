@article{ ISI:000388223600001,
Author = {Keshtkar, Hilda and Bozorg-Haddad, Omid and Jalali, Mohammad-Reza and
   Loaiciga, Hugo A.},
Title = {{Application of Wind Energy to Withdraw Groundwater for Irrigation
   Management}},
