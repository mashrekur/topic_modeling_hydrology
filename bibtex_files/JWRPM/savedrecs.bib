@article{ ISI:000438018100002,
Author = {Salehi, Sattar and Tabesh, Massoud and Ghazizadeh, Mohammadreza Jalili},
Title = {{HRDM Method for Rehabilitation of Pipes in Water Distribution Networks
   with Inaccurate Operational-Failure Data}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2018}},
Volume = {{144}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{In this research, a hybrid risk-based decision-making (HRDM) method is
   proposed, in which by using pipe design parameters (PDPs), it is
   possible to determine a rehabilitation road map for water distribution
   networks (WDNs) in the future. This method can be useful for networks
   that have imprecise operational-failure data when planning
   rehabilitation. In this regard, based on PDPs, the water supply failure
   risks (WSFR) of pipes in the network are assessed; subsequently, the
   pipes are prioritized based on their rehabilitation potential indices
   (RPI), which are calculated using a decision-making model (DMM) called
   Fuzzy TOPSIS (technique for order preferences by similarity to an ideal
   solution). This method is used to plan the water network rehabilitation
   needs for Qods Town of Qom Province in Iran. The results indicate that,
   in the absence of accurate operational-failure data, a road map of
   future rehabilitation of water networks can be determined by using PDPs.
   Furthermore, the network zones and their pipes can be prioritized
   according to their rehabilitation needs. It was also found that
   pressure, flow velocity, flow rate, and pipe diameter are more reliable
   PDPs for rehabilitation planning.}},
