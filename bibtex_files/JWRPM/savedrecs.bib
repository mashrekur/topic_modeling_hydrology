@article{ ISI:000422902900016,
Author = {Zhang, Xiaoqi and Liu, Pan and Xu, Chong-Yu and Ming, Bo and Xie, Aili
   and Feng, Maoyuan},
Title = {{Conditional Value-at-Risk for Nonstationary Streamflow and Its
   Application for Derivation of the Adaptive Reservoir Flood Limited Water
   Level}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2018}},
Volume = {{144}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{The existing risk-based approaches for quantifying expected flood damage
   loss may not be suitable in the changing environment when streamflow
   becomes nonstationary. Conditional value-at-risk (CVaR(alpha)), a
   modified form of value-at-risk (VaR(alpha)), that takes account of both
   the magnitude and probability of damage loss has been widely applied to
   water resources issues. However, quantification of CVaR(alpha) under
   nonstationary conditions has not been addressed in the literature. This
   study proposes an approach that incorporates CVaR(alpha) in quantifying
   the flood damage loss under nonstationary conditions, and then
   CVaR(alpha)(n) associated with flood risk over a specified time horizon
   is deduced. To illustrate the concept of CVaR(alpha), this study applies
   CVaR(alpha) into the adaptive flood limited water level (FLWL)
   optimization. With China's Three Gorges Reservoir (TGR) as a case study,
   the results indicate that (1) the CVaR(alpha) not only can represent the
   possible flood damage loss over a time horizon in the future, but also
   can reflect the flood risk probability by choosing a suitable confidence
   level; and (2) it is reliable to take account of the CVaR(alpha) value
   as a constraint when studying the adaptive FLWL optimization problem.
   (C) 2018 American Society of Civil Engineers.}},
