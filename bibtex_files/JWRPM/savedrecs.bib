@article{ ISI:000386360300002,
Author = {Mani, Amir and Tsai, Frank T. -C. and Paudel, Krishna P.},
Title = {{Mixed Integer Linear Fractional Programming for Conjunctive Use of
   Surface Water and Groundwater}},
