@article{ ISI:000416716000007,
Author = {Hwang, Hwee and Lansey, Kevin},
Title = {{Water Distribution System Classification Using System Characteristics
   and Graph-Theory Metrics}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
