@article{ ISI:000313354200008,
Author = {Zheng, Pearl Q. and Hobbs, Benjamin F.},
Title = {{Multiobjective Portfolio Analysis of Dam Removals Addressing Dam Safety,
   Fish Populations, and Cost}},
