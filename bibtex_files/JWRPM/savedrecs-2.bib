@article{ ISI:000368828800013,
Author = {Yoo, Do Guen and Jung, Donghwi and Kang, Doosun and Kim, Joong Hoon and
   Lansey, Kevin},
Title = {{Seismic Hazard Assessment Model for Urban Water Supply Networks}},
