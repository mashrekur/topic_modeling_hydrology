@article{ ISI:000361557400001,
Author = {Herman, Jonathan D. and Reed, Patrick M. and Zeff, Harrison B. and
   Characklis, Gregory W.},
Title = {{How Should Robustness Be Defined for Water Systems Planning under
   Change?}},
