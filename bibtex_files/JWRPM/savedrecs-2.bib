@article{ ISI:000334445800014,
Author = {Taghian, Mehrdad and Rosbjerg, Dan and Haghighi, Ali and Madsen, Henrik},
Title = {{Optimization of Conventional Rule Curves Coupled with Hedging Rules for
   Reservoir Operation}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2014}},
Volume = {{140}},
Number = {{5}},
Pages = {{693-698}},
Month = {{MAY 1}},
Abstract = {{As a common approach to reservoir operating policies, water levels at
   the end of each time interval should be kept at or above the rule curve.
   In this study, the policy is captured using rationing of the target
   yield to reduce the intensity of severe water shortages. For this
   purpose, a hybrid model is developed to optimize simultaneously both the
   conventional rule curve and the hedging rule. In the compound model, a
   simple genetic algorithm is coupled with a simulation program, including
   an inner linear programming algorithm. In this way, operational policies
   are imposed by priority concepts to achieve the optimal water allocation
   and the target storage levels for reservoirs. As a case study, a
   multipurpose, multireservoir system in southern Iran is selected. The
   results show that the model has good performance in extracting the
   optimum policy for reservoir operation under both normal and drought
   conditions.}},
