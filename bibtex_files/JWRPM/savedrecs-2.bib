@article{ ISI:000269061900010,
Author = {You, Jiing-Yun and Cai, Ximing},
Title = {{Reexamination of Critical Period for Reservoir Design and Operation}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2009}},
Volume = {{135}},
