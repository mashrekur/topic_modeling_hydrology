@article{ ISI:000313354200012,
Author = {Hansen, Annette K. and Madsen, Henrik and Bauer-Gottwein, Peter and
   Rosbjerg, Dan and Falk, Anne Katrine V.},
Title = {{Optimization of Well Field Operation: Case Study of Sonderso Waterworks,
   Denmark}},
