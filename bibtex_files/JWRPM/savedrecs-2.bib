@article{ ISI:000366750200008,
Author = {Wild, Thomas B. and Loucks, Daniel P. and Annandale, George W. and
   Kaini, Prakash},
Title = {{Maintaining Sediment Flows through Hydropower Dams in the Mekong River
   Basin}},
