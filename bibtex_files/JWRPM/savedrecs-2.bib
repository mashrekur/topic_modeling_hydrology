@article{ ISI:000330518000012,
Author = {Marchi, Angela and Simpson, Angus R.},
Title = {{Correction of the EPANET Inaccuracy in Computing the Efficiency of
   Variable Speed Pumps}},
Journal = {{JOURNAL OF WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{2013}},
Volume = {{139}},
Number = {{4}},
Pages = {{456-459}},
Month = {{JUL}},
Abstract = {{One attractive possibility for saving energy in water distribution
   systems is the introduction of variable speed pumps (VSPs). However, to
   assess the cost effectiveness of using VSPs, a correct estimate of VSP
   energy consumption, and therefore efficiency, is essential. This task
   involves estimating the efficiency of various components: pump, motor,
   and variable speed drive. Hydraulic solvers, which are used to check the
   hydraulics of the system, usually use the affinity laws to describe the
   pump behavior in a VSP pumping system. This paper demonstrates the
   inaccuracy of the popular hydraulic solver EPANET 2, which does not
   properly take into account the affinity laws in the efficiency
   computation when the speed changes for VSP operations. The correction of
   the code is presented and an additional improvement of the toolkit to
   directly retrieve the efficiency is introduced. (C) 2013 American
   Society of Civil Engineers.}},
