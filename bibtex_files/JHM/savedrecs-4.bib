@article{ ISI:000371612100001,
Author = {Pineda, Luis E. and Willems, Patrick},
Title = {{Multisite Downscaling of Seasonal Predictions to Daily Rainfall
   Characteristics over Pacific-Andean River Basins in Ecuador and Peru
   Using a Nonhomogeneous Hidden Markov Model}},
