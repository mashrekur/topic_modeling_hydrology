@article{ ISI:000355126500016,
Author = {Schleiss, Marc and Smith, James},
Title = {{A Method to Estimate the 3D-Time Structure of the Raindrop Size
   Distribution Using Radar and Disdrometer Data{*}}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2015}},
Volume = {{16}},
