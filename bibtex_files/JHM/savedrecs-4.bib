@article{ ISI:000352735100009,
Author = {Ionita, M. and Dima, M. and Lohmann, G. and Scholz, P. and Rimbu, N.},
Title = {{Predicting the June 2013 European Flooding Based on Precipitation, Soil
   Moisture, and Sea Level Pressure}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2015}},
Volume = {{16}},
Number = {{2}},
Pages = {{598-614}},
Month = {{APR}},
Abstract = {{Over recent decades Europe has experienced heavy floods, with major
   consequences for thousands of people and billions of euros worth of
   damage. In particular, the summer of 2013 flood in central Europe showed
   how vulnerable modern society is to hydrological extremes and emphasized
   once more the need for improved forecast methods of such extreme
   climatic events. Based on a multiple linear regression model, it is
   shown here that 55\% of the June 2013 Elbe River extreme discharge could
   have been predicted using May precipitation, soil moisture, and sea
   level pressure. Moreover, the model was able to predict more than 75\%
   of the total Elbe River discharge for June 2013 (in terms of magnitude)
   by also incorporating the amount of precipitation recorded during the
   days prior to the flood, but the predicted discharge for the June 2013
   event was still underestimated by 25\%. Given that all predictors used
   in the model are available at the end of each month, the forecast scheme
   can be used to predict extreme events and to provide early warnings for
   upcoming floods. The forecast methodology could be relevant for other
   rivers also, depending on their location and their climatic background.}},
