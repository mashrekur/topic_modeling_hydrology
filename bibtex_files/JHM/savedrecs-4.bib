@article{ ISI:000349367500016,
Author = {Daniels, Emma E. and Hutjes, Ronald W. A. and Lenderink, Geert and
   Ronda, Reinder J. and Holtslag, Albert A. M.},
Title = {{Land Surface Feedbacks on Spring Precipitation in the Netherlands}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
