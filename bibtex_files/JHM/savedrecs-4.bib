@article{ ISI:000371473000002,
Author = {Cocks, Stephen B. and Martinaitis, Steven M. and Kaney, Brian and Zhang,
   Jian and Howard, Kenneth},
Title = {{MRMS QPE Performance during the 2013/14 Cool Season}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
