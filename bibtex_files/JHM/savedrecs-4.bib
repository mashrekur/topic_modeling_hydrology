@article{ ISI:000376397600001,
Author = {Lin, Tzu-Shun and Cheng, Fang-Yi},
Title = {{Impact of Soil Moisture Initialization and Soil Texture on Simulated
   Land-Atmosphere Interaction in Taiwan}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
