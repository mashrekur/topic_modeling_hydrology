@article{ ISI:000364975900001,
Author = {Agel, Laurie and Barlow, Mathew and Qian, Jian-Hua and Colby, Frank and
   Douglas, Ellen and Eichler, Timothy},
Title = {{Climatology of Daily Precipitation and Extreme Precipitation Events in
   the Northeast United States}},
