@article{ ISI:000359762700028,
Author = {Huang, Yongjie and Cui, Xiaopeng},
Title = {{Moisture Sources of Torrential Rainfall Events in the Sichuan Basin of
   China during Summers of 2009-13}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
