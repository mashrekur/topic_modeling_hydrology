@article{ ISI:000367402600001,
Author = {Holsclaw, Tracy and Greene, Arthur M. and Robertson, Andrew W. and
   Smyth, Padhraic},
Title = {{A Bayesian Hidden Markov Model of Daily Precipitation over South and
   East Asia}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
