@article{ ISI:000377103500003,
Author = {Qi, Youcun and Martinaitis, Steven and Zhang, Jian and Cocks, Stephen},
Title = {{A Real-Time Automated Quality Control of Hourly Rain Gauge Data Based on
   Multiple Sensors in MRMS System}},
