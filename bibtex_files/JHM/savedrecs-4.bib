@article{ ISI:000367405000001,
Author = {Garnaud, Camille and Belair, Stephane and Berg, Aaron and Rowlandson,
   Tracy},
Title = {{Hyperresolution Land Surface Modeling in the Context of SMAP Cal-Val}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
