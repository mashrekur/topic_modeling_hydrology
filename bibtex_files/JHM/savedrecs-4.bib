@article{ ISI:000390079800009,
Author = {Sahlu, Dejene and Nikolopoulos, Efthymios I. and Moges, Semu A. and
   Anagnostou, Emmanouil N. and Hailu, Dereje},
Title = {{First Evaluation of the Day-1 IMERG over the Upper Blue Nile Basin}},
