@article{ ISI:000383310500001,
Author = {Zhou, Yaping and Wu, Di and Lau, William K. -M. and Tao, Wei-Kuo},
Title = {{Scale Dependence of Land-Atmosphere Interactions in Wet and Dry Regions
   as Simulated with NU-WRF over the Southwestern and South-Central United
   States}},
