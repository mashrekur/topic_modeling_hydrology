@article{ ISI:000367405100005,
Author = {Rhee, Jinyoung and Cho, Jaepil},
Title = {{Future Changes in Drought Characteristics: Regional Analysis for South
   Korea under CMIP5 Projections}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
