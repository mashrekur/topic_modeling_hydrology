@article{ ISI:000373313500001,
Author = {Hirpa, Feyera A. and Salamon, Peter and Alfieri, Lorenzo and Thielen-del
   Pozo, Jutta and Zsoter, Ervin and Pappenberger, Florian},
Title = {{The Effect of Reference Climatology on Global Flood Forecasting}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
