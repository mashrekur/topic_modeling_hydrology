@article{ ISI:000362222500001,
Author = {Tawfik, Ahmed B. and Dirmeyer, Paul A. and Santanello, Jr., Joseph A.},
Title = {{The Heated Condensation Framework. Part I: Description and Southern
   Great Plains Case Study}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
