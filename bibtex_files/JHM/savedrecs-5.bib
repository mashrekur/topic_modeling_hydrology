@article{ ISI:000480583100001,
Author = {Tong, Xuan and Yan, Zhongwei and Xia, Jiangjiang and Lou, Xiao},
Title = {{Decisive Atmospheric Circulation Indices for July-August Precipitation
   in North China Based on Tree Models}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
