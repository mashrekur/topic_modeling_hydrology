@article{ ISI:000454494100001,
Author = {Heredia, Maria Belen and Junquas, Clementine and Prieur, Clementine and
   Condom, Thomas},
Title = {{New Statistical Methods for Precipitation Bias Correction Applied to WRF
   Model Simulations in the Antisana Region, Ecuador}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
