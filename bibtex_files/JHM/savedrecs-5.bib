@article{ ISI:000440280800001,
Author = {Mernild, Sebastian H. and Liston, Glen E. and Hiemstra, Christopher A.
   and Yde, Jacob C. and Casassa, Gino},
Title = {{Annual River Runoff Variations and Trends for the Andes Cordillera}},
