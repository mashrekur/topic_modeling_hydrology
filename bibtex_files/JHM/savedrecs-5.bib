@article{ ISI:000477991700001,
Author = {Sullivan, Ryan C. and Kotamarthi, V. Rao and Feng, Yan},
Title = {{Recovering Evapotranspiration Trends from Biased CMIP5 Simulations and
   Sensitivity to Changing Climate over North America}},
