@article{ ISI:000484474900001,
Author = {Zhong, Ziqian and He, Bin and Guo, Lanlan and Zhang, Yafeng},
Title = {{Performance of Various Forms of the Palmer Drought Severity Index in
   China from 1961 to 2013}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
