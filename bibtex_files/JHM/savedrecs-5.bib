@article{ ISI:000474434600002,
Author = {Han, Shasha and Coulibaly, Paulin},
Title = {{Probabilistic Flood Forecasting Using Hydrologic Uncertainty Processor
   with Ensemble Weather Forecasts}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
