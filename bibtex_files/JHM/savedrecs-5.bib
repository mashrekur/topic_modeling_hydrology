@article{ ISI:000427359300012,
Author = {Gerken, Tobias and Bromley, Gabriel T. and Stoy, Paul C.},
Title = {{Surface Moistening Trends in the Northern North American Great Plains
   Increase the Likelihood of Convective Initiation}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
