@article{ ISI:000417353900008,
Author = {Zhang, Ning and Chen, Yan and Luo, Ling and Wang, Yongwei},
Title = {{Effectiveness of Different Urban Heat Island Mitigation Methods and
   Their Regional Impacts}},
