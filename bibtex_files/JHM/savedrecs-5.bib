@article{ ISI:000447120100004,
Author = {Watters, Daniel and Battaglia, Alessandro and Mroz, Kamil and Tridon,
   Frederic},
Title = {{Validation of the GPM Version-5 Surface Rainfall Products over Great
   Britain and Ireland}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
