@article{ ISI:000472081100001,
Author = {Koster, R. D. and Schubert, S. D. and Wang, H. and Mahanama, S. P. and
   DeAngelis, Anthony M.},
Title = {{Flash Drought as Captured by Reanalysis Data: Disentangling the
   Contributions of Precipitation Deficit and Excess Evapotranspiration}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2019}},
Volume = {{20}},
Number = {{6}},
