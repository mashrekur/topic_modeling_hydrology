@article{ ISI:000404347100001,
Author = {Garnaud, Camille and Belair, Stephane and Carrera, Marco L. and McNairn,
   Heather and Pacheco, Anna},
Title = {{Field-Scale Spatial Variability of Soil Moisture and L-Band Brightness
   Temperature from Land Surface Modeling}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
