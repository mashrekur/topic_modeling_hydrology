@article{ ISI:000461898700001,
Author = {Pritchard, David M. W. and Forsythe, Nathan and Fowler, Hayley J. and
   O'Donnell, Greg M. and Li, Xiao-Feng},
Title = {{Evaluation of Upper Indus Near-Surface Climate Representation by WRF in
   the High Asia Refined Analysis}},
