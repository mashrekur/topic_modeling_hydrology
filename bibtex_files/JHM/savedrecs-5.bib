@article{ ISI:000466440100002,
Author = {Seager, Richard and Nakamura, Jennifer and Ting, Mingfang},
Title = {{Mechanisms of Seasonal Soil Moisture Drought Onset and Termination in
   the Southern Great Plains}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
