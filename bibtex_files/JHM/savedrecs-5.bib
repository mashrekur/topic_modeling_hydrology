@article{ ISI:000489564700001,
Author = {An, Lixing and Ren, Xingyuan and Hao, Yonghong and Yeh, Tian-Chyi Jim
   and Zhang, Baoju},
Title = {{Utilizing Precipitation and Spring Discharge Data to Identify
   Groundwater Quick Flow Belts in a Karst Spring Catchment}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2019}},
Volume = {{20}},
