@article{ ISI:000503813200001,
Author = {Toride, Kinya and Iseri, Yoshihiko and Warner, Michael D. and Frans,
   Chris D. and Duren, Angela M. and England, John F. and Kavvas, M. Levent},
Title = {{Model-Based Probable Maximum Precipitation Estimation: How to Estimate
   the Worst-Case Scenario Induced by Atmospheric Rivers?}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
