@article{ ISI:000405926400007,
Author = {Smalley, Mark and Kirstetter, Pierre-Emmanuel and L'Ecuyer, Tristan},
Title = {{How Frequent is Precipitation over the Contiguous United States?
   Perspectives from Ground-Based and Spaceborne Radars}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
