@article{ ISI:000496133100001,
Author = {Lv, Meixia and Ma, Zhuguo and Chen, Liang and Peng, Shaoming},
Title = {{Evapotranspiration Reconstruction Based on Land Surface Models and
   Observed Water Budget Components While Considering Irrigation}},
