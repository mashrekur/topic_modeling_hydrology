@article{ ISI:000397006300011,
Author = {Yang, Long and Liu, Maofeng and Smith, James A. and Tian, Fuqiang},
Title = {{Typhoon Nina and the August 1975 Flood over Central China}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
