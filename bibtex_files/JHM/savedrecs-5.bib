@article{ ISI:000489122500001,
Author = {Fairbairn, David and de Rosnay, Patricia and Browne, Philip A.},
Title = {{The New Stand-Alone Surface Analysis at ECMWF: Implications for
   Land-Atmosphere DA Coupling}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
