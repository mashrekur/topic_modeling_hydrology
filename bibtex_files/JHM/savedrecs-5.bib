@article{ ISI:000445390800005,
Author = {Jia, Binghao and Liu, Jianguo and Xie, Zhenghui and Shi, Chunxiang},
Title = {{Interannual Variations and Trends in Remotely Sensed and Modeled Soil
   Moisture in China}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
