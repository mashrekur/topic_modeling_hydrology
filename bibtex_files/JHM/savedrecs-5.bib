@article{ ISI:000405926700005,
Author = {Cioni, Guido and Hohenegger, Cathy},
Title = {{Effect of Soil Moisture on Diurnal Convection and Precipitation in
   Large-Eddy Simulations}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
