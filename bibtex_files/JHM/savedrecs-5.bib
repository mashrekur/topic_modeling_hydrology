@article{ ISI:000404347100002,
Author = {Heiblum, Reuven H. and Koren, Ilan and Altaratz, Orit and Kostinski,
   Alexander B.},
Title = {{The Consistent Behavior of Tropical Rain: Average Reflectivity Vertical
   Profiles Determined by Rain Top Height}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
