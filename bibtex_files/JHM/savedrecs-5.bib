@article{ ISI:000452310500002,
Author = {Hayden, Lindsey and Liu, Chuntao},
Title = {{A Multiyear Analysis of Global Precipitation Combining CloudSat and GPM
   Precipitation Retrievals}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2018}},
