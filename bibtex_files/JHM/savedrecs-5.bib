@article{ ISI:000404347100005,
Author = {Kwon, Yonghwan and Yang, Zong-Liang and Hoar, Timothy J. and Toure, Ally
   M.},
Title = {{Improving the Radiance Assimilation Performance in Estimating Snow Water
   Storage across Snow and Land-Cover Types in North America}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
