@article{ ISI:000453905900001,
Author = {Zhang, Jiaying and Lin, Liao-Fan and Bras, Rafael L.},
Title = {{Evaluation of the Quality of Precipitation Products: A Case Study Using
   WRF and IMERG Data over the Central United States}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
