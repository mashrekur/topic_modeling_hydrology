@article{ ISI:000460744600001,
Author = {Zhang, Xinxuan and Anagnostou, Emmanouil N.},
Title = {{Evaluation of Numerical Weather Model-Based Satellite Precipitation
   Adjustment in Tropical Mountainous Regions}},
