@article{ ISI:000494433200001,
Author = {Fish, Meredith A. and Wilson, Anna M. and Ralph, F. Martin},
Title = {{Atmospheric River Families: Definition and Associated Synoptic
   Conditions}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
