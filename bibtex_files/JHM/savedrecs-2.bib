Author = {Zhuge, Xiao-Yong and Yu, Fan and Zhang, Cheng-Wei},
Title = {{Rainfall Retrieval and Nowcasting Based on Multispectral Satellite
   Images. Part I: Retrieval Study on Daytime 10-Minute Rain Rate}},
