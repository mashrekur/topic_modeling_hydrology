@article{ ISI:000286178800012,
Author = {Gutowski, Jr., William J. and Arritt, Raymond W. and Kawazoe, Sho and
   Flory, David M. and Takle, Eugene S. and Biner, Sebastien and Caya,
   Daniel and Jones, Richard G. and Laprise, Rene and Leung, L. Ruby and
