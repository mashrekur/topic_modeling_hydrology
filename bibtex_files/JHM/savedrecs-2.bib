@article{ ISI:000296029000012,
Author = {David, Cedric H. and Maidment, David R. and Niu, Guo-Yue and Yang,
   Zong-Liang and Habets, Florence and Eijkhout, Victor},
Title = {{River Network Routing on the NHDPlus Dataset}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
