@article{ ISI:000287991600009,
Author = {Zhang, Li and Dirmeyer, Paul A. and Wei, Jiangfeng and Guo, Zhichang and
   Lu, Cheng-Hsuan},
Title = {{Land-Atmosphere Coupling Strength in the Global Forecast}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
