@article{ ISI:000296029000017,
Author = {Dyer, Jamie},
Title = {{Analysis of a Warm-Season Surface-Influenced Mesoscale Convective
   Boundary in Northwest Mississippi}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
