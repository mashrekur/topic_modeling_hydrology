@article{ ISI:000298215400016,
Author = {Silvestro, Francesco and Rebora, Nicola and Ferraris, Luca},
Title = {{Quantitative Flood Forecasting on Small- and Medium-Sized Basins: A
   Probabilistic Approach for Operational Purposes}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
