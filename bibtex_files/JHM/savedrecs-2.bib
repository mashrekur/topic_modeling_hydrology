@article{ ISI:000273060800016,
Author = {Misra, Vasubandhu},
Title = {{The Amplification of the ENSO Forcing over Equatorial Amazon}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2009}},
Volume = {{10}},
Number = {{6}},
Pages = {{1561-1568}},
Month = {{DEC}},
Abstract = {{The remote influence of the El Nino-Southern Oscillation (ENSO) strongly
   manifests over the equatorial Amazon (EA)-including parts of southern
   Venezuela, Guyana, French Guiana, and Suriname-when there is a
   large-scale anomalous upper-level divergence over continental tropical
   South America. Modeling studies conducted in this paper suggest that it
   is because of the modulation of the local diurnal cycle of the moisture
   flux convergence, which results in the local amplification of the ENSO
   signal over the EA. Further, it is shown that the local land surface
   feedback plays a relatively passive but important role of maintaining
   these interannual precipitation anomalies over the EA region.}},
