@article{ ISI:000277601000021,
Author = {Tokay, Ali and Bashor, Paul G. and McDowell, Victoria L.},
Title = {{Comparison of Rain Gauge Measurements in the Mid-Atlantic Region}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
