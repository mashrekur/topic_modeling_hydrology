@article{ ISI:000261861600004,
Author = {Mernild, Sebastian H. and Liston, Glen E. and Hiemstra, Christopher A.
   and Steffen, Konrad},
Title = {{Surface Melt Area and Water Balance Modeling on the Greenland Ice Sheet
   1995-2005}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
