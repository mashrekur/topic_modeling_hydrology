@article{ ISI:000261861600003,
Author = {de Wit, A. J. W. and de Bruin, S. and Torfs, P. J. J. F.},
Title = {{Representing Uncertainty in Continental-Scale Gridded Precipitation
   Fields for Agrometeorological Modeling}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
