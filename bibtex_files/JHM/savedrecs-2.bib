@article{ ISI:000269262800012,
Author = {Lintner, Benjamin R. and Neelin, J. David},
Title = {{Soil Moisture Impacts on Convective Margins}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2009}},
Volume = {{10}},
