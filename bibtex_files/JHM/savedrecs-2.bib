@article{ ISI:000260012500009,
Author = {Liston, Glen E. and Hiemstra, Christopher A. and Elder, Kelly and Cline,
   Donald W.},
Title = {{Mesocell Study Area Snow Distributions for the Cold Land Processes
   Experiment (CLPX)}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
