@article{ ISI:000280955600009,
Author = {Zeng, Xubin and Barlage, Mike and Castro, Chris and Fling, Kelly},
Title = {{Comparison of Land-Precipitation Coupling Strength Using Observations
   and Models}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2010}},
Volume = {{11}},
Number = {{4}},
Pages = {{979-994}},
Month = {{AUG}},
Abstract = {{Numerous studies have attempted to address the land-precipitation
   coupling, but scientists' understanding remains limited and
   discrepancies still exist from different studies. A new parameter Gamma
   is proposed here to estimate the land-precipitation coupling strength
   based on the ratio of the covariance between monthly or seasonal
   precipitation and evaporation anomalies (from their climatological
   means) over the variance of precipitation anomalies. The Gamma value is
   easy to compute and insensitive to the horizontal scales used; however,
   it does not provide causality. A relatively high Gamma is a
   necessary-but not sufficient-condition for a relatively strong
   land-precipitation coupling. A computation of Gamma values using two
   global reanalyses (ECMWF and NCEP), one regional reanalysis {[}North
   American Regional Reanalysis (NARR)], and observed precipitation along
   with Variable Infiltration Capacity (VIC)-derived evaporation data
   indicates that the land-precipitation coupling is stronger in summer and
   weaker in winter. The strongest coupling (i.e., hot spots) occurs over
   the western and central parts of North America, part of the Eurasia
   midlatitude, and Sahel in boreal summer and over most of Australia,
   Argentina, and South Africa in austral summer. The Community Climate
   System Model, version 3 (CCSM3) shows much higher Gamma values,
   consistent with the strong coupling shown by its atmosphere-land coupled
   components in previous studies. Its overall spatial pattern of Gamma
   values is not affected much over most regions by the doubling of CO2 in
   CCSM3. The Gamma values from the Regional Atmospheric Modeling System
   (RAMS) are more realistic than those from CCSM3; however, they are still
   higher than those from observations over North America.}},
