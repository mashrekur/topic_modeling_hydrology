@article{ ISI:000263853000022,
Author = {Elder, Kelly and Cline, Don and Liston, Glen E. and Armstrong, Richard},
Title = {{NASA Cold Land Processes Experiment (CLPX 2002/03): Field Measurements
   of Snowpack Properties and Soil Moisture}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
