@article{ ISI:000256966000009,
Author = {Lauwaet, D. and De Ridder, K. and van Lipzig, N. P. M.},
Title = {{The influence of soil and vegetation parameters on atmospheric variables
   relevant for convection in the Sahel}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
