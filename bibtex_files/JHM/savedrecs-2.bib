@article{ ISI:000254897100005,
Author = {Vanderlinden, Karl and Giraldez, Juan Vicente and Van Meirvenne, Marc},
Title = {{Spatial estimation of reference evapotranspiration in Andalusia, Spain}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
