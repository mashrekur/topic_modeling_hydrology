@article{ ISI:000166701200006,
Author = {Cohen, JL and Salstein, DA and Rosen, RD},
Title = {{Interannual variability in the meridional transport of water vapor}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
