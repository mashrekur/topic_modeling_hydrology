@article{ ISI:000187534900011,
Author = {Adler, RF and Huffman, GJ and Chang, A and Ferraro, R and Xie, PP and
   Janowiak, J and Rudolf, B and Schneider, U and Curtis, S and Bolvin, D
   and Gruber, A and Susskind, J and Arkin, P and Nelkin, E},
Title = {{The version-2 global precipitation climatology project (GPCP) monthly
   precipitation analysis (1979-present)}},
