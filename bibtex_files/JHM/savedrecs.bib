@article{ ISI:000240653900003,
Author = {Guo, Zhichang and Dirmeyer, Paul A. and Koster, Randal D. and Bonan,
   Gordon and Chan, Edmond and Cox, Peter and Gordon, C. T. and Kanae,
   Shinjiro and Kowalczyk, Eva and Lawrence, David and Liu, Ping and Lu,
   Cheng-Hsuan and Malyshev, Sergey and McAvaney, Bryant and McGregor, J.
   L. and Mitchell, Ken and Mocko, David and Oki, Taikan and Oleson, Keith
   W. and Pitman, Andrew and Sud, Y. C. and Taylor, Christopher M. and
   Verseghy, Diana and Vasic, Ratko and Xue, Yongkang and Yamada, Tomohito},
Title = {{GLACE: The Global Land-Atmosphere Coupling Experiment. Part II: Analysis}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2006}},
Volume = {{7}},
Number = {{4}},
Pages = {{611-625}},
Month = {{AUG}},
Abstract = {{The 12 weather and climate models participating in the Global
   Land-Atmosphere Coupling Experiment ( GLACE) show both a wide variation
   in the strength of land-atmosphere coupling and some intriguing
   commonalities. In this paper, the causes of variations in coupling
   strength-both the geographic variations within a given model and the
   model-to-model differences-are addressed. The ability of soil moisture
   to affect precipitation is examined in two stages, namely, the ability
   of the soil moisture to affect evaporation, and the ability of
   evaporation to affect precipitation. Most of the differences between the
   models and within a given model are found to be associated with the
   first stage-an evaporation rate that varies strongly and consistently
   with soil moisture tends to lead to a higher coupling strength. The
   first-stage differences reflect identifiable differences in model
   parameterization and model climate. Intermodel differences in the
   evaporation-precipitation connection, however, also play a key role.}},
