@article{ ISI:000238689800008,
Author = {Coudert, B and Ottle, C and Boudevillain, B and Demarty, J and
   Guillevic, P},
Title = {{Contribution of thermal infrared remote sensing data in multiobjective
   calibration of a dual-source SVAT model}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2006}},
Volume = {{7}},
