@article{ ISI:000220693800002,
Author = {Ye, HC and Yang, DQ and Zhang, TJ and Zhang, XB and Ladochy, S and
   Ellison, M},
Title = {{The impact of climatic conditions on seasonal river discharges in
   Siberia}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
