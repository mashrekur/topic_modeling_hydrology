@article{ ISI:000182276800015,
Author = {Belair, S and Brown, R and Mailhot, J and Bilodeau, B and Crevier, LP},
Title = {{Operational implementation of the ISBA land surface scheme in the
   Canadian regional weather forecast model. Part II: Cold season results}},
