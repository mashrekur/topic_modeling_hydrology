@article{ ISI:000305776600014,
Author = {Mei, Rui and Wang, Guiling},
Title = {{Summer Land-Atmosphere Coupling Strength in the United States:
   Comparison among Observations, Reanalysis Data, and Numerical Models}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
