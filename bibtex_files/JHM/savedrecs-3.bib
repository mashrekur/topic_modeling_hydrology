@article{ ISI:000345898400027,
Author = {Getirana, Augusto C. V. and Dutra, Emanuel and Guimberteau, Matthieu and
   Kam, Jonghun and Li, Hong-Yi and Decharme, Bertrand and Zhang, Zhengqiu
   and Ducharne, Agnes and Boone, Aaron and Balsamo, Gianpaolo and Rodell,
