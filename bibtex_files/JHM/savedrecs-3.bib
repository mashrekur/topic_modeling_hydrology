@article{ ISI:000302483900014,
Author = {Schleiss, Marc and Jaffrain, Joel and Berne, Alexis},
Title = {{Stochastic Simulation of Intermittent DSD Fields in Time}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
