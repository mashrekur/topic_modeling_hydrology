@article{ ISI:000317561200011,
Author = {Smiatek, Gerhard and Kaspar, Severin and Kunstmann, Harald},
Title = {{Hydrological Climate Change Impact Analysis for the Figeh Spring near
   Damascus, Syria}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
