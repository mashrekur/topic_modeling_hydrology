@article{ ISI:000300138400022,
Author = {Tokay, Ali and Ozturk, Kurtulus},
Title = {{An Experimental Study of the Small-Scale Variability of Rainfall}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
