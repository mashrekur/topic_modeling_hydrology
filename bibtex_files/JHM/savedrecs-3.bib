@article{ ISI:000330532300015,
Author = {Wang, Linlin and Gao, Zhiqiu and Pan, Zaitao and Guo, Xiaofeng and
   Bou-Zeid, Elie},
Title = {{Evaluation of Turbulent Surface Flux Parameterizations over Tall Grass
   in a Beijing Suburb}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
