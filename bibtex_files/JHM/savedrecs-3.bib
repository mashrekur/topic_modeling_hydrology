@article{ ISI:000315363800020,
Author = {Rudolph, James V. and Friedrich, Katja},
Title = {{Seasonality of Vertical Structure in Radar-Observed Precipitation over
   Southern Switzerland}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
