@article{ ISI:000330532300005,
Author = {Kidd, Chris and Dawkins, Erin and Huffman, George},
Title = {{Comparison of Precipitation Derived from the ECMWF Operational Forecast
   Model and Satellite Precipitation Datasets}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2013}},
