@article{ ISI:000305776600003,
Author = {Brown, James D. and Seo, Dong-Jun and Du, Jun},
Title = {{Verification of Precipitation Forecasts from NCEP's Short-Range Ensemble
   Forecast (SREF) System with Reference to Ensemble Streamflow Prediction
   Using Lumped Hydrologic Models}},
