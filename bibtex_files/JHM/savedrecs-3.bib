@article{ ISI:000345898400018,
Author = {Chebana, F. and Charron, C. and Ouarda, T. B. M. J. and Martel, B.},
Title = {{Regional Frequency Analysis at Ungauged Sites with the Generalized
   Additive Model}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
