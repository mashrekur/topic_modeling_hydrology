@article{ ISI:000345898400007,
Author = {Wu, Chuanhao and Huang, Guoru and Yu, Haijun and Chen, Zhijing and Ma,
   Jingguang},
Title = {{Impact of Climate Change on Reservoir Flood Control in the Upstream Area
   of the Beijiang River Basin, South China}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2014}},
Volume = {{15}},
