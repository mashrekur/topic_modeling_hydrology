@article{ ISI:000336954000006,
Author = {Mo, Kingtse C. and Lettenmaier, Dennis P.},
Title = {{Objective Drought Classification Using Multiple Land Surface Models}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
