@article{ ISI:000327254200009,
Author = {Dyer, Jamie and Mercer, Andrew},
Title = {{Assessment of Spatial Rainfall Variability over the Lower Mississippi
   River Alluvial Valley}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
