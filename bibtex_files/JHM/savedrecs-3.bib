@article{ ISI:000345898400008,
Author = {Zhang, Yan and Smith, James A. and Luo, Lifeng and Wang, Zifa and Baeck,
   Mary Lynn},
Title = {{Urbanization and Rainfall Variability in the Beijing Metropolitan Region}},
