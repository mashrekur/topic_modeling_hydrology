@article{ ISI:000302483900008,
Author = {Dutra, Emanuel and Viterbo, Pedro and Miranda, Pedro M. A. and Balsamo,
   Gianpaolo},
Title = {{Complexity of Snow Schemes in a Climate Model and Its Impact on Surface
   Energy and Hydrology}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2012}},
Volume = {{13}},
Number = {{2}},
Pages = {{521-538}},
Month = {{APR}},
Abstract = {{Three different complexity snow schemes implemented in the ECMWF land
   surface scheme Hydrology Tiled ECMWF Scheme of Surface Exchanges over
   Land (HTESSEL) are evaluated within the EC-EARTH climate model. The snow
   schemes are (i) the original HTESSEL single-bulk-layer snow scheme, (ii)
   a new snow scheme in operations at ECMWF since September 2009, and (iii)
   a multilayer version of the previous. In offline site simulations, the
   multilayer scheme outperforms the single-layer schemes in deep snowpack
   conditions through its ability to simulate sporadic melting events
   thanks to the lower thermal inertial of the uppermost layer. Coupled
   atmosphere-land/snow simulations performed by the EC-EARTH climate model
   are validated against remote sensed snow cover and surface albedo. The
   original snow scheme has a systematic early melting linked to an
   underestimation of surface albedo during spring that was partially
   reduced with the new snow schemes. A key process to improve the realism
   of the near-surface atmospheric temperature and at the same time the
   soil freezing is the thermal insulation of the snowpack (tightly coupled
   with the accuracy of snow mass and density simulations). The multilayer
   snow scheme outperforms the single-layer schemes in open deep snowpack
   (such as prairies or tundra in northern latitudes) and is instead
   comparable in shallow snowpack conditions. However, the representation
   of orography in current climate models implies limitations for
   accurately simulating the snowpack, particularly over complex terrain
   regions such as the Rockies and the Himalayas.}},
