@article{ ISI:000327254200006,
Author = {Orth, Rene and Koster, Randal D. and Seneviratne, Sonia I.},
Title = {{Inferring Soil Moisture Memory from Streamflow Observations Using a
   Simple Water Balance Model}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
