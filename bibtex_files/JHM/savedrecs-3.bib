@article{ ISI:000302483900013,
Author = {Gudmundsson, Lukas and Tallaksen, Lena M. and Stahl, Kerstin and Clark,
   Douglas B. and Dumont, Egon and Hagemann, Stefan and Bertrand, Nathalie
   and Gerten, Dieter and Heinke, Jens and Hanasaki, Naota and Voss, Frank
   and Koirala, Sujan},
Title = {{Comparing Large-Scale Hydrological Model Simulations to Observed Runoff
   Percentiles in Europe}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
