@article{ ISI:000345898400022,
Author = {Stenz, Ronald and Dong, Xiquan and Xi, Baike and Kuligowski, Robert J.},
Title = {{Assessment of SCaMPR and NEXRAD Q2 Precipitation Estimates Using
   Oklahoma Mesonet Observations}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
