@article{ ISI:000302483900019,
Author = {Flesch, Thomas K. and Reuter, Gerhard W.},
Title = {{WRF Model Simulation of Two Alberta Flooding Events and the Impact of
   Topography}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2012}},
Volume = {{13}},
Number = {{2}},
Pages = {{695-708}},
Month = {{APR}},
Abstract = {{This study examines simulations of two flooding events in Alberta,
   Canada, during June 2005, made using the Weather Research and
   Forecasting Model (WRF). The model was used in a manner readily
   accessible to nonmeteorologists (e.g., accepting default choices and
   parameters) and with a relatively large spatial resolution for rapid
   model runs. The simulations were skillful: strong storms were developed
   having the correct timing and location, generating precipitation rates
   close to observations, and with precipitation amounts near that
   observed. The model was then used to examine the sensitivity of the two
   storms to the topography of the Rocky Mountains. Comparing model results
   using the actual topographic grid with those of a reduced-mountain grid,
   it is concluded that a reduction in mountain elevation decreases maximum
   precipitation by roughly 50\% over the mountains and foothills. There
   was little sensitivity to topography in the precipitation outside the
   mountains.}},
