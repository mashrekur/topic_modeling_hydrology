@article{ ISI:000305776600022,
Author = {Malik, M. Jahanzeb and van der Velde, Rogier and Vekerdy, Zoltan and Su,
   Zhongbo},
Title = {{Assimilation of Satellite-Observed Snow Albedo in a Land Surface Model}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
