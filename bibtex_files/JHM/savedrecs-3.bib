@article{ ISI:000332301900013,
Author = {Lomazzi, Marco and Entekhabi, Dara and Pinto, Joaquim G. and Roth,
   Giorgio and Rudari, Roberto},
Title = {{Synoptic Preconditions for Extreme Flooding during the Summer Asian
   Monsoon in the Mumbai Area}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
