@article{ ISI:000251842100004,
Author = {Suchomel, Eric J. and Ramsburg, C. Andrew and Pennell, Kurt D.},
Title = {{Evaluation of trichloroethene recovery processes in heterogeneous
   aquifer cells flushed with biodegradable surfactants}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
