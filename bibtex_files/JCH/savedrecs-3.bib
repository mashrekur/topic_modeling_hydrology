@article{ ISI:000235780200004,
Author = {Kubert, M and Finkel, M},
Title = {{Contaminant mass discharge estimation in groundwater based on
   multi-level point measurements: A numerical evaluation of expected
   errors}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2006}},
Volume = {{84}},
