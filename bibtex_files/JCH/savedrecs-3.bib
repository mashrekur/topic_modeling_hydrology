Author = {Nobre, R. C. M. and Rotunno Filho, O. C. and Mansur, W. J. and Nobre, M.
   M. M. and Cosenza, C. A. N.},
Title = {{Groundwater vulnerability and risk mapping using GIS, modeling and a
   fuzzy logic tool}},
