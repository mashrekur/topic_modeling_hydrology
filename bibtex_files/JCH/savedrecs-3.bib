@article{ ISI:000232844600002,
Author = {Jang, CS and Liu, CW},
Title = {{Contamination potential of nitrogen compounds in the heterogeneous
   aquifers of the Choushui River alluvial fan, Taiwan}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
