@article{ ISI:000249054100016,
Author = {Yabusaki, Steven B. and Fang, Yilin and Long, Philip E. and Resch,
   Charles T. and Peacock, Aaron D. and Komlos, John and Jaffe, Peter R.
   and Morrison, Stan J. and Dayvault, Richard D. and White, David C. and
