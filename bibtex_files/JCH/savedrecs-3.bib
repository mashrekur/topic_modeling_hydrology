@article{ ISI:000224673800007,
Author = {Guyonnet, D and Neville, C},
Title = {{Dimensionless analysis of two analytical solutions for 3-D solute
   transport in groundwater}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2004}},
Volume = {{75}},
Number = {{1-2}},
Pages = {{141-153}},
Month = {{NOV}},
Abstract = {{Analytical solutions are widely used as screening tools for estimating
   the potential for contaminant transport in groundwater, or for
   interpreting tracer tests or groundwater quality data. A solution for
   three-dimensional solute migration from a plane-source source that is
   frequently used in practice is the approximate solution of Domenico
   {[}J. Hydrol. 91 (1987) 49-58]. A more rigorous solution to the same
   problem was provided by Sagar {[}ASCE, J. Hydrant. Div. 108, no. HY1
   (1982) 47-62]. A comprehensive and unambiguous comparison between these
   two solutions is provided using dimensionless analysis. The solutions
   are first cast in tenus of dimensionless parameters and then used to
   provide type curves covering a wide range of dimensionless parameter
   values. Results show that while discrepancies between the two solutions
   are relatively negligible along the plume centreline (for flow regimes
   dominated by advection and mechanical dispersion), large concentration
   differences can be observed as lateral distance from the centreline
   increases, especially in the presence of solute decay. (C) 2004 Elsevier
   B.V. All rights reserved.}},
