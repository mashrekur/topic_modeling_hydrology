@article{ ISI:000262061400006,
Author = {Bracke, G. and Mueller, W.},
Title = {{Contribution to a more realistic approach in assessing the release of
   C-14 from low-level radioactive waste repositories}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2008}},
Volume = {{102}},
Number = {{3-4}},
Pages = {{210-216}},
Month = {{DEC 12}},
Abstract = {{The contribution of C-14 to radiation exposure in the biosphere can be
   significant as compared to that of other radionuclides disposed in a
   repository for low-level radioactive waste. The release pathways of C-14
   and processes relevant to its release from a closed final repository for
   low-level radioactive waste are discussed. Because a conservative
   approach may lead to undue overestimation of the potential radiation
   exposure, a more realistic approach is outlined. At the present level Of
