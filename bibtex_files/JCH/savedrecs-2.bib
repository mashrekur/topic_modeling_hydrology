@article{ ISI:000297388200005,
Author = {Paradelo, Marcos and Letzner, Adrian and Arias-Estevez, Manuel and
   Garrido-Rodriguez, Beatriz and Eugenio Lopez-Periago, J.},
Title = {{Influence of soluble copper on the electrokinetic properties and
   transport of copper oxychloride-based fungicide particles}},
