@article{ ISI:000279704600002,
Author = {DiFilippo, Erica L. and Carroll, Kenneth C. and Brusseau, Mark L.},
Title = {{Impact of organic-liquid distribution and flow-field heterogeneity on
   reductions in mass flux}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
