@article{ ISI:000279704600003,
Author = {Kristensen, Andreas H. and Thorbjorn, Anne and Jensen, Maria P. and
   Pedersen, Mette and Moldrup, Per},
Title = {{Gas-phase diffusivity and tortuosity of structured soils}},
