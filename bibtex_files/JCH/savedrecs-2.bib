@article{ ISI:000326430600003,
Author = {Personna, Yves Robert and Slater, Lee and Ntarlagiannis, Dimitrios and
   Werkema, Dale and Szabo, Zoltan},
Title = {{Complex resistivity signatures of ethanol biodegradation in porous media}},
