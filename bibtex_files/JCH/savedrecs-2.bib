@article{ ISI:000298463100015,
Author = {Martin, E. J. and Kueper, B. H.},
Title = {{Observation of trapped gas during electrical resistance heating of
   trichloroethylene under passive venting conditions}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
Year = {{2011}},
Volume = {{126}},
Number = {{3-4}},
Pages = {{291-300}},
Month = {{NOV 1}},
Abstract = {{A two-dimensional experiment employing a heterogeneous sand pack
   incorporating two pools of trichloroethylene (ICE) was performed to
   assess the efficacy of electrical resistance heating (ERH) under passive
   venting conditions. Temperature monitoring displayed the existence of a
   ICE-water co-boiling plateau at 73.4 degrees C, followed by continued
   heating to 100 C. A 5 cm thick gas accumulation formed beneath a
   fine-grained capillary barrier during and after co-boiling. The
   capillary barrier did not desaturate during the course of the
   experiment; the only pathway for gas escape being through perforated
   wells traversing the barrier. The thickness of the accumulation was
   dictated by the entry pressure of the perforated well. The theoretical
   maximum TCE soil concentration within the region of gas accumulation,
   following gas collapse, was estimated to be 888 mg/kg. Post-heating soil
   sampling revealed ICE concentrations in this region ranging from 27
   mg/kg to 96.7 mg/kg, indicating removal of aqueous and gas phase TCE
   following co-boiling as a result of subsequent boiling of water. The
   equilibrium concentrations of ICE in water corresponding to the range of
   post-treatment concentrations in soil (6.11 mg/kg to 136 mg/kg) are
   calculated to range from 19.8 mg/l to 440 mg/l. The results of this
   experiment illustrate the importance of providing gas phase venting
   during the application of ERH in heterogeneous porous media. (C) 2011
   Elsevier B.V. All rights reserved.}},
