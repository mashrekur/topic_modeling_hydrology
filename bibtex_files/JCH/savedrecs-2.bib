@article{ ISI:000297388200001,
Author = {Schmidt, Jeremy T. and Ahmad, Mushtaque and Teel, Amy L. and Watts,
   Richard J.},
Title = {{Hydrogen peroxide stabilization in one-dimensional flow columns}},
