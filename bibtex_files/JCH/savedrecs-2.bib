@article{ ISI:000291414200001,
Author = {Carlson, Andrea E. and Barnes, David L.},
Title = {{Movement of trichloroethene in a discontinuous permafrost zone}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
