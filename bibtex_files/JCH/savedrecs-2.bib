@article{ ISI:000318192800005,
Author = {Miller, Andrew W. and Rodriguez, Derrick R. and Honeyman, Bruce D.},
Title = {{Simplified behaviors from increased heterogeneity: II. 3-D Uranium
   transport at the decimeter scale and intertank comparisons}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
