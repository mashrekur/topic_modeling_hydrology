@article{ ISI:000315249500003,
Author = {Kocur, Chris M. and O'Carroll, Denis M. and Sleep, Brent E.},
Title = {{Impact of nZVI stability on mobility in porous media}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
