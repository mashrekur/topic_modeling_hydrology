@article{ ISI:000297388200011,
Author = {Chen, Jiazhou and Jin, Yan},
Title = {{Motility of Pseudomonas aeruginosa in saturated granular media as
   affected by chemoattractant}},
Journal = {{JOURNAL OF CONTAMINANT HYDROLOGY}},
