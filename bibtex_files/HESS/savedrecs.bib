@article{ ISI:000081370300003,
Author = {Pearson, D and Daamen, CC and Gurney, RJ and Simmonds, LP},
Title = {{Combined modelling of shortwave and thermal radiation for
   one-dimensional SVATs}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{1999}},
Volume = {{3}},
Number = {{1}},
Pages = {{15-30}},
Month = {{MAR}},
Abstract = {{Expressions for the upwelling and downwelling fluxes of optical and
   thermal radiation between soil, vegetation and the sky are derived,
   under certain simple assumptions. These are that interception of
   radiation by the vegetation is a purely geometric effect, while
   scattering is isotropic, with a strength given by a single-scattering
   albedo in the optical part of the spectrum, and by Kirchoff's Law in the
   thermal. The soil is assumed to be a lambertian reflector, also
   scattering according to an albedo and Kirchoff's Law. The model, called
   RM, conserves energy exactly. As part of a SVAT, it is driven by
   measured insolation instead of net radiation, with little increase in
   computational cost and number of parameters.}},
