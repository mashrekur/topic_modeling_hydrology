@article{ ISI:000208683200007,
Author = {Savenije, Hubert H. G.},
Title = {{Determination of evaporation from a catchment water balance at a monthly
   time scale}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{1997}},
Volume = {{1}},
Number = {{1}},
Pages = {{93-100}},
Abstract = {{A method is presented to determine total evaporation from the earth's
   surface at a spatial scale that is adequate for linkage with climate
   models. The method is based on the water balance of catchments, combined
   with a calibrated autoregressive rainfall runoff model. The time scale
   used is in the order of decades (10 days) to months. The rainfall runoff
   model makes a distinction between immediate processes (interception and
   short term storage) and the remaining longer-term processes. Besides the
   calibrated rainfall runoff model and the time series of observed
   rainfall and runoff, the method requires a relation between
   transpiration and soil moisture storage. The method is applied to data
   of the Bani catchment in Mali, a sub-catchment of the Niger river basin.}},
