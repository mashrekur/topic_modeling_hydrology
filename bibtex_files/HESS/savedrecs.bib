@article{ ISI:000167826900004,
Author = {Norton, SA and Wagai, R and Navratil, T and Kaste, JM and Rissberger, FA},
Title = {{Response of a first-order stream in Maine to short-term in-stream
   acidification}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2000}},
Volume = {{4}},
Number = {{3}},
Pages = {{383-391}},
Month = {{SEP}},
Abstract = {{An experimental short-term acidification with HCl at a first-order
   stream in central Maine, USA was used to study processes controlling the
   changes in stream chemistry and to assess the ability of stream
   substrate to buffer pH. The streambed exerted a strong buffering
   capacity against pH change by ion exchange during the 6-hour
   acidification. Streambed substrates had substantial cation and anion
   exchange capacity in the pH range of 4.1 to 6.5. The ion exchange for
   cations and SO42- were rapid and reversible. The speed of release of
   cations from stream substrates was Na1+ > Ca2+ > Mg2+ > Aln+ > Be2+,
   perhaps relating to charge density of these cations. Ca2+ desorption
   dominated neutralisation of excess H+ for the first 2 hr. As the
   reservoir of exchangeable Ca diminished, desorption land possibly
   dissolution) of Al3+ became the dominant neutralising mechanism. The
   exchangeable land possibly soluble) reservoir of Al was not depleted
   during the 6-hour acidification. Sulphate adsorption during the
   acidification reduced the concentration of SO42- in stream water by as
   much as 20 mu eq L-1 (from 70 mu eq L-1). Desorption of SO42- and
   adsorption of base cat ions after the artificial acidification resulted
   in a prolongation of the pH depression. The streambed had the capacity
   to buffer stream water chemistry significantly during an acidifying
   event affecting the entire upstream catchment.}},
