@article{ ISI:000396740200003,
Author = {Guimberteau, Matthieu and Ciais, Philippe and Ducharne, Agnes and Pablo
   Boisier, Juan and Dutra Aguiar, Ana Paula and Biemans, Hester and De
   Deurwaerder, Hannes and Galbraith, David and Kruijt, Bart and
