@article{ ISI:000396740200004,
Author = {Tian, Di and Wood, Eric F. and Yuan, Xing},
Title = {{CFSv2-based sub-seasonal precipitation and temperature forecast skill
   over the contiguous United States}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
