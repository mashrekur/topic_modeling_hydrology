@article{ ISI:000405866700001,
Author = {Mao, Feng and Clark, Julian and Karpouzoglou, Timothy and Dewulf, Art
   and Buytaert, Wouter and Hannah, David},
Title = {{HESS Opinions: A conceptual framework for assessing socio-hydrological
   resilience under change}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
Volume = {{21}},
Number = {{7}},
