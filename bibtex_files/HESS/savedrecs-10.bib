@article{ ISI:000409056400002,
Author = {Samaniego, Luis and Kumar, Rohini and Thober, Stephan and Rakovec,
   Oldrich and Zink, Matthias and Wanders, Niko and Eisner, Stephanie and
   Schmied, Hannes Mueller and Sutanudjaja, Edwin H. and Warrach-Sagi,
