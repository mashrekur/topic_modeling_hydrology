@article{ ISI:000403233200007,
Author = {Iwema, Joost and Rosolem, Rafael and Rahman, Mostaquimur and Blyth,
   Eleanor and Wagener, Thorsten},
Title = {{Land surface model performance using cosmic-ray and point-scale soil
   moisture measurements for calibration}},
