@article{ ISI:000412245400001,
Author = {Zhang, Hongjuan and Franssen, Harrie-Jan Hendricks and Han, Xujun and
   Vrugt, Jasper A. and Vereecken, Harry},
Title = {{State and parameter estimation of two land surface models using the
   ensemble Kalman filter and the particle filter}},
