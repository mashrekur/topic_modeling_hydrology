@article{ ISI:000401107500001,
Author = {Orth, Rene and Dutra, Emanuel and Trigo, Isabel F. and Balsamo,
   Gianpaolo},
Title = {{Advancing land surface model development with satellite-based Earth
   observations}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
Volume = {{21}},
Number = {{5}},
