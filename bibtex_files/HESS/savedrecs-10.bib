@article{ ISI:000395400000001,
Author = {Liu, Chunwei and Sun, Ge and McNulty, Steven G. and Noormets, Asko and
   Fang, Yuan},
Title = {{Environmental controls on seasonal ecosystem
   evapotranspiration/potential evapotranspiration ratio as determined by
   the global eddy flux measurements}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
