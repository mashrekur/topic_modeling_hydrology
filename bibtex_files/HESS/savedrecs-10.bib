@article{ ISI:000410560900002,
Author = {Shaha, Dinesh Chandra and Cho, Yang-Ki and Kim, Bong Guk and Sony, M.
   Rafi Afruz and Kundu, Sampa Rani and Islam, M. Faruqul},
