@article{ ISI:000396740200001,
Author = {Yuan, Chuan and Gao, Guangyao and Fu, Bojie},
Title = {{Comparisons of stemflow and its bio-/abiotic influential factors between
   two xerophytic shrub species}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
