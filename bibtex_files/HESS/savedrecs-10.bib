@article{ ISI:000402326300001,
Author = {Delavau, Carly J. and Stadnyk, Tricia and Holmes, Tegan},
Title = {{Examining the impacts of precipitation isotope input (delta O-18(ppt))
   on distributed, tracer-aided hydrological modelling}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
Volume = {{21}},
Number = {{5}},
