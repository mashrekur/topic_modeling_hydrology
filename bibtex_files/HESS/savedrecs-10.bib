@article{ ISI:000395418200003,
Author = {Li, Ji and Chen, Yangbo and Wang, Huanyu and Qin, Jianming and Li, Jie
   and Chiao, Sen},
Title = {{Extending flood forecasting lead time in a large watershed by coupling
   WRF QPF with a distributed hydrological model}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
Volume = {{21}},
Number = {{2}},
Pages = {{1279-1294}},
Month = {{MAR 2}},
Abstract = {{Long lead time flood forecasting is very important for large watershed
   flood mitigation as it provides more time for flood warning and
   emergency responses. The latest numerical weather forecast model could
   provide 1-15-day quantitative precipitation forecasting products in grid
   format, and by coupling this product with a distributed hydrological
   model could produce long lead time watershed flood forecasting products.
   This paper studied the feasibility of coupling the Liuxihe model with
   the Weather Research and Forecasting quantitative precipitation forecast
   (WRF QPF) for large watershed flood forecasting in southern China. The
   QPF of WRF products has three lead times, including 24, 48 and 72 h,
   with the grid resolution being 20 km x 20 km. The Liuxihe model is set
   up with freely downloaded terrain property; the model parameters were
   previously optimized with rain gauge observed precipitation, and
   re-optimized with the WRF QPF. Results show that the WRF QPF has bias
   with the rain gauge precipitation, and a post-processing method is
   proposed to post-process the WRF QPF products, which improves the flood
   forecasting capability. With model parameter re-optimization, the
   model's performance improves also. This suggests that the model
   parameters be optimized with QPF, not the rain gauge precipitation. With
   the increasing of lead time, the accuracy of the WRF QPF decreases, as
   does the flood forecasting capability. Flood forecasting products
   produced by coupling the Liuxihe model with the WRF QPF provide a good
   reference for large watershed flood warning due to its long lead time
   and rational results.}},
