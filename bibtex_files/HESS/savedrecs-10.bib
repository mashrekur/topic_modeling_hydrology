@article{ ISI:000403233200006,
Author = {Seibert, Simon Paul and Jackisch, Conrad and Ehret, Uwe and Pfister,
   Laurent and Zehe, Erwin},
Title = {{Unravelling abiotic and biotic controls on the seasonal water balance
   using data-driven dimensionless diagnostics}},
