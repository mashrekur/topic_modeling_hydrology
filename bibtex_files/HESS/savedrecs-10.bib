@article{ ISI:000412472700001,
Author = {Um, Myoung-Jin and Kim, Yeonjoo and Park, Daeryong and Kim, Jeongbin},
Title = {{Effects of different reference periods on drought index (SPEI)
   estimations from 1901 to 2014}},
