Title = {{MSWEP: 3-hourly 0.25 degrees global gridded precipitation (1979-2015) by
   merging gauge, satellite, and reanalysis data}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
Volume = {{21}},
