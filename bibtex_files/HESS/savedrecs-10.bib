Title = {{Daily Landsat-scale evapotranspiration estimation over a forested
   landscape in North Carolina, USA, using multi-satellite data fusion}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
