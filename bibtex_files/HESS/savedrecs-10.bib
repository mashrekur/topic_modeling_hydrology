@article{ ISI:000416264000001,
Author = {Asadieh, Behzad and Krakauer, Nir Y.},
Title = {{Global change in streamflow extremes under climate change over the 21st
   century}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
