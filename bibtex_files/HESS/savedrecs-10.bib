@article{ ISI:000395413600001,
Author = {Barella-Ortiz, Anais and Polcher, Jan and de Rosnay, Patricia and Piles,
   Maria and Gelati, Emiliano},
Title = {{Comparison of measured brightness temperatures from SMOS with modelled
   ones from ORCHIDEE and H-TESSEL over the Iberian Peninsula}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2017}},
Volume = {{21}},
