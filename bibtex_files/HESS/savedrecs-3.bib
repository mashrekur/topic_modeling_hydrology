@article{ ISI:000279390400004,
Author = {Sabzevari, T. and Talebi, A. and Ardakanian, R. and Shamsai, A.},
Title = {{A steady-state saturation model to determine the subsurface travel time
   (STT) in complex hillslopes}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
