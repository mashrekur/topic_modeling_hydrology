@article{ ISI:000264741100006,
Author = {Mattia, F. and Satalino, G. and Pauwels, V. R. N. and Loew, A.},
Title = {{Soil moisture retrieval through a merging of multi-temporal L-band SAR
   data and hydrologic modelling}},
