@article{ ISI:000271355200011,
Author = {Zhao, Q. and Liu, Z. and Ye, B. and Qin, Y. and Wei, Z. and Fang, S.},
Title = {{A snowmelt runoff forecasting model coupling WRF and DHSVM}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2009}},
