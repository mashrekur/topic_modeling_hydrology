@article{ ISI:000259986900007,
Author = {Simoniello, T. and Lanfredi, M. and Liberti, M. and Coppola, R. and
   Macchiato, M.},
Title = {{Estimation of vegetation cover resilience from satellite time series}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
