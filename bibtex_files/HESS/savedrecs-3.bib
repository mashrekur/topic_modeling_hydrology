@article{ ISI:000259986900016,
Author = {Barraque, B. and Johnsson, R. M. Formiga and Nogueira de Paiva Britto,
   A. L.},
Title = {{The development of water services and their interaction with water
   resources in European and Brazilian cities}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
