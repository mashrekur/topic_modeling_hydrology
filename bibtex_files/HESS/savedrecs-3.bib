@article{ ISI:000273059900008,
Author = {Shamsudduha, M. and Chandler, R. E. and Taylor, R. G. and Ahmed, K. M.},
Title = {{Recent trends in groundwater levels in a highly seasonal hydrological
   system: the Ganges-Brahmaputra-Meghna Delta}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2009}},
Volume = {{13}},
