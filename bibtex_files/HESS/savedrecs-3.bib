@article{ ISI:000268623600021,
Author = {Timmermans, J. and Verhoef, W. and van der Tol, C. and Su, Z.},
Title = {{Retrieval of canopy component temperatures through Bayesian inversion of
   directional thermal measurements}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
