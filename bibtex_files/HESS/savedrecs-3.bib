@article{ ISI:000272232400001,
Author = {Li, M. and Ma, Y. and Hu, Z. and Ishikawa, H. and Oku, Y.},
Title = {{Snow distribution over the Namco lake area of the Tibetan Plateau}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2009}},
Volume = {{13}},
Number = {{11}},
Pages = {{2023-2030}},
Abstract = {{The mesoscale snow distribution over the Namco lake area of the Tibetan
   Plateau on October 2005 has been investigated in this paper. The base
   and revised experiments were conducted using the Weather Research Model
   (WRF) with three nested grids that included a 1 km finest grid centered
   on the Namco station. Our simulation ran from 6 October through to 10
   October 2005, which was concurrent with long term meteorological
   observations. Evaluation against boundary layer meteorological tower
   measurements and flux observations showed that the model captured the
   observed 2 m temperature and 10 m winds reasonably well in the revised
   experiment. The results suggest that output snow depth maximum amounts
   from two simulated experiments were centered downwind of the Namco
   lakeshore. Modified surface state variable, for example, surface skin
   temperature on the lake help to increase simulated credibility.}},
