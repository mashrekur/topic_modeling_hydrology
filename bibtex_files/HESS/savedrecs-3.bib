@article{ ISI:000285576800004,
Author = {Teferi, E. and Uhlenbrook, S. and Bewket, W. and Wenninger, J. and
   Simane, B.},
Title = {{The use of remote sensing to quantify wetland loss in the Choke Mountain
   range, Upper Blue Nile basin, Ethiopia}},
