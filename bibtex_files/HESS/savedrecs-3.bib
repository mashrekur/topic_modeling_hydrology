@article{ ISI:000256968000022,
Author = {Uijlenhoet, R. and Berne, A.},
Title = {{Stochastic simulation experiment to assess radar rainfall retrieval
   uncertainties associated with attenuation and its correction}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
