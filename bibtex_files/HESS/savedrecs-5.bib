@article{ ISI:000302176600012,
Author = {Yang, J. -S. and Chung, E. -S. and Kim, S. -U. and Kim, T. -W.},
Title = {{Prioritization of water management under climate change and urbanization
   using multi-criteria decision making methods}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2012}},
Volume = {{16}},
Number = {{3}},
Pages = {{801-814}},
Abstract = {{This paper quantifies the transformed effectiveness of alternatives for
   watershed management caused by climate change and urbanization and
   prioritizes five options using multi-criteria decision making
   techniques. The climate change scenarios (A1B and A2) were obtained by
   using a statistical downscaling model (SDSM), and the urbanization
   scenario by surveying the existing urban planning. The flow and
   biochemical oxygen demand (BOD) concentration duration curves were
   derived, and the numbers of days required to satisfy the environmental
   flow requirement and the target BOD concentration were counted using the
   Hydrological Simulation Program-Fortran (HSPF) model. In addition, five
   feasible alternatives were prioritized by using multi-criteria decision
   making techniques, based on the driving
   force-pressure-state-impact-response (DPSIR) framework and cost
   component. Finally, a sensitivity analysis approach for MCDM methods was
   conducted to reduce the uncertainty of weights. The result indicates
   that the most sensitive decision criterion is cost, followed by criteria
   response, driving force, impact, state and pressure in that order. As it
   is certain that the importance of cost component is over 0.127,
   construction of a small wastewater treatment plant will be the most
   preferred alternative in this application.}},
