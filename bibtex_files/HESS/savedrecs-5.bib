@article{ ISI:000308245800005,
Author = {Bremer, J. E. and Harter, T.},
Title = {{Domestic wells have high probability of pumping septic tank leachate}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2012}},
