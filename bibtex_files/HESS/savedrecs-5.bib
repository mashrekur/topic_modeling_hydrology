@article{ ISI:000311514600034,
Author = {Cheng, L. and Yaeger, M. and Viglione, A. and Coopersmith, E. and Ye, S.
   and Sivapalan, M.},
Title = {{Exploring the physical controls of regional patterns of flow duration
   curves - Part 1: Insights from statistical analyses}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
