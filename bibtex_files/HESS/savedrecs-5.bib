Title = {{Evaluation of the ISBA-TRIP continental hydrologic system over the Niger
   basin using in situ and satellite derived datasets}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2012}},
Volume = {{16}},
