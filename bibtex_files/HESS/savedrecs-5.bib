@article{ ISI:000310475400030,
Author = {van Dijk, A. I. J. M. and Pena-Arancibia, J. L. and Bruijnzeel, L. A.
   (Sampurno)},
Title = {{Land cover and water yield: inference problems when comparing catchments
   with mixed land cover}},
