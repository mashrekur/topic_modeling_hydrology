@article{ ISI:000379420000002,
Author = {Bachmair, S. and Svensson, C. and Hannaford, J. and Barker, L. J. and
   Stahl, K.},
Title = {{A quantitative analysis to objectively appraise drought indicators and
   model drought impacts}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
