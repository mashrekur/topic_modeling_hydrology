@article{ ISI:000377862900014,
Author = {Somogyvari, Mark and Bayer, Peter and Brauchler, Ralf},
Title = {{Travel-time-based thermal tracer tomography}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2016}},
