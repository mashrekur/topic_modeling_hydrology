Author = {Pennino, Michael J. and Kaushal, Sujay S. and Mayer, Paul M. and Utz,
   Ryan M. and Cooper, Curtis A.},
Title = {{Stream restoration and sewers impact sources and fluxes of water,
   carbon, and nutrients in urban watersheds}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2016}},
Volume = {{20}},
Number = {{8}},
