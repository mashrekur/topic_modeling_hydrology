@article{ ISI:000384422300001,
Author = {Orlowski, Natalie and Kraft, Philipp and Pferdmenges, Jakob and Breuer,
   Lutz},
Title = {{Exploring water cycle dynamics by sampling multiple stable water isotope
   pools in a developed landscape in Germany}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
