@article{ ISI:000334501700004,
Author = {Bon, C. E. and Reeve, A. S. and Slater, L. and Comas, X.},
Title = {{Using hydrologic measurements to investigate free-phase gas ebullition
   in a Maine peatland, USA}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
