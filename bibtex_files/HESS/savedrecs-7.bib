@article{ ISI:000343118500013,
Author = {Hostache, R. and Hissler, C. and Matgen, P. and Guignard, C. and Bates,
   P.},
Title = {{Modelling suspended-sediment propagation and related heavy metal
   contamination in floodplains: a parameter sensitivity analysis}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
