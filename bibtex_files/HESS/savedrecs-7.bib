@article{ ISI:000331123300003,
Author = {Lim, W. H. and Roderick, M. L.},
Title = {{Up-scaling short-term process-level understanding to longer timescales
   using a covariance-based approach}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
