@article{ ISI:000345768100006,
Author = {Salinas, J. L. and Castellarin, A. and Viglione, A. and Kohnova, S. and
   Kjeldsen, T. R.},
Title = {{Regional parent flood frequency distributions in Europe - Part 1: Is the
   GEV model suitable as a pan-European parent?}},
