@article{ ISI:000347313600009,
Author = {You, J. and Tarboton, D. G. and Luce, C. H.},
Title = {{Modeling the snow surface temperature with a one-layer energy balance
   snowmelt model}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
