@article{ ISI:000345768100028,
Author = {Larsen, M. A. D. and Refsgaard, J. C. and Drews, M. and Butts, M. B. and
   Jensen, K. H. and Christensen, J. H. and Christensen, O. B.},
Title = {{Results from a full coupling of the HIRHAM regional climate model and
   the MIKE SHE hydrological model for a Danish catchment}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2014}},
Volume = {{18}},
Number = {{11}},
Pages = {{4733-4749}},
Abstract = {{A major challenge in the emerging research field of coupling of existing
   regional climate models (RCMs) and hydrology/land-surface models is the
   computational interaction between the models. Here we present results
   from a full two-way coupling of the HIRHAM RCM over a 4000 km x 2800 km
   domain at 11 km resolution and the combined MIKE SHE-SWET hydrology and
   land-surface models over the 2500 km(2) Skjern River catchment. A total
   of 26 one-year runs were performed to assess the influence of the data
   transfer interval (DTI) between the two models and the internal HIRHAM
   model variability of 10 variables. DTI frequencies between 12 and 120
   min were assessed, where the computational overhead was found to
   increase substantially with increasing exchange frequency. In terms of
   hourly and daily performance statistics the coupled model simulations
   performed less accurately than the uncoupled simulations, whereas for
   longer-term cumulative precipitation the opposite was found, especially
   for more frequent DTI rates. Four of six output variables from HIRHAM,
   precipitation, relative humidity, wind speed and air temperature, showed
   statistically significant improvements in root-meansquare error (RMSE)
   by reducing the DTI. For these four variables, the HIRHAM RMSE
   variability corresponded to approximately half of the influence from the
   DTI frequency and the variability resulted in a large spread in
   simulated precipitation. Conversely, DTI was found to have only a
   limited impact on the energy fluxes and discharge simulated by MIKE SHE.}},
