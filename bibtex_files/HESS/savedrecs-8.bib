@article{ ISI:000360653600023,
Author = {Kobierska, F. and Jonas, T. and Kirchner, J. W. and Bernasconi, S. M.},
Title = {{Linking baseflow separation and groundwater storage dynamics in an
   alpine basin (Dammagletscher, Switzerland)}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2015}},
Volume = {{19}},
Number = {{8}},
Pages = {{3681-3693}},
Abstract = {{This study aims at understanding interactions between stream and aquifer
   in a glacierized alpine catchment. We specifically focused on a glacier
   forefield, for which continuous measurements of stream water electrical
   conductivity, discharge and depth to the water table were available over
   4 consecutive years. Based on this data set, we developed a
   two-component mixing model in which the groundwater component was
   modelled using measured groundwater levels. The aquifer actively
   contributing to streamflow was assumed to be constituted of two linear
   storage units. Calibrating the model against measured total discharge
   yielded reliable sub-hourly estimates of discharge and insights into
   groundwater storage properties. Our conceptual model suggests that a
   near-surface aquifer with high hydraulic conductivity overlies a larger
   reservoir with longer response time.}},
