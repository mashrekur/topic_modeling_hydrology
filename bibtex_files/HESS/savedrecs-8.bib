Author = {He, Z. H. and Tian, F. Q. and Gupta, H. V. and Hu, H. C. and Hu, H. P.},
Title = {{Diagnostic calibration of a hydrological model in a mountain area by
   hydrograph partitioning}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
