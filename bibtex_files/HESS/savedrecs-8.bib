@article{ ISI:000352160600028,
Author = {Kann, A. and Meirold-Mautner, I. and Schmid, F. and Kirchengast, G. and
   Fuchsberger, J. and Meyer, V. and Tuechler, L. and Bica, B.},
Title = {{Evaluation of high-resolution precipitation analyses using a dense
   station network}},
