@article{ ISI:000364327300018,
Author = {Gerken, T. and Babel, W. and Herzog, M. and Fuchs, K. and Sun, F. and
   Ma, Y. and Foken, T. and Graf, H. -F.},
Title = {{High-resolution modelling of interactions between soil moisture and
   convective development in a mountain enclosed Tibetan Basin}},
