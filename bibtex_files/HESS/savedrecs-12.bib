@article{ ISI:000466118100002,
Author = {Abou Karaki, Najib and Fiaschi, Simone and Paenen, Killian and
   Al-Awabdeh, Mohammad and Closson, Damien},
Title = {{Exposure of tourism development to salt karst hazards along the
   Jordanian Dead Sea shore}},
