@article{ ISI:000460119000002,
Author = {Allen, Scott T. and Kirchner, James W. and Braun, Sabine and Siegwolf,
   Rolf T. W. and Goldsmith, Gregory R.},
Title = {{Seasonal origins of soil water used by trees}},
