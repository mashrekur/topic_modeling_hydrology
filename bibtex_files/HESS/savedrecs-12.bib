@article{ ISI:000458627800002,
Author = {Lv, Aifeng and Qu, Bo and Jia, Shaofeng and Zhu, Wenbin},
Title = {{Influence of three phases of El Nino-Southern Oscillation on daily
   precipitation regimes in China}},
