@article{ ISI:000481688200002,
Author = {Zhu, Xuehong and Dai, Qiang and Han, Dawei and Zhuo, Lu and Zhu, Shaonan
   and Zhang, Shuliang},
Title = {{Modeling the high-resolution dynamic exposure to flooding in a city
   region}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2019}},
Volume = {{23}},
