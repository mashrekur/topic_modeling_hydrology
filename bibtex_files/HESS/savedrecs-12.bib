@article{ ISI:000487738400001,
Author = {Maheu, Audrey and Hajji, Islem and Anctil, Francois and Nadeau, Daniel
   F. and Therrien, Rene},
Title = {{Using the maximum entropy production approach to integrate energy budget
   modelling in a hydrological model}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
