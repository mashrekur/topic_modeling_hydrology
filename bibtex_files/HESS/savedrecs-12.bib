@article{ ISI:000503445500002,
Author = {Barella-Ortiz, Anais and Quintana-Segui, Pere},
Title = {{Evaluation of drought representation and propagation in regional climate
   model simulations across Spain}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2019}},
Volume = {{23}},
Number = {{12}},
Pages = {{5111-5131}},
