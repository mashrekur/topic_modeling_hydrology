@article{ ISI:000488980200002,
Author = {Schroeder, Stefan and Springer, Anne and Kusche, Juergen and Uebbing,
   Bernd and Fenoglio-Marc, Luciana and Diekkrueger, Bernd and Pomeon,
   Thomas},
Title = {{Niger discharge from radar altimetry: bridging gaps between gauge and
   altimetry time series}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2019}},
Volume = {{23}},
Number = {{10}},
Pages = {{4113-4128}},
Month = {{OCT 2}},
Abstract = {{The Niger River represents a challenging target for deriving discharge
   from spaceborne radar altimeter measurements, particularly since most
   terrestrial gauges ceased to provide data during the 2000s. Here, we
   propose deriving altimetric rating curves by ``bridging{''} gaps between
   time series from gauge and altimeter measurements using hydrological
   model simulations. We show that classical pulse-limited altimetry
   (Jason-1 and Jason-2, Envisat, and SARAL/Altika) subsequently reproduces
   discharge well and enables continuing the gauge time series, albeit at a
   lower temporal resolution. Also, synthetic aperture radar (SAR)
   altimetry picks up the signal measured by earlier altimeters quite well
   and allows the building of extended time series of higher quality.
   However, radar retracking is necessary for pulse-limited altimetry and
   needs to be further investigated for SAR. Moreover, forcing data for
   calibrating and running the hydrological models must be chosen
   carefully. Furthermore, stage- discharge relations must be fitted
   empirically and may need to allow for break points.}},
