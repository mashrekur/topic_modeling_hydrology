@article{ ISI:000459685700001,
Author = {Ehmele, Florian and Kunz, Michael},
Title = {{Flood-related extreme precipitation in southwestern Germany: development
   of a two-dimensional stochastic precipitation model}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
