@article{ ISI:000458118900001,
Author = {Davison, Bruce and Fortin, Vincent and Pietroniro, Alain and Yau, Man K.
   and Leconte, Robert},
Title = {{Parameter-state ensemble thinning for short-term hydrological prediction}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
