@article{ ISI:000290016400002,
Author = {Kidd, C. and Levizzani, V.},
Title = {{Status of satellite precipitation retrievals}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2011}},
Volume = {{15}},
Number = {{4}},
Pages = {{1109-1116}},
Abstract = {{Satellites offer an unrivalled vantage point to observe and measure
   Earth system processes and parameters. Precipitation (rain and snow) in
   particular, benefit from such observations since precipitation is
   spatially and temporally highly variable and with satellites overcoming
   some of the deficiencies of conventional gauge and radar measurements.
   This paper provides an overall review of quantitative precipitation
   estimation, covering the basis of the satellite systems used in the
   observation of precipitation, the dissemination and processing of this
   data, and the generation, availability and validation of these
   precipitation estimates. A selection of applications utilising these
   precipitation estimates are then outlined to exemplify the utility of
   such products.}},
