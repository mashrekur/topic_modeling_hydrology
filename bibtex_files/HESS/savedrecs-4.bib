@article{ ISI:000296745600003,
Author = {Teuling, A. J.},
Title = {{Technical note: Towards a continuous classification of climate using
   bivariate colour mapping}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2011}},
Volume = {{15}},
Number = {{10}},
Pages = {{3071-3075}},
Abstract = {{Climate is often defined in terms of discrete classes. Here I use
   bivariate colour mapping to show that the global distribution of
   Koppen-Geiger climate classes can largely be reproduced by combining the
   simple means of two key states of the climate system (i.e. air
   temperature and relative humidity). This allows for a classification
   that is not only continuous in space, but can be applied at and
   transferred between timescales ranging from days to decades.}},
