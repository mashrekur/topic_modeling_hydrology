@article{ ISI:000329061400013,
Author = {Gbolo, P. and Gerla, P.},
Title = {{Statistical analysis to characterize transport of nutrients in
   groundwater near an abandoned feedlot}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
