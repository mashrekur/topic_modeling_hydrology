@article{ ISI:000326603200039,
Author = {Chebbi, A. and Bargaoui, Z. K. and da Conceicao Cunha, M.},
Title = {{Development of a method of robust rain gauge network optimization based
   on intensity-duration-frequency results}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
