@article{ ISI:000326603200010,
Author = {Jantze, E. J. and Lyon, S. W. and Destouni, G.},
Title = {{Subsurface release and transport of dissolved carbon in a discontinuous
   permafrost region}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
