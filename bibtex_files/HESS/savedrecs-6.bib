@article{ ISI:000322376000028,
Author = {Feng, X. Q. and Zhang, G. X. and Xu, Y. Jun},
Title = {{Simulation of hydrological processes in the Zhalong wetland within a
   river basin, Northeast China}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2013}},
