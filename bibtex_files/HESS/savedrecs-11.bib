@article{ ISI:000442399900003,
Author = {Burd, Katheryn and Tank, Suzanne E. and Dion, Nicole and Quinton,
   William L. and Spence, Christopher and Tanentzap, Andrew J. and
   Olefeldt, David},
Title = {{Seasonal shifts in export of DOC and nutrients from burned and unburned
   peatland-rich catchments, Northwest Territories, Canada}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
