@article{ ISI:000441419900001,
Author = {Botto, Anna and Belluco, Enrica and Camporese, Matteo},
Title = {{Multi-source data assimilation for physically based hydrological
   modeling of an experimental hillslope}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
