Author = {Ganot, Yonatan and Holtzman, Ran and Weisbrod, Noam and Bernstein, Anat
   and Siebner, Hagar and Katz, Yoram and Kurtzman, Daniel},
Title = {{Managed aquifer recharge with reverse-osmosis desalinated seawater:
   modeling the spreading in groundwater using stable water isotopes}},
