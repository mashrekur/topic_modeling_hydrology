@article{ ISI:000425602100002,
Author = {Elamri, Yassin and Cheviron, Bruno and Mange, Annabelle and Dejean,
   Cyril and Liron, Francois and Belaud, Gilles},
Title = {{Rain concentration and sheltering effect of solar panels on cultivated
   plots}},
