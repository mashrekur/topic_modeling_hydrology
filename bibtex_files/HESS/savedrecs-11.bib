@article{ ISI:000454056100001,
Author = {Chen, Qiyu and Mariethoz, Gregoire and Liu, Gang and Comunian,
   Alessandro and Ma, Xiaogang},
Title = {{Locality-based 3-D multiple-point statistics reconstruction using 2-D
   geological cross sections}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
