@article{ ISI:000424705400001,
Author = {Meier, Jonas and Zabel, Florian and Mauser, Wolfram},
Title = {{A global approach to estimate irrigated areas - a comparison between
   different data and statistics}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
