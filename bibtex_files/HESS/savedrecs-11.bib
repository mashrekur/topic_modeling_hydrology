@article{ ISI:000431649000001,
Author = {Zischg, Andreas Paul and Felder, Guido and Weingartner, Rolf and Quinn,
   Niall and Coxon, Gemma and Neal, Jeffrey and Freer, Jim and Bates, Paul},
Title = {{Effects of variability in probable maximum precipitation patterns on
   flood losses}},
