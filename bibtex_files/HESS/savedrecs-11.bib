Title = {{Assessment of actual evapotranspiration over a semiarid heterogeneous
   land surface by means of coupled low-resolution remote sensing data with
   an energy balance model: comparison to extra-large aperture
   scintillometer measurements}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2018}},
Volume = {{22}},
