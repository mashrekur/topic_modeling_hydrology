@article{ ISI:000423877400004,
Author = {Weng, Wei and Luedeke, Matthias K. B. and Zemp, Delphine C. and Lakes,
   Tobia and Kropp, Juergen P.},
Title = {{Aerial and surface rivers: downwind impacts on water availability from
   land use changes in Amazonia}},
