@article{ ISI:000419243300002,
Author = {Vinna, Love Raman and Wuest, Alfred and Zappa, Massimiliano and Fink,
   Gabriel and Bouffard, Damien},
Title = {{Tributaries affect the thermal response of lakes to climate change}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
