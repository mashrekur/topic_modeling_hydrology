@article{ ISI:000447636600001,
Author = {Alemohammad, Seyed Hamed and Kolassa, Jana and Prigent, Catherine and
   Aires, Filipe and Gentine, Pierre},
Title = {{Global downscaling of remotely sensed soil moisture using neural
   networks}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
