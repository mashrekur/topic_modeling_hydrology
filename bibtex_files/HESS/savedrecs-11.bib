@article{ ISI:000432362200001,
Author = {Benettin, Paolo and Volkmann, Till H. M. and von Freyberg, Jana and
   Frentress, Jay and Penna, Daniele and Dawson, Todd E. and Kirchner,
   JamesW.},
Title = {{Effects of climatic seasonality on the isotopic composition of
   evaporating soil waters}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
