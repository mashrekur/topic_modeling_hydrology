@article{ ISI:000440932600003,
Author = {Meredith, Edmund P. and Rust, Henning W. and Ulbrich, Uwe},
Title = {{A classification algorithm for selective dynamical downscaling of
   precipitation extremes}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2018}},
