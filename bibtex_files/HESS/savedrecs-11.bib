@article{ ISI:000449155200001,
Author = {Rubin, Yoram and Chang, Ching-Fu and Chen, Jiancong and Cucchi, Karina
   and Harken, Bradley and Hesse, Falk and Savoy, Heather},
Title = {{Stochastic hydrogeology's biggest hurdles analyzed and its big blind
   spot}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
