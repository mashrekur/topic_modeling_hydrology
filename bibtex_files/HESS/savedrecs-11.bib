@article{ ISI:000424394800001,
Author = {Berg, Peter and Donnelly, Chantal and Gustafsson, David},
Title = {{Near-real-time adjusted reanalysis forcing data for hydrology}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
