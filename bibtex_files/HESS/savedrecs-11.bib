
@article{ ISI:000454114100001,
Author = {Shih, Yu-Ting and Chen, Pei-Hao and Lee, Li-Chin and Liao, Chien-Sen and
   Jien, Shih-Hao and Shiah, Fuh-Kwo and Lee, Tsung-Yu and Hein, Thomas and
