@article{ ISI:000425602100001,
Author = {Minh Tu Pham and Vernieuwe, Hilde and De Baets, Bernard and Verhoest,
   Niko E. C.},
Title = {{A coupled stochastic rainfall-evapotranspiration model for hydrological
   impact analysis}},
