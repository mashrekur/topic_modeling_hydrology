@article{ ISI:000438188300001,
Author = {Loritz, Ralf and Gupta, Hoshin and Jackisch, Conrad and Westhoff,
   Martijn and Kleidon, Axel and Ehret, Uwe and Zehe, Erwin},
Title = {{On the dynamic nature of hydrological similarity}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
