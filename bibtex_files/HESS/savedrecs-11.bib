@article{ ISI:000451281900001,
Author = {Osborne, Joe M. and Lambert, F. Hugo},
Title = {{A simple tool for refining GCM water availability projections, applied
   to Chinese catchments}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
