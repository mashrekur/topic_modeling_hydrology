@article{ ISI:000244861200012,
Author = {Booker, D. J. and Acreman, M. C.},
Title = {{Generalisation of physical habitat-discharge relationships}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2007}},
