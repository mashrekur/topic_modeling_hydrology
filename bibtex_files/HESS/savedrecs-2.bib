@article{ ISI:000188934800013,
Author = {Chauvelon, P and Tournoud, MG and Sandoz, A},
Title = {{Integrated hydrological modelling of a managed coastal Mediterranean
   wetland (Rhone delta, France): initial calibration}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2003}},
Volume = {{7}},
