@article{ ISI:000244861200005,
Author = {Blackie, J. R. and Robinson, M.},
Title = {{Development of catchment research, with particular attention to
   Plynlimon and its forerunner, the East African catchments}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2007}},
