@article{ ISI:000226829900005,
Author = {Snook, DL and Whitehead, PG},
Title = {{Water quality and ecology of the River Lee: mass balance and a review of
   temporal and spatial data}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
