@article{ ISI:000221445700009,
Author = {Menziani, M and Pugnaghi, S and Vincenzi, S and Santangelo, R},
Title = {{Soil moisture monitoring in the Toce valley (Italy)}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
