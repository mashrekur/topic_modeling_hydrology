@article{ ISI:000226829900004,
Author = {Neal, C and Jarvie, HP and Wade, AJ and Neal, M and Wyatt, R and
   Wickham, H and Hill, L and Hewitt, N},
Title = {{The water quality of the LOCAR Pang and Lambourn catchments}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
