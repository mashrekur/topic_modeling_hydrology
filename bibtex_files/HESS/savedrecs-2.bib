@article{ ISI:000229769300004,
Author = {Essery, R},
Title = {{Statistical representation of mountain shading}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2004}},
Volume = {{8}},
Number = {{6}},
Pages = {{1045-1050}},
Month = {{DEC}},
Abstract = {{Shadows cast by the Mountains themselves have a strong influence on the
   Surface energy balance of mountainous regions. If the influence of
   shadows is to be included on sub-grid scales in a surface energy balance
   model, a statistical representation has to be used. Slope components
   calculated from digital elevation models of areas in North Wales and the
   French Alps are found to have double-exponential distributions. From
   this result, expressions are developed for the fractions of the areas
   that will be either self-shaded or shaded by remote topography as
   functions of solar elevation and time of day. These expressions are in
   good agreement with results from a terrain shading model.}},
