@article{ ISI:000235775000002,
Author = {Forti, MC and Bicudo, DC and Bourotte, C and de Cicco, V and Arcova, FCS},
Title = {{Rainfall and throughfall chemistry in the Atlantic Forest: a comparison
   between urban and natural areas (Sao Paulo State, Brazil)}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2005}},
Volume = {{9}},
Number = {{6}},
Pages = {{570-585}},
Abstract = {{Two areas in the Atlantic Forest (Sao Paulo State, Brazil), with
   contrasting environments in respect of human occupation, were monitored
   from 1999 to 2001. One area named PEFI (23 degrees 38'08{''}-23 degrees
   40'18{''} S and 46 degrees 36'48{''}-46 degrees 38'00{''} W) at an
   altitude of 798 m a.s.l., 526.4 ha in area and about 50 km from the sea,
   lies in a State Park within the largest metropolis of South America-Sao
   Paulo. The other area, named CUNHA (between 23 degrees 13'18{''} and 23
   degrees 16'10{''} S and 45 degrees 02'53{''} and 45 degrees 05'15{''} W)
   about 1050 m a.s.l. with an area of 2854 ha and about 15 km from the
   sea, is also within a State Park in the Atlantic Forest, but is
   surrounded by rural areas and small villages. For each area, the
   rainfall and throughfall chemistry were examined and pH and Na+, K+
   Mg2+, Ca2+, NH4+ Cl-, NO3- and SO42- as well as trace metals were
   determined. Compared with PEFI, CUNHA is characterised by low chemical
   fluxes and the largest differences are for the ions such as Ca2+, H+,
   NO3- and SO42- which are mainly anthropogenic in origin. Differences in
   throughfall chemical fluxes are linked to the nutritional status of the
   trees.}},
