@article{ ISI:000188935000008,
Author = {Altunkaynak, A and Ozger, M and Sen, Z},
Title = {{Triple diagram model of level fluctuations in Lake Van, Turkey}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
