@article{ ISI:000249981800009,
Author = {Sirangelo, B. and Versace, P. and De Luca, D. L.},
Title = {{Rainfall nowcasting by at site stochastic model PRAISE}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2007}},
Volume = {{11}},
Number = {{4}},
Pages = {{1341-1351}},
Abstract = {{The paper introduces a stochastic model to forecast rainfall heights at
   site: the P.R.A.I.S.E. model (Prediction of Rainfall Amount Inside Storm
   Events). PRAISE is based on the assumption that the rainfall height Hi+1
   accumulated on an interval At between the instants i Delta t and
   (i+1)Delta t is correlated with a variable Z(i)((nu)), representing
   antecedent precipitation. The mathematical background is given by a
   joined probability density f(Hi+1,Zi)((nu)) (h(i+1), z(i)((nu))) in
   which the variables have a mixed nature, that is a finite probability in
   correspondence to the null value and infinitesimal probabilities in
   correspondence to the positive values. As study area, the Calabria
   region, in Southern Italy, was selected, to test performances of the
   PRAISE model.}},
