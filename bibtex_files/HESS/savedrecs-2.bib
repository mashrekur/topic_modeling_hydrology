@article{ ISI:000188935000003,
Author = {Darling, WG and Talbot, JC},
Title = {{The O \& H stable isotopic composition of fresh waters in the British
   Isles. 1. Rainfall}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
