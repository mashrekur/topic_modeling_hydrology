@article{ ISI:000229769300006,
Author = {Garcia-Santos, G and Marzol, MV and Aschan, G},
Title = {{Water dynamics in a laurel montane cloud forest in the Garajonay
   National Park (Canary Islands, Spain)}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2004}},
Volume = {{8}},
Number = {{6}},
Pages = {{1065-1075}},
Month = {{DEC}},
Abstract = {{Field measurements front February 2003 to January 2004 in a humid (but
   dry in summer) crest heath wood-land (degraded laurel forest) in the
   National park of Garajonay, Canary Islands (Spain), were combined to
   calculate water balance components. The water balance domain is at the
   surface of the catchment and is controlled by atmospheric processes and
   vegetation. This Study found that annual water income (rainfall plus fog
   water) was 1440 nun year(1), half of which was occult (or fog)
   precipitation, while stand transpiration estimated front measurements of
   sap flow amounted, annually, to 40\% of potential evapotranspiration
   calculated front measurements of meteorological variables. The positive
   role of crest laurel forests, which transpire less water than is
   incoming front rain and fog is emphasised.}},
