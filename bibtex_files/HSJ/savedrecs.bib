@article{ ISI:A1997XP22000004,
Author = {Burn, DH},
Title = {{Hydrological information for sustainable development}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
