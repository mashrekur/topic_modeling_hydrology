@article{ ISI:000075062400003,
Author = {Simmonds, LP and Burke, EJ},
Title = {{Estimating near-surface soil water content from passive microwave remote
   sensing - an application of MICRO-SWEAT}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1998}},
Volume = {{43}},
Number = {{4}},
Pages = {{521-534}},
