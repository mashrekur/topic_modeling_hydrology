@article{ ISI:A1995RA57900009,
Author = {TANIGUCHI, M},
Title = {{ANALYZING THE LONG-TERM REDUCTION IN GROUNDWATER TEMPERATURE DUE TO
   PUMPING}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1995}},
Volume = {{40}},
Number = {{3}},
Pages = {{407-421}},
Month = {{JUN}},
Abstract = {{A heat transport model that incorporates the effect of decreases in
   hydraulic head due to pumping of groundwater has been developed to
   explain the decrease in groundwater temperature during the last three
   decades in the Nara basin, Japan. Vertical two-dimensional temperature
   analyses have been made based on a theory of simultaneous transfer of
   heat and water in porous media. The calculated relationships between the
   depth and the groundwater temperature in the discharge area agree well
   with the observed temperature-depth relationships. The model used
   suggests that the decrease in groundwater temperature is caused by the
   decrease in the hydraulic head due to pumping.}},
