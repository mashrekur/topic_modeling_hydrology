@article{ ISI:A1994PU53500002,
Author = {BADER, JC},
Title = {{ANALOG RUNOFF MODEL WITH SURFACE STORAGE - TEST ON PLOTS AND
   EXTRAPOLATION TO A HOMOGENEOUS SLOPE}},
