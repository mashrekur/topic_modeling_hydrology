@article{ ISI:000077425100007,
Author = {Gautier, F and Lubes-Niel, H and Sabatier, R and Masson, JM and Paturel,
   JE and Servat, E},
Title = {{Study of the rainfall regime variability in non-Sahelian West Africa
   from 1950 to 1989}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1998}},
Volume = {{43}},
Number = {{6}},
Pages = {{921-935}},
Month = {{DEC}},
Abstract = {{In the countries around the Guinea Gulf, the rainfall regime has varied
   since the end of the 1960s. To specify some characteristics of this
   variability, time series of annual amount of precipitation, rainy season
   duration, amount of precipitation in dry seasons, and number of rainy
   days have been generated with a one-year time step from 1950 to 1989.
   Statistical procedures have been used to test the presence of a shift in
   the mean. To characterize the change in space and in time, the results
   of the tests have been synthesized using multiple correspondence
   analyses. This study reveals that the rainfall regime began to change
   around 1966 in Senegal and Guinea Bissau, and then in Guinea, Mall,
   Burkina Faso and the northern part of Benin. The Ivory Coast, Togo and
   the south of Benin have been affected to various degrees. In Cameroon
   and the Central African Republic, the change is not uniform or has been
   slight.}},
Publisher = {{IAHS PRESS, INST HYDROLOGY}},
Address = {{C/O FRANCES WATKINS, WALLINGFORD OX10 8BB, ENGLAND}},
Type = {{Article}},
