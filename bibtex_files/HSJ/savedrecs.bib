@article{ ISI:A1996UZ64800005,
Author = {Kustas, WP and Norman, JM},
Title = {{Use of remote sensing for evapotranspiration monitoring over land
   surfaces}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1996}},
Volume = {{41}},
