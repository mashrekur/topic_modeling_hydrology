@article{ ISI:000075062400006,
Author = {Mdaghri-Alaoui, A and Germann, PF},
Title = {{Kinematic wave approach to drainage flow and moisture distribution in a
   structured soil}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1998}},
Volume = {{43}},
Number = {{4}},
Pages = {{561-578}},
Month = {{AUG}},
