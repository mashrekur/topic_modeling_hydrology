@article{ ISI:A1996UP50100003,
Author = {Kloppmann, W and Dever, L and Edmunds, WM},
Title = {{Redox zones in the chalk aquifer of the Paris and North German basins}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1996}},
Volume = {{41}},
Number = {{3}},
Pages = {{311-326}},
Month = {{JUN}},
Abstract = {{A combined geochemical and isotopic study of chalk groundwater in the
   Paris Basin and northern Germany shows a distinct chemical zonation with
   respect to elements with different oxidation states (O, N, S, Fe, Mn, U)
   and to measured Eh. During the passage of a `'redox-barrier'' separating
   oxidizing and reducing groundwater O-2 and N-NO3- are removed. The
   observed reaction sequence corresponds to the theoretical sequence
   proposed by Stumm \& Morgan (1981) and to those described by Champ ed
   al. (1979) and Edmunds et al. (1984). Measured Eh can be used as a
   semi-quantitative master variable in order to interpret the observed
   reactions, Apparent stabilities of redox species are very similar to
   those in English chalk groundwater.}},
