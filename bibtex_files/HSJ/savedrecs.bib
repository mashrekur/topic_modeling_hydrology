@article{ ISI:A1994NR81000003,
Author = {COULOMB, C and DEVER, L},
Title = {{SEASONAL-VARIATIONS OF WATER AND SOLUTE TRANSFER IN A CLAY SOIL -
   ISOTOPIC AND CHEMICAL STUDY}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1994}},
Volume = {{39}},
Number = {{3}},
Pages = {{217-233}},
Month = {{JUN}},
Abstract = {{Studies have been made of in situ water and solute transfer in a drained
   clay soil during a sixteen month period. The mean isotopic composition
   of drainage water (deltaO-18 = -7.5 parts-per-thousand) as compared with
   that of rain water during the recharge period (deltaO-18 = -8.8
   parts-per-thousand), showed the important influence of water from soil
   microporosity (deltaO-18 = -7.0 parts-per-thousand). In October 1990,
   however, water bypassing from the surface to the drain represented most
   of the drainage flow. Shrinkage cracks forming rapid transmission
   pathways led to a mobilisation of water in less than 4 \% of the total
   porosity and drainage water presented high chemical concentrations. In
   January 1991, shrinkage cracks were closed as the clay swelled and water
   flowed through 30 mum microchannels. Water from microporosity represents
   more than 80\% of the total drained flows and dilution by rain water is
   a progressive phenomenon.}},
