@article{ ISI:000071018300008,
Author = {Sanz, E},
Title = {{Management of an aquifer with artificial recharge using water balance}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1997}},
Volume = {{42}},
Number = {{6}},
Pages = {{909-918}},
Month = {{DEC}},
Abstract = {{This study considers the management of an aquifer with artificial
   recharge whose water resources are mainly used for irrigation and where
   the irrigation return flows and natural discharge are considerable.
   These two variables are dependent on the use and increase of water
   resources. The interaction between some inflow and outflow variables is
   taken into account in the computation of the actual volume of water
   available, there being a pre-established artificial recharge of the
   aquifer. Conversely, the artificial recharge for a given water demand
   may be calculated. The most important inflow and outflow variables are
   used in an equation system (the water balance for each period of time)
   for these interactions and the total inflow and outflow. An iterative
   procedure is developed that solves the equation and with it the
   computation of the actual availability of water and storage.}},
