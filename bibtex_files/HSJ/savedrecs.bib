@article{ ISI:A1996VL50200007,
Author = {Cosandey, C and deOliveira, M},
Title = {{Saturated and contributing areas: Location and spatial extent in a
   catchment}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1996}},
Volume = {{41}},
Number = {{5}},
Pages = {{751-761}},
Month = {{OCT}},
Abstract = {{Saturated excess overland flow has been recognized to be the major
   process responsible for rapid flow in temperate areas with good
   vegetation cover. However, the problem of the location of the saturated
   surfaces remains a field of research. Actual topography is assumed to be
   the main factor yet in some conditions, paleotopography seems to be more
   determining. This situation is observed near Sao Paulo (Brazil) in
   relation to the erosive crisis preceding the actual sedimentation phase.
   On the other hand, studies show that saturated areas are not always
   contributing areas when they are not directly connected with the main
   stream. In order to make progress with hydrological models, it is
   suggested that, prior to modelling, hydrological watershed patterns need
   to be known.}},
