@article{ ISI:A1995TH59100005,
Author = {VARAS, EA and VONCHRISMAR, M},
Title = {{EXPERT-SYSTEM FOR THE SELECTION OF METHODS TO CALCULATE DESIGN FLOOD
   FLOWS}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1995}},
Volume = {{40}},
Number = {{6}},
Pages = {{739-749}},
Month = {{DEC}},
Abstract = {{This paper presents an expert system to help select the best method to
   estimate design flood flows for civil engineering works based upon the
   procedures available, the nature and characteristics of the basin and
   existing hydrological records. The system presents the user with a list
   of possible methods ranked in descending grade order and optionally
   presents explanations which support the selected choices. Ordering is
   achieved using the knowledge base provided by the expert. The system
   recommends procedures for both preliminary estimates and final designs.
   The system also constitutes a valuable aid for junior engineers and
   experienced hydrologists in the selection of methods. Its conceptual
   structure can be easily generalized to treat other problems of a similar
   nature in the field of hydrology and water resources.}},
