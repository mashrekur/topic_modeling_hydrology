@article{ ISI:A1996UE64400003,
Author = {Berndtsson, R and Bahri, A},
Title = {{Soil water, soil chemical and crop variations in a clay soil}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1996}},
Volume = {{41}},
Number = {{2}},
Pages = {{171-178}},
Month = {{APR}},
Abstract = {{Spatial properties of field soils vary in a complex manner. Especially
   in arid and semiarid areas, this variability affects plant quality and
   crop production. An experimental field plot was extensively sampled
   regarding soil water (378 gravimetrical samples), soil chemical content
   (314 samples), and crop yield and chemical content (26 samples) at the
   Cherfech agricultural field research station in Tunisia. Geostatistical
   analyses were made to gain a better understanding of the in situ
   variability of soil water, soil chemical and crop properties. Ranges of
   correlation were found to vary over distances between 5 and over 40 m.
   Nugget and sill values for semivariograms were almost an order of
   magnitude larger for crop samples compared to soil samples, The crop
   yield components appeared to have larger variability as compared to crop
   chemical components}},
