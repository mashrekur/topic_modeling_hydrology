@article{ ISI:A1994MW03200004,
Author = {SENE, KJ and PLINSTON, DT},
Title = {{A REVIEW AND UPDATE OF THE HYDROLOGY OF LAKE VICTORIA IN EAST-AFRICA}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1994}},
Volume = {{39}},
Number = {{1}},
Pages = {{47-63}},
Month = {{FEB}},
Abstract = {{Water balance models have been used to examine trends in the level of
   Lake Victoria over this century. A monthly model developed in a previous
   study for the period 1925 to 1978 has been improved and the simulation
   period extended to 1990. Also, an annual model has been used to extend
   the water balance back to the start of the century. The simulations
   confirm the results from previous studies which showed that the observed
   variations in lake level can be explained primarily in terms of natural
   variations in rainfall over the lake and surrounding basin. An idealized
   interpretation of these variations is presented which illustrates the
   long response time of this large lake and which is used to speculate on
   the likely future behaviour of the lake.}},
