@article{ ISI:000075062400002,
Author = {Van Oevelen, PJ},
Title = {{Soil moisture variability: a comparison between detailed field
   measurements and remote sensing measurement techniques}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1998}},
Volume = {{43}},
Number = {{4}},
Pages = {{511-520}},
Month = {{AUG}},
