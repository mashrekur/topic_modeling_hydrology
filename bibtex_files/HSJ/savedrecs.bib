@article{ ISI:A1993MH63600003,
Author = {LEE, MD and SWINDOLL, CM},
Title = {{BIOVENTING FOR IN-SITU REMEDIATION}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1993}},
