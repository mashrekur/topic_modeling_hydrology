@article{ ISI:000075062400004,
Author = {Germann, PF and Niggli, T},
Title = {{Dissipation of momentum during flow in soils}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1998}},
Volume = {{43}},
Number = {{4}},
Pages = {{537-548}},
