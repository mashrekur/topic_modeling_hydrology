@article{ ISI:A1994PU53500001,
Author = {ZHANG, XF and MCCONNACHIE, GL},
Title = {{A REAPPRAISAL OF THE ENGELUND BED-LOAD EQUATION}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1994}},
Volume = {{39}},
Number = {{6}},
Pages = {{561-567}},
Month = {{DEC}},
Abstract = {{Many bed load transport equations have been developed from differing
   standpoints. The Engelund equation has been selected for scrutiny since,
   although it accords well with experimental data in general, it has
   limitations under conditions of high bed load transport rates. The
