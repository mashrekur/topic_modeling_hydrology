Title = {{A practical approach to rainfall-runoff modelling in arid zone drainage
   basins}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1998}},
