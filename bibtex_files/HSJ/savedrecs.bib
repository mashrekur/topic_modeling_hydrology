@article{ ISI:A1991GT36400003,
Author = {FERRO, V and GIORDANO, G and IOVINO, M},
Title = {{ISOEROSIVITY AND EROSION RISK MAP FOR SICILY}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1991}},
Volume = {{36}},
Number = {{6}},
Pages = {{549-564}},
Month = {{DEC}},
Abstract = {{This paper reviews simplified methods for evaluating the rainfall
   erosivity index and proposes two relationships for estimating the annual
   value of Wischmeier's rainfall erosivity index at sites equipped with
   recording and non-recording raingauges. For the Sicilian region, the FAO
   index was also found to represent the erosion risk. A regional
   relationship to estimate the standard deviation of the annual erosivity
   index is proposed. The isoerosivity map is plotted by using 41 values of
