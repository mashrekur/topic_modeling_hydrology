@article{ ISI:A1995TH59100001,
Author = {STRUPCZEWSKI, WG and DOOGE, JCI},
Title = {{RELATIONSHIPS BETWEEN HIGHER CUMULANTS OF CHANNEL RESPONSE .1.
   PROPERTIES OF THE LINEAR CHANNEL RESPONSE}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1995}},
Volume = {{40}},
Number = {{6}},
Pages = {{675-687}},
Month = {{DEC}},
Abstract = {{The use of dimensionless cumulants to characterize the shape of the
   linear response of a semi-infinite prismatic channel is extended and
   simplified by means of the dimensionless coefficient (gamma(R)) linking
   the Rth order shape factor to the 3rd order shape factor raised to the
   power of (R - 2). It is shown that, for the special structure of the
   cumulants of the linearized St. Venant equations, the dimensionless
   cumulant coefficient (gamma(R)) of any order R > 4 is a single-valued
   function of gamma(4). It is also shown that the value of gamma(R) for
   any Froude number F-0 combined with any shape of section and any
   friction law (both characterized by the parameter m) is intermediate
   between the values for the two limiting cases of the diffusion analogy
   model (F-0 = 0) and the rapid flow model (F-0 = 1). The dimensionless
   cumulant coefficient gamma(R) is related to gamma(4) through a parameter
   p(m, F-0) by a polynomial of order {[}R/2] - 1.}},
