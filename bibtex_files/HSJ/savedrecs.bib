@article{ ISI:A1996VV16400003,
Author = {Hughes, DA and Smakhtin, V},
Title = {{Daily flow time series patching or extension: A spatial interpolation
   approach based on flow duration curves}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1996}},
Volume = {{41}},
Number = {{6}},
Pages = {{851-871}},
Month = {{DEC}},
Abstract = {{An algorithm is described that was initially developed as a simple
   method for patching and extending observed time series of daily
   streamflow. It is based on the use of I-day flow duration curves for
   each month of the year and on the assumption that flows occurring
   simultaneously at sites in reasonably close proximity to each other
   correspond to similar percentage points on their respective duration
   curves. The algorithm has been incorporated into a model that allows
   flows at a destination site to be estimated from flows occurring at
   several source sites. The model has been applied to six groups of
   catchments within southern Africa and the resulting streamflow
   simulations compare favourably with those obtained using a
   semi-distributed, physically-based, daily time step rainfall-runoff
   model. The current limitations of the approach and its future potential
   value are discussed.}},
