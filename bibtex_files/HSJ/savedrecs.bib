@article{ ISI:A1996UZ64800013,
Author = {Engman, ET},
Title = {{Remote sensing applications to hydrology: Future impact}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1996}},
Volume = {{41}},
Number = {{4}},
Pages = {{637-647}},
Month = {{AUG}},
Abstract = {{The case is made that the hydrological sciences are data limited and
   that future progress in understanding hydrological processes is hampered
