@article{ ISI:A1997WJ18800001,
Author = {Kabala, ZJ and Thorne, B},
Title = {{Hydraulics of one- and two-dimensional flow fields in aquifers}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1997}},
Volume = {{42}},
