@article{ ISI:A1992JG10100003,
Author = {BLAZKOVA, S},
Title = {{EMPIRICAL-STUDY OF NONLINEARITY IN DIRECT RUNOFF ON A 100 KM2 BASIN}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1992}},
Volume = {{37}},
Number = {{4}},
Pages = {{347-358}},
Month = {{AUG}},
Abstract = {{One of the scale problems in hydrology is to relate non-linearity in
   basin response to size and other factors. On the Sputka basin (103.4
   km2), three groups of unit hydrographs were identified, each group
   having a common shape parameter, N, of the Nash model and each,
   therefore, representing one dimensionless response. The existence of the
   three dimensionless responses can be explained in the first place by
   there being different spatial rainfall patterns for the events from
   which they were derived. The time parameter, K, within the individual
   groups depends primarily on the initial flow and on the skewness of the
   rainfall time pattern. However, when the conditions of rainfall
   uniformity and of a minimum depth are strictly met, and the initial flow
   is in a certain range, the basin behaves in a linear fashion.}},
