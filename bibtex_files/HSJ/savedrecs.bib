@article{ ISI:A1993KW63700001,
Author = {KLEIN, GS and YUFIT, GA and SHKURKO, VK},
Title = {{A NEW MOVING BOAT METHOD FOR THE MEASUREMENT OF DISCHARGE IN LARGE
   RIVERS}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1993}},
Volume = {{38}},
Number = {{2}},
Pages = {{79-88}},
Month = {{APR}},
Abstract = {{A new system known as `'Pless'' and based on the moving boat method has
   been designed and manufactured at the State Hydrological Institute, St
   Petersburg, for measuring discharges in large rivers. The system is
   fully automatic and the boat is free to move in any trajectory. The
   paper describes the principle of the system, the methodological
   peculiarities of its use, the equipment and the results of field
   measurements in the Neva River. In conclusion, the `'Pless'' system is
   compared with the acoustic discharge measurement system ADCP (Acoustic
   Doppler Current Profilers) designed in the USA. It is concluded that the
   `'Pless'' system may be recommended as a simple and inexpensive but
   effective system for hydrologists which greatly simplifies the often
   difficult procedure of discharge measurements in large rivers.}},
