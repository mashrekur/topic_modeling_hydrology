@article{ ISI:000339100700009,
Author = {Gourley, Jonathan J. and Flamig, Zachary L. and Hong, Yang and Howard,
   Kenneth W.},
Title = {{Evaluation of past, present and future tools for radar-based flash-flood
   prediction in the USA}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2014}},
Volume = {{59}},
Number = {{7, SI}},
Pages = {{1377-1389}},
Abstract = {{The societal impacts of flash floods are more significant than any other
   weather-related hazard. They are often manifested in the form of damage
   to infrastructure, flooding of roadways and bridges, creating deadly
   hazards to motorists and inundation of crops and pasture. Some of these
   hazards can be anticipated and thus mitigated given effective warning
   systems. This study describes the tools proposed over recent decades in
   the USA to predict flash flooding and evaluates them using a common
   observational data set. Design recommendations for flash-flood
   forecasting systems are provided, taking into account today's
   availability of high-resolution rainfall data at scales commensurate
   with flash flooding, their archives, spatial data sets to describe
   physiographic properties, and ever-increasing computational resources.}},
