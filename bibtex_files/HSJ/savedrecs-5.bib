@article{ ISI:000341646100004,
Author = {Tokarczyk, Tamara and Szalinska, Wiwiana},
Title = {{Combined analysis of precipitation and water deficit for drought hazard
   assessment}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2014}},
Volume = {{59}},
Number = {{9}},
