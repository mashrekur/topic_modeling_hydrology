@article{ ISI:000368691400006,
Author = {Vasquez, Vicente and Thomsen, Anton and Iversen, Bo V. and Jensen,
   Rasmus and Ringgaard, Rasmus and Schelde, Kirsten},
Title = {{Integrating lysimeter drainage and eddy covariance flux measurements in
   a groundwater recharge model}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2015}},
Volume = {{60}},
Number = {{9}},
Pages = {{1520-1537}},
Abstract = {{Field-scale water balance is difficult to characterize because controls
   exerted by soils and vegetation are mostly inferred from local-scale
   measurements with relatively small support volumes. Eddy covariance flux
   and lysimeters have been used to infer and evaluate field-scale water
   balances because they have larger footprint areas than local soil
   moisture measurements. This study quantifies heterogeneity of soil deep
   drainage (D) in four 12.5-m(2) repacked lysimeters, compares
   evapotranspiration from eddy covariance (ETEC) and mass balance
   residuals of lysimeters (ETwb(Lys)), and models D to estimate
   groundwater recharge. Variation in measured D was attributed to
   redirection of snowmelt infiltration and differences in lysimeter
   hydraulic properties caused by surface soil treatment. During the
   growing seasons of 2010, 2011 and 2012, ETwb(Lys) (278, 289 and 269 mm,
   respectively) was in good agreement with ETEC (298, 301 and 335 mm).
   Annual recharge estimated from modelled D was 486, 624 and 613 mm for
   three calendar years 2010, 2011 and 2012, respectively. In summary,
   lysimeter D and ETEC can be integrated to estimate and model groundwater
   recharge.}},
