@article{ ISI:000356942100009,
Author = {Khan, Usman T. and Valeo, Caterina},
Title = {{A new fuzzy linear regression approach for dissolved oxygen prediction}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2015}},
Volume = {{60}},
Number = {{6}},
Pages = {{1096-1119}},
Month = {{JUN 3}},
Abstract = {{A new method for fuzzy linear regression is proposed to predict
   dissolved oxygen using abiotic factors in a riverine environment, in
   Calgary, Canada. The proposed method is designed to accommodate fuzzy
   regressors, regressand and coefficients, i.e. representing full system
   uncertainty. The regression equation is built to minimize the distance
   between fuzzy numbers, and generalizes to crisp regression when crisp
   parameters are used. The method is compared to two existing fuzzy linear
   regression techniques: the Tanaka method and the Diamond method. The
   proposed new method outperforms the existing methods with higher
   Nash-Sutcliffe efficiency, and lower RMSE, AIC and total fuzzy distance.
   The new method demonstrates that nonlinear membership functions are more
   suitable for representing uncertain environmental data than the typical
   triangular representations. A result of this research is that low DO
   prediction is improved and consequently the approach can be used for
   risk analysis by water resource managers.}},
