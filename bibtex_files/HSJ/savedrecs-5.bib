@article{ ISI:000354624000007,
Author = {Tan, Mou Leong and Ibrahim, Ab Latif and Yusop, Zulkifli and Duan, Zheng
   and Ling, Lloyd},
Title = {{Impacts of land-use and climate variability on hydrological components
   in the Johor River basin, Malaysia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2015}},
Volume = {{60}},
Number = {{5, SI}},
Pages = {{873-889}},
Month = {{MAY 4}},
Abstract = {{This study aims to investigate separate and combined impacts of land-use
   and climate variability on hydrological components in the Johor River
   Basin (JRB), Malaysia. The Mann-Kendall and Sen's slope tests were
   applied to detect the trends in precipitation, temperature and
   streamflow of the JRB. The Soil and Water Assessment Tool (SWAT) was
   calibrated and validated using measured monthly streamflow data. The
   validation results showed that SWAT was reliable in the tropical JRB.
   The trend analysis showed that there was an insignificant increasing
   trend for streamflow, whereas significant increasing trends for
   precipitation and temperature were found. The combined (climate +
   land-use change) impact caused the annual streamflow and evaporation to
   increase by 4.4\% and 1.2\%, respectively. Climate (land-use) raised
   annual streamflow by 4.4\% (0.06\%) and evaporation by 2.2\% (-0.2\%).
   Climate change imposed a stronger impact than land-use change on the
   streamflow and evaporation. These findings are useful for decision
   makers to develop better water and land-use policies.
   {[}GRAPHICS]
   Editor Z. W. Kundzewicz; Guest editor V. Krysanova}},
