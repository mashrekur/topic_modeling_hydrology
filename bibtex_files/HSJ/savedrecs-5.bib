@article{ ISI:000349540800006,
Author = {Gutry-Korycka, Malgorzata and Golebiowska, Izabela},
Title = {{Hydrological alteration of an ice-marginal valley and its influence on
   the extent of historical floods}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2015}},
Volume = {{60}},
Number = {{3}},
Pages = {{482-497}},
Month = {{MAR 4}},
Abstract = {{Hydrological alteration within an ice-marginal valley is analysed in
   relation to flooding by the River Vistula flowing within that valley.
   Specifically, the analysis covers the period of the last two centuries
   (up to the present day), making reference to human impacts and natural
   disasters. Seven topographic maps, as well as digital terrain model
   (DTM) analysis of the extent of flooded areas are used to evaluate the
   linkage with historical flood events. Within the ice-marginal valley,
   flooding processes are found to still play an important role on the
   floodplain, although human activities have limited these significantly
   through the construction of embankments. The changing characteristics of
   floods generated by different mechanisms (e.g. heavy rainfall and ice
   jams) are also discussed.
   {[}GRAPHICS]
   Editor D. Koutsoyiannis}},
