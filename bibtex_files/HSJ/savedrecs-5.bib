@article{ ISI:000352345000004,
Author = {Krysanova, Valentina and Hattermann, Fred and Huang, Shaochun and Hesse,
   Cornelia and Vetter, Tobias and Liersch, Stefan and Koch, Hagen and
   Kundzewicz, Zbigniew W.},
Title = {{Modelling climate and land-use change impacts with SWIM: lessons learnt
   from multiple applications}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2015}},
Volume = {{60}},
Number = {{4}},
Pages = {{606-635}},
Month = {{APR 3}},
Abstract = {{The Soil and Water Integrated Model (SWIM) is a continuous-time
   semi-distributed ecohydrological model, integrating hydrological
   processes, vegetation, nutrients and erosion. It was developed for
   impact assessment at the river basin scale. SWIM is coupled to GIS and
   has modest data requirements. During the last decade SWIM was
   extensively tested in mesoscale and large catchments for hydrological
   processes (discharge, groundwater), nutrients, extreme events (floods
   and low flows), crop yield and erosion. Several modules were developed
   further (wetlands and snow dynamics) or introduced (glaciers,
   reservoirs). After validation, SWIM can be applied for impact
   assessment. Four exemplary studies are presented here, and several
   questions important to the impact modelling community are discussed. For
   which processes and areas can the model be used? Where are the limits in
   model application? How to apply the model in data-poor situations or in
   ungauged basins? How to use the model in basins subject to strong
   anthropogenic pressure?
   {[}GRAPHICS]
   Editor D. Koutsoyiannis; Associate editor C. Perrin}},
