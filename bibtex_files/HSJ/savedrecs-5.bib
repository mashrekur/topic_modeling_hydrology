Title = {{Modelling regional impacts of climate change on water resources: the
   Jucar basin, Spain}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2015}},
Volume = {{60}},
