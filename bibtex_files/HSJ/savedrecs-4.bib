@article{ ISI:000301945800014,
Author = {Bouzid-Lagha, Souaad and Djelita, Belkheir},
Title = {{Study of eutrophication in the Hamman Boughrara Reservoir (Wilaya de
   Tlemcen, Algeria)}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2012}},
Volume = {{57}},
Number = {{1}},
Pages = {{186-201}},
Abstract = {{The study of the eutrophication of the Hammam Boughrara Reservoir,
   located in the northwest of Tlemcen Wilaya (Algeria) is described. The
   utilization of a 9-year database of 19 physico-chemical, chemical and
   biotic parameters enabled us to determine: the evolution over time of
   each parameter; correlations between parameters; the degree of reservoir
   water eutrophication, and the dominant phytoplankton species (e. g.
   Chlorophyceae, Diatomophyceae), as well as their main nutrient,
   phosphorus. Using principal component analysis (PCA), the multivariate
   statistical study confirmed the obtained results, in particular, the
   significant pollution and hyper-eutrophication of the waters. The fight
   against the phenomenon of reservoir water eutrophication requires
   adequate management of pollutants. Thus, the development of a
   mathematical model adapted to our site, allowing us to predict the
   concentrations of phosphorus from its outflow and to manage the
   proliferation of algae by limiting nutrient concentrations, is an
   important contribution to this campaign.}},
