@article{ ISI:000315212700011,
Author = {El Mahi, Aicha and Meddi, Mohamed and Bravard, Jean Paul},
Title = {{Analysis of sediment transport in the Wadi el Hammam basin, northern
   Algeria}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
