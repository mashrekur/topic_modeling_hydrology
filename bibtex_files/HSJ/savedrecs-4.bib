@article{ ISI:000299421900010,
Author = {Stratford, Charlie J. and Acreman, Mike C. and Rees, H. Gwyn},
Title = {{A simple method for assessing the vulnerability of wetland ecosystem
   services}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2011}},
Volume = {{56}},
Number = {{8, SI}},
Pages = {{1485-1500}},
Abstract = {{Wetlands can provide many ecosystem services, but throughout the world
   are exposed to a range of pressures. In some locations these are well
   documented and are being successfully addressed. However at other
   locations, lack of data, resources and methods are hindering thorough
   assessment. This is particularly the case in remote wetlands such as
   those in the Himalayan region. This paper presents a methodology
   developed and tested on three wetland sites in Nepal. The method gives a
   structure to data collection and analysis and leads the user through to
   producing a vulnerability assessment of wetlands. We found that in each
