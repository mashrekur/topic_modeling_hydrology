@article{ ISI:000318289800001,
Author = {Piniewski, Mikolaj and Voss, Frank and Baerlund, Ilona and Okruszko,
   Tomasz and Kundzewicz, Zbigniew W.},
Title = {{Effect of modelling scale on the assessment of climate change impact on
   river runoff}},
