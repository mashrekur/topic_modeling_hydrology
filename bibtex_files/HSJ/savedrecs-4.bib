@article{ ISI:000323242200007,
Author = {Katimon, Ayob and Shahid, Shamsuddin and Abd Wahab, Ahmad Khairi and
   Shabri, Ani},
Title = {{Hydrological behaviour of a drained agricultural peat catchment in the
   tropics. 2: Time series transfer function modelling approach}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2013}},
Volume = {{58}},
Number = {{6}},
Pages = {{1310-1325}},
Month = {{AUG 1}},
Abstract = {{Transfer function models of the rainfall-runoff relationship with
   various complexities are developed to investigate the hydrological
   behaviour of a tropical peat catchment that has undergone continuous
   drainage for a long time. The study reveals that a linear transfer
   function model of order one and noise term of ARIMA(1,0,0) best
   represents the monthly rainfall-runoff relationship of a drained peat
   catchment. The best-fitted transfer function model is capable of
   illustrating the cumulative hydrological effects of the catchment when
   subjected to drainage. Transfer function models of daily rainfall-runoff
   relationships for each year of the period 1983-1993 are also developed
   to decipher the changes in hydrological behaviour of the catchment due
   to drainage. The results show that the amount of rain water temporarily
   stored in the peat soil decreased and the catchment has become more
   responsive to rainfall over the study period.}},
