@article{ ISI:000299421900015,
Author = {Cohen-Shacham, E. and Dayan, T. and Feitelson, E. and de Groot, R. S.},
Title = {{Ecosystem service trade-offs in wetland management: drainage and
   rehabilitation of the Hula, Israel}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2011}},
Volume = {{56}},
Number = {{8, SI}},
Pages = {{1582-1601}},
Abstract = {{The Hula wetland, a freshwater lake surrounded by swamps in the
   watershed basin of the Sea of Galilee, was drained in the 1950s for
   arable land and to reduce evaporation, thereby increasing the usable
   water available. A nature reserve was then designated, but this did not
   prevent species extinction. Later, severe environmental problems
   developed in the area; therefore, a rehabilitation project was
   implemented. The Hula wetland provides a good example of changes in
   ecosystem services under different management regimes and is, therefore,
   an appropriate setting for analysing trade-offs between different
   ecosystem services. Before drainage, the wetland provided a variety of
   ecosystem services. Following drainage, agricultural production services
   were dominant, but they declined over time. An analysis of the impact of
   drainage and rehabilitation on the Hula wetland is presented, and
   ecosystem service trade-offs that occurred over the past 80 years are
   evaluated.}},
