@article{ ISI:000315212700002,
Author = {Volpi, E. and Fiori, A.},
Title = {{Design event selection in bivariate hydrological frequency analysis}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2012}},
Volume = {{57}},
