@article{ ISI:000304453300005,
Author = {Merzougui, Amira and Slimani, Mohamed},
Title = {{Regionalization of statistical distributions fitted to monthly rainfall
   in Tunisia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2012}},
Volume = {{57}},
Number = {{4}},
Pages = {{668-685}},
Abstract = {{Frequency analysis of monthly rainfall collected on a network of 102
   raingauge stations over the whole Tunisian territory enabled us to
   identify the most appropriate statistical distribution to represent
   monthly rainfall in Tunisia. Various statistical techniques were used to
   identify the distribution that best allows this variable to be fitted
   for different areas of Tunisia. The two most appropriate distributions
   adopted to represent monthly rainfall in Tunisia are: the compound
   Poisson exponential distribution and the Galton mixed distribution. The
   weight of these two distributions varies according to time and space.}},
