@article{ ISI:000306111300006,
Author = {Carlier, Erick and Zouhri, Lahcen and Shahrour, Isam},
Title = {{Development of analogue reservoir modelling for anomalous recession
   investigation}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2012}},
Volume = {{57}},
Number = {{5}},
Pages = {{913-927}},
Abstract = {{Analogue models of reservoirs were developed based on the analysis of
   measured discharge from two karstic springs. The first spring is
   characterized by linearity between the flow and time; the second by a
   power law between flow and time. In the two studied cases, the
   reservoirs are nonlinear, according to the view point of linearity
   between stored volume and the flow at the same moment. The analogue
   models developed consist of a vertical relationship in a series of
   reservoirs. For the first spring, the reservoirs are entirely vertical
   wall reservoirs. The second spring consists of a vertical wall reservoir
   and a reservoir for which the geometry of the walls follows a non
   integer power law. The plausibility and the reproducibility of the
   analogue models were validated according to the available data. It was
   shown that the case of linear reservoirs in a karstic medium is a very
   particular one, corresponding to a hyperbolic reservoir or to a vertical
   wall reservoir with porous filter.}},
