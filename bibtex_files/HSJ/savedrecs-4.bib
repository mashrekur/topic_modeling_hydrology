@article{ ISI:000291263600011,
Author = {Chulli, B. and Makni, J. and Bedir, M. and Ben Dhia, H.},
Title = {{A multidisciplinary approach for assessment of the potential of the
   hydrogeothermal basins: case of the Sahel of Sfax (eastern Tunisia)}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2011}},
Volume = {{56}},
Number = {{3}},
Pages = {{507-520}},
Abstract = {{Des donnees geologiques, geochimiques et geophysiques ont ete analysees
   dans le but d'evaluer le potentiel en eaux thermales du bassin du Sahel
   de Sfax (Tunisie orientale). Nous avons etudie le reservoir
   hydrogeothermique pour determiner les differents facies chimiques des
   eaux thermales et comprendre leurs origines. L'analyse des donnees
   existantes et nouvellement acquises montre que l'eau thermale a
   temperature plus elevee est d'origine meteorique. Elle s'est infiltree
   profondement dans des sables et gres du Miocene superieur
   (Serravalien-Tortonien) et a atteint une temperature de 75 degrees C,
   avant de resurgir par un drainage vertical en suivant les dysharmonies
   tectoniques. Le gradient geothermique eleve de cette region confirme
   qu'il s'agit bien d'un reservoir a interet geothermal exploitable.
   Citation Chulli, B., Makni, J., Bedir, M. Ben Dhia, H. (2011) Une
   approche multidisciplinaire pour la prospection des bassins
   hydrogeothermiques: cas du Sahel de Sfax (Tunisie orientale). Hydrol.
   Sci. J. 56(3), 507-520.}},
