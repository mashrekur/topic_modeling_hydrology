@article{ ISI:000288943400013,
Author = {Abd Elbasit, Mohamed A. M. and Yasuda, Hiroshi and Salmi, Atte},
Title = {{Application of piezoelectric transducers in simulated rainfall erosivity
   assessment}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2011}},
Volume = {{56}},
Number = {{1}},
Pages = {{187-194}},
Abstract = {{Rainfall simulators have often been used to mimic natural rainfall for
   studies of various land-surface and water interaction processes. The
   characteristics of the simulated rainfall are the main indicators used
   to judge the performance of the rainfall simulators. The aim of this
   study is to investigate the potential of piezoelectric transducers for
   measuring and evaluating a dripper-type simulated rainfall drop-size
   distribution (DSD) and kinetic energy (KE). The directly measured KE was
   significantly correlated with the estimated KE using the drop-size
   distribution and empirical rain drop fall velocity relationships. This
   result emphasizes the potential use of the piezoelectric sensor to
   directly measure and evaluate rainfall kinetic energy. Also, the
   relationship between rainfall intensity and KE showed good patterns of
   agreement between simulated rainfall and natural rainfall.}},
