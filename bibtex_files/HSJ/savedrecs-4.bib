@article{ ISI:000278352500014,
Author = {Chokmani, Karem and Dever, Kristell and Bernier, Monique and Gauthier,
   Yves and Paquet, Lisa-Marie},
Title = {{Adaptation of the SNOWMAP algorithm for snow mapping over eastern Canada
   using Landsat-TM imagery}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2010}},
Volume = {{55}},
Number = {{4}},
Pages = {{649-660}},
Abstract = {{The snow mapping algorithm SNOWMAP was adapted to Landsat-TM data and to
   the context of eastern Canada. Six Landsat-5 TM scenes were used. It was
   found that the original version of SNOWMAP greatly underestimates snow
   cover extent. The modification made to the original algorithm, by
   cancelling the minimum threshold of 0.1 on the NDVI value, allows gaps
   to be filled in. In addition, a spatial correction procedure applied to
   the modified SNOWMAP algorithm results improves snow detection under
   coniferous forests. Based on a limited data set of ground-based
   observations (only 40 sites were available), the modified SNOWMAP seems
   to perform better in snow detection than the original version of the
   algorithm. An application case is presented in order to demonstrate the
   relevance of the modified SNOWMAP results as a high spatial-resolution
   reference for the validation of historical snow maps derived from medium
   spatial-resolution satellite data.}},
