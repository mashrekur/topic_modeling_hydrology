@article{ ISI:000278013100005,
Author = {Roudier, Philippe and Mahe, Gil},
Title = {{Calculation of design rainfall and runoff on the Bani basin (Mali): a
   study of the vulnerability of hydraulic structures and of the population
   since the drought}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2010}},
Volume = {{55}},
Number = {{3}},
Pages = {{351-363}},
Abstract = {{In the context of a changing climate, hydraulic engineering design could
   play an important role in vulnerability assessment. Thus, using the most
   recent daily data available, the 10-20- and 100-year rainfall has been
   calculated for 51 stations and the 10-, 20- and 100-year discharge for
   six stations of the Bani basin in Mali (129 000 km(2)). These values are
   computed using the L-moments method and by dividing the watershed into
   homogeneous regions. Finally, an empirical formula to calculate 10-year
   discharge is computed, considering the important break in discharge
   values around 1970. The computed values show that the period selected
   for computation is important, as a difference of a few years could
   change the results significantly.}},
