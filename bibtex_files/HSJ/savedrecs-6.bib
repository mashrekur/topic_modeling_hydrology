@article{ ISI:000373781900014,
Author = {Nanko, Kazuki and Hudson, Sean A. and Levia, Delphis F.},
Title = {{Differences in throughfall drop size distributions in the presence and
   absence of foliage}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
Number = {{3}},
Pages = {{620-627}},
Month = {{FEB 17}},
Abstract = {{Throughfall drop size distributions (DSDs) are important for plant-soil
   interactions. This is the first known study to quantify differences in
   throughfall DSDs with the presence and absence of foliage. Employing a
   disdrometer, three parameters solely representing throughfall drip were
   measured and calculated: maximum drop diameter (D-MAX), median volume
   diameter of drops (D50DR) and relative volume percentage of drops
   (pD(R)). Beneath Liriodendron tulipifera L. in Maryland (USA), D-MAX,
   D50DR and pD(R) were substantially larger when the canopy was
   unfoliated. In fact, the presence or absence of foliage was one of the
   primary factors affecting all three throughfall DSDs along with air
   temperature, according to the boosted regression tree analysis.
   Experimental results were attributed to differing physical properties of
   intercepted water between foliated and unfoliated periods and
   differential water behavior on leaves and bark. Future work should
   examine the effects of concentrated drip points on the development of
   throughfall-induced hot spots.
   {[}GRAPHICS]}},
