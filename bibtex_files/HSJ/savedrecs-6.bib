@article{ ISI:000380065500012,
Author = {Piotrowski, Adam P. and Napiorkowski, Jaroslaw J. and Osuch, Marzena and
   Napiorkowski, Maciej J.},
Title = {{On the importance of training methods and ensemble aggregation for
   runoff prediction by means of artificial neural networks}},
