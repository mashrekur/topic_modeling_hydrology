@article{ ISI:000374659000005,
Author = {Arumi, Jose Luis and Maureira, Hector and Souvignet, Maxime and Perez,
   Cesar and Rivera, Diego and Oyarzun, Ricardo},
Title = {{Where does the water go? Understanding geohydrological behaviour of
   Andean catchments in south-central Chile}},
