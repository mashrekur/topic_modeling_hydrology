@article{ ISI:000404851900006,
Author = {Kim, Jun-Mo and Lee, Jeonghoon},
Title = {{Time series analysis for evaluating hydrological responses of pore-water
   pressure to rainfall in a slope}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2017}},
Volume = {{62}},
Number = {{9}},
Pages = {{1412-1421}},
Abstract = {{Characterization of pore-water pressure at the soil-atmosphere interface
   is a major requirement in relation to slope instability. A rain-gauge
   and five piezometers (BH1-BH5) were installed on a slope located in a
   Korean military base. The upper slope (BH4, BH5) was covered with
   plastic sheets to prevent rainwater from percolating into the slope due
   to safety issues. Rainfall is matched by prompt changes in the
   pore-water pressure except at BH5. Due to the plastic cover, the pore
   water does not show any significant change in the early period by
   evapotranspiration. From correlation analysis, two wells (BH3, BH5) have
   longer memory effects due to matrix flow of past precipitation. Two
   principal components show hydrological responses of pore water to
   rainfall during intense rainfall, but PC2 does not indicate any
   important changes in low or no rainfall. This study suggests that
   correlation analysis with PCA can be a valuable tool for interpreting
   datasets consisting only of pore-water pressure.}},
