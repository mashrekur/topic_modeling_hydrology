@article{ ISI:000418482200006,
Author = {Pan, Suli and Fu, Guangtao and Chiang, Yen-Ming and Ran, Qihua and Xu,
   Yue-Ping},
Title = {{A two-step sensitivity analysis for hydrological signatures in Jinhua
   River Basin, East China}},
