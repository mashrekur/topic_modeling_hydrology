@article{ ISI:000382256600002,
Author = {Chitsaz, Nastaran and Azarnivand, Ali and Araghinejad, Shahab},
Title = {{Pre-processing of data-driven river flow forecasting models by singular
   value decomposition (SVD) technique}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
