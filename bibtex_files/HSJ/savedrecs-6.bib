@article{ ISI:000368714200002,
Author = {Svensson, Cecilia},
Title = {{Seasonal river flow forecasts for the United Kingdom using persistence
   and historical analogues}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
