@article{ ISI:000378698400014,
Author = {Saggai, Sofiane and Boutoutaou, Djamel and Saker, Mohamed Lakhdar},
Title = {{Reduction of evaporation of open water reserviors: a strategy for
   increasing water stocks in Algerian arid areas}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
Number = {{8}},
Pages = {{1548-1554}},
Abstract = {{Water shortage has been a problem in Algeria for a long time. The
   State's strategy aims to raise water stocks by adopting several
   solutions, including the construction of dams. However, even this last
   solution is faced with meteorological conditions which favour water
   losses by evaporation, especially in arid areas. This article aims to
   present the first Algerian trials on preservation of water bodies by
   reducing evaporation using long-chain organic chemicals that form a
   thin, ``mono-molecular layer{''} at the air/water interface. The results
   obtained show significant rates of evaporation reduction that depend on
   the chemical substance used, the duration between two applications of
   chemicals, and the meteorological conditions.}},
