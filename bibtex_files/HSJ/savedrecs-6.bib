@article{ ISI:000382256600011,
Author = {Jing, Xiuyan and Yang, Hongbin and Wang, Wenke and Cao, Yuqing},
Title = {{Hydro-geochemical simulation for the evolution of groundwater quality in
   Yinchuan Plain, China}},
