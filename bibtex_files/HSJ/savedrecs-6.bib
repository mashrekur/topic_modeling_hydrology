@article{ ISI:000403800000003,
Author = {Farsang, Andrea and Fejes, Ildiko and Toth, Tivadar M.},
Title = {{Integrated evaluation of urban groundwater hydrogeochemistry in context
   of fractal behaviour of groundwater level fluctuations}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2017}},
Volume = {{62}},
Number = {{8}},
Pages = {{1216-1229}},
Abstract = {{The present study was carried out as part of a complex survey of urban
   groundwater quality and quantity in Szeged, southeast Hungary. The
   concentrations of 12 inorganic contaminants in 28 shallow groundwater
   monitoring wells were determined over a 2-year period (2010-2012). The
   evaluation of concentrations indicates remarkable contamination all over
   the city. Discriminant analysis (DA) was used to evaluate the spatial
   changes of groundwater quality. The groundwater levels were measured
   over a 14-year period (2000-2013). The fractal properties of water level
   fluctuations time series characterizing the groundwater system and Tisza
   River were investigated using rescaled range (R/S) analysis. The
   resulting Hurst exponents clearly showed the persistency and thus long
   memory effects of both the groundwater and the river flow. Comparison of
   the results of DA with the results of R/S analysis thus implies that the
   geological conditions and the changing groundwater quantities are not
   related to groundwater quality.}},
