@article{ ISI:000378698400007,
Author = {Cheng, Qin-Bo and Reinhardt-Imjela, Christian and Chen, Xi and Schulte,
   Achim and Ji, Xiang and Li, Fu-Lin},
Title = {{Improvement and comparison of the rainfall-runoff methods in SWAT at the
   monsoonal watershed of Baocun, Eastern China}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
Number = {{8}},
Pages = {{1460-1476}},
Abstract = {{A new physics-based rainfall-runoff method of the Soil and Water
   Assessment Tool (SWAT) was developed, which integrates a water balance
   (WB) approach with the variable source area (WB-VSA). This approach was
   further compared with four methods-soil-water-dependent curve number
   (CN-Soil), evaporation-dependent curve number (CN-ET), Green and Ampt
   equation (G\& A) and WB-in a monsoonal watershed, Eastern China. The
   regional sensitivity analysis shows that volumetric efficiency
   coefficient (VE) with river discharges is sensitive to the most
   parameters of all approaches. The results of model calibration against
   VE demonstrate that WB-VSA is the most accurate owing to its reflection
   of the spatial variation of runoff generation as affected by topography
   and soil properties. Other methods can also mimic baseflow well, but the
   G\& A and CN-ET simulate floods much worse than the saturation excess
   runoff approaches (WB-VSA, WB and CN-Soil). Meanwhile, CN-Soil as an
   empirical method fails to simulate groundwater levels. By contrast,
   WB-VSA captures them best.}},
