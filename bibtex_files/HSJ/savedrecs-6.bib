@article{ ISI:000403800000002,
Author = {Jillo, A. Y. and Demissie, S. S. and Viglione, A. and Asfaw, D. H. and
   Sivapalan, M.},
Title = {{Characterization of regional variability of seasonal water balance
   within Omo-Ghibe River Basin, Ethiopia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2017}},
Volume = {{62}},
Number = {{8}},
Pages = {{1200-1215}},
Abstract = {{In this study we quantify the spatial variability of seasonal water
   balances within the Omo-Ghibe River Basin in Ethiopia using methods
   proposed within the Prediction in Ungauged Basins initiative. Our
   analysis consists of: (1) application of the rainfall-runoff model
   HBV-Light to several sub-catchments for which runoff data are available,
   and (2) estimation of water balances in the remaining ungauged
   catchments through application of the model with regionalized
   parameters. The analyses of the resulting water balance outcomes reveal
   that the seasonal water balance across the Omo-Ghibe Basin is driven by
   precipitation regimes that change with latitude, from being strongly
   seasonal in the north to precipitation spread throughout the year, but
   with a definite wetter season in the south. The basin is divided into
   two distinct regions based on patterns of seasonal water balance and, in
   particular, seasonal patterns of soil moisture storage.}},
