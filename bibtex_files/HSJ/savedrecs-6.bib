@article{ ISI:000386425100013,
Author = {Mao, Yuna and Ye, Aizhong and Liu, Xiaojie and Ma, Feng and Deng,
   Xiaoxue and Zhou, Zheng},
Title = {{High-resolution simulation of the spatial pattern of water use in
   continental China}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2016}},
Volume = {{61}},
Number = {{14}},
Pages = {{2626-2638}},
Abstract = {{High-resolution data on the spatial pattern of water use are a
   prerequisite for appropriate and sustainable water management. Based on
   one well-validated hydrological model, the Distributed Time Variant
   Gains Model (DTVGM), this paper obtains reliable high-resolution spatial
   patterns of irrigation, industrial and domestic water use in continental
   China. During the validation periods, ranges of correlation coefficient
   (R) and Nash-Sutcliffe efficiency (NSE) coefficient are 0.67-0.96 and
   0.51-0.84, respectively, between the observed and simulated streamflow
   of six hydrological stations, indicating model applicability to simulate
   the distribution of water use. The simulated water use quantities have
   relative errors (RE) less than 5\% compared with the observed. In
   addition, the changes in streamflow discharge were also correctly
   simulated by our model, such as the Zhangjiafen station in the Hai River
   basin with a dramatic decrease in streamflow, and the Makou station in
   the Pearl River basin with no significant changes. These changes are
   combined results of basin available water resources and water use. The
   obtained high-resolution spatial pattern of water use could decrease
   uncertainty of hydrological simulation and guide water management
   efficiently.}},
