@article{ ISI:000399493900011,
Author = {Morales-Marin, Luis Alejandro and Chun, Kwok Pan and Wheater, Howard
   Simon and Lindenschmidt, Karl-Erich},
Title = {{Trend analysis of nutrient loadings in a large prairie catchment}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2017}},
Volume = {{62}},
Number = {{4}},
Pages = {{657-679}},
Month = {{APR}},
Abstract = {{Nutrient loadings in many river catchments continue to increase due to
   rapid expansion of agriculture, urban and industrial development, and
   population growth. Nutrient enrichment of water bodies has intensified
   eutrophication which degrades water quality and ecosystem health. In
   this study, we carried out a trend analysis of total phosphorus and
   total nitrogen loads in the South Saskatchewan River (SSR) catchment
   using a novel approach to analyse nutrient time series. Seasonal
   analysis of trends at each of the water quality stations was performed
   to determine the relationships between annual flow regimes and nutrient
   loads in the catchment, in particular, the influence of the high spring
   runoff on nutrient export. Decadal analysis was also performed to
   determine the long-term relationships of nutrients with anthropogenic
   changes in the catchment. Although it was found that seasonal and
   historical variability of nutrient load trends is mainly determined by
   streamflow regime changes, there is evidence that increases in nitrogen
   concentration can also be attributed to anthropogenic changes.}},
