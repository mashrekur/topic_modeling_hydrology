@article{ ISI:000371253100007,
Author = {Beyer, M. and Wallner, M. and Bahlmann, L. and Thiemig, V. and Dietrich,
   J. and Billib, M.},
Title = {{Rainfall characteristics and their implications for rain-fed
   agriculture: a case study in the Upper Zambezi River Basin}},
