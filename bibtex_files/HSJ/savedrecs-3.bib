@article{ ISI:000268997600012,
Author = {Scheumann, Waltina and Alker, Marianne},
Title = {{Cooperation on Africa's transboundary aquifers-conceptual ideas}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
