@article{ ISI:000260149300003,
Author = {Levesque, Etienne and Anctil, Francois and van Griensven, Ann and
   Beauchamp, Nicolas},
Title = {{Evaluation of streamflow simulation by SWAT model for two small
   watersheds under snowmelt and rainfall}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2008}},
Volume = {{53}},
