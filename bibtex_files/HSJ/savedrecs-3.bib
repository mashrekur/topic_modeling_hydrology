@article{ ISI:000268997600007,
Author = {Mileham, Lucinda and Taylor, Richard G. and Todd, Martin and
   Tindimugaya, Callist and Thompson, Julian},
Title = {{The impact of climate change on groundwater recharge and runoff in a
   humid, equatorial catchment: sensitivity of projections to rainfall
   intensity}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2009}},
Volume = {{54}},
Number = {{4}},
