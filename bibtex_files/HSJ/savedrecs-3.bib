@article{ ISI:000267265600012,
Author = {Chappell, Nick A. and Discenza, Antonio R. and Tych, Wlodek and
   Whittaker, Joe and Bidin, Kawi},
Title = {{Simulating hourly rainfall occurrence within an equatorial rainforest,
   Borneo Island}},
