@article{ ISI:000226659600010,
Author = {Jain, SK and Reddy, NSRK and Chaube, UC},
Title = {{Analysis of a large inter-basin water transfer system in India}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2005}},
Volume = {{50}},
Number = {{1}},
Pages = {{125-137}},
Month = {{FEB}},
Abstract = {{Inter-basin transfer of water in India is a long-term option to correct
   the spatial and temporal mismatch of water availability and demand,
   largely owing to the monsoon climate. This paper is concerned with
   analysis and preliminary design of a large inter-basin water transfer
   system in peninsular India. The system covers four major basins and
   involves operation of 13 major structures. The study was carried out in
   three stages. First, the surface water deficit in each basin was
   estimated. Then the net deficit was worked out by considering the
   availability of groundwater. Finally, the link systems were planned to
   transfer the amount of water needed to meet the demands with desired
   reliability.}},
