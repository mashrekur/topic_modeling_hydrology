@article{ ISI:000245549900003,
Author = {Mutua, Benedict Mwavu and Klik, Andreas},
Title = {{Predicting daily streamflow in ungauged rural catchments: the case of
   Masinga catchment, Kenya}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
