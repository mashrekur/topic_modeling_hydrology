@article{ ISI:000232584200003,
Author = {Hung, CP and Wang, RY},
Title = {{Coding random self-similar river networks and calculating geometric
   distances: 2. Application to runoff simulations}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2005}},
Volume = {{50}},
Number = {{5}},
Pages = {{769-781}},
Month = {{OCT}},
Abstract = {{A preliminary method for coding random self-similar river networks and
   the corresponding distance calculations are proposed in a companion
   paper. The coding method is applied to generate random self-similar
   river networks, and the corresponding algorithm for calculating the
   geometric distances of the links is employed to determine the width
   function of the river networks, and thus evaluates the adaptability of
   the process. The width function-based geomorphological instantaneous
   unit hydrograph (WF-GIUH) model is then applied to estimate the runoff
   of the Po-bridge watershed in northern Taiwan. The results imply that
   the separately random selfsimilar generating algorithm can be used to
   simulate river networks during the rainfall-runoff process. It can also
   help analyse the variations of the river network when rainfall locations
   change and study the influence on hydrological responses (IUH) when the
   shape of river network changes.}},
