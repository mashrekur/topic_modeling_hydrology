@article{ ISI:000268997600008,
Author = {Walraevens, Kristine and Vandecasteele, Ine and Martens, Kristine and
   Nyssen, Jan and Moeyersons, Jan and Gebreyohannes, Tesfamichael and de
   Smedt, Florimond and Poesen, Jean and Deckers, Jozef and van Camp, Marc},
Title = {{Groundwater recharge and flow in a small mountain catchment in northern
   Ethiopia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
