@article{ ISI:000267265600016,
Author = {Zhou, Maichun and Ishidaira, Hiroshi and Takeuchi, Kuniyoshi and Gao,
   Yongtong},
Title = {{Evapotranspiration in the Mekong and Yellow river basins}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2009}},
Volume = {{54}},
Number = {{3}},
Pages = {{623-638}},
