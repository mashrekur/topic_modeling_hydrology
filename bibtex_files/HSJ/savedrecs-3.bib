Title = {{Adaptation to climate change in international river basins in Africa: a
   review}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2009}},
Volume = {{54}},
