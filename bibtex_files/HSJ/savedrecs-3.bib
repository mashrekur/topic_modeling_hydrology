@article{ ISI:000267265600013,
Author = {Kwon, Hyun-Han and Brown, Casey and Xu, Kaiqin and Lall, Upmanu},
Title = {{Seasonal and annual maximum streamflow forecasting using climate
   information: application to the Three Gorges Dam in the Yangtze River
   basin, China}},
