@article{ ISI:000233886100006,
Author = {L'Hote, Y and Chevallier, P and Coudrain, A and Lejeune, Y and
   Etchevers, P},
Title = {{Relationship between precipitation phase and air temperature: comparison
   between the Bolivian Andes and the Swiss Alps}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2005}},
Volume = {{50}},
Number = {{6}},
Pages = {{989-997}},
Month = {{DEC}},
Abstract = {{Determining the precipitation phase-rain or snow-is an important factor
   in modelling discharge in mountainous basins. In a study carried out in
   the outer tropical Andes Cordillera of Bolivia, half-hourly
   determination of precipitation phase was obtained by applying a suitable
   expert system, taking I I meteorological parameters into consideration
   that are measured over 21 months at an altitude close to 4800 m.
   Straightforward relationships between the determined precipitation phase
   and observed air temperature were analysed in histograms that contain
   percentage occurrences of snowfall, rainfall and mixed precipitation
   events for 0.5 degrees C air temperature increments. The graph shows a
   nearly identical distribution of percentage occurrences of snowfall in
   the Andes to that on a 1600-m high site in the Swiss Alps. This result
   suggests that, for hydrological modelling purposes in the outer tropical
   Andes, the same rain/snow threshold temperature as in the compared Swiss
   site can be applied.}},
