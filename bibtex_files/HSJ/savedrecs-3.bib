@article{ ISI:000255063900002,
Author = {Singh, Pratap and Haritashya, Umesh K. and Kumar, Naresh},
Title = {{Modelling and estimation of different components of streamflow for
   Gangotri Glacier basin, Himalayas}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2008}},
Volume = {{53}},
