@article{ ISI:000246889000012,
Author = {Parasuraman, Kamban and Elshorbagy, Amin and Carey, Sean K.},
Title = {{Modelling the dynamics of the evapotranspiration process using genetic
   programming}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2007}},
Volume = {{52}},
Number = {{3}},
