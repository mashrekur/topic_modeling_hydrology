@article{ ISI:000239562700009,
Author = {Bonacci, Ognjen and Jukic, Damir and Ljubenkov, Igor},
Title = {{Definition of catchment area in karst: case of the rivers Krcic and
   Krka, Croatia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2006}},
Volume = {{51}},
Number = {{4}},
Pages = {{682-699}},
Month = {{AUG}},
Abstract = {{The study area consists of the spring zones of the Krcic, Krka and
   Cetina river catchments located in the Dinaric karst, Croatia. Classical
   hydrological approaches and some newer time and frequency domain methods
   are used in order to validate the existing hypotheses both qualitatively
   and quantitatively, and these contribute to factual information about
   the hydrological behaviour of the catchments. The groundwater recharge
   rates are calculated by a mathematical model based on Palmer's
   soil-moisture balance method. The values of parameters of the
   groundwater recharge model are estimated by the spectral method. The
   calculated monthly and annual groundwater recharge rates form the basis
   for estimating the hydrological catchment areas of the spring zones and
   also for the determination of quantitative relationships between the
   catchments.}},
