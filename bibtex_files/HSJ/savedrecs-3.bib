@article{ ISI:000265235000006,
Author = {Ellouze, Manel and Abida, Habib and Safi, Riadh},
Title = {{A triangular model for the generation of synthetic hyetographs}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
