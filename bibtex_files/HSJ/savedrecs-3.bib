@article{ ISI:000267265600014,
Author = {Woo, Ming-Ko and Long, Tian-Yu and Thorne, Robin},
Title = {{Simulating monthly streamflow for the Upper Changjiang, China, under
   climatic change scenarios}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2009}},
Volume = {{54}},
