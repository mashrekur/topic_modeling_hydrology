@article{ ISI:000267265600015,
Author = {Shao, Quanxi and Zhang, Lu and Chen, Yongqin D. and Singh, Vijay P.},
Title = {{A new method for modelling flow duration curves and predicting
   streamflow regimes under altered land-use conditions}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
