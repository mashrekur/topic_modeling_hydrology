Language = {{English}},
Affiliation = {{Jacobeit, J (Reprint Author), Univ Augsburg, Inst Geog, Univ Str 10, D-86135 Augsburg, Germany.
   Univ Augsburg, Inst Geog, D-86135 Augsburg, Germany.
   Univ Wurzburg, Inst Geog, D-97074 Wurzburg, Germany.}},
DOI = {{10.1623/hysj.51.5.946}},
