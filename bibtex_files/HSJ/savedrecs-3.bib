@article{ ISI:000248730300015,
Author = {Ma, Ronghua and Ma, Xiaodong and Dai, Jinfang},
Title = {{Hyperspectral feature analysis of chlorophyll a and suspended solids
   using field measurements from Taihu Lake, eastern China}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2007}},
Volume = {{52}},
Number = {{4}},
Pages = {{808-824}},
