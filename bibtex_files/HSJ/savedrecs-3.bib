Title = {{Hydrological modelling with SWAT under conditions of limited data
   availability: evaluation of results from a Chilean case study}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2008}},
Volume = {{53}},
