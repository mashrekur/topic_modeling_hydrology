@article{ ISI:000239562700002,
Author = {Keskin, M. Erol and Taylan, Dilek and Terzi, Oezlem},
Title = {{Adaptive neural-based fuzzy inference system (ANFIS) approach for
   modelling hydrological time series}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2006}},
Volume = {{51}},
Number = {{4}},
Pages = {{588-598}},
Month = {{AUG}},
Abstract = {{The main aim of this study is to develop a flow prediction method, based
   on the adaptive neural-based fuzzy inference system (ANFIS) coupled with
   stochastic hydrological models. An ANFIS methodology is applied to river
   flow prediction in Dim Stream in the southern part of Turkey.
   Application is given for hydrological time series modelling. Synthetic
   series, generated through autoregressinve moving-average (ARMA) models,
   are then used for training data sets of the ANFIS. It is seen that the
   extension of input and output data sets in the training stage improves
