   Jenco, Marian},
Title = {{Response of channel dynamics to recent meander neck cut-off in a lowland
   meandering river with artificial training history: the Morava River,
   Czech Republic}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{8}},
