@article{ ISI:000497538700001,
Author = {Caissie, Daniel and El-Jabi, Nassir},
Title = {{The importance of cross-calibration and protecting water temperature
   sensors against direct solar radiation heating in hydrological studies}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2020}},
Volume = {{65}},
Number = {{1}},
Pages = {{102-111}},
Month = {{JAN 2}},
Abstract = {{Water temperature monitoring is important in many scientific studies.
   This study compares three models of water temperature sensors (Vemco
   Minilog II, HOBO TidbiT and HOBO Pendant) to test the importance of: (1)
   using cross-calibration to minimize relative errors, (2) using
   cross-calibration to improve on the accuracy of less accurate sensors,
   and (3) protecting sensors against direct solar radiation heating. The
   results show that when the same sensor models are cross-calibrated, the
   relative error can be reduced (Vemco: 0.01 degrees C; HOBO TidbiT: 0.02
   degrees C; and HOBO Pendant: 0.07 degrees C). Cross-calibration can also
   improve less accurate sensors (HOBO TidbiT and Pendant) to similar
   accuracies of Vemco (+/- 0.1 degrees C). Finally, no evidence of solar
   radiation heating was observed for Vemcos (unprotected); however, HOBO
   TidbiT and Pendant showed heating up to 2 degrees C (maximum). When HOBO
   TidbiT and Pendant are shielded (flow-through system), heating is no
   longer an issue.}},
