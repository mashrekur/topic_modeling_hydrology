@article{ ISI:000428253000004,
Author = {Thomas, Jobin and Joseph, Sabu and Thrivikramji, K. P.},
Title = {{Assessment of soil erosion in a monsoon-dominated mountain river basin
   in India using RUSLE-SDR and AHP}},
