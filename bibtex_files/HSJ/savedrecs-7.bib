@article{ ISI:000461573900006,
Author = {Nadiri, Ata Allah and Naderi, Keyvan and Khatibi, Rahman and Gharekhani,
   Maryam},
Title = {{Modelling groundwater level variations by learning from multiple models
   using fuzzy logic}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2019}},
Volume = {{64}},
Number = {{2}},
Pages = {{210-226}},
Abstract = {{Modelling time series of groundwater levels is investigated by three
   fuzzy logic (FL) models, Sugeno (SFL), Mamdani (MFL) and Larsen (LFL),
   using data from observation wells. One novelty in the study is the
   re-use of these three models as multiple models through the following
   strategies: (a) simple averaging, (b) weighted averaging and (c)
   committee machine techniques; these are implemented using artificial
   neural networks (ANN). These strategies provide some evidence that (i)
   multiple models improve on the performance of individual models and
   those using committee machines perform better than the other two
   options; and (ii) committee machine models produce defensible modelling
   results to develop management scenarios. The study investigates water
   table declines through management scenarios and shows that in this
   aquifer water use has higher impacts on water table variations than
   climatic variations. This provides evidence of the need for planned
   management in the study area.}},
