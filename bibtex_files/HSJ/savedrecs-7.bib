@article{ ISI:000466688100001,
Author = {Kisi, Ozgur and Heddam, Salim},
Title = {{Evaporation modelling by heuristic regression approaches using only
   temperature data}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2019}},
Volume = {{64}},
Number = {{6}},
Pages = {{653-672}},
Month = {{APR 26}},
Abstract = {{Accurate estimation of pan evaporation (E-pan) is very important in
   water resources management, irrigation scheduling and water budget of
   lakes. This study investigates the accuracy of two heuristic regression
   approaches, multivariate adaptive regression splines (MARS) and M5 model
   tree (M5Tree) in estimating pan evaporation using only temperature data
   as input. Monthly minimum temperature, maximum temperature and E-pan
   data from three Turkish stations were used, with month number
   (periodicity information) added as input to see its effect on estimation
   accuracy. The models were compared with the calibrated Hargreaves-Samani
   (CHS), Stephens-Stewart (SS) and multiple linear regression methods.
   Three different train-test splitting strategies (50\%-50\%, 60\%-40\%
   and 75\%-25\%) were employed for better evaluation of the applied
   methods. The results show that the MARS method generally estimated
   monthly E-pan with higher accuracy compared to the M5Tree, CHS and SS
   methods. When extraterrestrial radiation, calculated from Julian date
   and latitude information, was used as input to the SS instead of solar
   radiation, satisfactory estimates were obtained. A positive effect on
   model accuracy was observed when involving periodicity information in
   inputs and increasing training data length.}},
