@article{ ISI:000451778900003,
Author = {Wu, Ming and Wu, Jianfeng and Lin, Jin and Zhu, Xiaobin and Wu, Jichun
   and Hu, Bill X.},
Title = {{Evaluating the interactions between surface water and groundwater in the
   arid mid-eastern Yanqi Basin, northwestern China}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{9}},
Pages = {{1313-1331}},
Abstract = {{The environment of Bosten Lake in the Mid-Eastern Yanqi Basin (MEYB), an
   arid inland area in northwest China, has deteriorated greatly due to
   increasing groundwater exploitation and changes in the interactions
   between groundwater and surface water. This study intended to simulate
   the spatio-temporal variability of groundwater and surface water across
   the entire MEYB over the period 2000-2013. The applicable groundwater
   flow model and mass balance calculation method for river water were
   constructed to evaluate the change in groundwater recharged by and
   discharged to different segments of the Kaidu River. Simulation results
   show that the entire river seepage in the MEYB increased from 1.05 to
   6.17 x 10(8) m(3)/year between 2000 and 2013. The increasing river
   seepage, induced by increasing groundwater exploitation, plays the most
   important role in the water level decline in the downstream reaches of
   the Kaidu River and in Bosten Lake. This implies that the current
   utilization of groundwater resources in the MEYB is unsustainable.}},
