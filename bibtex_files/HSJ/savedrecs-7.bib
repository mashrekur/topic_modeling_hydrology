@article{ ISI:000468694400001,
Author = {Liliana Arganis-Juarez, Maritza and Preciado-Jimenez, Margarita and
   Fuentes-Mariles, Oscar A. and Mendoza-Resendiz, Alejandro and
   Dominguez-Mora, Ramon and De Luna-Cruz, Faustino and Carrizosa Elizondo,
   Eliseo},
Title = {{Hybrid method to update the design flood shape for spillway operation in
   a dam: case study of the Huites Dam, Mexico}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2019}},
Volume = {{64}},
Number = {{8}},
Pages = {{934-947}},
Month = {{JUN 11}},
Abstract = {{To design and review the operation of spillways, it is necessary to
   estimate design hydrographs, considering their peak flow, shape and
   volume. A hybrid method is proposed that combines the shape of the
   design hydrograph obtained with the UNAM Institute of Engineering Method
   (UNAMIIM) with the peak flow and volume calculated from a bivariate
   method. This hybrid method is applied to historical data of the Huites
   Dam, Sinaloa, Mexico. The goal is to estimate return periods for the
   maximum discharge flows (that account for the damage caused downstream)
   and the maximum levels reached in the dam (measure of the hydrological
   dam safety) corresponding to a given spillway and its management policy.
   Therefore, to validate the method, the results obtained by the flood
   routing of the 50-year hydrograph are compared with those obtained by
   the flood routing of the three largest historical floods. Both maximum
   flow and elevation were in the range of values observed within 37.5-75
   years corresponding to the length of the historical record.}},
