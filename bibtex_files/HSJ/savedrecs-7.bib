@article{ ISI:000492508500001,
Author = {Fischer, Svenja and Schumann, Andreas and Buehler, Philipp},
Title = {{Timescale-based flood typing to estimate temporal changes in flood
   frequencies}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2019}},
Volume = {{64}},
Number = {{15}},
Pages = {{1867-1892}},
Month = {{NOV 18}},
Abstract = {{The flood peak is the dominating characteristic in nearly all
   flood-statistical analyses. Contrary to the general assumptions of
   design flood estimation, the peak is not closely related to other flood
   characteristics. Differentiation of floods into types provides a more
   realistic view. Often different parts of the probability distribution
   function of annual flood peaks are dominated by different flood types,
   which raises the question how shifts in flood regimes would modify the
   statistics of annual maxima. To answer this, a distinction into five
   flood types is proposed; then, temporal changes in flood-type
   frequencies are investigated. We show that the frequency of floods
   caused by heavy rain has increased significantly in recent years. A
   statistical model is developed that simulates peaks for each event type
   by type-specific peak-volume relationships. In a simulation study, we
   show how changes in frequency of flood event type lead to changes in the
   quantiles of annual maximum series.}},
