@article{ ISI:000448315900005,
Author = {Cho, Younghyun and Engel, Bernard A. and Merwade, Venkatesh M.},
Title = {{A spatially distributed Clark's unit hydrograph based hybrid hydrologic
   model (Distributed-Clark)}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{10}},
Pages = {{1519-1539}},
Abstract = {{A hybrid hydrologic model (Distributed-Clark), which is a lumped
   conceptual and distributed feature model, was developed based on the
   combined concept of Clark's unit hydrograph and its spatial
   decomposition methods, incorporating refined spatially variable flow
   dynamics to implement hydrological simulation for spatially distributed
   rainfall-runoff flow. In Distributed-Clark, the Soil Conservation
   Service (SCS) curve number method is utilized to estimate spatially
   distributed runoff depth and a set of separated unit hydrographs is used
   for runoff routing to obtain a direct runoff flow hydrograph. Case
   studies (four watersheds in the central part of the USA) using spatially
   distributed (Thiessen polygon-based) rainfall data of storm events were
   used to evaluate the model performance. Results demonstrate relatively
   good fit to observed streamflow, with a Nash-Sutcliffe efficiency (E-NS)
   of 0.84 and coefficient of determination (R-2) of 0.86, as well as a
   better fit in comparison with outputs of spatially averaged rainfall
   data simulations for two models including HEC-HMS.}},
