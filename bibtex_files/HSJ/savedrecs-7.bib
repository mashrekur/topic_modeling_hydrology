@article{ ISI:000434400300004,
Author = {Schneeberger, Klaus and Rossler, Ole and Weingartner, Rolf},
Title = {{Spatial patterns of frequent floods in Switzerland}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{6}},
Pages = {{895-908}},
Abstract = {{This study investigates the spatial dependence of high and extreme
   streamflows in Switzerland across different scales. First, using 56
   runoff time series from Swiss rivers, we determined the average length
   of high-streamflow events for different levels of extremeness. Second, a
   dependence measure that expressed the probability that streamflow peaks
   would meet or exceed streamflow peaks at a conditioning site was used to
   describe and map the spatial extent of joint streamflow-peak occurrences
   across Switzerland. Third, we analysed the spatial patterns of jointly
   occurring high streamflows using cluster analysis to identify groups
   that react similarly in terms of flood frequency at different sites. The
   results indicate that, on a coarse scale, high and extreme streamflows
   are asymptotically independent in the main Swiss basins. Additionally,
   mesoscale tributaries in the main basins show distinct flood regions
   across river systems.}},
