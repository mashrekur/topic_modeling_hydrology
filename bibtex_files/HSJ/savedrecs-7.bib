@article{ ISI:000488669600001,
Author = {Carletti, Alberto and Canu, Simona and Motroni, Andrea and Ghiglieri,
   Giorgio},
Title = {{A combined methodology for estimating the potential natural aquifer
   recharge in an arid environment}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2019}},
Volume = {{64}},
Number = {{14}},
Pages = {{1727-1745}},
Month = {{OCT 26}},
Abstract = {{An innovative methodology that combines an indirect physiography-based
   method for determining the runoff coefficient at a sub-basin scale and a
   water balance model applied on a daily time scale was developed to
   calculate the natural groundwater recharge in three watersheds within
   the Oum Zessar arid area, Tunisia. The effective infiltration was
   calculated as part of the water surplus by considering the average
   available water content (AWC) of soil and an average runoff coefficient
   for each sub-basin. The model indicates that the sub-basins covered
   mainly by the ?artificial? soils of tabias and jessour, characterized by
   average AWC values greater than 150 mm, did not contribute to natural
   groundwater recharge over the 10-year period (2003?2012) considered. The
   estimated volume for the Triassic aquifer amounted to about 4.5
   hm(3)?year(?1), which is consistent with previous studies. For the
   Jurassic and Cretaceous aquifers, the estimated volumes amounted to
   about 200 dm(3)?year(?1).}},
