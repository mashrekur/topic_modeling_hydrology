@article{ ISI:000428253000001,
Author = {Mangini, Walter and Viglione, Alberto and Hall, Julia and Hundecha,
   Yeshewatesfa and Ceola, Serena and Montanari, Alberto and Rogger,
   Magdalena and Salinas, Jose Luis and Borzi, Iolanda and Parajka, Juraj},
Title = {{Detection of trends in magnitude and frequency of flood peaks across
   Europe}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{4}},
Pages = {{493-512}},
Abstract = {{This study analyses the differences in significant trends in magnitude
   and frequency of floods detected in annual maximum flood (AMF) and peak
   over threshold (POT) flood peak series, for the period 1965-2005. Flood
   peaks are identified from European daily discharge data using a
   baseflow-based algorithm and significant trends in the AMF series are
   compared with those in the POT series, derived for six different
   exceedence thresholds. The results show that more trends in flood
   magnitude are detected in the AMF than in the POT series and for the POT
   series more significant trends are detected in flood frequency than in
   flood magnitude. Spatially coherent patterns of significant trends are
   detected, which are further investigated by stratifying the results into
   five regions based on catchment and hydro-climatic characteristics. All
   data and tools used in this study are open-access and the results are
   fully reproducible.}},
