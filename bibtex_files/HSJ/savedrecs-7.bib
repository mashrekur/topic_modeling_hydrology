@article{ ISI:000469599800001,
Author = {Liu, Xiaojiao and Chen, Rensheng and Liu, Junfeng and Wang, Xiqiang and
   Zhang, Baogui and Han, Chuntan and Liu, Guohua and Guo, Shuhai and Liu,
   Zhangwen and Song, Yaoxuan and Yang, Yong and Zheng, Qin and Wang, Lei},
Title = {{Effects of snow-depth change on spring runoff in cryosphere areas of
   China}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2019}},
Volume = {{64}},
Number = {{7}},
Pages = {{789-797}},
Month = {{MAY 19}},
Abstract = {{Based on snow-depth and measured runoff data in the 19 river basins of
   the cryosphere areas of China, changes in cold season snow depth (CSSD)
   and spring runoff, and the relationship between CSSD and spring runoff
   were analysed. Decreasing trends in CSSD were detected mainly in the
   plains (plateaus), while increasing trends were found mainly in the
   mountainous regions. Different combinations of precipitation and
   temperature may be responsible for these results. The response of spring
   runoff to CSSD change varied greatly under climate warming. The runoff
   in April and May was commonly affected by CSSD, while the runoff in
   March was less affected by CSSD, but more by the increasing temperature
   causing more snowmelt in March. Due to relatively greater snow
   accumulation in spring, the runoff during spring was less affected by
   CSSD in the southern Tienshan Mountains.}},
