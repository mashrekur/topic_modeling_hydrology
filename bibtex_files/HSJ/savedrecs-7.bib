@article{ ISI:000434400300006,
Author = {Mo, Chongxun and Mo, Guiyan and Liu, Peng and Zhong, Huanhuan and Wang,
   Dayang and Huang, Ya and Jin, Juliang},
Title = {{Reservoir operation by staging due to climate variability}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{6}},
Pages = {{926-937}},
Abstract = {{The changing environment enhances the hydrological cycle and increases
   the frequency of extreme floods. In this paper, the impacts of climate
   variability on flood season segmentation are determined and the
   scientific basis for determining corresponding flood limiting water
   levels (FLWLs) is provided. Climate variation was determined and then
   the flood season was divided into several sub-seasons using the results
   of the set pair analysis method (SPAM) and four indices; peak floods
   crossing the transitional periods were sampled to obtain a design flood
   hydrograph; and, finally, seasonal FLWLs were determined for reservoir
   operation. The performance of this reservoir staging operation was
   evaluated for a case study in the Chengbihe Reservoir, China.}},
