@article{ ISI:000492888300001,
Author = {Wang, Y. and Zhang, X. and Tang, Q. and Mu, M. and Zhang, C. and Lv, A.
   and Jia, S.},
Title = {{Assessing flood risk in Baiyangdian Lake area in a changing climate
   using an integrated hydrological-hydrodynamic modelling}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Abstract = {{Flood-risk is affected by both climatic and anthropogenic factors. In
   this study, we assess changes in flood risk induced by a combination of
   climate change and flood prevention sets in the Baiyangdian (BYD) Lake
   area of China. Extreme storm events are analysed by the bias-corrected
   climate data from global climate models. A hydrological model is
   implemented and integrated with a hydrodynamic model to assess flood
   risk under three scenarios. The streamflow into the BYD was validated
   against historical flash-flood events. The results indicate that the
   changing climate increased extreme precipitation, upstream total inflow
   and the flood risk at the core region of Xiong'an New Area (XNA), the
   newly announced special economic zone in the BYD area. However, flood
   prevention measures can effectively mitigate the climatic effect. The
   research highlights the severe flash-flood risk at BYD and demonstrates
   the urgent need for a climate-resilient plan for XNA.}},
