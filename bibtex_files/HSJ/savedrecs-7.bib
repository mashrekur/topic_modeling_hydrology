@article{ ISI:000448315900002,
Author = {Akhavan, Samira and Mousabeygi, Fatemeh and Peel, Murray C.},
Title = {{Assessment of eight reference evapotranspiration (ETo) methods
   considering Koppen climate class in Iran}},
