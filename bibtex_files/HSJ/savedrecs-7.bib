@article{ ISI:000424760700009,
Author = {Harchegani, Mahboobeh and Sadeghi, Seyed Hamidreza and Asadi, Hossein},
Title = {{Comparing grain size distribution of sediment and original soil under
   raindrop detachment and raindrop-induced and flow transport mechanism}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{2}},
Pages = {{312-323}},
Abstract = {{The grain size distribution (GSD) of sediment in comparison with the
   original soil GSD is discussed under different slopes (5, 15 and 25\%)
   and rainfall intensities (30, 60 and 90 mm h(-1) with respective
   duration of 30, 15 and 10 min) but identical runoff (15 mm). The
   sediment quantification was carried out by raindrop-induced flow
   transport (RIFT) or/and transport by flow (FT) using a rainfall
   simulator and a 6 x 1 m(2) erosion plot and a silt loam. The results
   show a high degree of enrichment for size classes of 2-4 and 4-8 mu m
   and a high degree of depletion for size classes of > 63 mu m under
   different slopes and rainfall intensities. In addition, the results show
   that the experimental enrichment ratio (ER) for particle size <16 mu m
   under different slopes and rainfall intensities was greater than 1,
   while the ER for particle size >32 mu m was less than 1.}},
