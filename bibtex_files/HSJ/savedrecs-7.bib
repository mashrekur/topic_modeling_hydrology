@article{ ISI:000450274900008,
Author = {Blazejewski, Ryszard and Niec, Jakub and Murat-Blazejewska, Sadzide and
   Zawadzki, Pawel},
Title = {{Comparison of infiltration models with regard to design of rectangular
   infiltration trenches}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2018}},
Volume = {{63}},
Number = {{11}},
Pages = {{1707-1716}},
Abstract = {{Guidelines for the design of infiltration trenches of rectangular
   cross-section are compared. Four concepts of infiltration into a native
   soil from linear soakaways of rectangular cross-section are discussed.
   The results of calculations using analytical equations are compared with
   numerical two-dimensional (2D) simulations using HYDRUS computer code
   and field measurements. Satisfactory agreement is achieved for Kozeny's
   concept of infiltration and the free-surface approach, applied to a
   trench in a highly saturated sandy loam. Theoretical solutions,
   neglecting matric potential, suggest that the mean infiltration rate
   along the wetted perimeter of a rectangular trench varies in the range
   1.0 to 1.5 of the value of saturated hydraulic conductivity.}},
