@article{ ISI:000462351800004,
Author = {De Luna-Cruz, Faustino and Ramos-Hernandez, J. G. and Fuentes-Mariles,
   O. A. and Gracia-Sanchez, J.},
Title = {{FluBiDi - a model to estimate flood based on runoff: validation using
   extreme and natural basin conditions}},
