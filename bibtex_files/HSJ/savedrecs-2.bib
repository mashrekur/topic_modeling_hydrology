@article{ ISI:000221662200003,
Author = {Zouhri, L and Gorini, C and Mania, J and Deffontaines, B and Zerouali,
   AEH},
Title = {{Spatial distribution of resistivity in the hydrogeological systems, and
   identification of the catchment area in the Rharb basin, Morocco}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2004}},
Volume = {{49}},
Number = {{3}},
Pages = {{387-398}},
Month = {{JUN}},
Abstract = {{Geophysical results obtained in the Rharb basin, Morocco are reported.
   Correlations between hydrogeological well logs reveal several
   water-bearing Plio-Quaternary units resting on a substrate of blue
   marls. Geo-electrical borehole analyses were interpreted using
   bi-logarithmic diagrams which indicate the permeable layers of the
   aquifer and also its basement. Resistivity data from NE-SW and NW-SE
   electrical sections allow definition of the permeable/impermeable
   levels, and identification of ``ditches{''} that may be favourable
   sectors for hydrogeological exploitation. Resistivity anomalies were
   investigated by analysing maps of resistivity at 400 and 1000 m AB.
   Anomalies identified in the Rharb basin are related to the thickening of
   the permeable bodies (sand, limestone, sandstone deposits). In the
   coastal zone (AB = 1000 m), there is a strong decrease of the
   resistivity gradient (35-10 Omega m), which is probably linked to marine
   intrusion. Electrical anomalies allow detection of the water-bearing
   zones notably in the western and southwestern parts of the Rharb basin.}},
Publisher = {{IAHS PRESS, INST HYDROLOGY}},
Address = {{C/O FRANCES WATKINS, WALLINGFORD OX10 8BB, ENGLAND}},
