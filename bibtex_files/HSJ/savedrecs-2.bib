Title = {{Challenges and opportunities for water resources management in southeast
   Asia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2001}},
Volume = {{46}},
Number = {{6}},
Pages = {{923-935}},
