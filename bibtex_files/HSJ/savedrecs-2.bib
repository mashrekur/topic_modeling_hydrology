@article{ ISI:000225364500003,
Author = {Guo, SL and Zhang, HG and Chen, H and Peng, DZ and Liu, P and Pang, B},
Title = {{A reservoir flood forecasting and control system for China}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
