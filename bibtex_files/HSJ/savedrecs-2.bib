@article{ ISI:000081689800004,
Author = {Kjeldsen, TR and Lundorf, A and Rosbjerg, D},
Title = {{Barriers to sustainable water resources management - a Zimbabwean case
   study}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1999}},
Volume = {{44}},
Number = {{4}},
Pages = {{529-539}},
Month = {{AUG}},
Abstract = {{An investigation of barriers to a more sustainable management of the
   available water resources in a Zimbabwean catchment has been conducted.
   Currently (spring 1997), the water resources of Zimbabwe are managed
   according to the 1976 Water Act. Two types of barrier have been
   identified: those directly related to the Water Act and others related
   to the traditional perception of water resources in Zimbabwe. Barriers
   related to the Water Act include the existing water right system, a
   highly centralized system of decision making, and a non-participatory
   system. Other barriers are the price of water, lack of information (both
   water resources system data and socio-economic data), lack of drought
   planning, and little or no environmental concern.}},
