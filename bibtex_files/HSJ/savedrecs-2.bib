@article{ ISI:000186920900010,
Author = {Bernier, M and Fortin, JP and Gauthier, Y and Corbane, C and Somma, J
   and Dedieu, JP},
Title = {{Integration of remotely sensed data with hydrological modelling of Mount
   Liban (Lebanon)}},
