@article{ ISI:000184608300011,
Author = {Blazejewski, R and Murat-Blazejewska, S},
Title = {{Analytical solutions of a routing problem for storm water in a detention
   basin}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2003}},
Volume = {{48}},
Number = {{4}},
Pages = {{665-671}},
Month = {{AUG}},
Abstract = {{Analytical solutions of a routing problem for storm water flowing
   through a linear reservoir are presented for the assumption of
   trapezoidal-shaped inflow hydrograph. The maximum ponded (water) depth
   in the detention basin is chosen as a main design criterion.
   Calculations are carried out for a given rain recurrence interval but
   for various rain durations and sand filter surface areas to reach the
