@article{ ISI:000220582600002,
Author = {Corradini, C and Morbidelli, R and Saltalippi, C and Melone, F},
Title = {{Flood forecasting and infiltration modelling}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2004}},
Volume = {{49}},
Number = {{2}},
Pages = {{227-236}},
Month = {{APR}},
Abstract = {{The role of accuracy in the representation of infiltration on the
   effectiveness of real-time flood forecasting models was investigated. A
   simple semi-distributed model of conceptual type with adaptive estimate
   of hydraulic characteristics included in the infiltration component was
   selected. Infiltration was described by a very accurate approach
   recently formulated for complex rainfall patterns, or alternatively
   through a simpler formulation known as all extension of the classical
   time compression approximation. The results indicated that, for
   situations involving a significant rainfall variability in space, the
   inaccuracy in the representation of infiltration cannot be corrected by
   the adaptive component of the rainfall-runoff model. A preliminary
   analysis of the role of all approximation of saturated hydraulic
   conductivity to be used in each homogeneous area of the semi-distributed
   model used both in non-adaptive version and in real-time is also
   presented.}},
