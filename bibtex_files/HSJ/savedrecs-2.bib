@article{ ISI:000168138800006,
Author = {Sen, Z and Oztopal, A},
Title = {{Genetic algorithms for the classification and prediction of
   precipitation occurrence}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
