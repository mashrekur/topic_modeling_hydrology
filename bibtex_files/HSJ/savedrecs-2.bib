@article{ ISI:000180824800002,
Author = {Burn, DH},
Title = {{The use of resampling for estimating confidence intervals for single
   site and pooled frequency analysis}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2003}},
Volume = {{48}},
Number = {{1}},
Pages = {{25-38}},
Month = {{FEB}},
Abstract = {{A balanced resampling approach is presented for estimating confidence
   intervals for extreme flow quantiles determined from data at a single
   site. The approach is also adapted to provide resampled estimates for
   confidence intervals for extreme flow quantiles obtained from pooled
   frequency analysis. The balanced resampling approach does not require
   assumptions, in contrast to conventional approaches that are typically
   based on an asymptotic formula and require a distributional assumption.
   The approach is demonstrated to provide useful information in the
   context of both single site and pooled frequency analysis. The
   application of the approach also demonstrates the benefits of employing
   a pooled frequency analysis approach for estimating extreme flow
   quantiles.}},
