@article{ ISI:000171980100007,
Author = {Mdaghri-Alaoui, A and Eugster, W},
Title = {{Field determination of the water balance of the Areuse River delta,
   Switzerland}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2001}},
Volume = {{46}},
Number = {{5}},
Pages = {{747-760}},
Month = {{OCT}},
Abstract = {{The water balance during a period of one year (15 October 1990-15
   October 1991) was determined at an experimental site in the Areuse River
   delta (Switzerland). The groundwater recharge rates were found to be 36
   and 33\% of total precipitation according to evapotranspiration
   estimates based on the Primault (1962) and the Penman-Monteith methods,
   respectively. Variations in the water storage were obtained by weekly
   measurements with a neutron probe. Observed hydraulic gradients
   indicated a zero-flux plane between depths of 0.55 and 1.02 in that
   separated the infiltration zone from the zone of evapotranspiration in
   all seasons.}},
