@article{ ISI:000085282400007,
Author = {Burn, DH and Goel, NK},
Title = {{The formation of groups for regional flood frequency analysis}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2000}},
Volume = {{45}},
Number = {{1}},
Pages = {{97-112}},
Month = {{FEB}},
Abstract = {{A new technique is developed for identifying groups for regional flood
   frequency analysis. The technique uses a clustering algorithm as a
   starting point for partitioning the collection of catchments. The groups
   formed using the clustering algorithm are subsequently revised to
   improve the regional characteristics based on three requirements that
   are defined for effective groups. The result is overlapping groups that
   can be used to estimate extreme flow quantiles for gauged or ungauged
   catchments. The technique is applied to a collection of catchments from
   India and the results indicate that regions with the desired
   characteristics can be identified using the technique. The use of the
   groups for estimating extreme flow quantiles is demonstrated for three
   example sites.}},
