@article{ ISI:000175177500010,
Author = {Coudrain, A and Loubet, M and Condom, T and Talbi, A and Ribstein, P and
   Pouyaud, B and Quintanilla, J and Dieulin, C and Dupre, B},
Title = {{Isotopic data (Sr-87/Sr-86) and hydrological changes during the last 15
   000 years on the Andean Altiplano}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2002}},
Volume = {{47}},
Number = {{2}},
Pages = {{293-306}},
Month = {{APR}},
Abstract = {{At present, the southern basin of the Altiplano (17degrees-22degreesS,
   126 000 km(2)) receives about 39.4 x 10(9) m(3) year(-1) including 4\%
   of surface runoff from the northern catchment (14degrees-17degreesS) via
   the Rio Desaguadero. The values of Sr-87/Sr-86 of present surface flow
   are distinct for the two catchments. This tracer can thus be used for
   studying the past and contrasting hydrological conditions of the
   southern basin. For the and period from 10 000 to 2000 years BP, the
   acquired data and the transport modelling of soluble strontium in an
   aquifer confirm that the levels of precipitation were about 80\% of the
   present amounts and that the inflow from the Desaguadero was
   insignificant. For the high levels of the palaeolake Tauca, around 15
   000 years BP, the hydrological and isotopic data indicate that, for a
   total inflow estimated at 110 x 10(9) m(3) year(-1) the northern
   contribution might have been limited to 5\%.}},
