@article{ ISI:000177622100003,
Author = {Panu, US and Sharma, TC},
Title = {{Challenges in drought research: some perspectives and future directions}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2002}},
Volume = {{47}},
Number = {{SI}},
Pages = {{S19-S30}},
