@article{ ISI:000078567600008,
Author = {Nawaz, NR and Adeloye, AJ},
Title = {{Evaluation of monthly runoff estimated by a rainfall-runoff regression
   model for reservoir yield assessment}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1999}},
Volume = {{44}},
Number = {{1}},
Pages = {{113-134}},
Month = {{FEB}},
Abstract = {{Results of further evaluation tests of a monthly rainfall-runoff model
   used for extending streamflow data records in England and Wales are
   presented. In particular, since the objective of the record extension
   exercise was to make available long enough data records for reservoir
   yield assessment, model performance in reproducing the reservoir
   storage-yield-reliability relationship during calibration was examined.
   Finally, it can be argued that this type of test should be among the
   suite of performance checks on rainfall-runoff models in situations
   where low flow is the primary concern.}},
