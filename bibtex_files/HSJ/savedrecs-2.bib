@article{ ISI:000181957700005,
Author = {Chen, SY and Fu, GT},
Title = {{A DRASTIC-based fuzzy pattern recognition methodology for groundwater
   vulnerability evaluation}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2003}},
Volume = {{48}},
Number = {{2}},
Pages = {{211-220}},
Month = {{APR}},
Abstract = {{The evaluation of groundwater vulnerability based on DRASTIC is a
   process that recognizes the level type of a sample by comparing it with
   the standard values from the data in the DRASTIC index. However, it is
   difficult to find the same value as those standard values in practice.
   As the above mentioned procedure is a typical fuzzy pattern recognition
   problem, a fuzzy pattern recognition methodology is proposed to solve
   it. The standard value matrix of factors is constructed on the basis of
   the data in the DRASTIC index and then converted into the membership
   degree matrix. A new fuzzy pattern recognition model is developed to
   provide the evaluation levels of samples, which are expressed by
   linguistic variables. Finally, a case study of the Dalian Peninsula is
   presented, which shows the proposed methodology is straightforward and
   its results are easily understood.}},
