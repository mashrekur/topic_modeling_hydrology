@article{ ISI:000177622000008,
Author = {Harbison, J and Cox, M},
Title = {{Hydrological characteristics of groundwater in a subtropical coastal
   plain with large variations in salinity: Pimpama, Queensland, Australia}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
