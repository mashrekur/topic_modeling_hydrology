@article{ ISI:000178362700003,
Author = {Bayazit, M and Onoz, B},
Title = {{LL-moments for estimating low flow quantiles}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2002}},
Volume = {{47}},
Number = {{5}},
Pages = {{707-720}},
Month = {{OCT}},
Abstract = {{A parameter estimation method is proposed for fitting probability
   distribution functions to low flow observations. LL-moments are variants
   of L-moments that are analogous to LH-moments, which were defined for
   the analysis of floods. LL-moments give higher weights to the small
   observations. Expressions are given that relate them to the probability
   distribution function for the case of normal, Weibull and power
   distributions. Sampling properties of the LL-moments and of the
   distribution parameters and quantiles estimated by them are found by a
   Monte Carlo simulation study. It is shown on an example that the low
   flow quantile estimates obtained by LL-moments may be significantly
   different from those obtained by L-moments.}},
