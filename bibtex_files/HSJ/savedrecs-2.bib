@article{ ISI:000177622000007,
Author = {Holzbecher, E},
Title = {{Advective flow in sediments under the influence of compaction}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2002}},
