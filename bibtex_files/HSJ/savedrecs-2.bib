@article{ ISI:000222737000002,
Author = {Tate, E and Sutcliffe, J and Conway, D and Farquharson, F},
Title = {{Water balance of Lake Victoria: update to 2000 and climate change
   modelling to 2100}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2004}},
Volume = {{49}},
Number = {{4}},
Pages = {{563-574}},
Month = {{AUG}},
Abstract = {{An annual water balance model of Lake Victoria is derived for the period
   1925-2000. Regression techniques are used to derive annual inputs to the
   water balance, based on lake rainfall data, measured and derived inflows
   and estimated evaporation during the historical period. This approach
   acknowledges that runoff is a nonlinear function of lake rainfall. A
   longer inflow series is produced here which is representative of the
   whole inflow to the lake, rather than just from individual tributaries.
   The results show a good simulation of annual lake levels and outflows
   and capture the high lake level in 1997-1998. Climate change scenarios,
   from a recent global climate model experiment, are applied to the lake
   rainfall inflow series and evaporation data to estimate future water
   balances of the lake. The scenarios produce a potential fall in lake
   levels by the 2030s horizon, and a rise by the 2080s horizon. A
   discussion of the application of climate change data to this complex
   hydrological system is presented.}},
