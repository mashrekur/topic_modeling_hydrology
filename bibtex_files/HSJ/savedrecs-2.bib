@article{ ISI:000088836200007,
Author = {Singh, VS},
Title = {{Well storage effect during pumping tests in an aquifer of low
   permeability}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2000}},
Volume = {{45}},
Number = {{4}},
Pages = {{589-594}},
Month = {{AUG}},
Abstract = {{Well storage effect is generally considered while interpreting pumping
   test data fi om large diameter wells. However, in an aquifer of low
   permeability, the well storage is found to br significant during pumping
   tests conducted on bore wells. The interpretation such data gives
   ambiguous results unless well storage effect is taken into account. A
   field example is presented to illustrate the difficulty in
   interpretation of the pumping test data. In order to take into account
   the well storage effect, a finite difference approach of interpreting
   pumping test data is suggested.}},
