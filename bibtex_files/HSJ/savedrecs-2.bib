@article{ ISI:000083080600003,
Author = {Srinivas, A and Rao, BV and Rao, VVSG},
Title = {{Recharge process and aquifer models of a small watershed}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{1999}},
Volume = {{44}},
Number = {{5}},
Pages = {{681-692}},
Month = {{OCT}},
Abstract = {{Kanchanapally watershed covering an area of about 11 km(2) in Nalgonda
   district, Andhra Pradesh, India is located in granitic terrain.
   Groundwater recharge has been estimated from a water balance model using
   hydrometeorological data from 1978-1994. The monthly recharge estimates
   obtained from the water balance model formed input for the groundwater
   flow model during transient model testing. The groundwater flow model
   has been prepared to simulate steady state groundwater conditions of
   1977 using the nested squares finite difference method. The transient
   groundwater flow model has been tested during 1977-1994 using the
   estimated recharge values. The present study helped verify the
   usefulness of monthly recharge estimates for accounting dynamic
   variations in recharge as reflected in water level fluctuations in
   hydrographs.}},
