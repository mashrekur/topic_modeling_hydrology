@article{ ISI:000173295700011,
Author = {Gottschalk, L and Beldring, S and Engeland, K and Tallaksen, L and
   Saelthun, NR and Kolberg, S and Motovilov, Y},
Title = {{Regional/macroscale hydrological modelling: a Scandinavian experience}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2001}},
Volume = {{46}},
Number = {{6}},
Pages = {{963-982}},
Month = {{DEC}},
