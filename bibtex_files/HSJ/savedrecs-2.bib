@article{ ISI:000173295700003,
Author = {Krasovskaia, I},
Title = {{Perception of the risk of flooding: the case of the 1995 flood in Norway}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2001}},
Volume = {{46}},
Number = {{6}},
Pages = {{855-868}},
