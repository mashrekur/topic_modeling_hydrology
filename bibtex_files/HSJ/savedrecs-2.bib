@article{ ISI:000176248400002,
Author = {Seidou, O and Rousselle, J and Lefebvre, M and Lauzon, N and Ribeiro, J},
Title = {{Modelling uncertainty on future river flow sequences}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2002}},
Volume = {{47}},
Number = {{3}},
Pages = {{367-385}},
Month = {{JUN}},
Abstract = {{Risk management in water resources, just like reservoir management, is a
   problem of decision under uncertainty. The quality of the solutions is
   thus extremely sensitive to the way in which the random variability of
   the inflows is taken into account. This paper proposes a method to model
   uncertainty of the short-term inflows according to the period of the
   year. A continuous time parametric Markovian model based on the filtered
   Poisson process is built and estimated with regularly spaced
   observations, then validated on three hydrometric stations located in
   Quebec (Canada). The points studied are the capacity of the model to
   correctly represent flow transitions as well as its performance as a
   predictor of the one-step-ahead flow value. This aspect is compared with
   those of the seasonal ARMA model. The results indicate that the model
   displays good behaviour on each of these two points.}},
