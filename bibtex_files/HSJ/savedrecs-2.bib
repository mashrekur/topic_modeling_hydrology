@article{ ISI:000173295700006,
Author = {Hubert, P},
Title = {{Multifractals as a tool to overcome scale problems in hydrology}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2001}},
Volume = {{46}},
Number = {{6}},
