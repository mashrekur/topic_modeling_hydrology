@article{ ISI:A1995RH11700009,
Author = {DIETRICH, WE and REISS, R and HSU, ML and MONTGOMERY, DR},
Title = {{A PROCESS-BASED MODEL FOR COLLUVIAL SOIL DEPTH AND SHALLOW LANDSLIDING
   USING DIGITAL ELEVATION DATA}},
