@article{ ISI:A1996WG09000003,
Author = {Chang, ATC and Foster, JL and Hall, DK},
Title = {{Effects of forest on the snow parameters derived from microwave
   measurements during the Boreas Winter Field Campaign}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1996}},
Volume = {{10}},
Number = {{12}},
Pages = {{1565-1574}},
Month = {{DEC}},
Note = {{53rd Eastern Snow Conference, WILLIAMSBURG, VA, MAY, 1996}},
Abstract = {{Passive microwave data have been used to infer the areal snow water
   equivalent (SWE) with some success. However, the accuracy of these
   retrieved SWE values have not been well determined for heterogeneous
   vegetated regions. The Boreal Ecosystem-Atmosphere Study (BOREAS) Winter
   Field Campaign (WFC), which took place in February 1994, provided the
   opportunity to study in detail the effects of boreal forests on snow
   parameter retrievals. Preliminary results reconfirmed the relationship
   between microwave brightness temperature and snow water equivalent. The
   pronounced effect of forest cover on SWE retrieval was studied. A
   modified vegetation mixing algorithm is proposed to account for the
   forest cover. The relationship between the microwave signature and
   observed snowpack parameters matches results from this model.}},
