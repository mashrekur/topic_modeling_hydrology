@article{ ISI:A1992JF99600007,
Author = {BINLEY, A and BEVEN, K},
Title = {{3-DIMENSIONAL MODELING OF HILLSLOPE HYDROLOGY}},
Journal = {{HYDROLOGICAL PROCESSES}},
