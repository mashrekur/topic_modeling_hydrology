@article{ ISI:A1994NF91800002,
Author = {STONE, M and DROPPO, IG},
Title = {{IN-CHANNEL SURFICIAL FINE-GRAINED SEDIMENT LAMINAE .2. CHEMICAL
   CHARACTERISTICS AND IMPLICATIONS FOR CONTAMINANT TRANSPORT IN FLUVIAL
   SYSTEMS}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1994}},
Volume = {{8}},
Number = {{2}},
Pages = {{113-124}},
Month = {{MAR-APR}},
Abstract = {{Samples of surficial fine-grained laminae (SFGL) were collected in three
   south-westem Ontario rivers. Each sediment sample was subjected to a
   sequential extraction procedure designed to partition particulate metals
   (Cd, Pb, Cu, Zn) into five operationally defined fractions: (1)
   exchangeable; (2) bound to carbonates; (3) bound to Fe-Mn oxides; (4)
   bound to organic matter; and (5) residual. Particulate phosphus was
   sequentially extracted from the sediment samples into three fractions:
   (1) non-apatite inorganic P; (2) apatite P; and (3) organic P.
   The major accumulate phases of trace metals in SFGL are carbonates,
   Fe-Mn oxides and organic matter. The content of NAIP in SFGL ranged from
   17 to 38\% of total particulate P. Compared with suspended and bed
   sediments, levels of P and trace metals in SFGL were lower at the study
   sites. A conceptual overview of physical, chemical and biological
   processes influencing formation of SFGL and the potential role of this
   fine-grained sediment for contaminant transport in fluvial systems is
   presented.}},
