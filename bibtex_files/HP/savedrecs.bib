@article{ ISI:A1996VH18800001,
Author = {DeRoo, APJ},
Title = {{The LISEM project: An introduction}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1996}},
Volume = {{10}},
Number = {{8}},
Pages = {{1021-1025}},
Month = {{AUG}},
Abstract = {{In the loess area of The Netherlands a soil erosion project has been
   carried out to examine the present-day amounts of overland how, erosion
   and deposition in drainage basins in the South Limburg region and to
   assess the effects of conservation measures and land use changes using
   simulation models. As part of the project, which was ordered by the
   Province of Limburg, the Waterboard `Roer en Overmaas', the Ministry of
   Agriculture and 14 municipalities of South Limburg, the physically based
   hydrological and soil erosion model LISEM (the Limburg soil erosion
   model) has been developed. In this paper the history and scope of the
   project are explained, as well as the held and laboratory programme. In
   addition the research drainage basins are described.}},
