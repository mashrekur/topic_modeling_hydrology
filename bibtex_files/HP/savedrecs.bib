@article{ ISI:A1995RH42600003,
Author = {BEVEN, K},
Title = {{LINKING PARAMETERS ACROSS SCALES - SUBGRID PARAMETERIZATIONS AND
   SCALE-DEPENDENT HYDROLOGICAL MODELS}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1995}},
Volume = {{9}},
