@article{ ISI:A1995RH42600014,
Author = {SIVAPALAN, M and WOODS, RA},
Title = {{EVALUATION OF THE EFFECTS OF GENERAL-CIRCULATION MODELS SUBGRID
   VARIABILITY AND PATCHINESS OF RAINFALL AND SOIL-MOISTURE ON LAND-SURFACE
   WATER-BALANCE FLUXES}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1995}},
Volume = {{9}},
