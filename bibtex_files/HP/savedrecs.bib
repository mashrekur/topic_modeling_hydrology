@article{ ISI:A1996UK10600011,
Author = {Alley, RB},
Title = {{Towards a hydrological model for computerized ice-sheet simulations}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1996}},
Volume = {{10}},
Number = {{4}},
Pages = {{649-660}},
Month = {{APR}},
Abstract = {{Ice-sheet modelling typically uses grid cells 10 km or more on a side,
   so any hydrological and sliding model must average or parameterize
   processes that vary over shorter distances than this. Observations and
   theory suggest that basally produced water remains in a distributed,
   high-pressure system unless it encounters low-pressure channels fed by
   surface melt. Such distributed systems appear to exhibit increasing
   water storage, water transmission and water lubrication of sliding with
   increasing water pressure. A model based on these assumptions
   successfully simulates some aspects of the non-steady response of
   mountain glaciers to externally forced channel-pressure variations; it
   merits testing in ice-sheet modelling.}},
