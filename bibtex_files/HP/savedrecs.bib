@article{ ISI:A1994MU80000002,
Author = {SHARMA, KD and MURTHY, JSR and DHIR, RP},
Title = {{STREAMFLOW ROUTING IN THE INDIAN ARID ZONE}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1994}},
Volume = {{8}},
Number = {{1}},
Pages = {{27-43}},
Month = {{JAN-FEB}},
Abstract = {{A lumped model for streamflow routing in arid ephemeral channels has
   been developed. The governing equations for movement of flood waves
   subjected to transmission losses are simplified through a time averaging
   process to develop an ordinary differential equation describing
   transmission losses as a function of distance, inflow, channel width,
   time parameters of flow and effective hydraulic conductivity. The
   resulting equation has an analytical solution and simulates runoff
   volume and peak discharge rates for individual storm events. The outflow
