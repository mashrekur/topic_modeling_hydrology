@article{ ISI:A1995RH42600012,
Author = {OKI, T and MUSIAKE, K and MATSUYAMA, H and MASUDA, K},
Title = {{GLOBAL ATMOSPHERIC WATER-BALANCE AND RUNOFF FROM LARGE RIVER BASINS}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1995}},
Volume = {{9}},
Number = {{5-6}},
