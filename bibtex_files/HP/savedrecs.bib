@article{ ISI:A1996VB13100002,
Author = {Sklash, MG and Beven, KJ and Gilman, K and Darling, WG},
Title = {{Isotope studies of pipeflow at Plynlimon, Wales, UK}},
Journal = {{HYDROLOGICAL PROCESSES}},
