@article{ ISI:A1995RH11700013,
Author = {KALMA, JD and BATES, BC and WOODS, RA},
Title = {{PREDICTING CATCHMENT-SCALE SOIL-MOISTURE STATUS WITH LIMITED
   FIELD-MEASUREMENTS}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1995}},
Volume = {{9}},
Number = {{3-4}},
