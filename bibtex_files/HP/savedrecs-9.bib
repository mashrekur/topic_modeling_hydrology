@article{ ISI:000261218900006,
Author = {Ghazavi, G. and Thomas, Z. and Hamon, Y. and Marie, J. C. and Corson, M.
   and Merot, P.},
Title = {{Hedgerow impacts on soil-water transfer due to rainfall interception and
   root-water uptake}},
Journal = {{HYDROLOGICAL PROCESSES}},
