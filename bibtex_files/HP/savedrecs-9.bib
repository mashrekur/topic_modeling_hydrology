@article{ ISI:000258519700012,
Author = {Chenard, Jean-Francois and Caissie, Daniel},
Title = {{Stream temperature modelling using artificial neural networks:
   application on Catamaran Brook, New Brunswick, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
