@article{ ISI:000258244500027,
Author = {Fox, Andrew M. and Willis, Ian C. and Arnold, Neil S.},
Title = {{Modification and testing of a one-dimensional energy and mass balance
   model for supraglacial snowpacks}},
Journal = {{HYDROLOGICAL PROCESSES}},
