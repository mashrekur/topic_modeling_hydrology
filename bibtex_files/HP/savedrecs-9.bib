@article{ ISI:000257954400022,
Author = {Chen, Jiann-Mou and Tan, Yih-Chi and Wu, Yu-Zhang},
Title = {{Analysis of infiltration of 2D trickle irrigation under multiple-line
   sources}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
