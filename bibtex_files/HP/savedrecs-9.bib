Author = {Blume, Theresa and Zehe, Erwin and Bronstert, Axel},
Title = {{Investigation of runoff generation in a pristine, poorly gauged
   catchment in the Chilean Andes II: Qualitative and quantitative use of
   tracers at three spatial scales}},
