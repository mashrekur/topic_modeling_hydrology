@article{ ISI:000262275400007,
Author = {Chen, Jiann-Mou},
Title = {{Analysis of the water content in an unconfined aquifer with a partially
   penetrated pumping well}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
Number = {{26}},
Pages = {{5037-5047}},
Month = {{DEC 30}},
Abstract = {{Most methods developed to represent water flow phenomena in an
   unconfined aquifer with a fully penetrated pumping well are either
   numerical, such as the well-known FEMWTER model, or experimental;
   analytical models of a partially penetrated pumping well are rare. This
   study employs the linearized Richards equation as the governing
   equation, with the aid of Fourier Integral Transformation, to obtain an
   analytical solution of the water content distribution in an unconfined
   aquifer with a partially penetrated pumping well. The results from this
   study could serve to substantiate in some sense results from numerical
   models. In addition, the theory developed herein can be modified to
   simulate a vacuum-pressured pumping well since it is derived by
   considering, among others, the location and length of a well screen with
   fluxes. Copyright (C) 2008 John Wiley Sons, Ltd.}},
