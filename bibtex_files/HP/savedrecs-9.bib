@article{ ISI:000262275400003,
Author = {Zhang, Qiang and Xu, Chong-Yu and Chen, Yongqin David and Yu, Zuguo},
Title = {{Multifractal detrended fluctuation analysis of streamflow series of the
   Yangtze River basin, China}},
