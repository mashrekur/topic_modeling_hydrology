@article{ ISI:000258244500018,
Author = {Aitkenhead, M. J. and Cooper, R. J.},
Title = {{Neural network time series prediction of environmental variables in a
   small upland headwater in NE Scotland}},
Journal = {{HYDROLOGICAL PROCESSES}},
