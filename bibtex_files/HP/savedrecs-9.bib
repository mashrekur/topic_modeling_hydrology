@article{ ISI:000261143000004,
Author = {Jukic, Damir and Denic-Jukic, Vesna},
Title = {{Estimating parameters of groundwater recharge model in frequency domain:
   Karst springs Jadro and Zrnovnica}},
Journal = {{HYDROLOGICAL PROCESSES}},
