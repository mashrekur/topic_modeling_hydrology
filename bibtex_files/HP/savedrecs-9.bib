@article{ ISI:000257090200013,
Author = {Waddington, J. M. and Toth, K. and Bourbonniere, R.},
Title = {{Dissolved organic carbon export from a cutover and restored peatland}},
Journal = {{HYDROLOGICAL PROCESSES}},
