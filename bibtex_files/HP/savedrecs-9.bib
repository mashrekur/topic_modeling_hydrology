@article{ ISI:000261143000013,
Author = {Muttil, Nitin and Jayawardena, A. W.},
Title = {{Shuffled Complex Evolution model calibrating algorithm: enhancing its
   robustness and efficiency}},
Journal = {{HYDROLOGICAL PROCESSES}},
