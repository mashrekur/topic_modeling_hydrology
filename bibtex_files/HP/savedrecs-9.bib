@article{ ISI:000260579000002,
Author = {Charlier, Jean-Baptiste and Cattan, Philippe and Moussa, Roger and
   Voltz, Marc},
Title = {{Hydrological behaviour and modelling of a volcanic tropical cultivated
   catchment}},
