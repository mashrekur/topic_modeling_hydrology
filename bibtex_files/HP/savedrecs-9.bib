@article{ ISI:000262275400002,
Author = {Osti, Rabindra and Egashira, Shinji},
Title = {{Method to improve the mitigative effectiveness of a series of check dams
   against debris flows}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
Number = {{26}},
Pages = {{4986-4996}},
Month = {{DEC 30}},
Abstract = {{To advance of technology has led to more competent countermeasures, but
   lives and properties still continue to suffer from water-induced
   disasters, such as floods, landslides, and debris flows. to increase the
   effectiveness of counter systems, improved methods of planning and
   designing such systems are prerequistie. This paper describes briefly a
   methodological approach for predicting debris flow characteristics, and
   propose techniques for evaluating and improving the mitigative
   effectiveness of check dams against debris flows in steep mountain
   torrents. Additionally, a non-dimensional parameters, namely potential
   storage volume, is introduced to generalize the evaluation processes. As
   an example, the 1999 debris-flow event in the San Julian River,
   Venezuela, is chosen for discussion. The paper also proposes a method of
   evaluting the control function of a series of check dams as well as the
   criteria for the selection of their sizes, numbers and locations, It is
   hoped that this work will help to determine which combinations of check
   dams will fit best together for the optimal control of debris flows and
   available resources in any river basin. Copyright (C) 2008 Wiley \&
   Sons. Ltd.}},
