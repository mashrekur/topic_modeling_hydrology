@article{ ISI:000258244500013,
Author = {Lee, Jin-Jing and Jang, Cheng-Shin and Wang, Sheng-Wei and Liang,
   Chin-Ping and Liu, Chen-Wuing},
Title = {{Delineation of spatial redox zones using discriminant analysis and
   geochemical modelling in arsenic-affected alluvial aquifers}},
Journal = {{HYDROLOGICAL PROCESSES}},
