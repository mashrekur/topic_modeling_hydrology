@article{ ISI:000259281300002,
Author = {Zhang, Yinsheng and Ishikawa, M. and Ohata, T. and Oyunbaatar, D.},
Title = {{Sublimation from thin snow cover at the edge of the Eurasian cryosphere
   in Mongolia}},
Journal = {{HYDROLOGICAL PROCESSES}},
