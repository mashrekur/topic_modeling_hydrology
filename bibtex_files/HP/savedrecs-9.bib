
@article{ ISI:000262275400001,
Author = {Mailhot, A. and Rousseau, A. N. and Talbot, G. and Gagnon, P. and
   Quilbe, R.},
Title = {{A framework to estimate sediment loads using distributions with
   covariates: Beaurivage River watershed (Quebec, Canada)}},
Journal = {{HYDROLOGICAL PROCESSES}},
