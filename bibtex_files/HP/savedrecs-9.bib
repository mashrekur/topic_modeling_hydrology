@article{ ISI:000260193500005,
Author = {Sen, Sumit and Srivastava, Puneet and Yoo, Kyung H. and Dane, Jacob H.
   and Shaw, Joey N. and Kang, Moon S.},
Title = {{Runoff generation mechanisms in pastures of the Sand Mountain region of
   Alabama-a field investigation}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
Number = {{21}},
Pages = {{4222-4232}},
Month = {{OCT 15}},
Abstract = {{Excessive application of poultry litter to pasteurs in the Sand Mountain
   region of north Alabama has resulted in phosphorus (P) contamination of
   surface water bodies and buildup of P in soils of this region. Since
   surface runoff is recognized as the primary mechanism of P transport,
   understanding surface runoff generation mechanisms are crucial for
   alleviating water quality problems in this region. Identification of
   surface runoff generation mechanisms is also important for delineation
   of hydrologically active areas (HAAs). Therefore, the specific objective
   of this study was to identify surface runoff generation mechanism
   (infiltration excess versus saturation excess) using distributed
   surfaces and subsurfaces sensors and rain guage. Results from three
   rainfall events (2.13-3.43) of differing characteristics, and sensor
   data at four locations with differing soil hydrolic properties along the
   hillslope showed that the main surface runoff generation mechanism in
   this region is infiltration excess. Because of this, rainfall intensity
   and soil hydrolic conductivity were found to play dominant roles in
   surface runoff generation in this region. This study indicates that
   perhaps subsurface flows and transport of P in subsurface flows need to
   be quantified to reduce P contamination of surface water bodies in this
   region. Current studies at this location are identifying spatial and
   temporal distribution of HAAs, quantifying rainfall characteristics that
   generate runoff, and estimating runoff volume that results from
   connected HAAs. Copyright (c) 2008 John Wiley \& Sons, Ltd.}},
