@article{ ISI:000256250000004,
Author = {Abad, Jorge D. and Buscaglia, Gustavo C. and Garcia, Marcelo H.},
Title = {{2D stream hydrodynamic, sediment transport and bed morphology model for
   engineering applications}},
Journal = {{HYDROLOGICAL PROCESSES}},
