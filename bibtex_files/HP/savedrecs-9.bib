@article{ ISI:000255154200001,
Author = {Meyer, Charles R. and Renschler, Chris S. and Vining, Roel C.},
Title = {{Implementing quality control on a random number stream to improve a
   stochastic weather generator}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
