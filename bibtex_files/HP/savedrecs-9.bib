@article{ ISI:000261218900005,
Author = {Hejazi, Mohamad I. and Moglen, Glenn E.},
Title = {{The effect of climate and land use change on flow duration in the
   Maryland Piedmont region}},
Journal = {{HYDROLOGICAL PROCESSES}},
