@article{ ISI:000253439900001,
Author = {Doglioni, A. and Giustolisi, O. and Savic, D. A. and Webb, B. W.},
Title = {{An investigation on stream temperature analysis based on evolutionary
   computing}},
Journal = {{HYDROLOGICAL PROCESSES}},
