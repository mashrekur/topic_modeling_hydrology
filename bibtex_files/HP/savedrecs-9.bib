@article{ ISI:000254815100007,
Author = {Cardenas, M. Bayani and Harvey, Judson W. and Packman, Aaron I. and
   Scott, Durelle T.},
Title = {{Ground-based thermography of fluvial systems at low and high discharge
   reveals potential complex thermal heterogeneity driven by flow variation
   and bioroughness}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
Number = {{7}},
