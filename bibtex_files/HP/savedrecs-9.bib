@article{ ISI:000256250000009,
Author = {Igboekwe, M. U. and Rao, V. V. S. Gurunadha and Okwueze, E. E.},
Title = {{Groundwater flow modelling of Kwa Ibo River watershed, southeastern
   Nigeria}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
