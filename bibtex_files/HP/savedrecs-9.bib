@article{ ISI:000257876300011,
Author = {Quinton, William L. and Hayashi, Masaki and Carey, Sean K.},
Title = {{Peat hydraulic conductivity in cold regions and its relation to pore
   size and geometry}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
