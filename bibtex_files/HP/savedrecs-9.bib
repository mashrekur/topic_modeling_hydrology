@article{ ISI:000261143000003,
Author = {Mizugaki, Shigeru and Onda, Yuichi and Fukuyama, Taijiro and Koga,
   Satoko and Asai, Hiroki and Hiramatsu, Shinya},
Title = {{Estimation of suspended sediment sources using Cs-137 and Pb-210(ex) in
   unmanaged Japanese cypress plantation watersheds in southern Japan}},
Journal = {{HYDROLOGICAL PROCESSES}},
