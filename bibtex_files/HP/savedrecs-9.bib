@article{ ISI:000257876300004,
Author = {Ryan, Wendy A. and Doesken, Nolan J. and Fassnacht, Steven R.},
Title = {{Preliminary results of ultrasonic snow depth sensor testing for National
   Weather Service (NWS) snow measurements in the US}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
