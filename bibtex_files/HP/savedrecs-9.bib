@article{ ISI:000258244500025,
Author = {Zhou, Xun},
Title = {{Determination of aquifer parameters based on measurements of tidal
   effects on a coastal aquifer near Beihai, China}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
Number = {{16}},
Pages = {{3176-3180}},
Month = {{JUL 30}},
Abstract = {{Measurements of the tide and groundwater levels in coastal zones are of
   importance in determining the properties of coastal aquifers. The
   solution to a one-dimensional unsteady groundwater flow model in a
   coastal confined aquifer with sinusoidal fluctuation of the tide shows
   that the tidal efficiency decreases exponentially with distance and the
   time lag increases linearly with distance from the coast. The aquifer
   property described by the ratio of storage coefficient to transmissivity
   is determined if the damping constant of the tidal efficiency or the
   slope of the time lag with distance are obtained on the basis of tidal
   measurements. Hourly observations of the tide and groundwater levels at
   10 wells on the northern coast near Beihai, China show that with
   distance from the coast, tidal efficiency decreases roughly
   exponentially and the time lag increases roughly linearly. The estimated
   ratio of storage coefficient to transmissivity of the confined aquifer
   ranges from 1.169 x 10(-6) d m(-2) to 1.83 x 10(-7) d m(-2). For a given
   transmissivity of 750 m(2) d(-1), the storage coefficient of the aquifer
   is 8.7675 x 10(-4) with the tidal efficiency method and 1.3725 x 10(-4)
   with the time lag method. The damping constant of the tidal efficiency
   with distance can be defined as the tidal propagation coefficient. The
   value of the confined aquifer is determined as 0.0018892 m(-1).
