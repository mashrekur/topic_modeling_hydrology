@article{ ISI:000256250000003,
Author = {Wu, Kangsheng and Johnston, Carol A.},
Title = {{Hydrologic comparison between a forested and a wetland/lake dominated
   watershed using SWAT}},
Journal = {{HYDROLOGICAL PROCESSES}},
