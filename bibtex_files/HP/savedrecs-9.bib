@article{ ISI:000261143000005,
Author = {Kumar, U. Saravana and Sharma, Suman and Navada, S. V.},
Title = {{Recent studies on surface water-groundwater relationships at
   hydro-projects in India using environmental isotopes}},
Journal = {{HYDROLOGICAL PROCESSES}},
