@article{ ISI:000258519700015,
Author = {Fortin, Vincent and Therrien, Charles and Anctil, Francois},
Title = {{Correcting wind-induced bias in solid precipitation measurements in case
   of limited and uncertain data}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
