@article{ ISI:000257954400021,
Author = {Bae, Deg-Hyo and Jung, Il-Won and Chang, Heejun},
Title = {{Long-term trend of precipitation and runoff in Korean river basins}},
Journal = {{HYDROLOGICAL PROCESSES}},
