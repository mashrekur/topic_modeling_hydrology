@article{ ISI:000254815100005,
Author = {Brown, Lee E. and Hannah, David M.},
Title = {{Spatial heterogeneity of water temperature across an alpine river basin}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
Number = {{7}},
Pages = {{954-967}},
