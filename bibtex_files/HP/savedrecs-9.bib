@article{ ISI:000256250000012,
Author = {Ayenew, Tenalem and Kebede, Seifu and Alemyahu, Tamiru},
Title = {{Environmental isotopes and hydrochemical study applied to surface water
   and groundwater interaction in the Awash River basin}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
