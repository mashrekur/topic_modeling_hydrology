@article{ ISI:000260329300006,
Author = {Michlmayr, Gernot and Lehning, Michael and Koboltschnig, Gernot and
   Holzmann, Hubert and Zappa, Massimiliano and Mott, Rebecca and Schoener,
   Wolfgang},
Title = {{Application of the Alpine 3D model for glacier mass balance and glacier
   runoff studies at Goldbergkees, Austria}},
