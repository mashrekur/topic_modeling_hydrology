@article{ ISI:000255154200010,
Author = {Krishna, B. and Rao, Y. R. Satyaji and Vijaya, T.},
Title = {{Modelling groundwater levels in an urban coastal aquifer using
   artificial neural networks}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2008}},
Volume = {{22}},
