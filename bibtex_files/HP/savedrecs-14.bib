@article{ ISI:000325218100010,
Author = {Hauer, Christoph and Schober, Bernhard and Habersack, Helmut},
Title = {{Impact analysis of river morphology and roughness variability on
   hydropeaking based on numerical modelling}},
Journal = {{HYDROLOGICAL PROCESSES}},
