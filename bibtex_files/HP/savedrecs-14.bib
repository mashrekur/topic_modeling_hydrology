@article{ ISI:000329432700003,
Author = {Mastrocicco, M. and Sbarbati, C. and Colombani, N. and Petitta, M.},
Title = {{Efficiency verification of a horizontal flow barrier via flowmeter tests
   and multilevel sampling}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2013}},
