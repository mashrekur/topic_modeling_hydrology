@article{ ISI:000319879300013,
Author = {McDonald, Alyson K. and Sheng, Zhuping and Hart, Charles R. and Wilcox,
   Bradford P.},
Title = {{Studies of a regulated dryland river: surface-groundwater interactions}},
Journal = {{HYDROLOGICAL PROCESSES}},
