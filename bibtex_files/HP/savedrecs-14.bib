@article{ ISI:000315924300014,
Author = {Erskine, Wayne D.},
Title = {{Soil colour as a tracer of sediment dispersion from erosion of forest
   roads in Chichester State Forest, NSW, Australia}},
Journal = {{HYDROLOGICAL PROCESSES}},
