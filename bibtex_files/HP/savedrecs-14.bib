@article{ ISI:000325218500012,
Author = {Carmona, Facundo and Rivas, Raul and Caselles, Vicente},
Title = {{Estimate of the alpha parameter in an oat crop under rain-fed conditions}},
Journal = {{HYDROLOGICAL PROCESSES}},
