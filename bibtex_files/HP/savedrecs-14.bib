@article{ ISI:000329432700010,
Author = {Nemery, Julien and Mano, Vincent and Coynel, Alexandra and Etcheber,
   Henri and Moatar, Florentina and Meybeck, Michel and Belleudy, Philippe
   and Poirel, Alain},
Title = {{Carbon and suspended sediment transport in an impounded alpine river
   (Isere, France)}},
