@article{ ISI:000325219100003,
Author = {Pisinaras, V. and Petalas, C. and Tsihrintzis, V. A. and Karatzas, G. P.},
Title = {{Integrated modeling as a decision-aiding tool for groundwater management
   in a Mediterranean agricultural watershed}},
