@article{ ISI:000327670500002,
Author = {Moatar, Florentina and Meybeck, Michel and Raymond, Sebastien and
   Birgand, Francois and Curie, Florence},
Title = {{River flux uncertainties predicted by hydrological variability and
   riverine material behaviour}},
