@article{ ISI:000325989300008,
Author = {Li, Sien and Kang, Shaozhong and Zhang, Lu and Li, Fusheng and Du,
   Taisheng and Tong, Ling and Wang, Shufen and Lu, Hongna},
Title = {{Greater effect of canopy conductance in regulating the energy partition
   above the maize field in arid northwest China}},
