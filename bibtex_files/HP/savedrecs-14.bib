@article{ ISI:000319879300009,
Author = {Dawidek, Jaroslaw and Ferencz, Beata and Sobolewski, Wojciech},
Title = {{Modelling of hydrogeochemical potential of three lake catchments in
   Polesie region (Eastern Poland)}},
