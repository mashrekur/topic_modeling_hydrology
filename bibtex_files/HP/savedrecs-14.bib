@article{ ISI:000329432700005,
Author = {Van Metre, Peter C. and Horowitz, Arthur J.},
Title = {{An 80-year record of sediment quality in the lower Mississippi River}},
Journal = {{HYDROLOGICAL PROCESSES}},
