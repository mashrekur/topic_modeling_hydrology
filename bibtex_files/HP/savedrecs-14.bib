@article{ ISI:000316967900005,
Author = {Ma, Mingwei and Song, Songbai and Ren, Liliang and Jiang, Shanhu and
   Song, Jiali},
Title = {{Multivariate drought characteristics using trivariate Gaussian and
   Student t copulas}},
