@article{ ISI:000325219600005,
Author = {Kagabu, Makoto and Shimada, Jun and Delinom, Robert and Nakamura, Toshio
   and Taniguchi, Makoto},
Title = {{Groundwater age rejuvenation caused by excessive urban pumping in
   Jakarta area, Indonesia}},
Journal = {{HYDROLOGICAL PROCESSES}},
