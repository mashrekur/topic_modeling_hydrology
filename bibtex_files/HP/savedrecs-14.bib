@article{ ISI:000325218500008,
Author = {Browne, D. and Deletic, A. and Mudd, G. M. and Fletcher, T. D.},
Title = {{A two-dimensional model of hydraulic performance of stormwater
   infiltration systems}},
Journal = {{HYDROLOGICAL PROCESSES}},
