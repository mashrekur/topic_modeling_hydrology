@article{ ISI:000313593500008,
Author = {Kuebeck, Christine and Maloszewski, Piotr Jan and Benischke, Ralf},
Title = {{Determination of the conduit structure in a karst aquifer based on
   tracer data-Lurbach system, Austria}},
Journal = {{HYDROLOGICAL PROCESSES}},
