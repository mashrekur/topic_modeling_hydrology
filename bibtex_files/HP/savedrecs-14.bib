@article{ ISI:000313593500009,
Author = {Ficklin, Darren L. and Luo, Yuzhou and Zhang, Minghua},
Title = {{Watershed modelling of hydrology and water quality in the Sacramento
   River watershed, California}},
Journal = {{HYDROLOGICAL PROCESSES}},
