@article{ ISI:000325217500005,
Author = {Schneiderman, Elliot M. and Matonse, Adao H. and Zion, Mark S. and
   Lounsbury, David G. and Mukundan, Rajith and Pradhanang, Soni M. and
   Pierson, Donald C.},
Title = {{Comparison of approaches for snowpack estimation in New York City
   watersheds}},
