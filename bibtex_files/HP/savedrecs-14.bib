@article{ ISI:000320403300010,
Author = {Trubilowicz, Joel W. and Moore, R. D. and Buttle, J. M.},
Title = {{Prediction of stream-flow regime using ecological classification zones}},
Journal = {{HYDROLOGICAL PROCESSES}},
