@article{ ISI:000317674900003,
Author = {Brandimarte, Luigia and Woldeyes, Martha Kebede},
Title = {{Uncertainty in the estimation of backwater effects at bridge crossings}},
Journal = {{HYDROLOGICAL PROCESSES}},
