@article{ ISI:000325217500003,
Author = {Burakowski, Elizabeth and Wake, Cameron P. and Dibb, Jack E. and
   Stampone, Mary},
Title = {{Putting the capital `A' in CoCoRAHS: an experimental programme to
   measure albedo using the Community Collaborative Rain, Hail \& Snow
   (CoCoRaHS) Network}},
Journal = {{HYDROLOGICAL PROCESSES}},
