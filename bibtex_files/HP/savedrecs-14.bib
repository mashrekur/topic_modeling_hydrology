@article{ ISI:000329430400006,
Author = {Ueta, Akihiro and Sugimoto, Atsuko and Iijima, Yoshihiro and Yabuki,
   Hironori and Maximov, Trofim C. and Velivetskaya, Tatiana A. and
   Ignatiev, Alexander V.},
Title = {{Factors controlling diurnal variation in the isotopic composition of
   atmospheric water vapour observed in the taiga, eastern Siberia}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2013}},
Volume = {{27}},
Number = {{16}},
Pages = {{2295-2305}},
Month = {{JUL 30}},
Abstract = {{Deciduous forest covers vast areas of permafrost under severe dry
   climate in eastern Siberia. Understanding the water cycle in this forest
   ecosystem is quite important for climate projection. In this study,
   diurnal variations in isotopic compositions of atmospheric water vapour
   were observed in eastern Siberia with isotope analyses of precipitation,
   sap water of larch trees, soil water, and water in surface organic layer
   during the late summer periods of 2006, 2007, and 2008. In these years,
   the soil moisture content was considerably high due to unusually large
   amounts of summer rainfall and winter snowfall. The observed sap water
   O-18 ranged from -17.9 parts per thousand to -13.3 parts per thousand,
   which was close to that of summer precipitation and soil water in the
   shallow layer, and represents that of transpired water vapour. On sunny
   days, as the air temperature and mixing ratio rose from predawn to
   morning, the atmospheric water vapour O-18 increased by 1 parts per
   thousand to 5 parts per thousand and then decreased by about 2 parts per
   thousand from morning to afternoon with the mixing ratio. On cloudy
   days, by contrast, the afternoon decrease in O-18 and the mixing ratio
   was not observed. These results show that water vapour that transpired
   from plants, with higher O-18 than the atmospheric water vapour,
   contributes to the increase in O-18 in the morning, whereas water vapour
   in the free atmosphere, with lower O-18, contributes to the decrease in
   the afternoon on sunny days. The observed results reveal the
   significance of transpired water vapour, with relatively high O-18, in
   the water cycle on a short diurnal time scale and confirm the importance
   of the recycling of precipitation through transpiration in continental
   forest environments such as the eastern Siberian taiga. Copyright (c)
   2012 John Wiley \& Sons, Ltd.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
