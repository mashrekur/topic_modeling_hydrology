@article{ ISI:000172855200004,
Author = {Taylor, CB and Trompetter, VJ and Brown, LJ and Bekesi, G},
Title = {{The Manawatu aquifers, North Island, New Zealand: Clarification of
   hydrogeology using a multidisciplinary environmental tracer approach}},
Journal = {{HYDROLOGICAL PROCESSES}},
