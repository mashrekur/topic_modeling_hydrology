@article{ ISI:000165685800006,
Author = {Brassard, P and Waddington, JM and Hill, AR and Roulet, NT},
Title = {{Modelling groundwater-surface water mixing in a headwater wetland:
   implications for hydrograph separation}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2000}},
Volume = {{14}},
Number = {{15}},
