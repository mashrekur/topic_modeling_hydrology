@article{ ISI:000169965300012,
Author = {Tsujimura, M and Onda, Y and Ito, J},
Title = {{Stream water chemistry in a steep headwater basin with high relief}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
