@article{ ISI:000171909200007,
Author = {Benavides-Solorio, J and MacDonald, LH},
Title = {{Post-fire runoff and erosion from simulated rainfall on small plots,
   Colorado Front Range}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
