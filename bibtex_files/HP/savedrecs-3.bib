@article{ ISI:000086986800004,
Author = {Smart, CC and Owens, IF and Lawson, W and Morris, AL},
Title = {{Exceptional ablation arising from rainfall-induced slushflows: Brewster
   Glacier, New Zealand}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2000}},
Volume = {{14}},
Number = {{6}},
Pages = {{1045-1052}},
Month = {{APR 30}},
Abstract = {{An exceptional ablation event resulted in the destruction of a network
   of glacier survey stakes on the Brewster Glacier, New Zealand. Up to 5 m
   of surface lowering occurred over 26 days, releasing at least 1.6 x
   10(6) m(3) of water. The episode is attributed to saturation of the
   snowpack by rainfall and consequent slushflows. Slushflows permit
   surface lowering at rates greater than expected from the surface energy
   balance on snow. Resulting slushflow events may significantly influence
   glacier mass balance, and may constitute a hazard. Little is known of
   their physical characteristics, but some descriptive background is
   provided. Copyright (C) 2000 John Wiley \& Sons, Ltd.}},
