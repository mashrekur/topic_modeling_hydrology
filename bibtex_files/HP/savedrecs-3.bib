@article{ ISI:000168557500007,
Author = {Rustomji, P and Prosser, I},
Title = {{Spatial patterns of sediment delivery to valley floors: sensitivity to
   sediment transport capacity and hillslope hydrology relations}},
Journal = {{HYDROLOGICAL PROCESSES}},
