@article{ ISI:000171229600018,
Author = {Viney, NR and Sivapalan, M},
Title = {{Modelling catchment processes in the Swan-Avon river basin}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
Number = {{13}},
Pages = {{2671-2685}},
Month = {{SEP}},
Abstract = {{A conceptual hydrological model, LASCAM, is applied to the Swan-Avon
   catchment (area 120000 km(-2)) in Western Australia to predict yields of
   water, salt, sediment, phosphorus and nitrogen. Each of the nutrients is
   further discriminated into soluble and particulate components. The model
   uses a single set of global parameters that are applied to each of the
   134 subcatchments. Spatial mapping of the model results shows that
   yields are closely related to the distribution of rainfall and
   vegetation cover. Application of the model to various land cover
   scenarios show that since European settlement in the catchment, there
   has been a five-fold increase in streamflow, a 40-fold increase in
   sediment yield, and approximately 16-fold increases in total nutrient
   yields. Copyright (C) 2001 John Wiley \& Sons, Ltd.}},
