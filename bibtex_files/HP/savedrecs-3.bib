@article{ ISI:000173341100003,
Author = {Kuchment, LS and Gelfan, AN},
Title = {{Statistical self-similarity of spatial variations of snow cover:
   verification of the hypothesis and application in the snowmelt runoff
   generation models}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
Number = {{18, SI}},
Pages = {{3343-3355}},
Month = {{DEC 30}},
