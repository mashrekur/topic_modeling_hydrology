@article{ ISI:000173341100018,
Author = {Harouche, IPF and Barber, DG},
Title = {{Seasonal characterization of microwave emissions from snow-covered
   first-year sea ice}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
Number = {{18, SI}},
Pages = {{3571-3583}},
