@article{ ISI:000086986800010,
Author = {Ophori, DU and Maharjan, B},
Title = {{First approximations of soil moisture retention curves using the
   filter-paper method}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2000}},
Volume = {{14}},
Number = {{6}},
Pages = {{1131-1138}},
Month = {{APR 30}},
Abstract = {{Relationships between gravimetric soil moisture content (w) and matric
   potential (phi(m)), and between volumetric soil moisture content
   (theta(v)) and pressure head (h) were approximated for the unsaturated
   zone on Long Island, New York. Soil samples were collected from two
   sites using a hand auger. The soil moisture content was determined using
   the filter-paper (w(f)) and gravimetric (w) methods, respectively. The
   w, was then used in an empirical equation to estimate phi(m). Each set
   of phi(m) and w was combined with a straight-line empirical model to
   obtain a w(phi(m)) relationship. Soil phi(m) was converted to h, and w
   to the volumetric moisture content theta(v), in order to produce a
   theta(v)(h) curve. Graphical and statistical comparison showed that the
