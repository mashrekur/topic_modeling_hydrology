@article{ ISI:000165685800009,
Author = {Coulibaly, P and Anctil, F and Rasmussen, P and Bobee, B},
Title = {{A recurrent neural networks approach using indices of low-frenquency
   climatic variability to forecast regional annual runoff}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2000}},
Volume = {{14}},
Number = {{15}},
Pages = {{2755-2777}},
