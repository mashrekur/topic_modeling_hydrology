@article{ ISI:000166110700018,
Author = {Reynolds, CS},
Title = {{Hydroecology of river plankton: the role of variability in channel flow}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2000}},
Volume = {{14}},
Number = {{16-17}},
Pages = {{3119-3132}},
Month = {{NOV-DEC}},
Abstract = {{The mechanisms by which entrained planktonic organisms survive in river
   systems, despite an inexorable, unidirectional downstream transport, are
   revisited. The importance of channel retentivity to downstream
   population recruitment is emphasized. The aggregated dead-zone (ADZ)
   model is shown to be adequate to explain downstream recruitment of a
   growing population. The ADZ behaviour is more prevalent in sinuous,
   low-gradient reaches than in other parts of the river. Plankton
   selection and dynamics relate conspicuously to flow at higher discharges
   but other environmental features are important at low flows. Discharge
   variability is pivotal to the opportunities for potamoplankton to
   thrive. Copyright (C) 2000 John Wiley \& Sons, Ltd.}},
