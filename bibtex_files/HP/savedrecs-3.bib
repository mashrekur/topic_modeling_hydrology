@article{ ISI:000169965300019,
Author = {Koichiro, K and Yuri, T and Nobuaki, T and Isamu, K},
Title = {{Generation of stemflow volume and chemistry in a mature Japanese cypress
   forest}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
Number = {{10}},
Pages = {{1967-1978}},
