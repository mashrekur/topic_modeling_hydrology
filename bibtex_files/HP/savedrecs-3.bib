@article{ ISI:000173341100015,
Author = {Fischer, UH and Porter, PR and Schuler, T and Evans, AJ and Gudmundsson,
   GH},
Title = {{Hydraulic and mechanical properties of glacial sediments beneath
   Unteraargletscher, Switzerland: implications for glacier basal motion}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
