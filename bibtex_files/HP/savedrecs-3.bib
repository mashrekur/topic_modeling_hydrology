@article{ ISI:000086247000004,
Author = {Yang, D and Herath, S and Musiake, K},
Title = {{Comparison of different distributed hydrological models for
   characterization of catchment spatial variability}},
Journal = {{HYDROLOGICAL PROCESSES}},
