@article{ ISI:000169965300015,
Author = {Burns, DA and McDonnell, JJ and Hooper, RP and Peters, NE and Freer, JE
   and Kendall, C and Beven, K},
Title = {{Quantifying contributions to storm runoff through end-member mixing
   analysis and hydrologic measurements at the Panola Mountain Research
   Watershed (Georgia, USA)}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2001}},
Volume = {{15}},
Number = {{10}},
Pages = {{1903-1924}},
Month = {{JUL}},
