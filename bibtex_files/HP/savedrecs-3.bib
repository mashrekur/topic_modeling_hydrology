@article{ ISI:000088068000002,
Author = {Slaymaker, O},
Title = {{Research developments in the hydrological sciences in Canada
   (1995-1998): surface water - quantity, quality and ecology}},
Journal = {{HYDROLOGICAL PROCESSES}},
