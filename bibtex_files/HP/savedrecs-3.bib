@article{ ISI:000171909200005,
Author = {Pierson, FB and Robichaud, PR and Spaeth, KE},
Title = {{Spatial and temporal effects of wildfire on the hydrology of a steep
   rangeland watershed}},
Journal = {{HYDROLOGICAL PROCESSES}},
