@article{ ISI:000086246900002,
Author = {Yanni, S and Keys, K and Meng, FR and Yin, XW and Clair, T and Arp, PA},
Title = {{Modelling hydrological conditions in the maritime forest region of
   south-western Nova Scotia}},
Journal = {{HYDROLOGICAL PROCESSES}},
