@article{ ISI:000374383800002,
Author = {Liu, Chuankun and Liu, Jie and Wang, Xu-Sheng and Zheng, Chunmiao},
Title = {{Analysis of groundwater-lake interaction by distributed temperature
   sensing in Badain Jaran Desert, Northwest China}},
Journal = {{HYDROLOGICAL PROCESSES}},
