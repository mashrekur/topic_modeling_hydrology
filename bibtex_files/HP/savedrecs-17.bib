@article{ ISI:000369160600005,
Author = {Maslova, I. and Ticlavilca, A. M. and McKee, M.},
Title = {{Adjusting wavelet-based multiresolution analysis boundary conditions for
   long-term streamflow forecasting}},
Journal = {{HYDROLOGICAL PROCESSES}},
