@article{ ISI:000373021600009,
Author = {Zheng, Zheng and Gao, Junqi and Ma, Zhen and Wang, Zhengfang and Yang,
   Xiaoying and Luo, Xingzhang and Jacquet, Thierry and Fu, Guangtao},
Title = {{Urban flooding in China: main causes and policy recommendations}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
Volume = {{30}},
Number = {{7}},
Pages = {{1149-1152}},
Month = {{MAR 30}},
Abstract = {{In recent years, urban flooding has become more and more severe and
   pervasive in China. In our opinion, the limited capacity of urban
   drainage systems is not the main cause of urban flooding in China. We
   propose that urban flooding in China is due to: 1) loss of natural water
   bodies; 2) urban land use and imperviousness; 3) fragmentation of
   natural water systems. To tackle the problem, we propose several
   strategies: 1) The Chinese government needs to strengthen its efforts in
   improving urban ecological environment. 2) Natural water bodies must be
   protected and recovered as much as possible. 3) The permeability of
   urban ground should be enhanced. 4) Urban drainage networks should be
   optimized so as to be well integrated with natural water systems.
