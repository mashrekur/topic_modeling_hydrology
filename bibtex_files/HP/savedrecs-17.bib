@article{ ISI:000369160600006,
Author = {Karwan, D. L. and Siegert, C. M. and Levia, D. F. and Pizzuto, J. and
   Marquard, J. and Aalto, R. and Aufdenkampe, A. K.},
Title = {{Beryllium-7 wet deposition variation with storm height, synoptic
   classification, and tree canopy state in the mid-Atlantic USA}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
Volume = {{30}},
Number = {{1}},
Pages = {{75-89}},
Month = {{JAN 1}},
Abstract = {{Short-lived fallout isotopes, such as beryllium-7 (Be-7), are
   increasingly used as erosion and sediment tracers in watersheds. Be-7 is
   produced in the atmosphere and delivered to the Earth's surface
   primarily in precipitation. However, relatively little has been
   published about the variation in Be-7 wet deposition caused by storm
   type and vegetation cover. Our analysis of precipitation, throughfall,
   and sediments in two forested, headwater catchments in the mid-Atlantic
   USA indicates significant variation in isotope deposition with storm
   type and storm height. Individual summer convective thunderstorms were
   associated with Be-7 activity concentrations up to 5.0Bql(-1) in
   precipitation and 4.7Bql(-1) in throughfall, while single-event wet
   depositional fluxes reached 168Bqm(-2) in precipitation and 103Bqm(-2)
   in throughfall. Storms originating from the continental USA were
   associated with lower Be-7 activity concentrations and single-event wet
   depositional fluxes for precipitation (0.7-1.2Bql(-1) and
   15.8-65.0Bqm(-2)) and throughfall (0.1-0.3Bql(-1) and 13.5-98.9Bqm(-2)).
   Tropical systems had relatively low activity concentrations,
   0.2-0.5Bql(-1) in precipitation and 0.2-1.0Bql(-1) in throughfall, but
   relatively high single-event depositional fluxes due to large rainfall
   volumes, 32.8-67.6Bqm(-2) in precipitation and 25.7-134Bqm(-2) in
   throughfall. The largest sources of Be-7 depositional variation were
   attributed to storm characteristics including precipitation amount and
   maximum storm height. Be-7 activity associated with fluvial suspended
   sediments also exhibited the highest concentration and variability in
   summer (175-1450Bqkg(-1)). We conclude the dominant source of variation
   on event-level Be-7 deposition is storm type. Our results illustrate the
   complex relationships between Be-7 deposition in precipitation and
   throughfall and demonstrate event-scale relationships between the Be-7
   in precipitation and on suspended sediment. Copyright (c) 2015 John
   Wiley \& Sons, Ltd.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
