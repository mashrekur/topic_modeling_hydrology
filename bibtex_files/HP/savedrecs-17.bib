@article{ ISI:000374383800004,
Author = {Joshi, Rajesh and Kumar, Kireet and Adhikari, Vijay Pal Singh},
Title = {{Modelling suspended sediment concentration using artificial neural
   networks for Gangotri glacier}},
Journal = {{HYDROLOGICAL PROCESSES}},
