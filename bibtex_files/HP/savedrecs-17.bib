@article{ ISI:000379739600027,
Author = {Trubilowicz, Joel W. and Shea, J. M. and Jost, G. and Moore, R. D.},
Title = {{Suitability of North American Regional Reanalysis (NARR) output for
   hydrologic modelling and analysis in mountainous terrain}},
Journal = {{HYDROLOGICAL PROCESSES}},
