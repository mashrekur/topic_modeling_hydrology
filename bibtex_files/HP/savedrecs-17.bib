@article{ ISI:000383466900006,
Author = {Dolant, Caroline and Langlois, Alexandre and Montpetit, Benoit and
   Brucker, Ludovic and Roy, Alexandre and Royer, Alain},
Title = {{Development of a rain-on-snow detection algorithm using passive
   microwave radiometry}},
Journal = {{HYDROLOGICAL PROCESSES}},
