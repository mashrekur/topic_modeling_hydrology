@article{ ISI:000379918300011,
Author = {Oh, Yong Hwa and Kim, Guebuem},
Title = {{Large seasonal changes in the recharge of seawater in a subterranean
   estuary revealed by a radon tracer}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
Volume = {{30}},
Number = {{14}},
Pages = {{2525-2532}},
Month = {{JUL 1}},
Abstract = {{Activities of radon (Rn-222) in groundwater were continuously monitored
   in a saline aquifer from September 2010 to July 2011. The activities of
   Rn-222 ranged from 200 to 4300Bqm(-3), with a large seasonal variation.
   Because the activity of Rn-222 in seawater is low, Rn-222 in saline
   groundwater must be produced in the aquifer from radium (Ra-226) in
   rocks and sediments. The Rn-222 activities were higher in the warm-dry
   seasons (September-November 2010 and April-May 2011) when the saline
   aquifer was stable. In contrast, the lowest Rn-222 activities were
   observed in the cold-dry season (December 2010 and January-March 2011),
   because of the effective exchange between groundwater and seawater. In
   addition, sudden decreases of Rn-222 activities coincided with episodic
   drops in groundwater temperatures. These results reveal that lower
   seawater temperature in winter may result in density-driven seawater
   intrusion. During the wet season (June-July 2011), Rn-222 activities
   were more clearly affected by semi-monthly and diurnal tidal pumping,
   showing higher Rn-222 activities during low and spring tides. Such a
   tidal effect was not clearly observed during the warm-dry and cold-dry
   seasons. This result implies that the residence time of SGD in coastal
   zones is significantly affected by seasonal changes in driving forces
   such as tidal pumping and seawater intrusion. Copyright (c) 2016 John
   Wiley \& Sons, Ltd.}},
