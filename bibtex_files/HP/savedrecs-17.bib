@article{ ISI:000384773400011,
Author = {Wang, Bo and Chen, Yunliang and Wu, Chao and Dong, Jianhua and Ma, Xiao
   and Song, Jiajun},
Title = {{A semi-analytical approach for predicting peak discharge of floods
   caused by embankment dam failures}},
Journal = {{HYDROLOGICAL PROCESSES}},
