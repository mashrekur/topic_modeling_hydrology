@article{ ISI:000388302900008,
Author = {Jesus Espejo-Perez, Antonio and Brocca, Luca and Moramarco, Tommaso and
   Giraldez, Juan V. and Triantafilis, John and Vanderlinden, Karl},
Title = {{Analysis of soil moisture dynamics beneath olive trees}},
