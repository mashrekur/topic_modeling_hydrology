@article{ ISI:000382957700004,
Author = {Klein, Trevor and Toran, Laura},
Title = {{Collocation of hydrological and biological attenuation of nitrate in an
   urban stream}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
