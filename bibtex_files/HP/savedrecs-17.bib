@article{ ISI:000382957700008,
Author = {Ronen-Eliraz, G. and Ginat, H. and Dody, A. and Blumberg, D. and Dahan,
   O.},
Title = {{Flood hydrograph reconstruction from the peak flow value in ephemeral
   streams using a simplified robust single-parameter model}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
