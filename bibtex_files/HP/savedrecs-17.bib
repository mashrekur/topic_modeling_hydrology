@article{ ISI:000393030500004,
Author = {Fernanda Hernandez-Lopez, Maria and Braud, Isabelle and Gironas, Jorge
   and Suarez, Francisco and Francisco Munoz, Jose},
Title = {{Modelling evaporation processes in soils from the Huasco salt flat
   basin, Chile}},
Journal = {{HYDROLOGICAL PROCESSES}},
