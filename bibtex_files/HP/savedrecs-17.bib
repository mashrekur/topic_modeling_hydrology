@article{ ISI:000379739600031,
Author = {de Figueiredo, Jose Vidal and de Araujo, Jose Carlos and Augusto
   Medeiros, Pedro Henrique and Costa, Alexandre C.},
Title = {{Runoff initiation in a preserved semiarid Caatinga small watershed,
   Northeastern Brazil}},
