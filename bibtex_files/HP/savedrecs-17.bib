@article{ ISI:000383466900011,
Author = {Pers, Charlotta and Temnerud, Johan and Lindstroem, Goeran},
Title = {{Modelling water, nutrients, and organic carbon in forested catchments: a
   HYPE application}},
Journal = {{HYDROLOGICAL PROCESSES}},
