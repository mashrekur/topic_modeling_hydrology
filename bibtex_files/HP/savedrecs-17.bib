@article{ ISI:000379940800008,
Author = {Bergstrom, Anna and McGlynn, Brian and Mallard, John and Covino, Tim},
Title = {{Watershed structural influences on the distributions of stream network
   water and solute travel times under baseflow conditions}},
Journal = {{HYDROLOGICAL PROCESSES}},
