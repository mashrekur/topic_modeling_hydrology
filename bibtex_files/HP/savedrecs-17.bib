@article{ ISI:000379940800001,
Author = {da Costa Silva, Robson Willians and Salemi, Luiz Felippe and Fernandes,
   Rafael Pires and Berteli de Andrade, Tatiana Morgan and de Moraes, Jorge
   Marcos and de Camargo, Plinio Barbosa and Martinelli, Luiz Antonio},
Title = {{Throughfall patterns in sugarcane and riparian forest: understanding the
   effect of sugarcane age and land use conversion}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
Volume = {{30}},
Number = {{15}},
Pages = {{2579-2589}},
Month = {{JUL 15}},
Abstract = {{Sugarcane is an annual crop with a dynamic canopy that changes over time
   mainly because of genetic adaptation. There is uncertainty about the
   temporal trends of throughfall (TF) in this important commercial crop.
   In the present paper, we used troughs to measure TF in a third and
   fourth ratoon and subsequently in a fourth and fifth ratoon. Additional
   measurements were carried out in an adjacent riparian forest. There were
   no significant differences between cycles of sugarcane, growth phases
   and riparian forest. The TF results for ratoon crop and riparian forest
   in 2011/2012 were 76\% and 79.5\% of gross rainfall, respectively, while
   in 2012/2013, they were 79\% and 78\%, respectively. However, TF was
   remarkably lower in the riparian forest relative to ratoon from the
   second half of the culm formation and elongation phase (280 days after
   harvest) until harvest. Copyright (c) 2016 John Wiley \& Sons, Ltd.}},
