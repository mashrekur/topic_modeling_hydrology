@article{ ISI:000384773400010,
Author = {Edokpa, D. A. and Evans, M. G. and Rothwell, J. J.},
Title = {{Reservoirs are hotspots of nitrogen cycling in peatland catchments}},
Journal = {{HYDROLOGICAL PROCESSES}},
