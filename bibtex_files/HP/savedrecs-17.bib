@article{ ISI:000385434400006,
Author = {Thanh Duc Dang and Cochrane, Thomas A. and Arias, Mauricio E. and Pham
   Dang Tri Van and de Vries, Tonny T.},
Title = {{Hydrological alterations from water infrastructure development in the
   Mekong floodplains}},
Journal = {{HYDROLOGICAL PROCESSES}},
