@article{ ISI:000384773400009,
Author = {Moyer, Alexis N. and Moore, R. D. and Koppes, Michele N.},
Title = {{Streamflow response to the rapid retreat of a lake-calving glacier}},
Journal = {{HYDROLOGICAL PROCESSES}},
