@article{ ISI:000379192400003,
Author = {Pracny, Pavel and Faimon, Jiri and Sracek, Ondra and Kabelka, Ludvik and
   Hebelka, Jiri},
Title = {{Anomalous drip in the Punkva caves (Moravian Karst): relevant
   implications for paleoclimatic proxies}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2016}},
Volume = {{30}},
Number = {{10}},
Pages = {{1506-1520}},
Month = {{MAY 15}},
Abstract = {{The anomalous drip in the Punkva caves (Moravian Karst) shows specific
   hydrogeochemical properties such as low SIcalcite similar to 0.14 +/-
   0.11 (standard deviation), low mineralization (4.53 +/- 0.42) x 10(-3)
   mol l(-1), and enhanced values of delta C-13 (-7.85 to -8.35\% VPDB),
   Mg/Ca x 1000 ratio (45.7 +/- 3.3), and Sr/Ca x 1000 ratio (0.65 +/-
   0.06). By these properties, the anomalous drip significantly differs
   from other regular drips in the same cave and other caves in the region.
   The study suggests that the anomalous drip properties are a consequence
   of prior calcite precipitation or/and water mixing along the water flow
   path. As the former processes are spatially controlled, the knowledge of
   dripwater flow path seems to be necessary for correct
   paleoclimatic/paleoenvironmental reconstructions. Copyright (C) 2015
   John Wiley \& Sons, Ltd.}},
