@article{ ISI:000426510600004,
Author = {Manda, Alex K. and Reyes, Enrique and Pitt, James M.},
Title = {{In situ measurements of wind-driven salt fluxes through constructed
   channels in a coastal wetland ecosystem}},
Journal = {{HYDROLOGICAL PROCESSES}},
