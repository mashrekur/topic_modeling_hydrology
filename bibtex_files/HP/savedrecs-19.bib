@article{ ISI:000439092200008,
Author = {Noor, Kashif and Elfeki, Amro M. M.},
Title = {{Stochastic modelling of a diffusive wave for flood propagation using the
   random walk particle tracking method in a hypothetical city}},
Journal = {{HYDROLOGICAL PROCESSES}},
