@article{ ISI:000452789800005,
Author = {Buchanan, Brian and Auerbach, Daniel A. and Knighton, James and Evensen,
   Darrick and Fuka, Daniel R. and Easton, Zachary and Wieczorek, Michael
   and Archibald, Josephine A. and McWilliams, Brandon and Walter, Todd},
Title = {{Estimating dominant runoff modes across the conterminous United States}},
