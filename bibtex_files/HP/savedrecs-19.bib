@article{ ISI:000434115000008,
Author = {Tu, Tongbi and Ercan, Ali and Kavvas, M. Levent},
Title = {{Time-space fractional governing equations of transient groundwater flow
   in confined aquifers: Numerical investigation}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2018}},
Volume = {{32}},
