@article{ ISI:000439092200007,
Author = {Giambastiani, Beatrice M. S. and Greggio, Nicolas and Nobili, Giovanni
   and Dinelli, Enrico and Antonellini, Marco},
Title = {{Forest fire effects on groundwater in a coastal aquifer (Ravenna, Italy)}},
Journal = {{HYDROLOGICAL PROCESSES}},
