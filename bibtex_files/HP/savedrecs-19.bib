@article{ ISI:000436250400001,
Author = {Samper-Villarreal, Jimena and Cortes, Jorge and Polunin, Nicholas V. C.},
Title = {{Isotopic evidence of subtle nutrient enrichment in mangrove habitats of
   Golfo Dulce, Costa Rica}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2018}},
Volume = {{32}},
Number = {{13}},
Pages = {{1956-1964}},
Month = {{JUN 30}},
Abstract = {{Mangroves are of great ecological and socio-economic importance, yet
   they are under threat from urban development on the southern Pacific
   coast of Costa Rica. To test for possible nutrient-related impacts, we
   compared water-column nutrient concentrations, C and N stable isotope
   values and other environmental variables between mangroves with known
   sewage loading (three ``nutrient loaded{''} locations) and those without
   such loading (three ``reference{''} locations). Instantaneous nutrient
   concentrations were low at all locations, Secchi depth was greater at
   reference locations, and chlorophyll concentrations were higher at
   nutrient loaded mangroves. Suspended matter did not vary between
   reference and nutrient loaded mangroves, and nor did bivalve and algal
   delta C-13 and delta N-15 values. Enrichment of delta N-15 and delta
   C-13 of red mangrove leaves at the nutrient loaded locations is
   attributed to pulsed inputs of materials that were not detected in the
   instantaneous nutrient data. We provide evidence of isotopic enrichment
   at nutrient loaded locations from mangrove material and recommend that
   adequate waste water treatment be carried out on all anthropogenic
   discharges into this vulnerable marine system.}},
