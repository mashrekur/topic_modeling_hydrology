@article{ ISI:000425515500012,
Author = {Fallico, Carmine and Ianchello, Mario and De Bartolo, Samuele and
   Severino, Gerardo},
Title = {{Spatial dependence of the hydraulic conductivity in awell-type
   configuration at the mesoscale}},
