@article{ ISI:000233511700005,
Author = {Koeniger, P and Wittmann, S and Leibundgut, C and Krause, WJ},
Title = {{Tritium balance modelling in a macroscale catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
Number = {{17}},
