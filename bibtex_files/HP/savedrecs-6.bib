@article{ ISI:000233511700002,
Author = {Ozyurt, NN and Bayari, CS},
Title = {{Steady- and unsteady-state lumped parameter modelling of tritium and
   chlorofluorocarbons transport: hypothetical analyses and application to
   an alpine karst aquifer}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
Number = {{17}},
