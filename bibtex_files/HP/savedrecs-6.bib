@article{ ISI:000231348100007,
Author = {Probeck, M and Ludwig, R and Mauser, W},
Title = {{Fusion of NOAA-AVHRR imagery and geographical information system
   techniques to derive subscale land cover information for the upper
   Danube watershed}},
Journal = {{HYDROLOGICAL PROCESSES}},
