@article{ ISI:000231587100004,
Author = {Yamada, H and Nakamura, F and Watanabe, Y and Murakami, M and Nogami, T},
Title = {{Measuring hydraulic permeability in a streambed using the packer test}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
Number = {{13}},
Pages = {{2507-2524}},
Month = {{AUG 30}},
Abstract = {{Permeability of a streambed is an important factor regulating nutrient
   and oxygen availability for aquatic biota. In order to investigate the
   relationship, an accurate permeability should be measured. However, it
   is difficult to measure permeability in a coarse gravel bed using a
   conventional permeability test. Moreover, turbulent flow may occur in
   coarse bed material, and then deviations from Darcy's law do occur.
   Thus, permeability calculated following Darcy's law may be overestimated
   under turbulent flow conditions and should be corrected. The packer test
   can be used in highly permeable gravel beds. We developed a field method
   applicable to a gravel bed using the packer test and derived an equation
   adopting a law of turbulent flow to study the problems under any type of
   flow condition. The accuracy of the equation was examined using a
   laboratory flume with a gravel bed. The results suggested that
   permeability calculated from Hvorslev's equation is overestimated for
   turbulent flow. In contrast, our equation, developed here, could
   evaluate permeability accurately under any type of flow condition.
   Copyright (c) 2005 John Wiley \& Sons, Ltd.}},
