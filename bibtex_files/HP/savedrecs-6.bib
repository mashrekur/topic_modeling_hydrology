@article{ ISI:000229232800011,
Author = {Wigington, PJ and Moser, TJ and Lindeman, DR},
Title = {{Stream network expansion: a riparian water quality factor}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
Number = {{8}},
Pages = {{1715-1721}},
Month = {{MAY}},
Abstract = {{Little is known about how active stream network expansion during
   rainstorms influences the ability of riparian buffers to improve water
   quality. We used aerial photographs to quantify stream network expansion
   during the wet winter season in five agricultural catchments in western
   Oregon, USA. Winter stream drainage densities were nearly two orders of
   magnitude greater than summer stream densities, and agricultural land
   use was much more abundant along transient portions (e.g. swales, road
   ditches) of stream networks. Water moving from agricultural fields into
   expanded stream networks during large hydrologic events has the
   opportunity to bypass downstream riparian buffers along perennial
   streams and contribute nonpoint-source pollutants directly into
   perennial stream channels. Copyright (c) 2005 John Wiley \& Sons, Ltd.}},
