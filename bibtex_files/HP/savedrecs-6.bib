@article{ ISI:000227231300016,
Author = {Lenhart, T and Van Rompaey, A and Steegen, A and Fohrer, N and Frede, HG
   and Govers, G},
Title = {{Considering spatial distribution and deposition of sediment in lumped
   and semi-distributed models}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
