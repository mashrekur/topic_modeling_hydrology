@article{ ISI:000227231300003,
Author = {Arnold, JG and Potter, KN and King, KW and Allen, PM},
Title = {{Estimation of soil cracking and the effect on surface runoff in a Texas
   Blackland Prairie watershed}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
Number = {{3, SI}},
Pages = {{589-603}},
