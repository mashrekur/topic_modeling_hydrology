@article{ ISI:000230831900001,
Author = {Vivoni, ER and Ivanov, VY and Bras, RL and Entekhabi, D},
Title = {{On the effects of triangulated terrain resolution on distributed
   hydrologic model response}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
Volume = {{19}},
