@article{ ISI:000233840600016,
Author = {Xu, CY and Chen, D},
Title = {{Comparison of seven models for estimation of evapotranspiration and
   groundwater recharge using lysimeter measurement data in Germany}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2005}},
