@article{ ISI:000402884400002,
Author = {Adhikary, Sajal Kumar and Muttil, Nitin and Yilmaz, Abdullah Gokhan},
Title = {{Cokriging for enhanced spatial interpolation of rainfall in two
   Australian catchments}},
Journal = {{HYDROLOGICAL PROCESSES}},
