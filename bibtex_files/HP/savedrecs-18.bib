@article{ ISI:000441296100011,
Author = {Rood, Stewart B. and Kaluthota, Sobadini and Philipsen, Laurens J. and
   Rood, Neil J. and Zanewich, Karen P.},
Title = {{Increasing discharge from the Mackenzie River system to the Arctic Ocean}},
Journal = {{HYDROLOGICAL PROCESSES}},
