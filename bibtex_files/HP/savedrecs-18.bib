@article{ ISI:000410745000004,
Author = {Scheliga, Bernhard and Tetzlaff, Doerthe and Nuetzmann, Gunnar and
   Soulsby, Chris},
Title = {{Groundwater isoscapes in a montane headwater catchment show dominance of
   well-mixed storage}},
Journal = {{HYDROLOGICAL PROCESSES}},
