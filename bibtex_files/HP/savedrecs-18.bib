@article{ ISI:000395628600016,
Author = {Lefrancq, Marie and Van Dijk, Paul and Jetten, Victor and Schwob,
   Matthieu and Payraudeau, Sylvain},
Title = {{Improving runoff prediction using agronomical information in a cropped,
   loess covered catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
