@article{ ISI:000391979400008,
Author = {Wayand, Nicholas E. and Clark, Martyn P. and Lundquist, Jessica D.},
Title = {{Diagnosing snow accumulation errors in a rain-snow transitional
   environment with snow board observations}},
Journal = {{HYDROLOGICAL PROCESSES}},
