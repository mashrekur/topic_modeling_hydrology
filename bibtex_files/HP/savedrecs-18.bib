@article{ ISI:000414389600001,
Author = {Pappalardo, Viviana and Campisano, Alberto and Martinico, Francesco and
   Modica, Carlo and Barbarossa, Luca},
Title = {{A hydraulic invariance-based methodology for the implementation of
   storm-water release restrictions in urban land use master plans}},
Journal = {{HYDROLOGICAL PROCESSES}},
