
@article{ ISI:000418407200001,
Author = {Stewart, Ryan D. and Lee, Joong Gwang and Shuster, William D. and
   Darner, Robert A.},
Title = {{Modelling hydrological response to a fully-monitored urban bioretention
   cell}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2017}},
Volume = {{31}},
