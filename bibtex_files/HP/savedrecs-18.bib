@article{ ISI:000408341800010,
Author = {Izady, Azizallah and Abdalla, Osman A. E. and Joodavi, Ata and Karimi,
   Akbar and Chen, Mingjie and Tompson, Andrew},
Title = {{Groundwater recharge estimation in arid hardrock-alluvium aquifers using
   combined water-table fluctuation and groundwater balance approaches}},
Journal = {{HYDROLOGICAL PROCESSES}},
