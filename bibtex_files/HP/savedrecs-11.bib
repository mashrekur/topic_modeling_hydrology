@article{ ISI:000282345400003,
Author = {de Vos, N. J. and Rientjes, T. H. M. and Gupta, H. V.},
Title = {{Diagnostic evaluation of conceptual rainfall-runoff models using
   temporal clustering}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2010}},
Volume = {{24}},
