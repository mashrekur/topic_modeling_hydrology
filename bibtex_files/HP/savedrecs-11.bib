@article{ ISI:000284067400001,
Author = {Rode, Michael and Arhonditsis, George and Balin, Daniela and Kebede,
   Tesfaye and Krysanova, Valentina and van Griensven, Ann and van der Zee,
   Sjoerd E. A. T. M.},
Title = {{New challenges in integrated water quality modelling}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2010}},
