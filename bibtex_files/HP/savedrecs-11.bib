@article{ ISI:000280140900010,
Author = {Kim, Nam Won and Lee, Jin Won and Lee, Jeongwoo and Lee, Jeong Eun},
Title = {{SWAT application to estimate design runoff curve number for South Korean
   conditions}},
Journal = {{HYDROLOGICAL PROCESSES}},
