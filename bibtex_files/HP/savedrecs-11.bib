@article{ ISI:000277383700012,
Author = {Noske, Philip J. and Lane, Patrick N. J. and Sheridan, Gary J.},
Title = {{Stream exports of coarse matter and phosphorus following wildfire in NE
   Victoria, Australia}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2010}},
