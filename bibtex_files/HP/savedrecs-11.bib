@article{ ISI:000276161500011,
Author = {Chen, Jui-Sheng},
Title = {{Analytical model for fully three-dimensional radial dispersion in a
   finite-thickness aquifer}},
Journal = {{HYDROLOGICAL PROCESSES}},
