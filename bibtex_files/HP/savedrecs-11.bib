@article{ ISI:000285254800006,
Author = {Koch, Hagen and Gruenewald, Uwe},
Title = {{Regression models for daily stream temperature simulation: case studies
   for the river Elbe, Germany}},
Journal = {{HYDROLOGICAL PROCESSES}},
