@article{ ISI:000274048300005,
Author = {Chen, Dingjiang and Lu, Jun and Shen, Yena},
Title = {{Artificial neural network modelling of concentrations of nitrogen,
   phosphorus and dissolved oxygen in a non-point source polluted river in
   Zhejiang Province, southeast China}},
Journal = {{HYDROLOGICAL PROCESSES}},
