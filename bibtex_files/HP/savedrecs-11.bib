@article{ ISI:000276161600006,
Author = {Wang, Yu-Chi and Yu, Pao-Shan and Yang, Tao-Chang},
Title = {{Comparison of genetic algorithms and shuffled complex evolution approach
   for calibrating distributed rainfall-runoff model}},
