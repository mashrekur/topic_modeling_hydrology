@article{ ISI:000282345400012,
Author = {Zhang, Zhao and Tao, Fulu and Shi, Peijun and Xu, Wei and Sun, Yu and
   Fukushima, Takehiko and Onda, Yuichi},
Title = {{Characterizing the flush of stream chemical runoff from forested
   watersheds}},
Journal = {{HYDROLOGICAL PROCESSES}},
