@article{ ISI:000282345400006,
Author = {Eum, Hyung-Il and Kim, Young-Oh},
Title = {{The value of updating ensemble streamflow prediction in reservoir
   operations}},
Journal = {{HYDROLOGICAL PROCESSES}},
