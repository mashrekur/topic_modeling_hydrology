@article{ ISI:000280140700008,
Author = {Dadic, R. and Mott, R. and Lehning, M. and Burlando, P.},
Title = {{Parameterization for wind-induced preferential deposition of snow}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2010}},
Volume = {{24}},
