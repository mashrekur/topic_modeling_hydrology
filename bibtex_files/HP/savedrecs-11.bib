@article{ ISI:000282345400011,
Author = {Xu, Ligang and Zhou, Hongfei and Liang, Chuan and Du, Li and Li, Hui},
Title = {{Spatial and temporal variability of annual and seasonal precipitation
   over the desert region of China during 1951-2005}},
Journal = {{HYDROLOGICAL PROCESSES}},
