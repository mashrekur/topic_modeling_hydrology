@article{ ISI:000285254000008,
Author = {Sung, Chan Yong and Li, Ming-Han},
Title = {{The effect of urbanization on stream hydrology in hillslope watersheds
   in central Texas}},
Journal = {{HYDROLOGICAL PROCESSES}},
