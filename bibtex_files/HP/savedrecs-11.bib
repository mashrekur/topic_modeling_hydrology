@article{ ISI:000276161000001,
Author = {Cannon, Alex J.},
Title = {{A flexible nonlinear modelling framework for nonstationary generalized
   extreme value analysis in hydroclimatology}},
Journal = {{HYDROLOGICAL PROCESSES}},
