@article{ ISI:000281521000009,
Author = {Neta, T. and Cheng, Q. and Bello, R. L. and Hu, B.},
Title = {{Lichens and mosses moisture content assessment through high-spectral
   resolution remote sensing technology: a case study of the Hudson Bay
   Lowlands, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
