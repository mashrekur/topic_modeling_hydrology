@article{ ISI:000279821900006,
Author = {Vidon, P. and Cuadra, P. E.},
Title = {{Impact of precipitation characteristics on soil hydrology in
   tile-drained landscapes}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2010}},
