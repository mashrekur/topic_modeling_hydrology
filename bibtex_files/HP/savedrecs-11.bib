@article{ ISI:000280140900001,
Author = {Nezhad, M. Kamali and Chokmani, K. and Ouarda, T. B. M. J. and Barbet,
   M. and Bruneau, P.},
Title = {{Regional flood frequency analysis using residual kriging in
   physiographical space}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2010}},
Volume = {{24}},
Number = {{15}},
Pages = {{2045-2055}},
Month = {{JUL 15}},
Abstract = {{In this article, an approach using residual kriging (RK) in
   physiographical space is proposed for regional flood frequency analysis.
   The physiographical space is constructed using physiographical/climatic
   characteristics of gauging basins by means of canonical correlation
   analysis (CCA). This approach is a modified version of the original
   method, based on ordinary kriging (OK). It is intended to handle
   effectively any possible spatial trends within the hydrological
   variables over the physiographical space. In this approach, the trend is
   first quantified and removed from the hydrological variable by a
   quadratic spatial regression. OK is therefore applied to the regression
   residual values. The final estimated value of a specific quantile at an
   ungauged station is the sum of the spatial regression estimate and the
   kriged residual. To evaluate the performance of the proposed method, a
   cross-validation procedure is applied. Results of the proposed method
   indicate that RK in CCA physiographical space leads to more efficient
   estimates of regional flood quantiles when compared to the original
   approach and to a straightforward regression-based estimator. Copyright
   (C) 2010 John Wiley \& Sons, Ltd.}},
