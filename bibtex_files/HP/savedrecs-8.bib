@article{ ISI:000248632400002,
Author = {Cumberland, Susan A. and Baker, Andy},
Title = {{The freshwater dissolved organic matter fluorescence-total organic
   carbon relationship}},
Journal = {{HYDROLOGICAL PROCESSES}},
