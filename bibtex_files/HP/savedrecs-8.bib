@article{ ISI:000249942500004,
Author = {Carlyle-Moses, D. E. and Price, A. G.},
Title = {{Modelling canopy interception loss from a Madrean pine-oak stand,
   Northeastern Mexico}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
Number = {{19}},
Pages = {{2572-2580}},
