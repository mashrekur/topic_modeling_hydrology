@article{ ISI:000248279900006,
Author = {Zhang, Yinsheng and Kadota, T. and Ohata, T. and Oyunbaatar, D.},
Title = {{Environmental controls on evapotranspiration from sparse grassland in
   Mongolia}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
