@article{ ISI:000246114000003,
Author = {Komatsu, Hikaru and Hotta, Norifumi and Kume, Tomonori},
Title = {{What is the best way to represent surface conductance for a range of
   vegetated sites?}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
Number = {{9}},
Pages = {{1142-1147}},
Month = {{APR 30}},
Abstract = {{Surface conductance G(s) is a significant parameter for indicating the
   evaporative and photosynthetic properties of a vegetated surface. When
   comparing G(s) values between different observation sites, some studies
   have used G(smax) and others have used (G) over cap (smax) (where
   G(smax) is the maximum G(s) value measured during the measurement
   period, and is the maximum G(s) value obtained with a vapour pressure
   deficit (VPD) of >= 1.0 kPa during the measurement period).
   In this study, we demonstrate a clear justification for using instead of
   (G) over cap (smax) when comparing G(s) values between different sites.
   We examined whether both (G) over cap (smax) and G(smax) lead to the
   same conclusions in classifying vegetated sites. Komatsu (2003b)
   {[}Hydrological Processes 17: 2503-2512] reported a clear relationship
   between canopy height h and (G) over cap (smax) for coniferous forests
   with a projected leaf area index (LAI) of >= 3-0. We examined not only
   the relationship between h and G(smax) but also the relationship between
   h and G(smax) for coniferous forests with a projected LAI of >= 3.0.
   Both (G) over cap (smax) and G(smax) decreased with increasing h.
   However, the relationship between h and G(s) was less well defined than
   the relationship between h and (G) over cap (smax) because of biased
   G(smax) data. Consequently, we conclude that 1 sx is a more appropriate
   index than G(smax) to represent G(s) for sites with different
   vegetation. Copyright (c) 2007 John Wiley \& Sons, Ltd.}},
Publisher = {{JOHN WILEY \& SONS LTD}},
Address = {{THE ATRIUM, SOUTHERN GATE, CHICHESTER PO19 8SQ, W SUSSEX, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Komatsu, H (Reprint Author), Kyushu Univ, 394 Tsubakuro, Fukuoka 8112415, Japan.
   Univ Tokyo, Oki Kanae Lab, Inst Ind Sci, Tokyo, Japan.
   Univ Tokyo, Grad Sch Agr \& Life Sci, Tokyo, Japan.}},
DOI = {{10.1002/hyp.6674}},
ISSN = {{0885-6087}},
