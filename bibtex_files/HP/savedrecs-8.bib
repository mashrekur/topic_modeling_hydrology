@article{ ISI:000252066700011,
Author = {de Salas, L. and Fernandez, J. A.},
Title = {{``In-site{''} regionalization to estimate an
   intensity-duration-frequency law: a solution to scarce spatial data in
   Spain}},
Journal = {{HYDROLOGICAL PROCESSES}},
