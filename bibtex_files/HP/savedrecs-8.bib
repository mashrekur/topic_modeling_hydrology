@article{ ISI:000249942500011,
Author = {Woo, Ming-Ko and Mielko, Corrinne},
Title = {{An integrated framework of lake-stream connectivity for a semi-arid,
   subarctic environment}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
