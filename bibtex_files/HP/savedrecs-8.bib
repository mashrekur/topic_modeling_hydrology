@article{ ISI:000245364000004,
Author = {Chang, Fi-John and Chang, Li-Chiu and Wang, Yan-Shiang},
Title = {{Enforced self-organizing map neural networks for river flood forecasting}},
Journal = {{HYDROLOGICAL PROCESSES}},
