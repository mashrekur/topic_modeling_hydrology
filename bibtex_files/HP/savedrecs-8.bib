@article{ ISI:000245757100009,
Author = {Liu, Q. Q. and Chen, L. and Li, J. C. and Singh, V. P.},
Title = {{A non-equilibrium sediment transport model for rill erosion}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
Number = {{8}},
Pages = {{1074-1084}},
Month = {{APR 15}},
Abstract = {{Sediment transport in rill flows exhibits the characteristics of
   non-equilibrium transport, and the sediment transport rate of rill flow
   gradually recovers along the flow direction by erosion. By employing the
   concept of partial equilibrium sediment transport from open channel
   hydraulics, a dynamic model of rill erosion on hillslopes was developed.
   In the model, a parameter, called the restoration coefficient of
   sediment transport capacity, was used to express the recovery process of
   sediment transport rate, which was analysed by dimensional analysis and
   determined from laboratory experimental data. The values of soil loss
   simulated by the model were in agreement with observed values. The model
   results showed that the length and gradient of the hillslope and
   rainfall intensity had different influences on rill erosion. Copyright
   (c) 2006 John Wiley \& Sons, Ltd.}},
