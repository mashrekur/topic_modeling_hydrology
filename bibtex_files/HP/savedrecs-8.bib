@article{ ISI:000247895600003,
Author = {Shanahan, Timothy M. and Overpeck, Jonathan T. and Sharp, W. E. and
   Scholz, Christopher A. and Arko, Justice A.},
Title = {{Simulating the response of a closed-basin lake to recent climate changes
   in tropical West Africa (Lake Bosumtwi, Ghana)}},
Journal = {{HYDROLOGICAL PROCESSES}},
