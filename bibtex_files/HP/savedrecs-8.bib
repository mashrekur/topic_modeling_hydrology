@article{ ISI:000251301200010,
Author = {Karabork, M. Cagatay and Kahya, Ercan and Komuscu, Ali Umran},
Title = {{Analysis of Turkish precipitation data: homogeneity and the Southern
   Oscillation forcings on frequency distributions}},
Journal = {{HYDROLOGICAL PROCESSES}},
