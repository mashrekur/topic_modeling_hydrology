@article{ ISI:000247438300005,
Author = {Molotch, Noah P. and Blanken, Peter D. and Williams, Mark W. and
   Turnipseed, Andrew A. and Monson, Russell K. and Margulis, Steven A.},
Title = {{Estimating sublimation of intercepted and sub-canopy snow using eddy
   covariance systems}},
Journal = {{HYDROLOGICAL PROCESSES}},
