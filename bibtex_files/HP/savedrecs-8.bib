@article{ ISI:000248279900009,
Author = {Huang, Wenfeng and Nakane, Kazurou and Matsuura, Reiko and Matsuura,
   Tomonori},
Title = {{Distributed tank model and GAME reanalysis data applied to the
   simulation of runoff within the Chao Phraya River Basin, Thailanda}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
Number = {{15}},
