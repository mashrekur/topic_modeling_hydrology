@article{ ISI:000244074200008,
Author = {McKenzie, Jeffrey M. and Siegel, Donald I. and Rosenberry, Donald O. and
   Glaser, Paul H. and Voss, Clifford I.},
Title = {{Heat transport in the Red Lake Bog, Glacial Lake Agassiz Peatlands}},
Journal = {{HYDROLOGICAL PROCESSES}},
