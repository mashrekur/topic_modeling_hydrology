@article{ ISI:000248632400006,
Author = {Shih, David Ching-Fang},
Title = {{Contaminant transport in one-dimensional single fractured media:
   semi-analytical solution for three-member decay chain with pulse and
   Heaviside input sources}},
Journal = {{HYDROLOGICAL PROCESSES}},
