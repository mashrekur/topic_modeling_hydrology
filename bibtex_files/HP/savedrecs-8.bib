@article{ ISI:000250251200008,
Author = {Fujita, Koji},
Title = {{Effect of dust event timing on glacier runoff: sensitivity analysis for
   a Tibetan glacier}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2007}},
Volume = {{21}},
Number = {{21}},
Pages = {{2892-2896}},
Month = {{OCT 15}},
Abstract = {{The impact of the timing of dust deposition on glacier runoff was
   evaluated using a glacier mass-balance model with a newly improved
   scheme to track a dusted layer in a snow layer of a glacier. The
   lowering of surface albedo due to the dusted layer appearing leads to a
   drastic increase of glacier runoff even under the same meteorological
   conditions. Calculations of seasonal sensitivity, the relationship
   between dusted date and resulting runoff, have shown that dust
   deposition during a melting season might cause a drastic mass outflow
   from a glacier through changing the surface albedo during the melting
   season. Copyright (c) 2006 John Wiley \& Sons, Ltd.}},
