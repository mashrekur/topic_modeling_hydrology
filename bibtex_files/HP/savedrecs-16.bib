@article{ ISI:000346705000007,
Author = {Dai, Qiang and Rico-Ramirez, Miguel A. and Han, Dawei and Islam, Tanvir
   and Liguori, Sara},
Title = {{Probabilistic radar rainfall nowcasts using empirical and theoretical
   uncertainty models}},
Journal = {{HYDROLOGICAL PROCESSES}},
