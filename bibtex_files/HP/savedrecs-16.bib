@article{ ISI:000360060700008,
Author = {Rasouli, Kabir and Pomeroy, John W. and Marks, Danny G.},
Title = {{Snowpack sensitivity to perturbed climate in a cool mid-latitude
   mountain catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
