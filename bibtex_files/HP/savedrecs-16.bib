@article{ ISI:000347847300001,
Author = {Hoomehr, Siavash and Schwartz, John S. and Yoder, Daniel and Drumm, Eric
   C. and Wright, Wesley},
Title = {{Erodibility of low-compaction steep-sloped reclaimed surface mine lands
   in the southern Appalachian region, USA}},
Journal = {{HYDROLOGICAL PROCESSES}},
