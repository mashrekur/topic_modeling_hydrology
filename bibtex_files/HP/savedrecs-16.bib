@article{ ISI:000352564200005,
Author = {Yang, Haiyan and Lin, Binliang and Zhou, Jianjun},
Title = {{Physics-based numerical modelling of large braided rivers dominated by
   suspended sediment}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
