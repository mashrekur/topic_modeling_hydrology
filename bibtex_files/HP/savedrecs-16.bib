@article{ ISI:000350548100032,
Author = {Yu, Han and Douglas, Craig C.},
Title = {{An analysis of infiltration with moisture content distribution in a
   two-dimensional discretized water content domain}},
Journal = {{HYDROLOGICAL PROCESSES}},
