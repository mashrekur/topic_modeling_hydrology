@article{ ISI:000368276600007,
Author = {Il Seo, Jung and Nakamura, Futoshi and Chun, Kun Woo and Kim, Suk Woo
   and Grant, Gordon E.},
Title = {{Precipitation patterns control the distribution and export of large wood
   at the catchment scale}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
