@article{ ISI:000363277400001,
Author = {Selle, Benny and Lange, Holger and Lischeid, Gunnar and Hauhs, Michael},
Title = {{Transit times of water under steady stormflow conditions in the Gardsjon
   G1 catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
Number = {{22}},
Pages = {{4657-4665}},
Month = {{OCT 30}},
Abstract = {{In this paper we report on a series of replicated tracer experiments
   with deuterium conducted under controlled, steady stormflow conditions
   at the Gardsjon G1 catchment in south-western Sweden. In five different
   years, these experiments were carried out in a subcatchment of G1.
   Deuterium was applied as a narrow pulse so that distributions of water
   transit times could be directly inferred from the observed tracer
   breakthrough curves. Significantly different transit times of water were
   observed under similar experimental conditions. Coefficients of
   variation for estimated mean transit times were greater than 60\%, which
   can be understood as a measure of the interannual variability for this
   type of experiments. Implications for water transit times under more
   natural flow conditions as wells as for future experimentation are
   discussed. Copyright (c) 2015 John Wiley \& Sons, Ltd.}},
