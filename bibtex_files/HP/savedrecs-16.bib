Author = {Haynes, Kristine M. and Mitchell, Carl P. J.},
Title = {{Precipitation input and antecedent soil moisture effects on mercury
   mobility in soillaboratory experiments with an enriched stable isotope
   tracer}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
