@article{ ISI:000360060700018,
Author = {Carlyle-Moses, D. E. and Schooling, J. T.},
Title = {{Tree traits and meteorological factors influencing the initiation and
   rate of stemflow from isolated deciduous trees}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
