@article{ ISI:000351463600006,
Author = {Xia, Youlong and Hobbins, Michael T. and Mu, Qiaozhen and Ek, Michael B.},
Title = {{Evaluation of NLDAS-2 evapotranspiration against tower flux site
   observations}},
