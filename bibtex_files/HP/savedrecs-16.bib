@article{ ISI:000356810500003,
Author = {Guan, Mingfu and Sillanpaa, Nora and Koivusalo, Harri},
Title = {{Modelling and assessment of hydrological changes in a developing urban
   catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
