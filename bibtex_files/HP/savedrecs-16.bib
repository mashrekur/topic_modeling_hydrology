@article{ ISI:000350548100003,
Author = {Carrer, Gwenael E. and Rousseau, Alain N. and St-Hilaire, Andre and
   Jutras, Sylvain},
Title = {{Mosaic surface storages of a small boreal catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
