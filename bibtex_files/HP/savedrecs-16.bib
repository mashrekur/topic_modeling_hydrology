@article{ ISI:000350548100025,
Author = {Brown, Kristopher R. and McGuire, Kevin J. and Aust, W. Michael and
   Hession, W. Cully and Dolloff, C. Andrew},
Title = {{The effect of increasing gravel cover on forest roads for reduced
   sediment delivery to stream crossings}},
Journal = {{HYDROLOGICAL PROCESSES}},
