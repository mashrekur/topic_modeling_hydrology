@article{ ISI:000368276600006,
Author = {Yeh, Chun-Kuo and Liaw, Shyue-Cherng},
Title = {{Application of landscape metrics and a Markov chain model to assess land
   cover changes within a forested watershed, Taiwan}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
Number = {{24, SI}},
Pages = {{5031-5043}},
