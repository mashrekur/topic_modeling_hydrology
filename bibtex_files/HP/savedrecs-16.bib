@article{ ISI:000360759400009,
Author = {Ionita, M. and Scholz, P. and Chelcea, S.},
Title = {{Spatio-temporal variability of dryness/wetness in the Danube River Basin}},
Journal = {{HYDROLOGICAL PROCESSES}},
