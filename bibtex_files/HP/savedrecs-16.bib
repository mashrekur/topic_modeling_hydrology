@article{ ISI:000350548100022,
Author = {Webster, Clare S. and Kingston, Daniel G. and Kerr, Tim},
Title = {{Inter-annual variation in the topographic controls on catchment-scale
   snow distribution in a maritime alpine catchment, New Zealand}},
Journal = {{HYDROLOGICAL PROCESSES}},
