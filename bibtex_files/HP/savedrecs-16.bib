@article{ ISI:000360759400007,
Author = {Grimaldi, Stefania and Orellana, Felipe and Daly, Edoardo},
Title = {{Modelling the effects of soil type and root distribution on shallow
   groundwater resources}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
