@article{ ISI:000360060700013,
Author = {Fu, Congsheng and James, April L. and Yao, Huaxia},
Title = {{Investigations of uncertainty in SWAT hydrologic simulations: a case
   study of a Canadian Shield catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
