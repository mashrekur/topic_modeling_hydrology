@article{ ISI:000360060700015,
Author = {Kinar, N. J. and Pomeroy, J. W.},
Title = {{SAS2: the system for acoustic sensing of snow}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
