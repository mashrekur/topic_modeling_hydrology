@article{ ISI:000357519300004,
Author = {Cranswick, Roger H. and Cook, Peter G.},
Title = {{Scales and magnitude of hyporheic, river-aquifer and bank storage
   exchange fluxes}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
