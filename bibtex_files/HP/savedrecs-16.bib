@article{ ISI:000354806200014,
Author = {Fang, Haiyan and Sun, Liying and Tang, Zhenghong},
Title = {{Effects of rainfall and slope on runoff, soil erosion and rill
   development: an experimental study using two loess soils}},
Journal = {{HYDROLOGICAL PROCESSES}},
