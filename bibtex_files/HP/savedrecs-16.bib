@article{ ISI:000350548100039,
Author = {McGuire, Molly M. and Herman, Ellen K.},
Title = {{A novel ATR-FTIR technique for identifying colloid composition in
   natural waters}},
Journal = {{HYDROLOGICAL PROCESSES}},
