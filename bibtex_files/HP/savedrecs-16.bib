@article{ ISI:000363277400006,
Author = {Kamari, Maria and Alho, Petteri and Veijalainen, Noora and Aaltonen,
   Juha and Huokuna, Mikko and Lotsari, Eliisa},
Title = {{River ice cover influence on sediment transportation at present and
   under projected hydroclimatic conditions}},
Journal = {{HYDROLOGICAL PROCESSES}},
