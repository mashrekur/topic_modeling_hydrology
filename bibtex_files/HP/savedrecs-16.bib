@article{ ISI:000356810500008,
Author = {Zhao, Liwen and Zhao, Wenzhi},
Title = {{Canopy transpiration obtained from leaf transpiration, sap flow and
   FAO-56 dual crop coefficient method}},
Journal = {{HYDROLOGICAL PROCESSES}},
