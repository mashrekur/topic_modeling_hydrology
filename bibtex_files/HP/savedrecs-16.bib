@article{ ISI:000360060700017,
Author = {Bialkowski, R. and Buttle, J. M.},
Title = {{Stemflow and throughfall contributions to soil water recharge under
   trees with differing branch architectures}},
Journal = {{HYDROLOGICAL PROCESSES}},
