@article{ ISI:000363277700002,
Author = {Gerke, Kirill M. and Sidle, Roy C. and Mallants, Dirk},
Title = {{Preferential flow mechanisms identified from staining experiments in
   forested hillslopes}},
Journal = {{HYDROLOGICAL PROCESSES}},
