@article{ ISI:000347848900007,
Author = {Stewart, Ryan D. and Abou Najm, Majdi R. and Rupp, David E. and Lane,
   John W. and Uribe, Hamil C. and Luis Arumi, Jose and Selker, John S.},
Title = {{Hillslope run-off thresholds with shrink-swell clay soils}},
