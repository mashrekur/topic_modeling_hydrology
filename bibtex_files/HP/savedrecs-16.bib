@article{ ISI:000353296900003,
Author = {Fiorillo, Francesco and Pagnozzi, Mauro and Ventafridda, Gerardo},
Title = {{A model to simulate recharge processes of karst massifs}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2015}},
Volume = {{29}},
Number = {{10}},
Pages = {{2301-2314}},
Month = {{MAY 15}},
Abstract = {{The recharge processes have been evaluated for two karst massifs of
   southern Italy, the Mt Terminio and Mt Cervialto, characterized by wide
   endorheic areas. The annual mean recharge has been estimated by
   Geographic Information System (GIS) tools, from regression of annual
   mean values of different ground-elevated rain gauges and thermometers.
   The recharge has been distinguished for endorheic areas and the other
   areas of spring catchment, and the ratio between the output spring and
   input rainfall has been also estimated (recharge coefficient). The
   annual recharge has been used to calibrate a daily scale model, which
   allows to estimate the amount of effective rainfall, which is retained
   as soil moisture; the amount reaching the water table (recharge s.s.);
   and the amount of rainfall, which develops the runoff and leaves the
   catchment. All these amounts vary through the hydrological year, in
   function of soil moisture deficit and daily rainfall intensity. The
   model allows estimating the recharge conditions through the hydrological
   year, and it is a useful tool for water management. Copyright (c) 2014
   John Wiley \& Sons, Ltd.}},
