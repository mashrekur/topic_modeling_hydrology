@article{ ISI:000224746100008,
Author = {Shi, Z and Zhou, HJ},
Title = {{Controls on effective settling velocities of mud flocs in the Changjiang
   Estuary, China}},
Journal = {{HYDROLOGICAL PROCESSES}},
