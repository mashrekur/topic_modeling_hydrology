@article{ ISI:000188506700003,
Author = {Petrone, RM and Price, JS and Carey, SK and Waddington, JM},
Title = {{Statistical characterization of the spatial variability of soil moisture
   in a cutover peatland}},
Journal = {{HYDROLOGICAL PROCESSES}},
