Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Bromley, J (Reprint Author), Ctr Ecol \& Hydrol, Maclean Bldg, Wallingford OX10 8BB, Oxon, England.
   Ctr Ecol \& Hydrol, Wallingford OX10 8BB, Oxon, England.
   UCL, Dept Geol Sci, London WC1E 6BT, England.}},
DOI = {{10.1002/hyp.1341}},
ISSN = {{0885-6087}},
EISSN = {{1099-1085}},
