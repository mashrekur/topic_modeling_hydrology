@article{ ISI:000225365700008,
Author = {Shrestha, R and Takara, K and Tachikawa, Y and Jha, RN},
Title = {{Water resources assessment in a poorly gauged mountainous catchment
   using a geographical information system and remote sensing}},
Journal = {{HYDROLOGICAL PROCESSES}},
