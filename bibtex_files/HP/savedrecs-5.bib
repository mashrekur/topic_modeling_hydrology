@article{ ISI:000220457200002,
Author = {Shantz, M and Dowsett, E and Canham, E and Tavernier, G and Stone, M and
   Price, J},
Title = {{The effect of drawdown on suspended solids and phosphorus export from
   Columbia Lake, Waterloo, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
