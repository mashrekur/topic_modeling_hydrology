@article{ ISI:000225365700009,
Author = {Strunin, MA and Hiyama, T},
Title = {{Applying wavelet transforms to analyse aircraft-measured turbulence and
   turbulent fluxes in the atmospheric boundary layer over eastern Siberia}},
