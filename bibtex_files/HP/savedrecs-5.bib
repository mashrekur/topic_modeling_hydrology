@article{ ISI:000221781900004,
Author = {Luce, CH and Tarboton, DG},
Title = {{The application of depletion curves for parameterization of subgrid
   variability of snow}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2004}},
Volume = {{18}},
Number = {{8}},
