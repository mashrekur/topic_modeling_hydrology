@article{ ISI:000221781900007,
Author = {Kokkonen, T and Koivusalo, H and Karvonen, T and Croke, B and Jakeman, A},
Title = {{Exploring streamflow response to effective rainfall across event
   magnitude scale}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2004}},
