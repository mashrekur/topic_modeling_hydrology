@article{ ISI:000220735100007,
Author = {Langlois, A and Royer, A and Fillol, E and Frigon, A and Laprise, R},
Title = {{Evaluation of the snow cover variation in the Canadian Regional Climate
   Model over eastern Canada using passive microwave satellite data}},
