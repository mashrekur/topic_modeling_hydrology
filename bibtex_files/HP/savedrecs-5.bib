@article{ ISI:000222375900003,
Author = {Blanken, PD and Black, TA},
Title = {{The canopy conductance of a boreal aspen forest, Prince Albert National
   Park, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
