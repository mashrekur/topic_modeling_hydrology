@article{ ISI:000225365700007,
Author = {Kasih, GAA and Kitada, T},
Title = {{Numerical simulation of water quality response to nutrient loading and
   sediment resuspension in Mikawa Bay, central Japan: quantitative
   evaluation of the effects of nutrient-reduction measures on algal blooms}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2004}},
