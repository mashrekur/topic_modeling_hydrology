@article{ ISI:000220457200003,
Author = {Lane, PNJ and Croke, JC and Dignan, P},
Title = {{Runoff generation from logged and burnt convergent hillslopes: rainfall
   simulation and modelling}},
Journal = {{HYDROLOGICAL PROCESSES}},
