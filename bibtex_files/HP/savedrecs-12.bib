@article{ ISI:000289675700001,
Author = {Fischer, Urs H. and Mair, Doug and Kavanaugh, Jeffrey L. and Willis, Ian
   and Nienow, Peter and Hubbard, Bryn},
Title = {{Modelling ice-bed coupling during a glacier speed-up event: Haut Glacier
   d'Arolla, Switzerland}},
