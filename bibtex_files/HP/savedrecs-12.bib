@article{ ISI:000296276800007,
Author = {Lawler, Robert R. and Link, Timothy E.},
Title = {{Quantification of incoming all-wave radiation in discontinuous forest
   canopies with application to snowmelt prediction}},
Journal = {{HYDROLOGICAL PROCESSES}},
