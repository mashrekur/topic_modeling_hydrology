@article{ ISI:000297247300001,
Author = {Ta, Wanquan and Wang, Haibin and Jia, Xiaopeng},
Title = {{Downstream fining in contrasting reaches of the sand-bedded Yellow River}},
Journal = {{HYDROLOGICAL PROCESSES}},
