@article{ ISI:000287376900012,
Author = {Komatsu, Hikaru and Kume, Tomonori and Otsuki, Kyoichi},
Title = {{Increasing annual runoff-broadleaf or coniferous forests?}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
Volume = {{25}},
Number = {{2}},
Pages = {{302-318}},
Month = {{JAN 15}},
Abstract = {{According to widely held belief, annual evapotranspiration (ET) for
   broadleaf forests is less than that for coniferous forests, resulting in
   higher annual runoff for broadleaf forests. We processed 82 catchment
   runoff and 126 interception loss data from temperate regions and found
   that although the belief is valid under conditions of broadleaf
   deciduous forests and high winter precipitation (e.g. the United
   States), it is invalid under conditions of broadleaf evergreen forests
   (e.g. New Zealand) or low winter precipitation (e.g. Japan). Thus,
   forest management policies based on this belief should be reconsidered
   on the basis of our results for regions with broadleaf evergreen forests
   or low winter precipitation. Copyright (C) 2010 John Wiley \& Sons, Ltd.}},
