@article{ ISI:000288035000001,
Author = {Rouxel, Matthieu and Molenat, Jerome and Ruiz, Laurent and Legout,
   Cedric and Faucheux, Mickael and Gascuel-Odoux, Chantal},
Title = {{Seasonal and spatial variation in groundwater quality along the
   hillslope of an agricultural research catchment (Western France)}},
Journal = {{HYDROLOGICAL PROCESSES}},
