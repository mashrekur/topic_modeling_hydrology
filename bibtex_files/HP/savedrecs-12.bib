@article{ ISI:000286282300003,
Author = {Yu, D. and Lane, S. N.},
Title = {{Interactions between subgrid-scale resolution, feature representation
   and grid-scale resolution in flood inundation modelling}},
Journal = {{HYDROLOGICAL PROCESSES}},
