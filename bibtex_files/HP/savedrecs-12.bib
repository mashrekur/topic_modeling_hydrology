@article{ ISI:000289675700002,
Author = {Dabney, Seth M. and Yoder, Daniel C. and Vieira, Dalmo A. N. and
   Bingner, Ronald L.},
Title = {{Enhancing RUSLE to include runoff-driven phenomena}},
Journal = {{HYDROLOGICAL PROCESSES}},
