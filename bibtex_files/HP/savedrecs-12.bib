@article{ ISI:000287377200002,
Author = {Clark, Martyn P. and McMillan, Hilary K. and Collins, Daniel B. G. and
   Kavetski, Dmitri and Woods, Ross A.},
Title = {{Hydrological field data from a modeller's perspective: Part 2:
   process-based evaluation of model hypotheses}},
