@article{ ISI:000288463600003,
Author = {Wong, C. L. and Venneker, R. and Jamil, A. B. M. and Uhlenbrook, S.},
Title = {{Development of a gridded daily hydrometeorological data set for
   Peninsular Malaysia}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
