@article{ ISI:000294201400008,
Author = {Burles, Katie and Boon, Sarah},
Title = {{Snowmelt energy balance in a burned forest plot, Crowsnest Pass,
   Alberta, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
Volume = {{25}},
