@article{ ISI:000288463600004,
Author = {Kingston, Daniel G. and Hannah, David M. and Lawler, Damian M. and
   McGregor, Glenn R.},
Title = {{Regional classification, variability, and trends of northern North
   Atlantic river flow}},
Journal = {{HYDROLOGICAL PROCESSES}},
