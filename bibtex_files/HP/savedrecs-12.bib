@article{ ISI:000296278100014,
Author = {Mirus, Benjamin B. and Loague, Keith and Cristea, Nicoleta C. and
   Burges, Stephen J. and Kampf, Stephanie K.},
Title = {{A synthetic hydrologic-response dataset}},
Journal = {{HYDROLOGICAL PROCESSES}},
