@article{ ISI:000294201400005,
Author = {Langston, Gregory and Bentley, Laurence R. and Hayashi, Masaki and
   McClymont, Alastair and Pidlisecky, Adam},
Title = {{Internal structure and hydrological functions of an alpine proglacial
   moraine}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
Volume = {{25}},
Number = {{19, SI}},
Pages = {{2967-2982}},
