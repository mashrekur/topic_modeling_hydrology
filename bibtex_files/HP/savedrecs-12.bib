@article{ ISI:000292553800007,
Author = {Magnusson, Jan and Farinotti, Daniel and Jonas, Tobias and Bavay,
   Mathias},
Title = {{Quantitative evaluation of different hydrological modelling approaches
   in a partly glacierized Swiss watershed}},
Journal = {{HYDROLOGICAL PROCESSES}},
