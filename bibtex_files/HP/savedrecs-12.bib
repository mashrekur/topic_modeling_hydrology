@article{ ISI:000294201400009,
Author = {Buttle, J. M.},
Title = {{Streamflow response to headwater reforestation in the Ganaraska River
   basin, southern Ontario, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
Volume = {{25}},
Number = {{19, SI}},
Pages = {{3030-3041}},
