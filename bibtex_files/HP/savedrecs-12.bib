@article{ ISI:000294201400014,
Author = {Foppen, Jan Willem and Orup, Caesar and Adell, Renaud and Poulalion,
   Vincent and Uhlenbrook, Stefan},
Title = {{Using multiple artificial DNA tracers in hydrology}},
