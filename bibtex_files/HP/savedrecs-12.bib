@article{ ISI:000296276000006,
Author = {Fewtrell, Timothy J. and Neal, Jeffrey C. and Bates, Paul D. and
   Harrison, Peter J.},
Title = {{Geometric and structural river channel complexity and the prediction of
   urban inundation}},
