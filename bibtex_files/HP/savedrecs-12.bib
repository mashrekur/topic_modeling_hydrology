@article{ ISI:000289675700003,
Author = {Zheng, Hua and Zhao, Xiaojie and Zhao, Tongqian and Chen, Falin and Xu,
   Weihua and Duan, Xiaonan and Wang, Xiaoke and Ouyang, Zhiyun},
Title = {{Spatial-temporal variations of methane emissions from the Ertan
   hydroelectric reservoir in southwest China}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
