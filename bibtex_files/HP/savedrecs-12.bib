@article{ ISI:000296277400002,
Author = {Song, Xianfeng and Duan, Zheng and Kono, Yasuyuki and Wang, Mingyu},
Title = {{Integration of remotely sensed C factor into SWAT for modelling sediment
   yield}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2011}},
Volume = {{25}},
