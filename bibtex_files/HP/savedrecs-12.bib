@article{ ISI:000296277400006,
Author = {Ryu, Jae H. and Lee, Joo Heon and Jeong, Sangman and Park, Seon K. and
   Han, Kyuha},
Title = {{The impacts of climate change on local hydrology and low flow frequency
   in the Geum River Basin, Korea}},
