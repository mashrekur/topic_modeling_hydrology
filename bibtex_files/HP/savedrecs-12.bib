@article{ ISI:000296277400013,
Author = {Valyrakis, Manousos and Diplas, Panayiotis and Dancey, Clint L.},
Title = {{Prediction of coarse particle movement with adaptive neuro-fuzzy
   inference systems}},
Journal = {{HYDROLOGICAL PROCESSES}},
