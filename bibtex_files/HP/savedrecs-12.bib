@article{ ISI:000287376900003,
Author = {Lohani, Anil Kumar and Goel, N. K. and Bhatia, K. K. S.},
Title = {{Comparative study of neural network, fuzzy logic and linear transfer
   function techniques in daily rainfall-runoff modelling under different
   input domains}},
Journal = {{HYDROLOGICAL PROCESSES}},
