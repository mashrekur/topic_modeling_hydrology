@article{ ISI:000296276800003,
Author = {Matonse, Adao H. and Pierson, Donald C. and Frei, Allan and Zion, Mark
   S. and Schneiderman, Elliot M. and Anandhi, Aavudai and Mukundan, Rajith
   and Pradhanang, Soni M.},
Title = {{Effects of changes in snow pattern and the timing of runoff on NYC water
   supply system}},
Journal = {{HYDROLOGICAL PROCESSES}},
