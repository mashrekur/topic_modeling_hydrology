@article{ ISI:000287377200006,
Author = {Merten, Eric Christopher and Finlay, Jacques and Johnson, Lucinda and
   Newman, Raymond and Stefan, Heinz and Vondracek, Bruce},
Title = {{Environmental controls of wood entrapment in upper Midwestern streams}},
Journal = {{HYDROLOGICAL PROCESSES}},
