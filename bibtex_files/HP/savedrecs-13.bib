@article{ ISI:000307122600014,
Author = {Daly, Steven F. and Vuyovich, Carrie M. and Deeb, Elias J. and Newman,
   Stephen D. and Baldwin, Timothy B. and Gagnon, John J.},
Title = {{Assessment of the snow conditions in the major watersheds of Afghanistan
   using multispectral and passive microwave remote sensing}},
Journal = {{HYDROLOGICAL PROCESSES}},
