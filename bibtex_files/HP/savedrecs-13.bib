@article{ ISI:000312143100002,
Author = {Warrier, C. Unnikrishnan and Babu, M. Praveen},
Title = {{A study on the spatial variations in stable isotopic composition of
   precipitation in a semiarid region of Southern India}},
Journal = {{HYDROLOGICAL PROCESSES}},
