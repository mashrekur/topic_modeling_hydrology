@article{ ISI:000303499600004,
Author = {Yang, Yonggang and Xiao, Honglang and Wei, Yongping and Zhao, Liangju
   and Zou, Songbing and Yang, Qiu and Yin, Zhenliang},
Title = {{Hydrological processes in the different landscape zones of alpine cold
   regions in the wet season, combining isotopic and hydrochemical tracers}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2012}},
Volume = {{26}},
