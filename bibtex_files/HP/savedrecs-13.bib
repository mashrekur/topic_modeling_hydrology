@article{ ISI:000307122600013,
Author = {Jabot, E. and Zin, I. and Lebel, T. and Gautheron, A. and Obled, C.},
Title = {{Spatial interpolation of sub-daily air temperatures for snow and
   hydrologic applications in mesoscale Alpine catchments}},
