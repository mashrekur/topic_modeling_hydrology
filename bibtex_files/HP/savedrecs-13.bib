@article{ ISI:000311381800010,
Author = {Alhama, I. and Meca, A. Soto and Alhama, F.},
Title = {{Simulation of flow and solute coupled 2-D problems with
   velocity-dependent dispersion coefficient based on the network method}},
Journal = {{HYDROLOGICAL PROCESSES}},
