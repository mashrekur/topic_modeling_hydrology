@article{ ISI:000302795100015,
Author = {Nathanson, Marcus and Kean, Jason W. and Grabs, Thomas J. and Seibert,
   Jan and Laudon, Hjalmar and Lyon, Steve W.},
Title = {{Modelling rating curves using remotely sensed LiDAR data}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2012}},
Volume = {{26}},
