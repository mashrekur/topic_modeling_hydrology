@article{ ISI:000304909700012,
Author = {Knox, Sara H. and Carey, Sean K. and Humphreys, Elyn R.},
Title = {{Snow surface energy exchanges and snowmelt in a shrub-covered bog in
   eastern Ontario, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
