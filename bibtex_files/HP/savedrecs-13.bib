@article{ ISI:000300698100009,
Author = {Kochanek, K. and Strupczewski, W. G. and Bogdanowicz, E.},
Title = {{On seasonal approach to flood frequency modelling. Part II: flood
   frequency analysis of Polish rivers}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2012}},
Volume = {{26}},
