@article{ ISI:000305568800004,
Author = {Chu, Jinggang and Zhang, Chi and Wang, Yilun and Zhou, Huicheng and
   Shoemaker, Christine A.},
Title = {{A watershed rainfall data recovery approach with application to
   distributed hydrological models}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2012}},
