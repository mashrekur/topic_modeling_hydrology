@article{ ISI:000301430200012,
Author = {Soria, Freddy and Kazama, So},
Title = {{Assessing the investigation of streamflow source areas through
   uncertainty evaluation of numerical experiments in small catchments}},
Journal = {{HYDROLOGICAL PROCESSES}},
