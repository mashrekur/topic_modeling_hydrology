@article{ ISI:000308941600009,
Author = {Baker, Daniel W. and Bledsoe, Brian P. and Price, Jennifer Mueller},
Title = {{Stream nitrate uptake and transient storage over a gradient of
   geomorphic complexity, north-central Colorado, USA}},
Journal = {{HYDROLOGICAL PROCESSES}},
