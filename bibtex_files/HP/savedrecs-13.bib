@article{ ISI:000298874500010,
Author = {Schulz, Marcus and Priegnitz, Jan and Klasmeier, Joerg and Heller,
   Stefan and Meinecke, Stefan and Feibicke, Michael},
Title = {{Effect of bed surface roughness on longitudinal dispersion in artificial
   open channels}},
