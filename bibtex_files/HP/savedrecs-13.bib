@article{ ISI:000298874500003,
Author = {Jaafar, Wan Zurina Wan and Han, Dawei},
Title = {{Uncertainty in index flood modelling due to calibration data sizes}},
Journal = {{HYDROLOGICAL PROCESSES}},
