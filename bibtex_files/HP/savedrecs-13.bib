@article{ ISI:000299631900004,
Author = {Liu, Ran and Li, Yan and Wang, Qin-Xue},
Title = {{Variations in water and CO2 fluxes over a saline desert in western China}},
Journal = {{HYDROLOGICAL PROCESSES}},
