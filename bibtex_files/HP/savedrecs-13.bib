@article{ ISI:000303499600006,
Author = {Kourgialas, Nektarios N. and Karatzas, George P. and Nikolaidis,
   Nikolaos P.},
Title = {{Development of a thresholds approach for real-time flash flood
   prediction in complex geomorphological river basins}},
Journal = {{HYDROLOGICAL PROCESSES}},
