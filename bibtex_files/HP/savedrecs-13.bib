@article{ ISI:000302012900010,
Author = {Ishak, Asnor Muizan and Bray, Michaela and Remesan, Renji and Han, Dawei},
Title = {{Seasonal evaluation of rainfall estimation by four cumulus
   parameterization schemes and their sensitivity analysis}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2012}},
Volume = {{26}},
