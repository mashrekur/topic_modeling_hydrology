@article{ ISI:000306805400008,
Author = {Nakayama, Tadanobu and Hashimoto, Shizuka and Hamano, Hiroyuki},
Title = {{Multiscaled analysis of hydrothermal dynamics in Japanese megalopolis by
   using integrated approach}},
Journal = {{HYDROLOGICAL PROCESSES}},
