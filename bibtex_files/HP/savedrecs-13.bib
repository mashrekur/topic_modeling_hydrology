@article{ ISI:000303499600005,
Author = {Gervais, M. and Mkhabela, M. and Bullock, P. and Raddatz, R. and Finlay,
   G.},
Title = {{Comparison of standard and actual crop evapotranspiration estimates
   derived from different evapotranspiration methods on the Canadian
   Prairies}},
Journal = {{HYDROLOGICAL PROCESSES}},
