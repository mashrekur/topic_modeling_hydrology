@article{ ISI:000298597300005,
Author = {McJannet, David and Wallace, Jim and Keen, Rex and Hawdon, Aaron and
   Kemei, Joseph},
Title = {{The filtering capacity of a tropical riverine wetland: II. Sediment and
   nutrient balances}},
Journal = {{HYDROLOGICAL PROCESSES}},
