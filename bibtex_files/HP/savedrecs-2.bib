@article{ ISI:A1997YB23100006,
Author = {Singh, VP},
Title = {{Effect of spatial and temporal variability in rainfall and watershed
   characteristics on stream flow hydrograph}},
Journal = {{HYDROLOGICAL PROCESSES}},
