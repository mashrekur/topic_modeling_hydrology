@article{ ISI:A1997WV04500002,
Author = {Gandolfi, C and Bischetti, GB},
Title = {{Influence of the drainage network identification method on
   geomorphological properties and hydrological response}},
Journal = {{HYDROLOGICAL PROCESSES}},
