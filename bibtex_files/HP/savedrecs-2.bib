@article{ ISI:A1997WZ76100003,
Author = {Amin, MHG and Chorley, RJ and Richards, KS and Hall, LD and Carpenter,
   TA and Cislerova, M and Vogel, T},
Title = {{Study of infiltration into a heterogeneous soil using magnetic resonance
   imaging}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1997}},
Volume = {{11}},
Number = {{5}},
Pages = {{471-483}},
Month = {{APR}},
Abstract = {{In this paper, the feasibility of using magnetic resonance imaging (MRI)
   to study water infiltration into a heterogeneous soil is examined,
   together with its difficulties and limitations. MRI studies of ponded
   water infiltration into an undisturbed soil core show that the
   combination of one- and two-dimensional imaging techniques provides a
   visual and non-destructive means of monitoring the temporal changes of
   soil water content and the moisture profile, and the movement of the
   wetting front. Two-dimensional images show air entrapment in repetitive
   ponded infiltration experiments. During the early stages of filtration,
   one-dimensional images of soil moisture profiles clearly indicate
   preferential how phenomena. The observed advance of wetting fronts can
   be described by a linear relationship between the square root of
   infiltration time (root s) and the distance of the wetting front from
   the soil surface. Similarly, the cumulative infiltration is also
   directly proportional to root t. Furthermore, from the MRI infiltration
   moisture profiles, it is possible to estimate the parameters that
   feature in infiltration equations. (C) 1997 by John Wiley \& Sons, Ltd.}},
