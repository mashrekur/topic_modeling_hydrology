@article{ ISI:A1997YE59800003,
Author = {English, MC and Hill, RB and Stone, MA and Ormson, R},
Title = {{Geomorphological and botanical change on the outer Slave River Delta,
   NWT, before and after impoundment of the Peace River}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1997}},
