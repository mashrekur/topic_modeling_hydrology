@article{ ISI:000084002600021,
Author = {Koivusalo, H and Heikinheimo, H},
Title = {{Surface energy exchange over a boreal snowpack: comparison of two snow
   energy balance models}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1999}},
Volume = {{13}},
Number = {{14-15}},
Pages = {{2395-2408}},
Month = {{OCT}},
Abstract = {{The objective of this study was to compare two energy balance models to
   simulate energy exchange, skin temperature and albedo at the snow
   surface. Point measurements of vertical snow temperature and density
   profiles, snow surface temperature, net radiation and albedo were taken
   during spring 1997 at a site within a sparse coniferous stand in
   northern Finland. Standard meteorological measurements were available
   near the snow site at the Sodankyla Meteorological Observatory. The
   study period included two weeks of measurements of turbulent heat fluxes
   with the eddy-correlation method above snow as a part of the
   NOPEX-WINTEX project ``Land-surface-atmosphere interactions in a
   wintertime boreal landscape{''}. Two point models with different snow
   process representations were tested and the modelled energy fluxes at
   the snow surface were compared. UEB was chosen as a simple model, which
   derived the surface energy fluxes but treated the snowpack as one layer.
   SNTHERM represented a sophisticated model to simulate mass and energy
   transfer at the snowpack boundaries and within the snow cover. The
   results showed major differences in the modelled radiation balance due
   to different albedo procedures implemented in the models. The model
   comparison using identical radiation input data indicated that SNTHERM
   had a better overall agreement to the measurements as compared with UEB.
   Major differences between the two models were found in the calculation
   of sensible heat flux and snow surface temperature, which were sensitive
   to the correction for atmospheric stability and snow thermal
   conductivity, respectively. The comparison of the SNTHERM results to the
   eddy-correlation measurements of sensible heat flux supported the use of
   a restricted correction for atmospheric stability. The forest canopy had
   a notable effect on the short-wave radiation balance, the vertical wind
   profile, and the measured sensible heat flux. Copyright (C) 1999 John
   Wiley \& Sons, Ltd.}},
