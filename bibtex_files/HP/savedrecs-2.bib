@article{ ISI:000075740100019,
Author = {Elder, K and Rosenthal, W and Davis, RE},
Title = {{Estimating the spatial distribution of snow water equivalence in a
   montane watershed}},
Journal = {{HYDROLOGICAL PROCESSES}},
