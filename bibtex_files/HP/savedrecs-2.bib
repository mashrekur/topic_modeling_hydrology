ISSN = {{0885-6087}},
Keywords = {{desert areas; longitudinal dunes; water percolation; water movement;
   subsurface lateral flow}},
Keywords-Plus = {{RECHARGE; SANDS}},
Research-Areas = {{Water Resources}},
