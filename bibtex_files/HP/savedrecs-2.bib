@article{ ISI:000084002600026,
Author = {Jin, JM and Gao, XG and Sorooshian, S and Yang, ZL and Bales, R and
   Dickinson, RE and Sun, SF and Wu, GX},
Title = {{One-dimensional snow water and energy balance model for vegetated
   surfaces}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1999}},
Volume = {{13}},
Number = {{14-15}},
Pages = {{2467-2482}},
Month = {{OCT}},
Abstract = {{We developed and evaluated a three-layer snow model for application in
   general circulation models. This one-dimensional snow model has many
   features of the detailed physically based model SNTHERM, yet is
   computationally much simpler. We have also extended the point model to
   vegetated areas using the parameterization concepts of the
   Biosphere-Atmosphere Transfer Scheme (BATS). Results of model
   applications for two types of vegetated fields - a short grassland in
   the French Alps and an old aspen forest in the southern study area of
   BOREAS - were presented. The results, on one hand, indicate the
   suitability of the model structure and parameter setting; on the other
   hand, the results explore the limitation of using `point' field
   observations to evaluate an area model. Copyright (C) 1999 John Wiley \&
   Sons, Ltd.}},
