@article{ ISI:000084002700010,
Author = {Hannah, DM and Gurnell, AM and McGregor, GR},
Title = {{A methodology for investigation of the seasonal evolution in proglacial
   hydrograph form}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1999}},
Volume = {{13}},
