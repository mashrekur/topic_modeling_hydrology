@article{ ISI:A1997WZ76100002,
Author = {Aizen, V and Aizen, E},
Title = {{Hydrological cycles on the north and south peripheries of
   mountain-glacial basins of central Asia}},
Journal = {{HYDROLOGICAL PROCESSES}},
