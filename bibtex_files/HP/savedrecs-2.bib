@article{ ISI:000084002600019,
Author = {Nakai, Y and Sakamoto, T and Terajima, T and Kitamura, K and Shirai, T},
Title = {{The effect of canopy-snow on the energy balance above a coniferous
   forest}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1999}},
