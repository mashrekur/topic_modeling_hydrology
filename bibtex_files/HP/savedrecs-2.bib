@article{ ISI:A1997WJ75300005,
Author = {Singh, VP and Bengtsson, L and Westerstrom, G},
Title = {{Kinematic wave modelling of saturated basal flow in a snowpack}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1997}},
Volume = {{11}},
Number = {{2}},
Pages = {{177-187}},
Month = {{FEB}},
Abstract = {{Movement of snowmelt water through a thin saturated layer at the
   infiltrating base of a snowpack is modelled by applying the kinematic
   wave theory. Analytical solutions are obtained for flow depth, velocity
   and discharge assuming that the rate of input to the saturated layer due
   to vertical percolation is constant. This assumption results in a linear
   rise and recession of the snowmelt hydrograph. The solutions are
   extended to the case of time-varying input. An explicit consideration of
   infiltration leads to a free boundary problem.}},
