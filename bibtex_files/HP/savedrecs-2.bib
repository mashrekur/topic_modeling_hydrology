@article{ ISI:A1997XQ93700013,
Author = {Wolock, DM and Fan, J and Lawrence, GB},
Title = {{Effects of basin size on low-flow stream chemistry and subsurface
   contact time in the Neversink River Watershed, New York}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1997}},
Volume = {{11}},
Number = {{9}},
Pages = {{1273-1286}},
Month = {{JUL}},
Abstract = {{The effects of basin size on low-flow stream chemistry and subsurface
   contact time were examined for a part of the Neversink River watershed
   in southern New York State. Acid neutralizing capacity (ANC), the sum of
   base cation concentrations (SBC), pH and concentrations of total
   aluminum (Al), dissolved organic carbon (DOC) and silicon (Si) were
   measured during low stream flow at the outlets of nested basins ranging
   in size from 0.2 to 166.3 km(2). ANC, SBC, pH, Al and DOC showed
   pronounced changes as basin size increased from 0.2 to 3 km(2), but
   relatively small variations were observed as basin size increased beyond
   3 km(2). An index of subsurface contact time computed from basin
   topography and soil hydraulic conductivity also showed pronounced
   changes as basin size increased from 0.2 to 3 km(2) and smaller changes
   as basin size increased beyond 3 km(2). These results suggest that basin
   size affects low-flow stream chemistry because of the effects of basin
   size on subsurface contact time. (C) 1997 by John Wiley \& Sons, Ltd.}},
