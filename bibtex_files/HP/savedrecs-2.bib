@article{ ISI:000084686600015,
Author = {Lee, YH and Singh, VP},
Title = {{Prediction of sediment yield by coupling Kalman filter with
   instantaneous unit sediment graph}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1999}},
Volume = {{13}},
Number = {{17}},
Pages = {{2861-2875}},
Month = {{DEC 15}},
Abstract = {{An instantaneous unit sediment graph (IUSG) model in conjunction with
   Kalman filter was investigated for prediction of sediment yield from an
   upland watershed in Northwestern Mississippi. The state vector of the
   watershed sediment yield system was constituted by the IUSG and then the
   sediment yield was estimated by the IUSG model using Kalman filter. The
   initial values of the state:vector were assumed as the average of the
   IUSG values and the initial sediment yield estimated from the average
   IUSG. The IUSG model using Kalman filler with a recursive algorithm
   accurately predicted sediment yield from watershed W-5, Mississippi. The
   filter allowed the IUSG to vary in time, increased the accuracy of the
   IUSG model, and reduced physical uncertainty of the sediment yield
   process in the watershed. Copyright (C) 1999 John Wiley \& Sons, Ltd.}},
