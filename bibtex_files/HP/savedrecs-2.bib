@article{ ISI:000080392200001,
Author = {Leeks, GJL and Walling, DE},
Title = {{River basin sediment dynamics and interactions within the UK Land-Ocean
   Interaction Study: the context}},
Journal = {{HYDROLOGICAL PROCESSES}},
