@article{ ISI:A1997XZ39200002,
Author = {Singh, VP and Xu, CY},
Title = {{Sensitivity of mass transfer-based evaporation equations to errors in
   daily and monthly input data}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{1997}},
Volume = {{11}},
Number = {{11}},
Pages = {{1465-1473}},
Month = {{SEP}},
Abstract = {{The influence of data errors on the performance of mass transfer-based
   evaporation equations was investigated for both monthly and daily data
   from a climatological station in the State of Vaud in Switzerland.
   Evaporation estimates were found to be particularly sensitive to vapour
   pressure gradient, less sensitive to wind speed and most insensitive to
   temperature. A quantitative analysis showed that: (1) systematic errors
   in vapour pressure data influenced evaporation estimates inversely to
   more or less the same magnitude for both monthly and daily cases; (2)
   systematic errors in wind speed and temperature data influenced
   evaporation estimates inversely to the magnitude of about a half and a
   quarter of the magnitude of the influence of vapour pressure errors,
   respectively; and (3) evaporation estimates were much more sensitive to
   random errors in the case of monthly data than daily data. (C) 1997 John
   Wiley \& Sons Ltd.}},
