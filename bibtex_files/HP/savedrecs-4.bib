@article{ ISI:000185382500001,
Author = {Rawlins, MA and Lammers, RB and Frolking, S and Fekete, BM and
   Vorosmarty, CJ},
Title = {{Simulating pan-Arctic runoff with a macro-scale terrestrial water
   balance model}},
Journal = {{HYDROLOGICAL PROCESSES}},
