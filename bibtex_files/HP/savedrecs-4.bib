@article{ ISI:000177662600003,
Author = {Thies, H and Nickus, U and Arnold, C and Psenner, R},
Title = {{A hydrological tracer experiment with LiCl in a high mountain lake}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2002}},
Volume = {{16}},
Number = {{12}},
Pages = {{2329-2337}},
Month = {{AUG 30}},
Abstract = {{The water residence time of the high mountain seepage lake
   Gossenkollesee (2413 m, Tyrol, Austria) was determined by measuring the
   flushing rate of a tracer substance. During holomixis in July 1997, when
   circulation occurred throughout the entire water column, a dilute
   lithium chloride solution was injected into the lake causing the lithium
   concentration to rise from the background value of 0.06 to 3.1 mug L-1.
   The water residence time was derived from the exponential decline of the
   lithium mass in the lake between July and October holomixis. The water
   residence time was about 2 months, i.e. the lake volume was exchanged
   twice between the two periods of holomictic conditions. Copyright (C)
   2002 John Wiley Sons, Ltd.}},
