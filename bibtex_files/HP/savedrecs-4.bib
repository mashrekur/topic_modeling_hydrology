@article{ ISI:000187201000002,
Author = {Edwin, JM and Thirumalaivasan, D},
Title = {{Stack unit mapping of coastal aquifer to predict and control sea water
   intrusion using remote sensing and a geographical information system}},
Journal = {{HYDROLOGICAL PROCESSES}},
