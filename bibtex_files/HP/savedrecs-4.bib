@article{ ISI:000187612800006,
Author = {Galagedara, LW and Parkin, GW and Redman, JD},
Title = {{An analysis of the ground-penetrating radar direct ground wave method
   for soil water content measurement}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2003}},
Volume = {{17}},
Number = {{18}},
Pages = {{3615-3628}},
Month = {{DEC 30}},
Note = {{Annual Meeting of the Canadian-Geophysical-Union, BANFF, CANADA, MAY,
   2002}},
Organization = {{Canadian Geophys Union}},
Abstract = {{The spatial variability of soil water content can be measured with the
   ground wave velocity of ground-penetrating radar (GPR) using short
   antenna offsets, but picking the correct ground wave arrival time is
   rather difficult. In applying the GPR ground wave method to soil water
   content estimation it is also important to know the effective sampling
   depth of the method. Uniform drainage experiments were conducted with
   100 and 450 MHz GPR antennas using 1.0 and 2.0 in fixed antenna
   separations on a sandy loam soil to investigate time zero picking
   methodologies and to estimate the sampling depth of the GPR method. The
   GPR water content data were compared with time-domain reflectometry
   (TDR)-measured data using six vertical TDR probes of different lengths.
   Time zero was calculated from an air calibration at a 2-0 m antenna
   separation and from wide-angle reflection and refraction data, and a
   difference was found between the two time-zero calibration methods. A
   method was analysed to determine the arrival time of the leading edge of
   the direct ground wavelet using the arrival time of the peak amplitude,
   since the arrival time of the leading edge of the ground wave can be
   difficult to pick. Regression analysis showed that the GPR (100 MHz)
   measured water content was not different from the water content measured
   with TDR at 0-0.1 in depth, implying that this may be a reasonable
   estimate of the GPR ground wave method's sampling depth. A similar
   analysis based on the differences between the 0-0.2 m TDR and the GPR
   shows that the effective sampling depth of the direct ground wave of the
   450 MHz data is less than the sampling depth of the 100 MHz data.
   Copyright (C) 2003 John Wiley Sons, Ltd.}},
