@article{ ISI:000184312100009,
Author = {Kendy, E and Gerard-Marchant, P and Walter, MT and Zhang, YQ and Liu, CM
   and Steenhuis, TS},
Title = {{A soil-water-balance approach to quantify groundwater recharge from
   irrigated cropland in the North China Plain}},
Journal = {{HYDROLOGICAL PROCESSES}},
