@article{ ISI:000180909000009,
Author = {Koivusalo, H and Kokkonen, T},
Title = {{Modelling runoff generation in a forested catchment in southern Finland}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2003}},
Volume = {{17}},
Number = {{2}},
Pages = {{313-328}},
