Address = {{COMMERCE PLACE, 350 MAIN ST, MALDEN 02148, MA USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Holden, J (Reprint Author), Univ Leeds, Sch Geog, Leeds LS2 9JT, W Yorkshire, England.
   Univ Leeds, Sch Geog, Leeds LS2 9JT, W Yorkshire, England.
   Univ Durham, Dept Geog, Sci Labs, Durham DH1 3LE, England.}},
DOI = {{10.1002/hyp.1014}},
