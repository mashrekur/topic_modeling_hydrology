@article{ ISI:000180909000005,
Author = {Meyles, E and Williams, A and Ternan, L and Dowd, J},
Title = {{Runoff generation in relation to soil moisture patterns in a small
   Dartmoor catchment, Southwest England}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2003}},
Volume = {{17}},
Number = {{2}},
Pages = {{251-264}},
