@article{ ISI:000182601000016,
Author = {Keim, RF and Skaugset, AE},
Title = {{Modelling effects of forest canopies on slope stability}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2003}},
Volume = {{17}},
Number = {{7}},
Pages = {{1457-1467}},
Month = {{MAY}},
Abstract = {{We investigated the potential effects of rainfall intensity smoothing by
   forest canopies on slope stability by modelling soil responses to
   measured rainfall and throughfall during high-intensity rain. Field
   measurements showed that maximum intensities of precipitation were
   generally reduced under forest canopies at two sites in the Pacific
   Northwest, USA. Modelling soil water pore-pressure responses of a
   hypothetical hillslope to the field data resulted in estimates of slope
   stability that were generally greater under forest canopy than for the
   same hillslope without forest canopy. Results indicate that smoothing of
   precipitation intensities may translate into overall greater stability
   of hillslopes under forest canopies. Copyright (C) 2003 John Wiley Sons,
   Ltd.}},
