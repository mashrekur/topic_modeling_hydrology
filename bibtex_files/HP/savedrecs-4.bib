@article{ ISI:000186664400010,
Author = {Litvin, LF and Zorina, YF and Sidorchuk, AY and Chernov, AV and Golosov,
   VN},
Title = {{Erosion and sedimentation on the Russian Plain, part 1: contemporary
   processes}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2003}},
Volume = {{17}},
