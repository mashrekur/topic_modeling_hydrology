@article{ ISI:000184731200005,
Author = {Struthers, I and Hinz, C and Sivapalan, M and Deutschmann, G and Beese,
   F and Meissner, R},
Title = {{Modelling the water balance of a free-draining lysimeter using the
   downward approach}},
Journal = {{HYDROLOGICAL PROCESSES}},
