@article{ ISI:000180909000013,
Author = {Seibert, J and Rodhe, A and Bishop, K},
Title = {{Simulating interactions between saturated and unsaturated storage in a
   conceptual runoff model}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2003}},
