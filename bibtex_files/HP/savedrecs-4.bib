@article{ ISI:000187201000005,
Author = {Duguay, CR and Flato, GM and Jeffries, MO and Menard, P and Morris, K
   and Rouse, WR},
Title = {{Ice-cover variability on shallow lakes at high latitudes: model
   simulations and observations}},
