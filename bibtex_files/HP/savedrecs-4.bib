@article{ ISI:000183315300008,
Author = {Asmar, BN and Ergenzinger, P},
Title = {{Effect of the Dead Sea-Red Sea canal modelling on the prediction of the
   Dead Sea conditions}},
Journal = {{HYDROLOGICAL PROCESSES}},
