@article{ ISI:000187612800012,
Author = {Javelle, P and Ouarda, TBMJ and Bobee, B},
Title = {{Spring flood analysis using the flood-duration-frequency approach:
   application to the provinces of Quebec and Ontario, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
