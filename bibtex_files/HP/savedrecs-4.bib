@article{ ISI:000179836600007,
Author = {Moramarco, T and Singh, VP},
Title = {{Accuracy of kinematic wave and diffusion wave for spatial-varying
   rainfall excess over a plane}},
Journal = {{HYDROLOGICAL PROCESSES}},
