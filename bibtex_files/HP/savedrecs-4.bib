@article{ ISI:000175610900008,
Author = {Joel, A and Messing, I and Seguel, O and Casanova, M},
Title = {{Measurement of surface water runoff from plots of two different sizes}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2002}},
Volume = {{16}},
