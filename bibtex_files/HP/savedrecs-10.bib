@article{ ISI:000270078100008,
Author = {Patro, Shivananda and Chatterjee, Chandranath and Singh, Rajendra and
   Raghuwanshi, Narendra Singh},
Title = {{Hydrodynamic modelling of a large flood-prone river system in India with
   limited data}},
Journal = {{HYDROLOGICAL PROCESSES}},
