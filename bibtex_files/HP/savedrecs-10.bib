@article{ ISI:000264228900011,
Author = {Tetzlaff, D. and Seibert, J. and McGuire, K. J. and Laudon, H. and Burn,
   D. A. and Dunn, S. M. and Soulsby, C.},
Title = {{How does landscape structure influence catchment transit time across
   different geomorphic provinces?}},
