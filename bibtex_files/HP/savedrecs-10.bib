@article{ ISI:000271367800017,
Author = {Molinier, Michel and Ronchail, Josyane and Loup Guyot, Jean and
   Cochonneau, Gerard and Guimaraes, Valdemar and de Oliveira, Eurides},
Title = {{Hydrological variability in the Amazon drainage basin and African
   tropical basins}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2009}},
Volume = {{23}},
Number = {{22, SI}},
Pages = {{3245-3252}},
Month = {{OCT 30}},
Abstract = {{Long-term monthly mean rainfall and water discharge data from about 50
   rain gauge stations and approximately the same number of hydrometric
   stations in the Amazon drainage basin were systematically analysed. The
   longest records approached 100 years in length. Comparison with climatic
   indices derived from the Tropical Pacific (Southern Oscillation
   Index-SOI) and the Atlantic {[}sea surface temperature (SST) anomalies]
   reveals the influence of the El Nino-Southern Oscillation (ENSO)
   phenomenon on the variability of Amazonian hydrological regimes. This
   influence, however, may be strengthened by the tropical Atlantic SST. A
   similar study of several big rivers in Africa shows that the influence
   of ENSO is weaker in that continent. The hydrological variability of
