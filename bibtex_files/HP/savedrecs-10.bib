@article{ ISI:000272748100009,
Author = {Eris, Ebru and Agiralioglu, Necati},
Title = {{Effect of coastline configuration on precipitation distribution in
   coastal zones}},
Journal = {{HYDROLOGICAL PROCESSES}},
