@article{ ISI:000266394500007,
Author = {Michaelides, Katerina and Lister, Debbie and Wainwright, John and
   Parsons, Anthony J.},
Title = {{Vegetation controls on small-scale runoff and erosion dynamics in a
   degrading dryland environment}},
Journal = {{HYDROLOGICAL PROCESSES}},
