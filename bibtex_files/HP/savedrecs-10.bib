@article{ ISI:000263214700012,
Author = {Faramarzi, Monireh and Abbaspour, Karim C. and Schulin, Rainer and Yang,
   Hong},
Title = {{Modelling blue and green water resources availability in Iran}},
Journal = {{HYDROLOGICAL PROCESSES}},
