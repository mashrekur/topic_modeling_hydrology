@article{ ISI:000269675300009,
Author = {Goulding, Holly L. and Prowse, Terry D. and Beltaos, Spyros},
Title = {{Spatial and temporal patterns of break-up and ice-jam flooding in the
   Mackenzie Delta, NWT}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2009}},
Volume = {{23}},
Number = {{18, SI}},
Pages = {{2654-2670}},
