@article{ ISI:000268013600010,
Author = {Li, Yijun and Zhou, Li and Xu, Zhenzhu and Zhou, Guangsheng},
Title = {{Comparison of water vapour, heat and energy exchanges over agricultural
   and wetland ecosystems}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2009}},
