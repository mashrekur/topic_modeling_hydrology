@article{ ISI:000266193200012,
Author = {Burford, Jason E. and Dery, Stephen J. and Holmes, Richard D.},
Title = {{Some aspects of the hydroclimatology of the Quesnel River Basin, British
   Columbia, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
