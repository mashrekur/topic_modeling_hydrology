@article{ ISI:000263902300008,
Author = {Parajuli, Prem B. and Nelson, Nathan O. and Frees, Lyle D. and Mankin,
   Kyle R.},
Title = {{Comparison of AnnAGNPS and SWAT model simulation results in USDA-CEAP
   agricultural watersheds in south-central Kansas}},
