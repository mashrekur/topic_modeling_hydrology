@article{ ISI:000263902300006,
Author = {Yasuda, H. and Berndtsson, R. and Saito, T. and Anyoji, H. and Zhang, X.},
Title = {{Prediction of Chinese Loess Plateau summer rainfall using Pacific Ocean
   spring sea surface temperature}},
