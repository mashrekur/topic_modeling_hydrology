@article{ ISI:000270935400007,
Author = {Petersen-Overleir, Asgeir and Reitan, Trond},
Title = {{Bayesian analysis of stage-fall-discharge models for gauging stations
   affected by variable backwater}},
Journal = {{HYDROLOGICAL PROCESSES}},
