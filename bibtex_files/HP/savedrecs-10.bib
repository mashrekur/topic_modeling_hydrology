@article{ ISI:000271367800004,
Author = {Filizola, Naziano and Guyot, Jean Loup and Guimaraes, Valdemar},
Title = {{Measuring the discharge of the Amazon River using Doppler technology
   (Manacapuru, Amazonas, Brazil)}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2009}},
Volume = {{23}},
Number = {{22}},
Pages = {{3151-3156}},
Month = {{OCT 30}},
Abstract = {{The purpose of this article is to report all experiment performed with
   an Acoustic Doppler Current Profiler-ADCP, under tropical environmental
   conditions, aimed at testing and evaluating the ADCP and comparing the
   results obtained with those provided by traditional methods used in the
   Amazon River Basin. The HiBAm research team tested the ADCP over a 4-day
   period at the Manacapuru section on the Solimoes River. During this
   period, 34 water discharge measurements were completed, whereas only a
   maximum of only 3 measurements would have been possible using
   traditional methods. due to operational limitations. The ADCP results
   differed from the other methods by about 9\%. However, the filial water
   discharge results (93 x 10(3) m(3).s(-1) for the experiment period)
   showed that the ADCP offers fundamental advantages ill terms of
   measurement reproducibility (less than 1\%), speed (only 30 min for a
   3000-m transect), reduced number Of people (two) and safety. The results
   also indicate that the ADCP call help solve the problem of scatter
   associated with the rating curve for Manacapuru. Copyright (C) 2009 John
   Wiley \& Sons, Ltd.}},
