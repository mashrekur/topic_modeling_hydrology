@article{ ISI:000473256700001,
Author = {Frank, Simon and Goeppert, Nadine and Ohmer, Marc and Goldscheider, Nico},
Title = {{Sulfate variations as a natural tracer for conduit-matrix interaction in
   a complex karst aquifer}},
