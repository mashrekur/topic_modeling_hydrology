@article{ ISI:000497481100001,
Author = {Snyder, Keirith A. and Scott, Russell L.},
Title = {{Longer term effects of biological control on tamarisk evapotranspiration
   and carbon dioxide exchange}},
Journal = {{HYDROLOGICAL PROCESSES}},
