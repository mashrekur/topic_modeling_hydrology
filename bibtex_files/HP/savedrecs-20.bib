@article{ ISI:000479331100001,
Author = {Kobler, Ulrike Gabriele and Schmid, Martin},
Title = {{Ensemble modelling of ice cover for a reservoir affected by
   pumped-storage operation and climate change}},
Journal = {{HYDROLOGICAL PROCESSES}},
