@article{ ISI:000499880800001,
Author = {Ledford, Sarah H. and Toran, Laura},
Title = {{Downstream evolution of wastewater treatment plant nutrient signals
   using high-temporal monitoring}},
Journal = {{HYDROLOGICAL PROCESSES}},
