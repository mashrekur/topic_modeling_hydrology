@article{ ISI:000477751000006,
Author = {Schalge, Bernd and Haefliger, Vincent and Kollet, Stefan and Simmer,
   Clemens},
Title = {{Improvement of surface run-off in the hydrological model ParFlow by a
   scale-consistent river parameterization}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2019}},
Volume = {{33}},
Number = {{14}},
