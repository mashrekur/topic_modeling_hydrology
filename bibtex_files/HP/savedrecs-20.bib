@article{ ISI:000477751000003,
Author = {Ross, Cody A. and Ali, Genevieve and Spence, Chris and Oswald, Claire
   and Casson, Nora},
Title = {{Comparison of event-specific rainfall-runoff responses and their
   controls in contrasting geographic areas}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2019}},
Volume = {{33}},
Number = {{14}},
