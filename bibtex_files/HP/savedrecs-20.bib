@article{ ISI:000458700900008,
Author = {Goodbrand, Amy and Westbrook, Cherie J. and van der Kamp, Garth},
Title = {{Hydrological functions of a peatland in a Boreal Plains catchment}},
Journal = {{HYDROLOGICAL PROCESSES}},
