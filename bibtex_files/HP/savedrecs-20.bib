@article{ ISI:000486518600001,
Author = {Escobar-Ruiz, Veronica and Smith, Hugh G. and Blake, William H. and
   Macdonald, Neil},
Title = {{Assessing the performance of a physically based hydrological model using
   a proxy-catchment approach in an agricultural environment}},
