@article{ ISI:000242223600007,
Author = {Carlyle-Moses, D. E. and Price, A. G.},
Title = {{Growing-season stemflow production within a deciduous forest of southern
   Ontario}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2006}},
Volume = {{20}},
