@article{ ISI:000237230500010,
Author = {Hung, CJ and Tan, YC and Chen, CH and Chen, JM and Chang, PW},
Title = {{Analytical solution of water table fluctuations above an inclined leaky
   layer due to ditch recharge}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2006}},
Volume = {{20}},
Number = {{7}},
Pages = {{1597-1609}},
Month = {{APR 30}},
Abstract = {{Ditch recharge is a method used for raising a water table. However, if
   the aquifer lies above a leaky layer, then the leaky bed will have an
   effect on the height of the water table. This paper attempts to provide
   an analytical solution of the transient state to the linearized
   Boussinesq equation. The solution proposed in this paper is intended for
   a generalized form of the leaky layer, with the boundary condition of
   the water level moving according to an exponential form. The results of
   theoretical analysis and experimental studies can provide a much better
   explanation of ditch recharge on an inclined leaky layer. Copyright (c)
   2006 John Wiley \& Sons, Ltd.}},
