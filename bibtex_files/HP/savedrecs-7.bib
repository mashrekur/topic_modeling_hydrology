@article{ ISI:000239038100006,
Author = {Rees, H. Gwyn and Collins, David N.},
Title = {{Regional differences in response of flow in glacier-fed Himalayan rivers
   to climatic warming}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2006}},
Volume = {{20}},
