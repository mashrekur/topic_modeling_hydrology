@article{ ISI:000241752100001,
Author = {Inamdar, S. P. and O'Leary, N. and Mitchell, M. J. and Riley, J. T.},
Title = {{The impact of storm events on solute exports from a glaciated forested
   watershed in western New York, USA}},
Journal = {{HYDROLOGICAL PROCESSES}},
