@article{ ISI:000239852400014,
Author = {He, Yuanqing and Pang, Hongxi and Theakstone, W. H. and Zhang, Zhonglin
   and Lu, Aigang and Gu, Juan},
Title = {{Isotopic variations in precipitation at Bangkok and their climatological
   significance}},
Journal = {{HYDROLOGICAL PROCESSES}},
