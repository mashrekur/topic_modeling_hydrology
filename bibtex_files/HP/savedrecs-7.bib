@article{ ISI:000239038600005,
Author = {McKergow, Lucy A. and Prosser, Ian P. and Weaver, David M. and Grayson,
   Rodger B. and Reed, Adrian E. G.},
Title = {{Performance of grass and eucalyptus riparian buffers in a pasture
   catchment, Western Australia, part 2: water quality}},
