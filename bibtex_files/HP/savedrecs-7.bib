@article{ ISI:000238321000006,
Author = {Helie, Jean-Francois and Hillaire-Marcel, Claude},
Title = {{Sources of particulate and dissolved organic carbon in the St Lawrence
   River: isotopic approach}},
Journal = {{HYDROLOGICAL PROCESSES}},
