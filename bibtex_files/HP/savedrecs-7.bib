@article{ ISI:000239038100004,
Author = {Woo, Ming-Ko and Thorne, Robin},
Title = {{Snowmelt contribution to discharge from a large mountainous catchment in
   subarctic Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2006}},
Volume = {{20}},
Number = {{10}},
Pages = {{2129-2139}},
Month = {{JUN 30}},
