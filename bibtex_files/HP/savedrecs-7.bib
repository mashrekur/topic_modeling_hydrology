@article{ ISI:000236332400019,
Author = {Sensoy, A and Sorman, AA and Tekeli, AE and Sorman, AU and Garen, DC},
Title = {{Point-scale energy and mass balance snowpack simulations in the upper
   Karasu basin, Turkey}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2006}},
