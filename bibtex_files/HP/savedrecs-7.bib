@article{ ISI:000236332400012,
Author = {Duguay, CR and Prowse, TD and Bonsal, BR and Brown, RD and Lacroix, MP
   and Menard, P},
Title = {{Recent trends in Canadian lake ice cover}},
Journal = {{HYDROLOGICAL PROCESSES}},
