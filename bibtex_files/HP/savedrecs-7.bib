@article{ ISI:000238321000011,
Author = {Tarits, Corinne and Renaut, Robin W. and Tiercelin, Jean-Jacques and Le
   Herisse, Alain and Cotten, Jo and Cabon, Jean-Yves},
Title = {{Geochemical evidence of hydrothermal recharge in Lake Baringo, central
   Kenya Rift Valley}},
Journal = {{HYDROLOGICAL PROCESSES}},
