@article{ ISI:000239038100007,
Author = {de Woul, Mattias and Hock, Regine and Braun, Matthias and Thorsteinsson,
   Thorsteinn and Johannesson, Tomas and Halldorsdottir, Stefania},
Title = {{Firn layer impact on glacial runoff: a case study at Hofsjoekull,
   Iceland}},
