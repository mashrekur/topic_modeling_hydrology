@article{ ISI:000243078400010,
Author = {Peters, Daniel L. and Prowse, Terry D.},
Title = {{Generation of streamflow to seasonal high waters in a freshwater delta,
   northwestern Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
