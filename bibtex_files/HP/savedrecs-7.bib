@article{ ISI:000237230500003,
Author = {Lundberg, A and Richardson-Naslund, C and Andersson, C},
Title = {{Snow density variations: consequences for ground-penetrating radar}},
Journal = {{HYDROLOGICAL PROCESSES}},
