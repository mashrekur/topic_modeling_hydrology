Author = {Fuka, Daniel R. and Walter, M. Todd and MacAlister, Charlotte and
   Degaetano, Arthur T. and Steenhuis, Tammo S. and Easton, Zachary M.},
Title = {{Using the Climate Forecast System Reanalysis as weather input data for
   watershed models}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2014}},
