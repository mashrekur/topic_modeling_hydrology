@article{ ISI:000333835600009,
Author = {Laiolo, P. and Gabellani, S. and Rebora, N. and Rudari, R. and Ferraris,
   L. and Ratto, S. and Stevenin, H. and Cauduro, M.},
Title = {{Validation of the Flood-PROOFS probabilistic forecasting system}},
