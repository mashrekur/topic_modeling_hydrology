@article{ ISI:000331254700002,
Author = {Kaste, James M. and Magilligan, Francis J. and Renshaw, Carl E. and
   Fisher, G. Burch and Dade, W. Brian},
Title = {{Seasonal controls on meteoric Be-7 in coarse-grained river channels}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2014}},
Volume = {{28}},
Number = {{5}},
Pages = {{2738-2748}},
Month = {{FEB 28}},
Abstract = {{Cosmogenic Be-7 is a natural tracer of short-term hydrological
   processes, but its distribution in upland fluvial environments over
   different temporal and spatial scales has not been well described. We
   measured Be-7 in 450 sediment samples collected from perennial channels
   draining the middle of the Connecticut River Basin, an environment that
   is predominantly well-sorted sand. By sampling tributaries that have
   natural and managed fluctuations in discharge, we find that the Be-7
   activity in thalweg sediments is not necessarily limited by the supply
   of new or fine-grained sediment, but is controlled seasonally by
   atmospheric flux variations and the magnitude and frequency of bed
   mobilizing events. In late winter, Be-7 concentrations in transitional
   bedload are lowest, typically 1 to 3Bqkg(-1) as Be-7 is lost from
   watersheds via radioactive decay in the snowpack. In mid-summer,
   however, Be-7 concentrations are at least twice as high because of
   increased convective storm activity which delivers high Be-7 fluxes
   directly to the fluvial system. A mixed layer of sediment at least 8cm
   thick is maintained for months in channels during persistent low
   rainfall and flow conditions, indicating that stationary sediments can
   be recharged with Be-7. However, bed mobilizing rain on snowmelt events
   in late Spring can reset' Be-7 amounts and concentrations in the channel
   as previously buried old' sediment with low Be-7 is mixed into the
   thalweg. We conclude that given proper temporal and spatial sampling,
   Be-7 is a valuable tracer of seasonal-timescale mass transport and
   exchange in coarse-grained fluvial systems. Copyright (c) 2013 John
   Wiley \& Sons, Ltd.}},
