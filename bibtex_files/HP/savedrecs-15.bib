@article{ ISI:000330743000062,
Author = {Li, Sien and Hao, Xingmei and Du, Taisheng and Tong, Ling and Zhang,
   Jianhua and Kang, Shaozhong},
Title = {{A coupled surface resistance model to estimate crop evapotranspiration
   in arid region of northwest China}},
Journal = {{HYDROLOGICAL PROCESSES}},
