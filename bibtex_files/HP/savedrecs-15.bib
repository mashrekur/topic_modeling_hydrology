@article{ ISI:000330743000070,
Author = {Piro, Patrizia and Carbone, Marco},
Title = {{A modelling approach to assessing variations of total suspended solids
   (tss) mass fluxes during storm events}},
Journal = {{HYDROLOGICAL PROCESSES}},
