@article{ ISI:000343862100004,
Author = {Ragettli, S. and Cortes, G. and McPhee, J. and Pellicciotti, F.},
Title = {{An evaluation of approaches for modelling hydrological processes in
   high-elevation, glacierized Andean watersheds}},
Journal = {{HYDROLOGICAL PROCESSES}},
