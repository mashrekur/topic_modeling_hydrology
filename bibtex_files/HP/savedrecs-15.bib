@article{ ISI:000330743000009,
Author = {Palleiro, L. and Rodriguez-Blanco, M. L. and Taboada-Castro, M. M. and
   Taboada-Castro, M. T.},
Title = {{Hydrological response of a humid agroforestry catchment at different
   time scales}},
Journal = {{HYDROLOGICAL PROCESSES}},
