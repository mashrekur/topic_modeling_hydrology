@article{ ISI:000333029100011,
Author = {Pavelsky, Tamlin M.},
Title = {{Using width- based rating curves from spatially discontinuous satellite
   imagery to monitor river discharge}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2014}},
Volume = {{28}},
Number = {{6}},
Pages = {{3035-3040}},
Month = {{MAR 15}},
Abstract = {{Remote estimation of river discharge from river width variations is an
   intriguing method for gauging rivers without conventional measurements.
   Entirely cloud-free imagery of an entire river reach is often rare, but
   partial coverage is more frequent. Discharge is estimated from spatially
   discontinuous imagery via construction of multiple width-discharge
   rating curves within a 62-km reach of the Tanana River, Alaska. The
   resulting discharge error is as low as 6.7\% root mean squared error.
   Imagery covering <20\% of the study reach can be used. Copyright (c)
   2014 John Wiley \& Sons, Ltd.}},
