@article{ ISI:000328569800004,
Author = {Scavone, Giuseppina and Manuel Sanchez, Juan and Telesca, Vito and
   Caselles, Vicente and Copertino, Vito A. and Pastore, Vittoria and
   Valor, Enric},
Title = {{Pixel-oriented land use classification in energy balance modelling}},
