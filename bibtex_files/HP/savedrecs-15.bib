@article{ ISI:000329352400003,
Author = {Giri, Subhasis and Nejadhashemi, A. Pouyan and Woznicki, Sean and Zhang,
   Zhen},
Title = {{Analysis of best management practice effectiveness and spatiotemporal
   variability based on different targeting strategies}},
