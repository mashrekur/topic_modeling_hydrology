@article{ ISI:000343862100009,
Author = {Ye, Aizhong and Duan, Qingyun and Chu, Wei and Xu, Jing and Mao, Yuna},
Title = {{The impact of the South-North Water Transfer Project (CTP)'s central
   route on groundwater table in the Hai River basin, North China}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2014}},
Volume = {{28}},
Number = {{23}},
Pages = {{5755-5768}},
Month = {{NOV 15}},
Abstract = {{The central route of the South-North Water Transfer Project (CTP) is
   designed to divert approximately 9.5 billion m(3) of water per year from
   the Han River, a major tributary of the Yangtze River, to the Hai River
   basin in the north China. The main purpose of this study is to assess
   the impact of CTP on groundwater table in the Hai River basin. Our study
   features a large-scale distributed hydrological model that couples a
   physically based groundwater module, which is sub-basin-based, with a
   conceptual surface water module, which is grid-based. There are several
   grids in each sub-basin and water exchange among grid that are
   considered. Our model couples surface water module and groundwater
   module and calculates human water use at the same time. The simulation
   results indicate that even with the water supply by CTP, the groundwater
   table will continue to decline in the Hai River basin. However, the CTP
   water can evidently reduce the decline rate, helping alleviate
   groundwater overexploitation in Hai River region. Copyright (c) 2013
   John Wiley \& Sons, Ltd.}},
