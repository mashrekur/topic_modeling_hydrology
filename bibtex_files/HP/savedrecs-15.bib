@article{ ISI:000329352400065,
Author = {Shiri, Jalal and Marti, Pau and Singh, Vijay P.},
Title = {{Evaluation of gene expression programming approaches for estimating
   daily evaporation through spatial and temporal data scanning}},
Journal = {{HYDROLOGICAL PROCESSES}},
