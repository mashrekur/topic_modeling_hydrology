@article{ ISI:000329352400071,
Author = {Dixon, David and Boon, Sarah and Silins, Uldis},
Title = {{Watershed-scale controls on snow accumulation in a small montane
   watershed, southwestern Alberta, Canada}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2014}},
