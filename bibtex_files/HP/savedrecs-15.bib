@article{ ISI:000341885800009,
Author = {Dickerson-Lange, Susan E. and Mitchell, Robert},
Title = {{Modeling the effects of climate change projections on streamflow in the
   Nooksack River basin, Northwest Washington}},
Journal = {{HYDROLOGICAL PROCESSES}},
