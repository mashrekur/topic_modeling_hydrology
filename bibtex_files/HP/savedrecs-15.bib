@article{ ISI:000329367900002,
Author = {Chen, Min and Willgoose, Garry R. and Saco, Patricia M.},
Title = {{Spatial prediction of temporal soil moisture dynamics using HYDRUS-1D}},
Journal = {{HYDROLOGICAL PROCESSES}},
