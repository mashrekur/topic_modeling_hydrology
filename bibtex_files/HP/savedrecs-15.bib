@article{ ISI:000329367900003,
Author = {Zhang, Yongfang and Guan, Dexin and Jin, Changjie and Wang, Anzhi and
   Wu, Jiabing and Yuan, Fenghui},
Title = {{Impacts of climate change and land use change on runoff of forest
   catchment in northeast China}},
Journal = {{HYDROLOGICAL PROCESSES}},
