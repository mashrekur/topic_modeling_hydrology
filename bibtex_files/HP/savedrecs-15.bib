@article{ ISI:000337671400008,
Author = {Rees, Andrew and English, Michael and Derksen, Chris and Toose, Peter
   and Silis, Arvids},
Title = {{Observations of late winter Canadian tundra snow cover properties}},
Journal = {{HYDROLOGICAL PROCESSES}},
