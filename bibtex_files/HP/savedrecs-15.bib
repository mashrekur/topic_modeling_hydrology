@article{ ISI:000331254700007,
Author = {Hughes, D. A. and Gush, M. and Tanner, J. and Dye, P.},
Title = {{Using targeted short-term field investigations to calibrate and evaluate
   the structure of a hydrological model}},
Journal = {{HYDROLOGICAL PROCESSES}},
