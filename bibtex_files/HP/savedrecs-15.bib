@article{ ISI:000342670900005,
Author = {Martinez-Santos, M. and Antigueedad, I. and Ruiz-Romera, E.},
Title = {{Hydrochemical variability during flood events within a small forested
   catchment in Basque Country (Northern Spain)}},
Journal = {{HYDROLOGICAL PROCESSES}},
