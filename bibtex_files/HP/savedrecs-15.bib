@article{ ISI:000333683100011,
Author = {Zhou, Shiqiao and Wang, Zheng and Joswiak, Daniel R.},
Title = {{From precipitation to runoff: stable isotopic fractionation effect of
   glacier melting on a catchment scale}},
Journal = {{HYDROLOGICAL PROCESSES}},
