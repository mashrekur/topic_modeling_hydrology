@article{ ISI:000340611500002,
Author = {Gaborit, E. and Anctil, F. and Fortin, V. and Pelletier, G.},
Title = {{Hydrologic evaluation of spatially disaggregated global ensemble
   rainfall forecasts}},
Journal = {{HYDROLOGICAL PROCESSES}},
Year = {{2014}},
Volume = {{28}},
Number = {{17}},
Pages = {{4682-4693}},
Month = {{AUG 15}},
Abstract = {{A statistic-stochastic multi-fractal downscaling technique was evaluated
   from a hydrologic point of view. Ensemble hydrologic forecasts with a
   time step of 3 h were performed for original and disaggregated ensemble
   rainfall forecasts issued by the Canadian Global Ensemble Prediction
   System in its 2009 operational version. This hydro-meteorological
   operational forecasting chain was conducted using the hydrological model
   SWMM5. The model was implemented on a small 6-km(2) urban catchment
   located in the Quebec City region. The hydrological evaluation was based
   on the comparison of forecasted flows to the observed ones, calculating
   several deterministic and probabilistic scores, and drawing rank
   histograms and reliability diagrams. Disaggregated products led to a
   better representation of the ensemble members' dispersion. This
   disaggregation technique represents an interesting way of bridging the
   gap between the meteorological models' resolution and the high degree of
   spatial precision sometimes required by hydrological models in their
   precipitation representation. Copyright (C) 2013 John Wiley \& Sons,
   Ltd.}},
