@article{ ISI:000477038200008,
Author = {Hassaballah, Abdulrahman H. and Nyitrai, Jeremy and Hart, Christine H.
   and Dai, Ning and Sassoubre, Lauren M.},
Title = {{A pilot-scale study of peracetic acid and ultraviolet light for
   wastewater disinfection}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
