@article{ ISI:000394270400007,
Author = {Belila, A. and El-Chakhtoura, J. and Saikaly, P. E. and van Loosdrecht,
   M. C. M. and Vrouwenvelder, J. S.},
Title = {{Eukaryotic community diversity and spatial variation during drinking
   water production (by seawater desalination) and distribution in a
   full-scale network}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
Year = {{2017}},
Volume = {{3}},
