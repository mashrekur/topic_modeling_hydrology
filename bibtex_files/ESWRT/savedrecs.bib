@article{ ISI:000394270400002,
Author = {Raseman, William J. and Kasprzyk, Joseph R. and Rosario-Ortiz, Fernando
   L. and Stewart, Jenna R. and Livneh, Ben},
Title = {{Emerging investigators series: a critical review of decision support
   systems for water treatment: making the case for incorporating climate
   change and climate extremes}},
