@article{ ISI:000461548300013,
Author = {Maghsoodi, Mostafa and Jacquin, Celine and Teychene, Benoit and Heran,
   Marc and Tarabara, Volodymyr V. and Lesage, Geoffroy and Snow, Samuel D.},
Title = {{Emerging investigator series: photocatalysis for MBR effluent
   post-treatment: assessing the effects of effluent organic matter
   characteristics}},
