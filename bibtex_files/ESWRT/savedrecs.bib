@article{ ISI:000454086700014,
Author = {Cui, Yanyan and Yu, Jianwei and Su, Ming and Jia, Zeyu and Liu, Tingting
   and Oinuma, Gaku and Yamauchi, Tokiko},
Title = {{Humic acid removal by gas-liquid interface discharge plasma:
   performance, mechanism and comparison to ozonation}},
