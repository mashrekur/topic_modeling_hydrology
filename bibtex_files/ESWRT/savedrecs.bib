@article{ ISI:000492992100002,
Author = {Wei, Zongsu and Xu, Tianyuan and Zhao, Dongye},
Title = {{Treatment of per- and polyfluoroalkyl substances in landfill leachate:
   status, chemistry and prospects}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
