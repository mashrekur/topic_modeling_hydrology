@article{ ISI:000454086700002,
Author = {Zuo, Zhiqiang and Zheng, Min and Xiong, Huilei and Shi, Hanchang and
   Huang, Xia and Liu, Yanchen},
Title = {{Short-chain fatty acid (SCFA) production maximization by modeling
   thermophilic sludge fermentation}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
