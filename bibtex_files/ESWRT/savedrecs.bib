@article{ ISI:000473057800002,
Author = {Choi, Youngkwon and Naidu, Gayathri and Nghiem, Long D. and Lee, Sangho
   and Vigneswaran, Saravanamuthu},
Title = {{Membrane distillation crystallization for brine mining and zero liquid
   discharge: opportunities, challenges, and recent progress}},
