@article{ ISI:000426705000008,
Author = {Du, Hongxia and Lung, Christie Y. K. and Lau, Tai-Chu},
Title = {{Efficient adsorption, removal and recovery of phosphate and nitrate from
   water by a novel lanthanum(iii)-Dowex M4195 polymeric ligand exchanger}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
Year = {{2018}},
Volume = {{4}},
Number = {{3}},
Pages = {{421-427}},
Month = {{MAR}},
Abstract = {{A new polymeric ligand exchanger (PLE) has been designed to remove
   phosphate and nitrate from water under various conditions. This PLE was
   prepared by passing a LaCl3 solution through a column of Dowex M4195
   cation-exchange resin. Uptake of phosphate and nitrate from water by
   this La(iii)-Dowex resin was investigated in the column mode. The
   La(iii)-Dowex resin was able to remove phosphate and nitrate efficiently
   from water down to <0.1 ppm with little interference from other anions
   such as chloride and sulphate. The column could be readily regenerated
   by eluting with 0.1 M hydrochloric acid.}},
