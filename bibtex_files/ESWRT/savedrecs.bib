Author = {Chen, Siming and Harb, Moustapha and Sinha, Pooja and Smith, Adam L.},
Title = {{Emerging investigators series: revisiting greenhouse gas mitigation from
   conventional activated sludge and anaerobic-based wastewater treatment
   systems}},
