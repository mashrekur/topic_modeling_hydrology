@article{ ISI:000454086700008,
Author = {Lone, Saifullah and Yoon, Dong Ho and Lee, Hyangmoo and Cheong, In Woo},
Title = {{Gelatin-chitosan hydrogel particles for efficient removal of Hg(ii) from
   wastewater}},
Journal = {{ENVIRONMENTAL SCIENCE-WATER RESEARCH \& TECHNOLOGY}},
