@article{ ISI:000365797700012,
Author = {Tang, Zhenghong and Gu, Yue and Drahota, Jeff and LaGrange, Ted and
   Bishop, Andy and Kuzila, Mark S.},
Title = {{Using Fly Ash as a Marker to Quantify Culturally-Accelerated Sediment
   Accumulation in Playa Wetlands}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2015}},
Volume = {{51}},
Number = {{6}},
Pages = {{1643-1655}},
Month = {{DEC}},
Abstract = {{Wetlands in the Rainwater Basin in Nebraska are vulnerable to sediment
   accumulation from the surrounding watershed. Sediment accumulation has a
   negative impact on wetland quality by decreasing the depth and volume of
   water stored, and the plant community species composition and density
   growing in the wetland. The objective of this study was to determine the
   amount of sediment that has accumulated in five selected wetlands in the
   Rainwater Basin in Nebraska. Soil cores were taken at five or six
   locations along transects across each wetland. This study used the fly
   ash, which is generated by coal-burning locomotives that were present
   generally in the late 1800s and early 1900s, as a marker to quantify the
   sediment deposition rates. The cores were divided into 5cm sections and
   the soils were analyzed using a fly ash extraction and identification
   technique. Results indicate that the average depth of sediment ranged
   from 23.00 to 38.00cm. The annual average depth of sediment accumulation
   ranged from 0.18cm/yr to 0.29cm/yr. The annual sediment accumulation
   rate from both wind erosion and water erosion in these five sampling
   wetlands was between 1.946 and 3.225kg/m(2)/yr. The results of this
   research can be used to develop restoration plans for wetlands. The fly
   ash testing technology can also be applied to other areas with the
   railroads across the United States.}},
