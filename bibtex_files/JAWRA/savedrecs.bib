@article{ ISI:000477979100003,
Author = {Sridhar, Venkataramana and Modi, Parthkumar and Billah, Mirza M. and
   Valayamkunnath, Prasanth and Goodall, Jonathan L.},
Title = {{Precipitation Extremes and Flood Frequency in a Changing Climate in
   Southeastern Virginia}},
