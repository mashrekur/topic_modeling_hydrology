@article{ ISI:000369999100010,
Author = {Ganguli, Poulomi and Ganguly, Auroop R.},
Title = {{ROBUSTNESS OF METEOROLOGICAL DROUGHTS IN DYNAMICALLY DOWNSCALED CLIMATE
   SIMULATIONS}},
