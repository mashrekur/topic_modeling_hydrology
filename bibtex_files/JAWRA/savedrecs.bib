@article{ ISI:000400598400010,
Author = {Aulenbach, Brent T. and Landers, Mark N. and Musser, Jonathan W. and
   Painter, Jaime A.},
Title = {{EFFECTS OF IMPERVIOUS AREA AND BMP IMPLEMENTATION AND DESIGN ON STORM
   RUNOFF AND WATER QUALITY IN EIGHT SMALL WATERSHEDS}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2017}},
Volume = {{53}},
Number = {{2}},
Pages = {{382-399}},
Month = {{APR}},
Abstract = {{The effects of increases in effective impervious area (EIA) and the
   implementation of water quality protection designed detention pond best
   management practices (BMPs) on storm runoff and stormwater quality were
   assessed in Gwinnett County, Georgia, for the period 2001-2008. Trends
   among eight small watersheds were compared, using a time trend study
   design. Significant trends were detected in three storm hydrologic
   metrics and in five water quality constituents that were adjusted for
   variability in storm characteristics and climate. Trends in EIA ranged
   from 0.10 to 1.35, and changes in EIA treated by BMPs ranged from 0.19
   to 1.32; both expressed in units of percentage of drainage area per
   year. Trend relations indicated that for every 1\% increase in watershed
   EIA, about 2.6, 1.1, and 1.5\% increases in EIA treated by BMPs would be
   required to counteract the effects of EIA added to the watersheds on
   peak streamflow, stormwater yield, and storm streamflow runoff,
   respectively. Relations between trends in EIA, BMP implementation, and
   water quality were counterintuitive. This may be the result of (1)
   changes in constituent inputs in the watersheds, especially downstream
   of areas treated by BMPs; (2) BMPs may have increased the duration of
   stormflow that results in downstream channel erosion; and/or (3)
   spurious relationships between increases in EIA, BMP implementation, and
   constituent inputs with development rates.}},
