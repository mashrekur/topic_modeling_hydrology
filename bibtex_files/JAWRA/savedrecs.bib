@article{ ISI:000400598400004,
Author = {Follum, Michael L. and Tavakoly, Ahmad A. and Niemann, Jeffrey D. and
   Snow, Alan D.},
Title = {{AUTORAPID: A MODEL FOR PROMPT STREAMFLOW ESTIMATION AND FLOOD INUNDATION
   MAPPING OVER REGIONAL TO CONTINENTAL EXTENTS}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2017}},
Volume = {{53}},
Number = {{2}},
Pages = {{280-299}},
Month = {{APR}},
Abstract = {{This article couples two existing models to quickly generate flow and
   flood-inundation estimates at high resolutions over large spatial
   extents for use in emergency response situations. Input data are gridded
   runoff values from a climate model, which are used by the Routing
   Application for Parallel computatIon of Discharge (RAPID) model to
   simulate flow rates within a vector river network. Peak flows in each
   river reach are then supplied to the AutoRoute model, which produces
   raster flood inundation maps. The coupled tool (AutoRAPID) is tested for
   the June 2008 floods in the Midwest and the April-June 2011 floods in
   the Mississippi Delta. RAPID was implemented from 2005 to 2014 for the
   entire Mississippi River Basin (1.2 million river reaches) in
   approximately 45 min. Discretizing a 230,000-km(2) area in the Midwest
   and a 109,500-km(2) area in the Mississippi Delta into thirty-nine 1
   degrees by 1 degrees tiles, AutoRoute simulated a high-resolution
   (similar to 10 m) flood inundation map in 20 min for each tile. The
   hydrographs simulated by RAPID are found to perform better in reaches
   without influences from unrepresented dams and without backwater
   effects. Flood inundation maps using the RAPID peak flows vary in
   accuracy with F-statistic values between 38.1 and 90.9\%. Better
   performance is observed in regions with more accurate peak flows from
   RAPID and moderate to high topographic relief.}},
