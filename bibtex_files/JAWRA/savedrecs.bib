@article{ ISI:000387170400002,
Author = {Shanley, James B. and Chalmers, Ann T. and Mack, Thomas J. and Smith,
   Thor E. and Harte, Philip T.},
Title = {{GROUNDWATER LEVEL TRENDS AND DRIVERS IN TWO NORTHERN NEW ENGLAND GLACIAL
   AQUIFERS}},
