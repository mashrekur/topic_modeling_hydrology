Author = {Shastry, Apoorva and Egbert, Ryan and Aristizabal, Fernando and Luo,
   Cehong and Yu, Cheng-Wei and Praskievicz, Sarah},
Title = {{Using Steady-State Backwater Analysis to Predict Inundated Area from
   National Water Model Streamflow Simulations}},
