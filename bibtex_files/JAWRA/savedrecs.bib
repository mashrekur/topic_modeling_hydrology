@article{ ISI:000451895200013,
Author = {Nicholson, Christopher and Shinker, Jacqueline J. and Hanway, Veronica
   M. and Zavala, Samantha},
Title = {{The Influence of Atmospheric Circulation on Abnormal Snowpack Melt-Out
   Events and Drought in Wyoming}},
