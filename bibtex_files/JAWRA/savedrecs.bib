@article{ ISI:000359017800008,
Author = {Kalcic, Margaret M. and Frankenberger, Jane and Chaubey, Indrajeet and
   Prokopy, Linda and Bowling, Laura},
Title = {{Adaptive Targeting: Engaging Farmers to Improve Targeting and Adoption
   of Agricultural Conservation Practices}},
