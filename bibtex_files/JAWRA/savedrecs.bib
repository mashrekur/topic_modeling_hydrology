@article{ ISI:000502598600001,
Author = {Ercan, Mehmet B. and Maghami, Iman and Bowes, Benjamin and Morsy,
   Mohamed M. and Goodall, Jonathan L.},
Title = {{Estimating Potential Climate Change Effects on the Upper Neuse Watershed
   Water Balance Using the SWAT Model}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
