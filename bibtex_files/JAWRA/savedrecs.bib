@article{ ISI:000362369900014,
Author = {Villarini, Gabriele and Scoccimarro, Enrico and White, Kathleen D. and
   Arnold, Jeffrey R. and Schilling, Keith E. and Ghosh, Joyee},
Title = {{Projected Changes in Discharge in an Agricultural Watershed in Iowa}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
