@article{ ISI:000362369900004,
Author = {Keum, Jongho and Kaluarachchi, Jagath J.},
Title = {{Calibration and Uncertainty Analysis Using the Sparrow Model for
   Dissolved-Solids Transport in the Upper Colorado River Basin}},
