@article{ ISI:000440411200009,
Author = {Alcantara, Michael A. Souffront and Kesler, Christian and Stealey,
   Michael J. and Nelson, E. James and Ames, Daniel P. and Jones, Norm L.},
Title = {{Cyberinfrastructure and Web Apps for Managing and Disseminating the
   National Water Model}},
