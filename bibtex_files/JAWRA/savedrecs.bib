@article{ ISI:000385007100013,
Author = {Lopez, Sonya R. and Maxwell, Reed M.},
Title = {{IDENTIFYING URBAN FEATURES FROM LIDAR FOR A HIGH-RESOLUTION URBAN
   HYDROLOGIC MODEL}},
