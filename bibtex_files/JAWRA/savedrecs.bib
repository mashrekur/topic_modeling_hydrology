@article{ ISI:000428882400007,
Author = {Bigham, Kari A. and Moore, Trisha L. and Vogel, Jason R. and Keane, Tim
   D.},
Title = {{Repeatability, Sensitivity, and Uncertainty Analyses of the BANCS Model
   Developed to Predict Annual Streambank Erosion Rates}},
