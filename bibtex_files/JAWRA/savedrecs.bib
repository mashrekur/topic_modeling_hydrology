@article{ ISI:000446264500011,
Author = {Hale, Rebecca L. and Flint, Courtney G. and Jackson-Smith, Douglas and
   Endter-Wada, Joanna},
Title = {{Social Dimensions of Urban Flood Experience, Exposure, and Concern}},
