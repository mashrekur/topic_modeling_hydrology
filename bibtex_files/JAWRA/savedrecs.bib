@article{ ISI:000470033600005,
Author = {Jones, C. Nathan and Ameli, Ali and Neff, Brian P. and Evenson, Grey R.
   and McLaughlin, Daniel L. and Golden, Heather E. and Lane, Charles R.},
Title = {{Modeling Connectivity of Non-floodplain Wetlands: Insights, Approaches,
   and Recommendations}},
