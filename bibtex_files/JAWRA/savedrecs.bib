@article{ ISI:000362369900019,
Author = {Evans, Daniel M. and Zipper, Carl E. and Hester, Erich T. and
   Schoenholtz, Stephen H.},
Title = {{Hydrologic Effects of Surface Coal Mining in Appalachia (US)}},
