@article{ ISI:000389307000010,
Author = {Kea, Kandace and Dymond, Randel and Campbell, Warren},
Title = {{AN ANALYSIS OF PATTERNS AND TRENDS IN UNITED STATES STORMWATER UTILITY
   SYSTEMS}},
