@article{ ISI:000352789900013,
Author = {VanLandeghem, Matthew M. and Farooqi, Mukhtar and Southard, Greg M. and
   Patino, Reynaldo},
Title = {{SPATIOTEMPORAL ASSOCIATIONS OF RESERVOIR NUTRIENT CHARACTERISTICS AND
   THE INVASIVE, HARMFUL ALGA PRYMNESIUM PARVUM IN WEST TEXAS}},
