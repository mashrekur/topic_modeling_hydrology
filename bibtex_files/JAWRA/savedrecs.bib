@article{ ISI:000440411200004,
Author = {Zheng, Xing and Tarboton, David G. and Maidment, David R. and Liu, Yan
   Y. and Passalacqua, Paola},
Title = {{River Channel Geometry and Rating Curve Estimation Using Height above
   the Nearest Drainage}},
