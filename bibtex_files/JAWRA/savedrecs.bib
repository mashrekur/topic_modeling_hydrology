@article{ ISI:000365797700010,
Author = {Hancock, Gregory and Hamilton, Stuart E. and Stone, Monica and Kaste,
   Jim and Lovette, John},
Title = {{A Geospatial Methodology to Identify Locations of Concentrated Runoff
   from Agricultural Fields}},
