@article{ ISI:000365797700004,
Author = {Pendergrass, David and McFarland, Anne and Hauck, Larry},
Title = {{Instream Bacteria Influences from Bird Habitation of Bridges}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
