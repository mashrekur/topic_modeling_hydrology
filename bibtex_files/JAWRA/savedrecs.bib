@article{ ISI:000387170400008,
Author = {Rice, Clif'ford P. and Bialek, Krystyna and Hapeman, Cathleen J. and
   McCarty, Gregory W.},
Title = {{ROLE OF RIPARIAN AREAS IN ATMOSPHERIC PESTICIDE DEPOSITION AND ITS
   POTENTIAL EFFECT ON WATER QUALITY}},
