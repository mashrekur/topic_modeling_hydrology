@article{ ISI:000476188400001,
Author = {Murshed, Sonia Binte and Rahman, Rezaur and Kaluarachchi, Jagath J.},
Title = {{Changes in Hydrology of the Ganges Delta of Bangladesh and Corresponding
   Impacts on Water Resources}},
