@article{ ISI:000369999100018,
Author = {White, Michael and Gambone, Marilyn and Yen, Haw and Daggupati, Prasad
   and Bieger, Katrin and Deb, Debjani and Arnold, Jeff},
Title = {{DEVELOPMENT OF A CROPLAND MANAGEMENT DATASET TO SUPPORT US SWAT
   ASSESSMENTS}},
