@article{ ISI:000365797700014,
Author = {Villines, Jonathan A. and Agouridis, Carmen T. and Warner, Richard C.
   and Barton, Christopher D.},
Title = {{Using GIS to Delineate Headwater Stream Origins in the Appalachian
   Coalfields of Kentucky}},
