@article{ ISI:000423819300012,
Author = {Schifman, L. A. and Tryby, M. E. and Berner, J. and Shuster, W. D.},
Title = {{Managing Uncertainty in Runoff Estimation with the US Environmental
   Protection Agency National Stormwater Calculator}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
