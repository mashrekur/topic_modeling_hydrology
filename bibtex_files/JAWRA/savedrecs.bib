@article{ ISI:000407043700008,
Author = {Walker, Ella L. and Anderson, Aspen M. and Read, Laura K. and Hogue,
   Terri S.},
Title = {{Water Use for Hydraulic Fracturing of Oil and Gas in the South Platte
   River Basin, Colorado}},
