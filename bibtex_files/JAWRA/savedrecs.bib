@article{ ISI:000387168800005,
Author = {Blodgett, David and Read, Emily and Lucido, Jessica and Slawecki, Tad
   and Young, Dwane},
Title = {{AN ANALYSIS OF WATER DATA SYSTEMS TO INFORM THE OPEN WATER DATA
   INITIATIVE}},
