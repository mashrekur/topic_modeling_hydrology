@article{ ISI:000477979100018,
Author = {Baker, Sarah A. and Wood, Andrew W. and Rajagopalan, Balaji},
Title = {{Developing Subseasonal to Seasonal Climate Forecast Products for
   Hydrology and Water Management}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
