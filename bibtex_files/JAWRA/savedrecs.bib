@article{ ISI:000440411200013,
Author = {Sefick, Stephen A. and Kosnicki, Ely and Paller, Michael H. and
   Feminella, Jack W.},
Title = {{Hydrogeomorphic Reference Condition and Its Relationship
   withMacroinvertebrate Assemblages in Southeastern US Sand Hills Streams}},
