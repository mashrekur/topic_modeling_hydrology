@article{ ISI:000389307000004,
Author = {Safeeq, Mohammad and Hunsaker, Carolyn T.},
Title = {{CHARACTERIZING RUNOFF AND WATER YIELD FOR HEADWATER CATCHMENTS IN THE
   SOUTHERN SIERRA NEVADA}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
