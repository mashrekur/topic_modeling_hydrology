@article{ ISI:000261334000001,
Author = {Azar, Amir E. and Ghedira, Hosni and Romanov, Peter and Mahani,
   Shayesteh and Tedesco, Marco and Khanbilvardi, Reza},
Title = {{Application of Satellite Microwave Images in Estimating Snow Water
   Equivalent}},
