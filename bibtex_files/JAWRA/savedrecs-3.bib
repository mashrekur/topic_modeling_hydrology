@article{ ISI:000270550800011,
Author = {Newlin, Jessica T. and Johnson, Peggy A.},
Title = {{Adaptive Management of Stream Channel Maintenance at Bridge Crossings in
   the Northern Tier Region, Pennsylvania1}},
