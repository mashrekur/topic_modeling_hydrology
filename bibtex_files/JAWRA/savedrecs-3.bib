@article{ ISI:000243612100007,
Author = {Johnson, Billy E. and Gerald, Terry K.},
Title = {{Development of nutrient submodules for use in the gridded surface
   subsurface hydrologic analysis (GSSHA) distributed watershed model}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2006}},
Volume = {{42}},
