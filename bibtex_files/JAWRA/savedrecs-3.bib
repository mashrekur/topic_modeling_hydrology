@article{ ISI:000274175400013,
Author = {Medlin, Linda K. and Diercks, Sonja and Beszteri, Sara},
Title = {{MINI REVIEW: PROBES FOR DETECTING PRYMNESIUM PARVUM AND PRELIMINARY
   RESULTS FROM GENE EXPRESSION STUDIES}},
