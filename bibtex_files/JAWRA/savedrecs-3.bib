@article{ ISI:000263027400002,
Author = {Chalew, Talia E. A. and Halden, Rolf U.},
Title = {{ENVIRONMENTAL EXPOSURE OF AQUATIC AND TERRESTRIAL BIOTA TO TRICLOSAN AND
   TRICLOCARBAN}},
