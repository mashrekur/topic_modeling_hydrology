@article{ ISI:000268531900015,
Author = {Zhang, Wei and Keller, Arturo A. and Yue, Dapan and Wang, Xuejun},
Title = {{Management of Urban Road Runoff Containing PAHs: Probabilistic Modeling
   and Its Application in Beijing, China(1)}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2009}},
