@article{ ISI:000249488500005,
Author = {Opsahl, Stephen P. and Chapal, Scott E. and Hicks, David W. and Wheeler,
   Christopher K.},
Title = {{Evaluation of ground-water and surface-water exchanges using streamflow
   difference analyses}},
