@article{ ISI:000263027400005,
Author = {Wu, Jianyong and Rees, Paula and Storrer, Sara and Alderisio, Kerri and
   Dorner, Sarah},
Title = {{FATE AND TRANSPORT MODELING OF POTENTIAL PATHOGENS: THE CONTRIBUTION
   FROM SEDIMENTS}},
