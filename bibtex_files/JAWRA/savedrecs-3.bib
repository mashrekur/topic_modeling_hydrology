@article{ ISI:000249488500007,
Author = {Shamir, Eylon and Meko, David M. and Graham, Nicholas E. and
   Georgakakos, Konstantine P.},
Title = {{Hydrologic model framework for water resources planning in the Santa
   Cruz River, southern Arizona}},
