@article{ ISI:000268531900003,
Author = {Leh, Mansour D. and Chaubey, Indrajeet},
Title = {{GIS-Based Predictive Models of Hillslope Runoff Generation Processes(1)}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2009}},
