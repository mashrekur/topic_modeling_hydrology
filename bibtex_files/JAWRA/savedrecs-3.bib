@article{ ISI:000243612100009,
Author = {Simunek, Jirka and Jacques, Diederik and van Genuchten, Martinus Th. and
   Mallants, Dirk},
Title = {{Multicomponent geochemical transport modeling using HYDRUS-1D and HP1}},
