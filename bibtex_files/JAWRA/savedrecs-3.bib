@article{ ISI:000243612100018,
Author = {Sennatt, Kelly M. and Salant, Nira L. and Renshaw, Carl E. and
   Magilligan, Francis J.},
Title = {{Assessment of methods for measuring embeddedness: Application to
   sedimentation in flow regulated streams}},
