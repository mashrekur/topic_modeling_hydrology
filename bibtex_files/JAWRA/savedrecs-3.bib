@article{ ISI:000240644500004,
Author = {Zhai, Guofang and Fukuzono, Teruki and Ikeda, Saburo},
Title = {{An empirical model of fatalities and injuries due to floods in Japan}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2006}},
Volume = {{42}},
Number = {{4}},
Pages = {{863-875}},
Month = {{AUG}},
Abstract = {{This paper provides a framework for analyzing flood fatalities and
   injuries, and it describes the derivation of a generalized flood risk
   (i.e., flood consequences and their probabilities) function by
   introducing an integrated index (the number of residential buildings
   affected by a flood) that represents the major change in the power
   relation among the flood intensity, regional vulnerability, and
   resilience. Both the probabilities and the numbers of fatalities and
   injuries clearly increase significantly after the flood severity (in
   terms of the number of inundated buildings) passes a branch point. Below
   the branch point, it is still possible for fatalities and injuries to
   occur because of the variability in the data and the uncertainty in the
   predicted fatality values. The empirical models of fatalities and
   injuries due to floods in Japan suggested the usefulness in predicting
   fatalities and injuries and evaluating the efficacy of the warning or
   other emergency response measures.}},
