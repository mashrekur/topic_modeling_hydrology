@article{ ISI:000264564400009,
Author = {Chen, Jie and Zhang, Xun-chang and Liu, Wen-zhao and Li, Zhi},
Title = {{EVALUATING AND EXTENDING CLIGEN PRECIPITATION GENERATION FOR THE LOESS
   PLATEAU OF CHINA}},
