@article{ ISI:000259865100005,
Author = {Wang, Shengping and Zhang, Zhiqiang and Sun, Ge and McNulty, Steven G.
   and Zhang, Huayong and Li, Jianlao and Zhang, Manliang},
Title = {{Long-term streamflow response to climatic variability in the Loess
   Plateau, China}},
