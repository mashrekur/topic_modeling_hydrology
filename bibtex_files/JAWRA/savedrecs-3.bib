@article{ ISI:000243612100019,
Author = {Sudheer, K. P. and Chaubey, Indrajeet and Garg, Vijay},
Title = {{Lake water quality assessment from Landsat thematic mapper data using
   neural network: An approach to optimal band combination selection}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2006}},
Volume = {{42}},
