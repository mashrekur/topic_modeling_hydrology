@article{ ISI:000272439300014,
Author = {Todd, M. Jason and Vellidis, George and Lowrance, R. Richard and
   Pringle, Catherine M.},
Title = {{High Sediment Oxygen Demand Within an Instream Swamp in Southern
   Georgia: Implications for Low Dissolved Oxygen Levels in Coastal
   Blackwater Streams1}},
