@article{ ISI:000276223200002,
Author = {Opperman, Jeffrey J. and Luster, Ryan and McKenney, Bruce A. and
   Roberts, Michael and Meadows, Amanda Wrona},
Title = {{Ecologically Functional Floodplains: Connectivity, Flow Regime, and
   Scale}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
