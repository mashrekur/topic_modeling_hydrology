@article{ ISI:000263027400008,
Author = {Brown, Juliane B. and Battaglin, William A. and Zuellig, Robert E.},
Title = {{LAGRANGIAN SAMPLING FOR EMERGING CONTAMINANTS THROUGH AN URBAN STREAM
   CORRIDOR IN COLORADO}},
