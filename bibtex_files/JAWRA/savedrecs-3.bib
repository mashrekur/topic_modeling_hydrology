@article{ ISI:000272439300013,
Author = {Griffith, Michael B. and Daniel, F. Bernard and Morrison, Matthew A. and
   Troyer, Michael E. and Lazorchak, James M. and Schubauer-Berigan, Joseph
   P.},
Title = {{Linking Excess Nutrients, Light, and Fine Bedded Sediments to Impacts on
   Faunal Assemblages in Headwater Agricultural Streams1}},
Journal = {{JOURNAL OF THE AMERICAN WATER RESOURCES ASSOCIATION}},
Year = {{2009}},
Volume = {{45}},
