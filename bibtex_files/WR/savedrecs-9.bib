@article{ ISI:000089806300022,
Author = {Szymanski, A and Wyrwas, B and Swit, Z and Jaroszynski, T and
   Lukaszewski, Z},
Title = {{Biodegradation of fatty alcohol ethoxylates in the continuous flow
   activated sludge test}},
Journal = {{WATER RESEARCH}},
