@article{ ISI:000090046700011,
Author = {Balasubramanian, S and Pugalenthi, V},
Title = {{A comparative study of the determination of sulphide in tannery waste
   water by ion selective electrode (ISE) and iodimetry}},
Journal = {{WATER RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{17}},
Pages = {{4201-4206}},
Month = {{DEC}},
Abstract = {{The determination of sulphide present in tannery waste water by two
   different methods namely, iodimetry and ion selective electrode (ISE)
