@article{ ISI:000087543900007,
Author = {Zhou, HY and Wong, MH},
Title = {{Accumulation of sediment-sorbed PCBS in tilapia}},
Journal = {{WATER RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{11}},
Pages = {{2905-2914}},
Month = {{AUG}},
Abstract = {{Tilapia were exposed to sediment and water contaminated with PCBs in the
   laboratory. Ingestion of sediment and/or contact of contaminated
   particulates were the predominant pathways for sediment-sorbed PCBs
   accumulation in tilapia rather than water. Tilapia exposed to spiked
   sediment and river sediment accumulated significantly higher levels of
   PCBs (27.5 and 3.9 mu g/g lipids, respectively) than those exposed to
   contaminated water (1.6 mu g/g lipid). The biota-sediment accumulation
   factors (BSAFs) of PCBs domains were less than 2.5. PCBs congeners
   containing four to six chlorine atoms on the biphenyl rings showed a
   relatively higher accumulation potential. Minor effects of lipid
   contents, weight and length of fish were observed on PCBs accumulation.
   (C) 2000 Elsevier Science Ltd. All rights reserved.}},
