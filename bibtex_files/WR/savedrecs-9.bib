@article{ ISI:000086089700020,
Author = {Jeon, CO and Park, JM},
Title = {{Enhanced biological phosphorus removal in a sequencing batch reactor
   supplied with glucose as a sole carbon source}},
Journal = {{WATER RESEARCH}},
