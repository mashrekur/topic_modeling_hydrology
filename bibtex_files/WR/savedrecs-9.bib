@article{ ISI:000085521800017,
Author = {Gupta, VK and Srivastava, SK and Tyagi, R},
Title = {{Design parameters for the treatment of phenolic wastes by carbon columns
   (obtained from fertilizer waste material)}},
Journal = {{WATER RESEARCH}},
