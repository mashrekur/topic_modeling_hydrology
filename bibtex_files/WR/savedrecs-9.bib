@article{ ISI:000084601300041,
Author = {Jobbagy, A and Nemeth, N and Altermatt, RH and Samhaber, WM},
Title = {{Encouraging filament growth in an activated sludge treatment plant of
   the chemical industry}},
Journal = {{WATER RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{2}},
Pages = {{699-703}},
Month = {{FEB}},
Abstract = {{An activated sludge treatment plant of Sandoz and Ciba-Geigy (currently
   Novartis) had difficulty meeting the effluent total suspended solids
   (TSS) permit requirement of 40 mg l(-1). The plant commonly contained
   small, weak flocs with a characteristic lack of filaments. It was
   hypothesized that optimization of the plant's bioreactor arrangement
   (i.e. the manner in which the feed was introduced into the six activated
   sludge basins connected in series) would lead to stronger activated
   sludge flocs with filamentous backbones, and that these flocs would
   provide a clearer supernatant and be less sensitive to changes in the
   wastewater composition. In order to optimize the bioreactor arrangement,
   bench-scale experiments were carried our using two model activated
   sludge systems operated simultaneously. A belter floc structure and
   consequently lower effluent TSS concentrations were achieved in the
   system in which the influent was split between basins 1 and 2 rather
   than being introduced into basin 1. Furthermore, an excess sludge
   removal strategy of controlling the MLSS concentration proved to be more
   favorable than that of maintaining a stable solids retention time (SRT).
   Through implementation of the results in the full-scale plant, the
   effluent TSS values were stabilized well below the permitted level. (C)
   1999 Elsevier Science Ltd. All rights reserved.}},
