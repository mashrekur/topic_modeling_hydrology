@article{ ISI:000085360800039,
Author = {Larsen, T},
Title = {{Measuring the variations of the apparent settling velocity for fine
   particles}},
Journal = {{WATER RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{4}},
Pages = {{1417-1418}},
Month = {{MAR}},
Abstract = {{This note establishes the fact that the settling velocity for fine
   flocculent particles in flowing aquatic systems vary considerably and
   the settling velocity should therefore be understood as a variable which
   varies temporally and spatially in the flow field. In the mathematical
   formulation the settling velocity of a suspension appears in the actual
   and local mass conservation equation as an apparent settling velocity. A
   new approach to measuring this variable settling velocity is mentioned
   and its relation to well-known principles. e.g. settling columns, is
   discussed. (C) 2000 Elsevier Science Ltd. All rights reserved.}},
