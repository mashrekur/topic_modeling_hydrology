@article{ ISI:000084822200024,
Author = {Zhou, QX and Gibson, CE and Foy, RH},
Title = {{Long-term changes of nitrogen and phosphorus loadings to a large lake in
   north-west Ireland}},
Journal = {{WATER RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{3}},
Pages = {{922-926}},
Month = {{FEB}},
Abstract = {{Lower Lough Erne is a large (109.5km(2)) eutrophic lake in north-west
   Ireland. Nutrient data from 14 years in the period 1974 to 1997 are
   analysed for temporal trends. Over the period of observation, soluble
   reactive phosphorus loadings have increased by approximately 2.0 tonnes
   SRP yr(-1) so that between 1974 and 1997 the mean concentration of the
   inflow has increased from 15 to 33 mu g SRP 1(-1). The increase is
   probably caused by diffuse agricultural sources. There is no temporal
   trend in inorganic nitrogen inputs. but the total nitrogen and total
   Kjeldhal N both show a weak upward trend with time. (C) 2000 Elsevier
   Science Ltd. All rights reserved.}},
