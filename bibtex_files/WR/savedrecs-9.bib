@article{ ISI:000086614600019,
Author = {McGinnis, BD and Adams, VD and Middlebrooks, EJ},
Title = {{Degradation of ethylene glycol in photo Fenton systems}},
Journal = {{WATER RESEARCH}},
Year = {{2000}},
Volume = {{34}},
Number = {{8}},
Pages = {{2346-2354}},
Month = {{JUN}},
Abstract = {{Ethylene glycol loss rate constants of 1.0 +/- 0.40 h(-1) were achieved
   in photo Fenton systems containing concentrations of 1000 mg/l ethylene
   glycol. Ethylene glycol was converted to formic acid resulting in a loss
   of calculated chemical oxygen demand. Iron was not catalytic in the
   system which may be due to the formation of ferric oxalate complexes.
   The system was inhibited by elevated concentrations of sulfate and a
   phosphate buffer solution. The optimal pH was 3.0, with significant
   decreases in the degradation rates below 2.8 and above 3.2. increased
   TOC loss was noted in similar UV/H2O2 systems indicating a potential for
   increased mineralization in optimized photo Fenton systems with longer
   residence times. (C) 2000 Elsevier Science Ltd. All rights reserved.}},
