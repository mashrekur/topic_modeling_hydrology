@article{ ISI:000233812000024,
Author = {Khan, E and Sy-Savane, O and Jittawattanarat, R},
Title = {{Application of commercial biochemical oxygen demand inocula for
   biodegradable dissolved organic carbon determination}},
Journal = {{WATER RESEARCH}},
