@article{ ISI:000229351400020,
Author = {Park, SK and Bielefeldt, AR},
Title = {{Non-ionic surfactant flushing of pentachlorophenol from
   NAPL-contaminated soil}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{7}},
Pages = {{1388-1396}},
Month = {{APR}},
Abstract = {{Column studies were conducted to assess the suitability of a non-ionic
   surfactant Tergitol NP-10 (TNP10) for washing pentachlorophenol (PCP)
   from soil and non-aqueous phase liquids (NAPLs). Flushing of 50 and 200
   pore volumes of 5 g/L TNP10 was required to exhaust the surfactant
   sorption capacity of the soil and soil plus NAPL, respectively. The
   sorption of surfactant to the soil in the columns was four times greater
   than the quantity previously observed in batch tests. Flushing with 5g/L
   TNP10 removed 71-79\% of the 200 mg/kg soil-sorbed PCP after 160 pore
   volumes compared to 0.7-2\% PCP removal without surfactant. In columns
   additionally containing 0.2\% and 0.4\% PCP-contaminated heavy oil NAPL,
   the PCP removal efficiency after flushing 200 pore volumes of 5 g/L
   TNP10 was nearly 100\%. Therefore, removal of the PCP was more efficient
   in the NAPL-containing columns, potentially due to competition of the
   NAPL for PCP sorption sites. Rate-limited desorption of PCP and TNP10
   likely occurred. (c) 2005 Elsevier Ltd. All rights reserved.}},
