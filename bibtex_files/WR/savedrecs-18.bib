@article{ ISI:000227273000023,
Author = {Son, H and Cho, M and Kim, J and Oh, B and Chung, HM and Yoon, J},
Title = {{Enhanced disinfection efficiency of mechanically mixed oxidants with
   free chlorine}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{4}},
Pages = {{721-727}},
Month = {{FEB}},
Abstract = {{To the best of our knowledge, this study is the first investigation to
   be performed into the potential benefits of mechanically mixed
   disinfectants in controlling bacterial inactivation. The purpose of this
   study was to evaluate the disinfection efficiency of mechanically mixed
   oxidants with identical oxidant concentrations, which were made by
   adding small amounts of subsidiary oxidants, namely ozone (O-3),
   chlorine dioxide (ClO2), hydrogen peroxide (H2O2) and chlorite (ClO2-),
   to free available chlorine (Cl-2), using Bacillus subtilis spores as the
   indicator microorganisms. The mechanically mixed oxidants containing
   Cl-2/O-3, Cl-2/ClO2 and Cl-2/ClO2- showed enhanced efficiencies (of up
   to 52\%) in comparison with Cl-2 alone, whereas no significant
   difference was observed between the mixed oxidant, Cl-2/H2O2, and Cl-2
   alone. This enhanced disinfection efficiency can be explained by the
   synergistic effect of the mixed oxidant itself and the effect of
   intermediates such as ClO2-/ClO2, which are generated from the reaction
   between an excess of Cl-2 and a small amount of O-3/ClO2-. Overall, this
   study suggests that mechanically mixed oxidants incorporating excess
   chlorine can constitute a new and moderately efficient method of
   disinfection. (C) 2004 Elsevier Ltd. All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Yoon, J (Reprint Author), Seoul Natl Univ, Sch Chem Engn, Coll Engn, San 56-1 Sillim Dong, Seoul 151742, South Korea.
