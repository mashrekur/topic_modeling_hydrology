@article{ ISI:000234442600017,
Author = {Fiore, MD and Neilan, BA and Copp, JN and Rodrigues, JLM and Tsai, SM
   and Lee, H and Trevors, JT},
Title = {{Characterization of nitrogen-fixing cyanobacteria in the Brazilian
   Amazon floodplain}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{20}},
Pages = {{5017-5026}},
Month = {{DEC}},
Abstract = {{The diversity of the free-living nitrogen-fixing cyanobacterial
   community in the floodplain sediments along the Solimoes and Amazon
   Rivers and some of their tributaries (Japura, Negro and Madeira) was
   investigated. Five cyanobacterial genera were morphologically
   identified, four of which (Nostoc, Calothrix, Cylindrospermum and
   Fischerella) have not previously been isolated from the Brazilian Amazon
   floodplain. Nostoc strains were the most commonly found
   heterocyst-forming cyanobacteria. Five strains (N. muscorum CENA18 and
   CENA61, N. piscinale CENA21, Cylindrospermum sp. CENA33 and Fischerella
   sp. CENA19) were selected for growth measurement, ability to fix N-2 and
   phylogenetic analysis, based on their widespread distribution and
   morphological distinction. Molecular analyses employing 16S rRNA
   sequences indicated that some of the isolates may represent novel
   cyanobacterial species. Dinitrogen fixed by these strains was measured
   indirectly as acetylene reduction activity and ranged from 11.5 to 22.2
   nmol C2H4 mu g Chl a(-1) h(-1). These results provide evidence of
   widespread and importance of nitrogen-fixing cyanobacteria as a source
   of N inputs in the Amazonian ecosystem. (c) 2005 Elsevier Ltd. All
   rights reserved.}},
