@article{ ISI:000230241200020,
Author = {Shang, C and Qi, YN and Xie, L and Liu, W and Yang, X},
Title = {{Kinetics of cyanogen chloride destruction by chemical reduction methods}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{10}},
Pages = {{2114-2124}},
Month = {{MAY}},
Abstract = {{In this study, membrane introduction mass spectrometry (MIMS) was
   applied to evaluate the kinetics of cyanogen chloride (CICN) destruction
   by chemical reduction methods, using thiosulfate, sulfite,
   metabisulfite, ferrous ions and zero-valent iron at various
   concentrations and pH. The CICN destruction followed second-order
   reaction kinetics in all cases of using sulfur compounds, though the
   second-order rate constants varied substantially from approximately
   0.3-25.7 M-1 s(-1) under different experimental conditions. The
   destruction of CICN was primarily attributable to the chemical reductiom
   pathway. Hydroxide-assisted CICN hydrolysis was only significant at pH 9
   and also when the observed reduction rate was relatively slow. The
   second-order rate constants achieved by sulfur(IV) compounds in the form
   of sulfite were found to be higher than those obtained with thiosulfate
   and S(IV) compounds in the form of bisulfite. Ferrous ions and
   zero-valent iron demonstrated slow or no CICN reduction up to dosages of
   1000 mg L-1 and 100gL(-1), respectively. These findings suggest that
   applying moderately high dosages of S(IV) compounds under neutral or
   alkali conditions with sufficient contact time is required for
   wastewater CICN destruction. In addition, CICN losses during long-term
   preservation with excess reducing sulfur compounds prior to analysis can
   be substantial and should be avoided. (c) 2005 Elsevier Ltd. All rights
   reserved.}},
