@article{ ISI:000227273000003,
Author = {Park, D and Yun, YS and Jo, JH and Park, JM},
Title = {{Mechanism of hexavalent chromium removal by dead fungal biomass of
   Aspergillus niger}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{4}},
Pages = {{533-540}},
Month = {{FEB}},
Abstract = {{When synthetic wastewater containing Cr(VI) was placed in contact with
   the dead fungal biomass of Aspergillus niger, the Cr(VI) was completely
   removed from aqueous solution, whereas Cr(III), which was not initially
   present, appeared in aqueous solution. Desorption and X-ray
   photoelectron spectroscopy (XPS) studies showed that most of the Cr
   bound on the biomass was in trivalent form. These results indicated that
   the main mechanism of Cr(VI) removal was a redox reaction between Cr(VI)
   and the dead fungal biomass, which is quite different from previously
   reported mechanisms. The influences of contact time, pH, Cr(VI)
   concentration, biomass concentration and temperature on Cr(VI) removal
   were also evaluated. The Cr(VI) removal rate increased with a decrease
   in pH and with increases in Cr(VI) concentration, biomass concentration
   and temperature. Although removal kinetics was dependent on the
   experimental conditions, Cr(VI) was completely removed in the aqueous
   solution. In conclusion, a new mechanism of Cr(VI) removal by the dead
   fungal biomass has been proposed. From a practical viewpoint, this
   abundant and inexpensive dead fungal biomass has potential application
   in the conversion of toxic Cr(VI) into less toxic or nontoxic Cr(III).
   (C) 2004 Elsevier Ltd. All rights reserved.}},
