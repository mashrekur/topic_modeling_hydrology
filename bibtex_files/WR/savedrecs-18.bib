@article{ ISI:000234442600028,
Author = {Aryal, RK and Furumai, H and Nakajima, F and Boller, M},
Title = {{Dynamic behavior of fractional suspended solids and particle-bound
   polycyclic aromatic hydrocarbons in highway runoff}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{20}},
Pages = {{5126-5134}},
Month = {{DEC}},
Abstract = {{A long-term continuous runoff monitoring was carried out in a highway in
   Winterthur, Switzerland. The total suspended solids (TSS) samples were
   fractionated into fine (< 45 mu m) and coarse (> 45 mu m) fraction and
   their washoff behavior was studied. The fine and coarse fraction showed
   different washoff behaviors. During the runoff the concentration of the
   fine fraction was less fluctuated compared to coarse. The fluctuation of
   the coarse fraction was more influenced by TSS concentration. The PAH
   content measurement in fine fraction showed less fluctuation compared to
   the coarse fraction. The PAH content in the coarse fraction was found
   decreasing with increasing the coarse fraction contribution to TSS. (c)
   2005 Elsevier Ltd. All rights reserved.}},
