Affiliation = {{Vieira, MJ (Reprint Author), Univ Minho, Ctr Engn Biol, P-4710057 Braga, Portugal.
   Univ Minho, Ctr Engn Biol, P-4710057 Braga, Portugal.}},
DOI = {{10.1016/j.watres.2005.09.028}},
ISSN = {{0043-1354}},
Keywords = {{biofilm behaviour; biofilm control; chemical treatment; mechanical
   stability; mechanical stress}},
