@article{ ISI:000227273000019,
Author = {Quesnel, D and Nakhla, G},
Title = {{Characterization and treatability of aerobic bacterial thermophilically
   treated wastewater by a conventional activated sludge and granular
   activated carbon}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{4}},
Pages = {{677-687}},
Month = {{FEB}},
Abstract = {{An industrial wastewater that was pretreated by an aerobic thermophilic
   bacterial consortium (THE) was subjected to additional treatability
   studies by granular activated carbon (GAC) and a conventional activated
   sludge (CAS). The removal of dissolved organic carbon (DOC) in both
   systems was generally found to be similar. While GAC was able to attain
   better effluent concentrations of toluene and methyl isobutyl ketone
   (MIBK), the CAS was much more efficient at removing acetone.
   Furthermore, unlike the GAC, the performance of the CAS was not
   influenced by the high degree of variability in the influent wastewater.
