@article{ ISI:000230980600004,
Author = {Nicolella, C and Zoezzi, M and Rabino, M and Furfaro, M and Rovatti, M},
Title = {{Development of particle-based biofilms for degradation of xenobiotic
   organic compounds}},
Journal = {{WATER RESEARCH}},
Year = {{2005}},
Volume = {{39}},
Number = {{12}},
Pages = {{2495-2504}},
Month = {{JUL}},
Abstract = {{The aim of the experiments performed in this work was to develop a
   biofilm airlift suspension (BAS) process for the degradation of a
   mixture of organic sulfonates contained in the infiltration water from a
   contaminated site. To achieve this goal, active biomass growing on the
   contaminating xenobiotic organics as the sole source of carbon was
   obtained by enriching a mixed microbial culture sampled from an
   activated sludge treatment plant. After kinetic characterisation, the
   enriched culture was inoculated in the BAS reactor, where it colonised
   carrier particles and formed stable and uniform biofilms. In spite of
   the slow growth and degradation kinetics (mu(max) = 0.014h(-1)), due to
   high biomass concentration (up to 12gvs L-1) a high rate process was
   performed in the BAS reactor, achieving a degradation capacity of
   8.7kg(COD)m(-3) d(-1), with an overall degradation efficiency of 70\%
   based on COD measurements. (c) 2005 Elsevier Ltd. All rights reserved.}},
