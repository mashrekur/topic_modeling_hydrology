@article{ ISI:000079011300012,
Author = {Kuivinen, J and Johnsson, H},
Title = {{Determination of trihalomethanes and some chlorinated solvents in
   drinking water by headspace technique with capillary column
   gas-chromatography}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{5}},
Pages = {{1201-1208}},
Month = {{APR}},
Abstract = {{A simple and rapid headspace method for gas chromatographic
   determination of trihalomethanes and some chlorinated solvents in
   drinking water is described. The quantification is based upon the
   internal standard 1,1,2-trichloroethane, which elutes in the middle of
   the chromatogram. The relative standard deviations for most of the
   substances are between 0.8-8\% for concentrations of 1 and 50 mu g/l,
   respectively. The limits of quantification (LOQ) are less than or equal
   to 0.1 mu g/l for three of the THM analytes and 0.2 mu g/l for
   bromoform. The chlorinated solvents have LOQ less than or equal to 0.1
   mu g/l except dichloromethane which has 2 mu g/l. The analytes show
   linear ranges up to 75 mu g/l. The results of THM from the survey of 209
   Swedish drinking water samples showed that 3\% exceeded the Swedish
   limit value of 50 mu g/l of total THM. (C) 1999 Elsevier Science Ltd.
   All rights reserved.}},
