@article{ ISI:000077530300027,
Author = {Boniardi, N and Rota, R and Nano, G},
Title = {{Effect of dissolved metals on the organic load removal efficiency of
   Lemna gibba}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{2}},
Pages = {{530-538}},
Month = {{FEB}},
Abstract = {{This work is focused on assessing the potentialities of Lemna gibba in
   the treatment of metal polluted wastewaters. This was done by evaluating
   the effects of various metals on plant growth, bioaccumulation and
   organic pollutant removal rate. In particular, several experiments were
   carried out using synthetic wastewaters contaminated with Al-III, Cu-II,
   Cr-VI, Fe-III and Zn-II at various concentrations. It was found that
   wastewaters containing organic load with Fe-III (up to about 20 mg/l),
   Zn-II (up to about 20 mg/l) and Al-III (less than 30 mg/l) do not affect
   the performance of a treatment based on Lemna gibba. On the other hand,
   concentrations of chromium higher than about 0.1 mg/l and copper larger
   than about 1 mg/l have a detrimental effect on the plant performance.
   (C) 1998 Elsevier Science Ltd. All rights reserved.}},
