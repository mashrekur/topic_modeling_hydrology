@article{ ISI:000081183400028,
Author = {Hsu, BM and Huang, CP and Hsu, CLL and Hsu, YF and Yeh, JH},
Title = {{Occurrence of Giardia and Cryptosporidium in the Kau-Ping River and its
   watershed in southern Taiwan}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{11}},
Pages = {{2701-2707}},
Month = {{AUG}},
Abstract = {{Giardia and Cryptosporidium are important waterborne parasites. Thirteen
   water samples and 32 fecal specimens were collected from the Kau-Ping
   River and its watershed to test for Giardia cysts and Cryptosporidium
   oocysts. The detection methods are immunofluorescence assay and
   enzyme-linked immunosorbent assay for water samples and fecal specimens,
   respectively. Seven out of eight samples collected from raw water
   samples showed the presence of cysts, while six out of eight raw water
   samples contained oocysts. Cryptosporidium was present in 40\% of the
   treated water, while Giardia occurred in all of them. Four out of 32
   fecal specimens, collected from the hog farming region, tested positive
   for Giardia, and seven specimens tested positive for Cryptosporidium.
   Giardia was related only to Cryptosporidium but not to the others. (C)
   1999 Elsevier Science Ltd. Ali rights reserved.}},
