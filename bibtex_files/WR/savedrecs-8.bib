@article{ ISI:000077621600019,
Author = {Liu, D and Pacepavicius, GJ and Maguire, RJ and Lau, YL and Okamura, H
   and Aoyama, I},
Title = {{Mercuric chloride-catalyzed hydrolysis of the new antifouling compound
   Irgarol 1051}},
Journal = {{WATER RESEARCH}},
