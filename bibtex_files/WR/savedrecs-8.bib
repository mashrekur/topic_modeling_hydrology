Language = {{English}},
Affiliation = {{Jones, FW (Reprint Author), CSIRO, Div Wool Technol, POB 21, Belmont 3216, Australia.
   CSIRO, Div Wool Technol, Belmont 3216, Australia.}},
DOI = {{10.1016/S0043-1354(98)00399-6}},
ISSN = {{0043-1354}},
