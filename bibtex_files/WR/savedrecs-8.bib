@article{ ISI:000083672900013,
Author = {Chen, ST and Stevens, DK and Kang, GY},
Title = {{Pentachlorophenol and crystal violet degradation in water and soils
   using heme and hydrogen peroxide}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{17}},
Pages = {{3657-3665}},
Month = {{DEC}},
Abstract = {{An abiotic method for oxidative PCP degradation in soil under
   unsaturated conditions and neutral pH was developed. Reagents used were
   heme (a catalyst) and peroxide tan oxidant). The aqueous phase
   degradation of crystal violet-and PCP, and the mineralization of PCP in
   soil were determined. Five factors were investigated to assess their
   impact on PCP degradation in a soil contaminated with wood preserving
   chemicals. The results showed that heme and peroxide could efficiently
   degrade PCP and crystal violet in a short period of time in either
   liquid or unsaturated soil systems. In soil, three control runs showed
   little degradation of PCP, but treatments with heme and peroxide showed
   a maximum of 13\% mineralization of PCP. Heme and peroxide concentration
   were the two most important factors in improving degradation of PCP in
   soil. (C) 1999 Published by Elsevier Science Ltd. All rights reserved.}},
