@article{ ISI:000078648100016,
Author = {Mudge, SM and Bebianno, MJAF and East, JA and Barreira, LA},
Title = {{Sterols in the Ria Formosa lagoon, Portugal}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{4}},
Pages = {{1038-1048}},
Month = {{MAR}},
Abstract = {{Fifty nine surface sediment samples from the Ria Formosa lagoon were
   analysed for lipid biomarkers and 26 sterols were identified and
   quantified using a CC-MS technique. The total concentrations ranged from
   0.1 to 27.8 mu g g(-1) DW although this accounted for much less than 1\%
   of the total organic carbon content. The principal sterols were
   cholesterol and beta-sitosterol indicating marine fauna and terrestrial
   plants, respectively. Sewage markers (coprostanol and epi-coprostanol)
   were present around the known sewage discharge points at Fare, Tavira
   and Olhao; the former two sites had untreated effluent whereas the
   latter site has partially treated discharges which was reflected in an
   increase in the epi-coprostanol/coprostanol ratio. Phytoplankton
