@article{ ISI:000080959500026,
Author = {Rugge, K and Bjerg, PL and Mosbaek, H and Christensen, TH},
Title = {{Fate of MCPP and atrazine in an anaerobic landfill leachate plume
   (Grindsted, Denmark)}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{10}},
Pages = {{2455-2458}},
Month = {{JUL}},
Abstract = {{The fate of the two pesticides MCPP (mecoprop) and atrazine was
   investigated in an anaerobic aquifer downgradient from the Grindsted
   Landfill, Denmark, by means of ii field injection experiment, in situ
   microcosms and laboratory batch experiments. The redox conditions in the
   experiments were dominated by iron reduction, but also methanogenesis
   and sulfate reduction occurred closest to the landfill. No degradation
   nor sorption was observed for the two pesticides in any of the
   biologically active experiments. In microbially inhibited laboratory
   experiments, supplied with sodium azide as a sterilizing agent, a
   chemical degradation of atrazine occurred. (C) 1999 Elsevier Science
   Ltd. Aii rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
