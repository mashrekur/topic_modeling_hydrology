@article{ ISI:000077621600036,
Author = {Ceron, JC and Pulido-Bosch, A},
Title = {{Geochemistry of thermomineral waters in the overexploited Alto
   Guadalentin aquifer (South-East Spain)}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{1}},
Pages = {{295-300}},
Month = {{JAN}},
Abstract = {{The hydrochemical study of Alto Guadalentin aquifer shows an unusually
   high bicarbonate content between 15 to 35 mmol l(-1) and P-CO2 between
   0.044 to 1.289 bars and, in accordance with electric geophysical data,
   he is in relation with horst areas. The waters have temperatures between
   19 to 32 degrees C and conductivity between 1087 and 6810 mu S cm(-1)
   (which increased in zones with high exploitation) and are fundamentally
   sulphate and chloride types. Chemical equilibrium modelling shows that
   the waters are oversaturated in quartz, calcite and dolomite and
   undersaturated in anhydrite and gypsum. O-18, H-2 and C-13 isotopes
   revealed that there is an enrichment in O-18 and mixing processes
   between meteoric waters and deep circulating waters. (C) 1998 Elsevier
   Science Ltd. All rights reserved.}},
