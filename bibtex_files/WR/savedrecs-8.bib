@article{ ISI:000080017200002,
Author = {Shipin, OV and Meiring, PGJ and Phaswana, R and Kluever, H},
Title = {{Integrating ponds and activated sludge process in the PETRO concept}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{8}},
Pages = {{1767-1774}},
Month = {{JUN}},
Abstract = {{The activated sludge process (ASP) can be successfully integrated into
   the pending system in the framework of the PETRO concept (ASP-variant).
   The role of microalgae in the ASP was elucidated and biological
   mechanisms of algae removal are suggested. Microalgae contribute to
   flocculation by the production of exopolysaccharides (EPS) under stress
   conditions. Superior flocculation appears to result in a greater
   nitrification rate contributing to N-removal in the ASP reactor.
   Consequences and advantages of the secondary position of the polishing
   ASP (downstream of the ponds) are discussed. (C) 1999 Elsevier Science
   Ltd. All rights reserved.}},
