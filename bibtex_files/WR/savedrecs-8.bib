@article{ ISI:000079485400017,
Author = {Rodgers, M},
Title = {{Organic carbon removal using a new biofilm reactor}},
Journal = {{WATER RESEARCH}},
Year = {{1999}},
Volume = {{33}},
Number = {{6}},
Pages = {{1495-1499}},
Month = {{APR}},
Abstract = {{This study consisted of the construction and laboratory testing of a new
   experimental biofilm reactor for the carbonaceous oxidation of a
   synthetic wastewater. The biofilm unit used in the reactor was
   constructed from corrugated plastic sheets in the form of a cube which
   was moved vertically, and was alternately immersed in and lifted out of
   the wastewater. In the experimental work, soluble chemical oxygen demand
   removal rates of 43 g CODs/m(2) d or 3.8 kg CODs/m(3) d were achieved
   and these rates compare favourably with other biofilm systems. The new
   biofilm experimental unit was simple to construct and operate, and was
   very effective in biologically treating the synthetic wastewater used in
   the study. (C) 1999 Elsevier Science Ltd. All rights reserved.}},
