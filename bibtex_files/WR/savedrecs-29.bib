@article{ ISI:000296826200019,
Author = {Rodriguez-Garcia, G. and Molinos-Senante, M. and Hospido, A. and
   Hernandez-Sancho, F. and Moreira, M. T. and Feijoo, G.},
Title = {{Environmental and economic profile of six typologies of wastewater
   treatment plants}},
