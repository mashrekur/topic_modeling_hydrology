@article{ ISI:000291087300013,
Author = {Pavelic, P. and Dillon, P. J. and Mucha, M. and Nakai, T. and Barry, K.
   E. and Bestland, E.},
Title = {{Laboratory assessment of factors affecting soil clogging of soil aquifer
   treatment systems}},
