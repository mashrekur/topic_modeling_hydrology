@article{ ISI:000294101300026,
Author = {Carabineiro, S. A. C. and Thavorn-Amornsri, T. and Pereira, M. F. R. and
   Figueiredo, J. L.},
Title = {{Adsorption of ciprofloxacin on surface-modified carbon materials}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{15}},
Pages = {{4583-4591}},
Month = {{OCT 1}},
Abstract = {{The adsorption capacity of ciprofloxacin (CPX) was determined on three
   types of carbon-based materials: activated carbon (commercial sample),
   carbon nanotubes (commercial multi-walled carbon nanotubes) and carbon
   xerogel (prepared by the resorcinol/formaldehyde approach at pH 6.0).
   These materials were used as received/prepared and functionalised
   through oxidation with nitric acid. The oxidised materials were then
   heat treated under inert atmosphere (N(2)) at different temperatures
   (between 350 and 900 degrees C). The obtained samples were characterised
   by adsorption of N(2) at -196 degrees C, determination of the point of
   zero charge and by temperature programmed desorption. High adsorption
   capacities ranging from approximately 60 to 300 mg(CPx) g(C)(-1) were
   obtained (for oxidised carbon xerogel, and oxidised thermally treated
   activated carbon Norit ROX 8.0, respectively). In general, it was found
   that the nitric acid treatment of samples has a detrimental effect in
   adsorption capacity, whereas thermal treatments, especially at 900
   degrees C after oxidation, enhance adsorption performance. This is due
   to the positive effect of the surface basicity. The kinetic curves
   obtained were fitted using 1st or 2nd order models, and the Langmuir and
   Freundlich models were used to describe the equilibrium isotherms
   obtained. The 2nd order and the Langmuir models, respectively, were
   shown to present the best fittings. (C) 2011 Elsevier Ltd. All rights
   reserved.}},
