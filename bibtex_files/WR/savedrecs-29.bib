@article{ ISI:000298124500005,
Author = {Mathieu, Christoph and Rieckermann, Joerg and Berset, Jean-Daniel and
   Schuerch, Stefan and Brenneisen, Rudolf},
Title = {{Assessment of total uncertainty in cocaine and benzoylecgonine
   wastewater load measurements}},
Journal = {{WATER RESEARCH}},
