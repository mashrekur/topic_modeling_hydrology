@article{ ISI:000292947800013,
Author = {Takagi, Sokichi and Adachi, Fumie and Miyano, Keiichi and Koizumi,
   Yoshihiko and Tanaka, Hidetsugu and Watanabe, Isao and Tanabe, Shinsuke
   and Kannan, Kurunthachalam},
Title = {{Fate of Perfluorooctanesulfonate and perfluorooctanoate in drinking
   water treatment processes}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{13}},
Pages = {{3925-3932}},
Month = {{JUL}},
Abstract = {{Perfluorooctanesulfonate (PFOS) and perfluorooctanoate (PFOA) have been
   recognized as global environmental pollutants. Although PFOS and PFOA
   have been detected in tap water from Japan and several other countries,
   very few studies have examined the fate, especially removal, of
   perfluorinated compounds (PFCs) in drinking water treatment processes.
   In this study, we analyzed PFOS and PFOA at every stages of drinking
   water treatment processes in several water purification plants that
   employ advanced water treatment technologies. PFOS and PFOA
   concentrations did not vary considerably in raw water, sand filtered
   water, settled water, and ozonated water. Sand filtration and ozonation
   did not have an effect on the removal of PFOS and PFOA in drinking
   water. PFOS and PFOA were removed effectively by activated carbon that
   had been used for less than one year. However, activated carbon that had
   been used for a longer period of time (>1 year) was not effective in
   removing PFOS and PFOA from water. Variations in the removal ratios of
   PFOS and PFOA by activated carbon were found between summer and winter
   months. (C) 2011 Elsevier Ltd. All rights reserved.}},
