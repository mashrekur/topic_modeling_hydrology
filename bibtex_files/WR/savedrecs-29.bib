@article{ ISI:000295894600045,
Author = {Xu, Huijuan and Liu, Yu},
Title = {{Reduced microbial attachment by D-amino acid-inhibited AI-2 and EPS
   production}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{17}},
Pages = {{5796-5804}},
Month = {{NOV 1}},
Abstract = {{This study investigated the effect of a-tyrosine on microbial attachment
   to hydrophilic glass and hydrophobic polypropylene surfaces. Results
   showed that D-tyrosine did not influence microbial growth, ATP and
   substrate utilization, but significantly inhibited the synthesis of
   autoinducer-2 (AI-2), eDNA and extracellular polysaccharides and
   proteins, and subsequently reduced microbial attachment onto glass and
   polypropylene surfaces was observed. It was shown that a-amino acid
   would be a non-toxic agent for control of microbial attachment. (C) 2011
   Elsevier Ltd. All rights reserved.}},
