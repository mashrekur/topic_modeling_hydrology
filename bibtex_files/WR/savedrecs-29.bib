@article{ ISI:000298124500020,
Author = {Chen, Yingbo and Hu, Xiaoyu and Kim, Hern},
Title = {{Monte Carlo simulation of pore blocking phenomena in cross-flow
   microfiltration}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{20}},
Pages = {{6789-6797}},
Month = {{DEC 15}},
Abstract = {{An off-lattice Monte Carlo method was developed to examine the pore
   blocking phenomena in cross-flow microfiltration. Membranes were
   generated by randomly distributed pores with a given pore size
   distribution on a two-dimensional plane. The permeability of particles
   through the membrane pores was calculated, and the effects of crucial
   factors on the reduction of permeability were discussed. Reasonable
   fouling rules for simulations were examined and selected. At the initial
   stage the flux decreases sharply as the filtration time increases and
   then a steady state is reached. The results fit the pore blocking
   mechanism well. The simulation model developed in this study can be
   effectively used for analyzing a number of factors influencing physical
   fouling of membranes. (C) 2011 Elsevier Ltd. All rights reserved.}},
