@article{ ISI:000292947800024,
Author = {Morisada, Shintaro and Rin, Tetsuzen and Ogata, Takeshi and Kim, Yoen-Ho
   and Nakano, Yoshio},
Title = {{Adsorption removal of boron in aqueous solutions by amine-modified
   tannin gel}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{13}},
Pages = {{4028-4034}},
Month = {{JUL}},
Abstract = {{A tannin gel (TG) synthesized from condensed tannin molecules has a
   remarkable ability to adsorb various metal ions in aqueous solutions. In
   the present study, the adsorption removal of boron in solutions at
   various pHs and temperatures has been examined using the TG and the
   amine-modified tannin gel (ATG) prepared with ammonia treatment of the
   TG. The adsorption amounts of boron for the TG and the ATG were
   relatively small and almost constant below pH 7, whereas the boron
   adsorption amounts increased with increasing pH in the range of pH above
   7. Considering that in aqueous solutions above pH 7, the mole fraction
   of boric acid decreases while that of tetrahydroxyborate ion increases
   with increasing pH, the boron adsorption onto both gels takes place
   probably through the chelate formation of tetrahydroxyborate ion with
   the hydroxy and the amino groups in the gels. Besides, the adsorbability
