@article{ ISI:000288638500015,
Author = {Seredynska-Sobecka, Bozena and Stedmon, Colin A. and Boe-Hansen, Rasmus
   and Waul, Christopher K. and Arvin, Erik},
Title = {{Monitoring organic loading to swimming pools by fluorescence
   excitation-emission matrix with parallel factor analysis (PARAFAC)}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{6}},
Pages = {{2306-2314}},
Month = {{MAR}},
Abstract = {{Fluorescence Excitation-Emission Matrix spectroscopy combined with
   parallel factor analysis was employed to monitor water quality and
   organic contamination in swimming pools. The fluorescence signal of the
   swimming pool organic matter was low but increased slightly through the
   day. The analysis revealed that the organic matter fluorescence was
   characterised by five different components, one of which was unique to
   swimming pool organic matter and one which was specific to organic
   contamination. The latter component had emission peaks at 420 nm and was
   found to be a sensitive indicator of organic loading in swimming pool
   water. The fluorescence at 420 nm gradually increased during opening
   hours and represented material accumulating through the day. (C) 2011
   Elsevier Ltd. All rights reserved.}},
