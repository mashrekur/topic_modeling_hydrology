@article{ ISI:000286995000039,
Author = {Jin, Jing and El-Din, Mohamed Gamal and Bolton, James R.},
Title = {{Assessment of the UV/Chlorine process as an advanced oxidation process}},
Journal = {{WATER RESEARCH}},
