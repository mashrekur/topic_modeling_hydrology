@article{ ISI:000294101300025,
Author = {Kniggendorf, Ann-Kathrin and Meinhardt-Wollweber, Merve},
Title = {{Of microparticles and bacteria identification - (resonance) Raman
   micro-spectroscopy as a tool for biofilm analysis}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{15}},
Pages = {{4571-4582}},
Month = {{OCT 1}},
Abstract = {{Confocal resonance Raman microscopy is a powerful tool for the
   non-invasive analysis of complex biological aggregates without
   preparation and prior knowledge of the samples. We present the
   capabilities of confocal resonance Raman microscopy with a spatial
   resolution of 350 nm(2) x 2.0 mu m and excitation times of 1 s and less
   per recorded spectrum. Granules sampled from two sequencing batch
   reactors (SBR) for anaerobic ammonium oxidization (anammox) were
   regularly mapped in vivo for three months after SBR startup. Uncultured
   microorganisms and mineral particles were tracked throughout operation
   and identified in situ by their (resonance) Raman spectra. Co-existing
   microcolonies of Nitrosomonae formed the outer layer of anammox
   granules. Polymorph TiO2 microparticles were found embedded in the outer
   layer of granules overgrown with purple bacteria, indicating bacterial
   response to the variant toxicity of the mineral phase. (C) 2011 Elsevier
   Ltd. All rights reserved.}},
