@article{ ISI:000288638500018,
Author = {Martinez Bueno, M. J. and Ucles, S. and Hernando, M. D. and Davoli, E.
   and Fernandez-Alba, A. R.},
Title = {{Evaluation of selected ubiquitous contaminants in the aquatic
   environment and their transformation products. A pilot study of their
   removal from a sewage treatment plant}},
Journal = {{WATER RESEARCH}},
Year = {{2011}},
Volume = {{45}},
Number = {{6}},
Pages = {{2331-2341}},
Month = {{MAR}},
Abstract = {{A simple method using direct sample injection combined with liquid
   chromatography tandem mass spectrometry has been developed for the
   simultaneous analysis of six alkaloid compounds in environmental
   samples. The target list includes two psychostimulants (nicotine and
   caffeine), three metabolites (cotinine, nicotinic acid and paraxanthine)
   and a coffee chemical (trigonelline). The analytical method was
   evaluated in three different matrices (surface water, influent and
   effluent wastewater). The method developed showed an adequate
   sensitivity, below 0.6 mu g L-1 for wastewater and 0.1 mu g L-1 for
   river matrices, without any prior treatment of the samples. Finally, the
   methodology was applied to real samples for evaluation of their removal
   from a sewage treatment plant and their persistence/fate in the aquatic
   environment. All compounds studied in this work were detected at all
   sampling points collected along the Henares River. However, nicotinic
   acid was only detected three times in treated sewage samples at levels
   above its detection limit. Crown Copyright (C) 2011 Published by
   Elsevier Ltd. All rights reserved.}},
