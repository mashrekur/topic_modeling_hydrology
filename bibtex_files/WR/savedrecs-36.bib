@article{ ISI:000331681200019,
Author = {Jurado-Sanchez, Beatriz and Ballesteros, Evaristo and Gallego, Mercedes},
Title = {{Occurrence of carboxylic acids in different steps of two drinking-water
   treatment plants using different disinfectants}},
Journal = {{WATER RESEARCH}},
Year = {{2014}},
Volume = {{51}},
