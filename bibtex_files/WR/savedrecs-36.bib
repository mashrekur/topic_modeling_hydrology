@article{ ISI:000329553100038,
Author = {Yang, Guang-Xi and Jiang, Hong},
Title = {{Amino modification of biochar for enhanced adsorption of copper ions
   from synthetic wastewater}},
Journal = {{WATER RESEARCH}},
Year = {{2014}},
Volume = {{48}},
Pages = {{396-405}},
Month = {{JAN 1}},
Abstract = {{Biochar was modified as a high efficient and selective absorbent for
   copper ions (Cu(II)) by nitration and reduction. Results of X-ray
   photoelectron spectroscopy (XPS) and attenuated total
   reflectance-Fourier transform infrared spectroscopy (ATR-FTIR) analyses
   indicated that the amino groups were chemically bound to the functional
   groups on the biochar surface. Kinetics, thermal dynamics, and
   adsorption and desorption of Cu(II) in fixed-bed were investigated. The
   results demonstrated that the amino-modified biochar exhibited excellent
   adsorption performance for Cu(II). The adsorption capacity and bed
   volume of the modified biochar are five- and eight- folds of the
   pristine biochar, respectively. The Cu(II) combined with the amino
   groups through strong complexation based on the comparison of XPS and
   ATR-FTIR analyses before and after adsorption, which endows it with the
   high pH stability and ion selectivity. (C) 2013 Elsevier Ltd. All rights
   reserved.}},
