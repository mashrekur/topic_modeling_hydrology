@article{ ISI:000330601400041,
Author = {Dereli, Recep Kaan and van der Zee, Frank P. and Heffernan, Barry and
   Grelot, Aurelie and van Lier, Jules B.},
Title = {{Effect of sludge retention time on the biological performance of
   anaerobic membrane bioreactors treating corn-to-ethanol thin stillage
   with high lipid content}},
Journal = {{WATER RESEARCH}},
Year = {{2014}},
Volume = {{49}},
Pages = {{453-464}},
Month = {{FEB 1}},
Abstract = {{The potential of anaerobic membrane bioreactors (AnMBRs) for the
   treatment of lipid rich cornto-ethanol thin stillage was investigated at
   three different sludge retention times (SRT), i.e. 20, 30 and 50 days.
   The membrane assisted biomass retention in AnMBRs provided an excellent
   solution to sludge washout problems reported for the treatment of lipid
   rich wastewaters by granular sludge bed reactors. The AnMBRs achieved
   high COD removal efficiencies up to 99\% and excellent effluent quality.
   Although higher organic loading rates (OLRs) up to 8.0 kg COD m(-3)
   d(-1) could be applied to the reactors operated at shorter SRTs, better
   biological degradation efficiencies, i.e. up to 83\%, was achieved at
   increased SRTs. Severe long chain fatty acid (LCFA) inhibition was
   observed at 50 days SRT, possibly caused by the extensive dissolution of
   LCFA in the reactor broth, inhibiting the methanogenic biomass.
   Physicochemical mechanisms such as precipitation with divalent cations
   and adsorption on the sludge played an important role in the occurrence
   of LCFA removal, conversion, and inhibition. (C) 2013 Elsevier Ltd. All
   rights reserved.}},
