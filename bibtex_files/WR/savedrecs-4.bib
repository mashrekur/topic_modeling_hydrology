@article{ ISI:A1995QK24700009,
Author = {ORON, G and GOEMANS, M and MANOR, Y and FEYEN, J},
Title = {{POLIOVIRUS DISTRIBUTION IN THE SOIL-PLANT SYSTEM UNDER REUSE OF
   SECONDARY WASTE-WATER}},
