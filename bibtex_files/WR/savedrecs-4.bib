@article{ ISI:A1995PY23800002,
Author = {LAMBERT, D and MAHER, W},
Title = {{AN EVALUATION OF THE EFFICIENCY OF THE ALKALINE PERSULFATE DIGESTION
   METHOD FOR THE DETERMINATION OF TOTAL PHOSPHORUS IN TURBID WATERS}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{1}},
Pages = {{7-9}},
Month = {{JAN}},
Abstract = {{The use of an alkaline persulphate digestion procedure has been found to
   be suitable for the determination of total phosphorus in turbid waters.
   The procedure was evaluated by analysis of suspensions of standard
   reference materials (NIES No. 3 Chlorella and NIES No. 2 Pond sediment)
   and the analysis of model phosphorus compounds added to lake water.
   Recoveries of phosphorus from suspensions (up to 100 mu gP/l) and
   phosphorus compounds added to lake water were 92-109\% and 94-103\%,
   respectively.
   When compared with the APHA-AWWA-WPCF nitric-sulphuric acid digestion
   procedure no significant difference between the two digestion procedures
   was found at the 95\% confidence interval.}},
