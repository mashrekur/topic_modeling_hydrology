@article{ ISI:A1995RA05600011,
Author = {BARBUSINSKI, K and KOSCIELNIAK, H},
Title = {{INFLUENCE OF SUBSTRATE LOADING INTENSITY ON FLOC SIZE IN
   ACTIVATED-SLUDGE PROCESS}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{7}},
Pages = {{1703-1710}},
Month = {{JUL}},
Abstract = {{The activated sludge flocs size variability and their size distribution
   were analyzed for the Aocs sizes larger than 10 mu m and for different
   intensities (rates) of bidirectional organic loading changes. Under
   these conditions the distributions of flee sizes expressed as average
   diameter and longest dimension fitted well the log-normal model. The
   logarithmic average flee size variability proved to be proportional to
   the loading if a time delay due to the inertia of biochemical and
   physical processes of activated sludge was taken into account. It has
   also been found that flocs larger than 50 mu m constituted the main
   source of the surface area and volume of the activated sludge. The tests
   performed have shown that organic loading strongly influences flee size
   distribution in the activated sludge process. The long-term loading
   changes caused larger disturbances to the floc size distribution than
   more rapid but shorter ones. Generally, after the substrate overload
   occurred, the flocs were more prone to breakup (pin-point flee effect).}},
