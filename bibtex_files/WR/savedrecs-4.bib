@article{ ISI:A1995RH94500016,
Author = {HERMIDA-AMEIJEIRAS, A and SIMAL-GANDARA, J and LOPEZ-HERNANDEZ, J and
   SIMAL-LOZANO, J},
Title = {{EVALUATION OF THE PERFORMANCE OF A MULTIRESIDUE GAS-CHROMATOGRAPHIC
   METHOD FOR THE DETERMINATION OF ALIPHATIC-HYDROCARBONS IN MUSSELS OF
   GALICIA (NW SPAIN)}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{9}},
Pages = {{2118-2124}},
Month = {{SEP}},
Abstract = {{The determination of aliphatic hydrocarbons in blue mussel (Mytilus
   galloprovincialis) was performed using a multiple component extraction
   method and temperature-programmed capillary GC using splitless injection
   mode and dame ionization detection. Calculated recoveries for the
   overall procedure to give concentrations of the different aliphatic
   hydrocarbons in the low level end (ca 10 mu g/ml) of the calibration
   range (2-200 mu g/ml) ranged from 70 to 92\% {[}coefficients of
   variation (CVs) 9-20\%]. Limits of detection for the overall procedure
   were between 40 and 60 ng/g of sample (extraction concentration factor
   25 g/ml) and GC-FID determination precision was good (CVs 1-4\%).
   Aliphatic hydrocarbon levers in mussels in Galicia were similar to those
   found in areas generally considered unpolluted.}},
