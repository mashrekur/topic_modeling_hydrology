@article{ ISI:A1995RR97600021,
Author = {TENNO, R and RENKO, EK and PELKONEN, M},
Title = {{ONLINE IDENTIFICATION OF ACTIVATED-SLUDGE SETTLING PROPERTIES}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{11}},
Pages = {{2587-2590}},
Month = {{NOV}},
Abstract = {{Activated sludge settling properties can be characterised with two
   time-varying parameters. A simple method is presented for online
   identification of these parameters. The method is obtained as an exact
   solution of the filtration problem; it is obtained on the basis of the
   settling process description and the parameters variation process
   description. The method is suitable for practical application. It is a
   more exact solution of the identification problem than the alternative
   regression analysis method. An easily implementable system is proposed
   for automatic characterisation of the sludge settling properties.}},
