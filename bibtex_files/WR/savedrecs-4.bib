@article{ ISI:A1995RA05600024,
Author = {KNAPP, JS and NEWBY, PS},
Title = {{THE MICROBIOLOGICAL DECOLORIZATION OF AN INDUSTRIAL EFFLUENT CONTAINING
   A DIAZO-LINKED CHROMOPHORE}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{7}},
Pages = {{1807-1809}},
Month = {{JUL}},
Abstract = {{Mixed bacterial cultures from a wide variety of habitats were shown to
   be able to decolorize the diazo-linked chromophore of a highly coloured
   industrial effluent from the manufacture of nitrated stilbene sulphonic
   acid. Decolorization was favoured by strictly anaerobic conditions and
   highly proteinaceous media. 10\% v/v dilutions of the effluent with
   initial absorbances of IDD units at lambda(max) were rapidly
   decolorized-ca. 85\% in 2 days. When decolorized effluent was exposed to
   air it slowly developed a different colour which absorbed light more
   strongly in the visible region. The mechanisms involved in these
   processes and their implications for treatment of coloured effluents are
   discussed.}},
