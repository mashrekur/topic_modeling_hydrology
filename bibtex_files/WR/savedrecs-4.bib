Affiliation = {{INST NATL SCI APPL,DEPT GENIE PROC IND,UNITE RECH TRAITEMENT BIOL EAUX,F-31077 TOULOUSE,FRANCE.
   SOC AMENAGEMENT URBAIN \& RURAL,F-78064 ST QUENTIN,FRANCE.}},
DOI = {{10.1016/0043-1354(94)00324-Z}},
ISSN = {{0043-1354}},
