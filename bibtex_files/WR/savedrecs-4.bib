@article{ ISI:A1995PY23800014,
Author = {SAFARIK, I},
Title = {{REMOVAL OF ORGANIC POLYCYCLIC COMPOUNDS FROM WATER SOLUTIONS WITH A
   MAGNETIC CHITOSAN BASED SORBENT BEARING COPPER PHTHALOCYANINE DYE}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{1}},
Pages = {{101-105}},
Month = {{JAN}},
Abstract = {{Magnetic chitosan gel particles, bearing covalently immobilized copper
   phthalocyanine dye (''magnetic blue chitosan''), were prepared and used
   for the isolation and/or removal of polycyclic dyes from water solutions
   and suspensions. Binding of these polycyclic dyes occures by a chemical
   equilibrated and saturatable mechanism, following the Langmuir
   adsorption model. The values of maximum uptake (maximum adsorption
   capacity) were calculated. The bound dyes can be eluted from the sorbent
   with methanol, methanol-conc. ammonia solution (50/1, v/v) and 10\%
