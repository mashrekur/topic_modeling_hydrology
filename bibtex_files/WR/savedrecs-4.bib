@article{ ISI:A1995RR97600025,
Author = {BAI, R and MACKIE, RI},
Title = {{MODELING THE TRANSITION BETWEEN DEPOSITION MODES IN DEEP BED FILTRATION}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{11}},
Pages = {{2601-2604}},
Month = {{NOV}},
Abstract = {{In deep bed filtration different mechanisms are dominant in the initial
   and latter stages, therefore different models are needed to describe
   these two situations. This raises the question of how to combine the two
   models into one composite model. A technique is described whereby models
   for the initial and latter stages of removal in deep bed filtration can
   be smoothly linked together. The efficacy of the method is demonstrated
   with both simple and complex mathematical models of removal in
   filtration, and with experimental data.}},
