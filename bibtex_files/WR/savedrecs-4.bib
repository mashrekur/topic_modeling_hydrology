@article{ ISI:A1995QG42600021,
Author = {CHRISTIANSEN, P and HOLLESEN, L and HARREMOES, P},
Title = {{LIQUID-FILM DIFFUSION ON REACTION-RATE IN SUBMERGED BIOFILTERS}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{3}},
Pages = {{947-952}},
Month = {{MAR}},
Abstract = {{Experiments were carried out in order to investigate the influence of
   liquid film diffusion on reaction rate in a submerged biofilter with
   denitrification and in order to compare with a theoretical study of the
