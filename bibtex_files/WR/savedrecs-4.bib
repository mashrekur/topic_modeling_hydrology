@article{ ISI:A1995RE55200009,
Author = {HUANG, GL and YONG, W},
Title = {{EFFECTS OF TRIBUTYLTIN CHLORIDE ON MARINE BIVALVE MUSSELS}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{8}},
Pages = {{1877-1884}},
Month = {{AUG}},
Abstract = {{Using marine bivalve mussels Mytidus edulis and Aequipecten irradians as
   the biological indicators, the chronic (60 days) bioaccumulation tests
   were conducted to evaluate the effect of tributyltin (TBT) on bivalve
   mussels. TBT and its degraded products-Dibutyltin and Monobutyltin-were
   determined in mussels and seawater. Bioconcentration Factor (BCF)
   increased exponentially with decreasing concentration of TBT, BCFs were
   much higher than predicted values by Kow. Some physiological effects of
   TBT on the shell length, width, whole body weight and Condition Index
   were measured and used as indicating indices on the contamination of TBT
   to bivalve mussels. Some pathological effects at cellular level were
   observed under the electron-microscope, the mitochondria of test M.
   edulis's liver swelled and disrupted, so the mechanism of toxicity was
   suggested. Based on the results of the bioaccumulation test the risk
   evaluation for humans was made. The results of bioaccumulation of TBT by
   algae revealed that accumulation through the food chain was very
   important.}},
