@article{ ISI:A1995PY23800021,
Author = {BUFFLAP, SE and ALLEN, HE},
Title = {{SEDIMENT PORE-WATER COLLECTION METHODS FOR TRACE-METAL ANALYSIS - A
   REVIEW}},
