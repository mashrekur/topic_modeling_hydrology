@article{ ISI:A1995RH94500030,
Author = {ARCANGELI, JP and ARVIN, E},
Title = {{A MEMBRANE DE-OXYGENATOR FOR THE STUDY OF ANOXIC PROCESSES}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{9}},
Pages = {{2220-2222}},
Month = {{SEP}},
Abstract = {{A membrane reactor for the elimination of oxygen in an anoxic medium for
   microbial growth has been studied. The reactor consists of a reservoir
   containing a solution of sulfite and cobalt salt where silicone tubing,
   through which the medium hows, is immersed. Silicone is highly permeable
   to gases, so oxygen diffuses through the silicone membrane and is
   absorbed in the sulfite solution. The maximum oxygen removal rate was
   1.1 g m(-2)d(-1) at 15 degrees C. The advantage of this de-oxygenator is
   its low cost and the ease with which it can be handled. A calculation
   method is discussed in relation to the design of the membrane
   de-oxygenator.}},
