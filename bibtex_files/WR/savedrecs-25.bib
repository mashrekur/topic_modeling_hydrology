@article{ ISI:000271439600022,
Author = {Boonchayaanant, Benjaporn and Nayak, Dipti and Du, Xin and Criddle,
   Craig S.},
Title = {{Uranium reduction and resistance to reoxidation under iron-reducing and
   sulfate-reducing conditions}},
Journal = {{WATER RESEARCH}},
