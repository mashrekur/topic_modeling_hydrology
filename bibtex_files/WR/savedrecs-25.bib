@article{ ISI:000263658700002,
Author = {Hashimoto, T. and Murakami, T.},
Title = {{Removal and degradation characteristics of natural and synthetic
   estrogens by activated sludge in batch experiments}},
Journal = {{WATER RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{3}},
Pages = {{573-582}},
Month = {{FEB}},
Abstract = {{The removal and degradation characteristics of natural and synthetic
   estrogens by activated sludge were investigated by a series of batch
   experiments using the activated sludge samples of four actual wastewater
   treatment plants and synthetic wastewater spiked with estrogen, The
   rapid removal and degradation of 17 beta-estradiol (E2) and estrone (E1)
   were observed by the activated sludge samples of the oxidation ditch
   process which operated at higher solids retention time (SRT). On the
   other hand, El tended to remain both in the water phase and the sludge
   phase in the activated sludge samples of the conventional activated
   sludge process which operated at lower SRT. The anoxic condition was
   considered to be not favorable to the effective removal of estrogens as
   compared with the aerobic condition. The removal and degradation of EE2
   showed the lag phase, which neither E2 nor E1 showed, but EE2 was
   finally removed and degraded completely after 24 h. The removal of
   estrogens in the water phase did not follow the first-order-rate
   reaction because a large part of the spiked estrogen was immediately
   removed from the water phase to the sludge phase by adsorption. (C) 2008
   Elsevier Ltd. All rights reserved.}},
