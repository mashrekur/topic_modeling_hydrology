@article{ ISI:000266183100009,
Author = {Saez, Veronica and Esclapez Vicente, M. D. and Frias-Ferrer, Angel J.
   and Bonete, Pedro and Gonzalez-Garcia, Jose},
Title = {{Electrochemical degradation of perchloroethylene in aqueous media: An
   approach to different strategies}},
Journal = {{WATER RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{8}},
Pages = {{2169-2178}},
Month = {{MAY}},
Abstract = {{An approaching study to the electrochemical degradation of
   perchloroethylene (PCE) in water has been carried out using controlled
   current density degradation electrolyses. The different electrochemical
   strategies to degrade perchloroethylene in aqueous media (i.e. cathodic,
   anodic and dual treatments) have been checked using divided and
   undivided configurations. The influence of the initial concentration, pH
   and cur-rent density on the general behavior of the system has been
   studied, and special attention was paid to the nature of the byproducts
   formed and to the analysis of the closed mass balance at the end of the
   reaction. Results from several analytical techniques have been compared.
   Undivided configuration provides the best results in these experimental
   conditions, with degradation percentages higher than 50\% and with only
   6\% of the initial perchloroethylene concentration remaining in the
   system. (C) 2009 Elsevier Ltd. All rights reserved.}},
