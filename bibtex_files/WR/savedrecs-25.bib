@article{ ISI:000267580500001,
Author = {Zhang, Xue and Hu, Hong-Ying and Men, Yu-jie and Yang, Jia and
   Christoffersen, Kirsten},
Title = {{Feeding characteristics of a golden alga (Poterioochromonas sp.) grazing
   on toxic cyanobacterium Microcystis aeruginosa}},
Journal = {{WATER RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{12}},
Pages = {{2953-2960}},
Month = {{JUL}},
Abstract = {{Microcystis aeruginosa has quickly risen in infamy as one of the most
   universal and toxic bloom-forming cyanobacteria. Here we presented a
   species of golden alga (Poterioochromonas sp. strain ZX1), which can
   feed on toxic M. aeruginosa without any adverse effects from the
   cyanotoxins. Using flow cytometry, the ingestion and maximal digestion
   rates were estimated to be 0.2 similar to 1.2 and 0.2 M. aeruginosa
   cells (ZX1 cell)(-1) h(-1), respectively. M. aeruyinosa in densities
   below 10(7) cells mL(-1) could be grazed down by ZX1, but no significant
   decrease was observed when the initial density was 3.2 x 10(7) cells
   mL(-1). ZX1 grazing was a little influenced by the light intensity (0.5
   similar to 2500 1x) and initial pH of the medium (pH = 5.0 similar to
   9.5). ZX1 could not survive in continuous darkness for longer than 10
   days. The pH value was adjusted to 8 by ZX1 while to 10 by M.
   aeruginosa. This study may shed light on understanding the ecological
   interactions between M. aeruginosa and mixotrophic Poterioochromonas sp.
   in aquatic ecosystems. (C) 2009 Elsevier Ltd. All rights reserved.}},
