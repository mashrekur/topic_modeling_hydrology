@article{ ISI:000267580500012,
Author = {Dion-Fortier, Annick and Rodriguez, Manuel J. and Serodes, Jean and
   Proulx, Francois},
Title = {{Impact of water stagnation in residential cold and hot water plumbing on
   concentrations of trihalomethanes and haloacetic acids}},
Journal = {{WATER RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{12}},
Pages = {{3057-3066}},
Month = {{JUL}},
Abstract = {{This study demonstrates that levels of trihalomethanes (THMs) increase
   considerably when cold water stagnates in residential pipes and, more
   significantly, when water remains in the hot water tank. Levels of
   haloacetic acids (HAAs) increase as well in both cases, but less
   significantly in comparison to THMs. The study also demonstrates that in
   both the plumbing system and residential hot water tank, chlorinated and
   brominated DBP species do not behave in the same manner. Finally, the
   study shows that sustained use of water in households helps to maintain
   THM and HAA levels close to those found in water of the distribution
   system. The results are useful to identify methods of indoor water use
   that minimize population exposure to DBPs and improve DBP exposure
   assessment for epidemiological studies. (C) 2009 Elsevier Ltd. All
   rights reserved.}},
