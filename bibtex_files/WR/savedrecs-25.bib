@article{ ISI:000267580500007,
Author = {Huang, J. and Graham, N. and Templeton, M. R. and Zhang, Y. and Collins,
   C. and Nieuwenhuijsen, M.},
Title = {{A comparison of the role of two blue-green algae in THM and HAA
   formation}},
