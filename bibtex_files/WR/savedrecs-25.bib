@article{ ISI:000263595500017,
Author = {Kolehmainen, Reija E. and Korpela, Jaana P. and Munster, Uwe and
   Puhakka, Jaakko A. and Tuovinen, Olli H.},
Title = {{Extracellular enzyme activities and nutrient availability during
   artificial groundwater recharge}},
