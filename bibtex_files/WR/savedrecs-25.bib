@article{ ISI:000266765700014,
Author = {Canterino, M. and Di Somma, I. and Marotta, R. and Andreozzi, R. and
   Caprio, V.},
Title = {{Energy recovery in wastewater decontamination: Simultaneous
   photocatalytic oxidation of an organic substrate and electricity
   generation}},
Journal = {{WATER RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{10}},
Pages = {{2710-2716}},
Month = {{JUN}},
Abstract = {{The contemporary removal of organic pollutants from aqueous solution and
   electricity generation is studied in the present work by means of an
   experimental device resulting from the combination of a photocatalytic
   reactor and an electrochemical cell. The proposed system relies on the
   capability of Cu2+ ions to reduce in the presence of TiO2, (solar) UV
   radiations and a sacrificial organic agent. In the anodic solution of
   the combined photoreactor-cell, Cu-0 is oxidized to Cu2+ and the latter
   is reduced again to the lowest oxidation state. The use of different
   sacrificial agents ranging from formic acid (FA) to glycerol (GLY) to
   glucose (GLU) is investigated along with the adoption of two different
   cathodes for the cell, the first based again on the couple Cu2+/Cu-0 and
   the second on the couple O-2/H2O. (C) 2009 Elsevier Ltd. All rights
   reserved.}},
