@article{ ISI:000270629500023,
Author = {Menniti, Adrienne and Kang, Seoktae and Elimelech, Menachem and
   Morgenroth, Eberhard},
Title = {{Influence of shear on the production of extracellular polymeric
   substances in membrane bioreactors}},
Journal = {{WATER RESEARCH}},
Year = {{2009}},
Volume = {{43}},
Number = {{17}},
Pages = {{4305-4315}},
Month = {{SEP}},
Abstract = {{Shear is used to control fouling in membrane bioreactor (MBR) systems.
   However, shear also influences the physicochemical and biological
   properties of MBR biomass. The current study examines the relationship
   between the level of shear and extracellular polymeric substance (EPS)
   production in MBRs. Two identical MBRs were operated in parallel where
   the biomass in one reactor was exposed to seven times greater shear
   forces. The concentrations of floc-associated and soluble EPS were
   monitored for the duration of the experiment. The stickiness of
   extracted floc-associated EPS from each reactor was also characterized
   using atomic force microscopy. A mathematical model of floc-associated
   and soluble EPS production was applied to quantitatively describe
   changes in EPS production with shear. Biomass grown in a high shear
   environment has lower floc-associated EPS production compared to biomass
   grown in a lower shear environment. This decrease in floc-associated EPS
   production also corresponds to a decrease in soluble EPS production,
   which can be explained by both the lower concentration of
   floc-associated EPS and the production of stickier floc-associated EPS
   that is more erosion resistant in the high shear reactor. This research
   suggests that mechanical stresses can have a significant impact on the
   production rates of floc-associated and soluble EPS-key parameters
   governing membrane fouling in MBRs. (C) 2009 Elsevier Ltd. All rights
   reserved.}},
