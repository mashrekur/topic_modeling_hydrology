@article{ ISI:000270345800013,
Author = {Chatzisymeon, Efthalia and Xekoukoulotakis, Nikolaos P. and
   Diamadopoulos, Evan and Katsaounis, Alexandros and Mantzavinos,
   Dionissios},
Title = {{Boron-doped diamond anodic treatment of olive mill wastewaters:
   Statistical analysis, kinetic modeling and biodegradability}},
