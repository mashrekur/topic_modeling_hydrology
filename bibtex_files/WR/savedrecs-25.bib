@article{ ISI:000264328600024,
Author = {Wu, Qinglong and Liu, Wen-Tso},
Title = {{Determination of Virus abundance, diversity and distribution in a
   municipal wastewater treatment plant}},
Journal = {{WATER RESEARCH}},
