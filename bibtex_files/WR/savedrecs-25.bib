@article{ ISI:000263658700006,
Author = {Comerton, Anna M. and Andyews, Robert C. and Bagley, David M.},
Title = {{The influence of natural organic matter and cations on the rejection of
   endocrine disrupting and pharmaceutically active compounds by
   nanofiltration}},
