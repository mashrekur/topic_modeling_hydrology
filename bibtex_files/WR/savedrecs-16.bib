@article{ ISI:000179563500016,
Author = {Sahoo, GB and Luketina, D},
Title = {{Modeling of bubble plume design and oxygen transfer for reservoir
   restoration}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{2}},
Pages = {{393-401}},
Month = {{JAN}},
Abstract = {{A one-dimensional bubble plume model is described to predict the gaseous
   mass transfer of air bubbles into ambient water and the appropriate
   design criteria of a bubbler for a linearly stratified reservoir. The
   bubble plume behavior is governed by two dimensionless parameters: G,
   the strength of stratification, and M, the source strength (via airflow
   rate). Mechanical efficiency, an indicator of bubbler operating Costa
   the rate of plume entrainment flux, an indicator of mixing
   effectiveness; and the oxygen dissolution efficiency are analyzed as
   functions of G and,If for different air bubble sizes to describe optimum
   bubbler design conditions. A bubble radius close to 1 mm is recommended
   as it favors higher mechanical efficiency and oxygen dissolution
   efficiency. (C) 2002 Elsevier Science Ltd. All rights reserved.}},
