@article{ ISI:000179316500005,
Author = {Chu, W and Ching, MH},
Title = {{Modeling the ozonation of 2,4-dichlorophoxyacetic acid through a kinetic
   approach}},
Journal = {{WATER RESEARCH}},
Year = {{2003}},
Volume = {{37}},
Number = {{1}},
Pages = {{39-46}},
Month = {{JAN}},
Abstract = {{The ozonation of herbicide 2,4-dichlorophoxyacetic acid (2,4-D) was
   studied and modeled at different initial pH levels and initial
   concentrations. The degradation rate of 2,4-D was found to increase with
   the initial pH level and decrease with initial 2,4-D concentration. A
   two-stage ozonation of 2,4-D was observed, in which the initial stage of
   ozonation was slightly faster than the second stage due to the higher
   initial (saturated) ozone concentration; under these circumstances, the
   direct molecular ozone oxidation was likely to predominate. At the
   second stage, the indirect radical oxidation became more significant in
   determining the overall 2,4-D oxidation, and the reaction was more
   pH-dependent. A mathematic model associated with the analytical
   solutions consisting of direct and indirect oxidations contributed by
   the molecular ozone and free radical is proposed. The proposed model is
   capable of predicting the decay of 2,4-D quantitatively by ozonation at
   different initial pH levels and initial 2,4-D concentrations. (C) 2002
   Elsevier Science Ltd. All rights reserved.}},
