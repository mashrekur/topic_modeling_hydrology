@article{ ISI:A1996VT98000023,
Author = {Walsh, AR and OHalloran, J},
Title = {{Chromium speciation in tannery effluent .1. An assessment of techniques
   and the role of organic Cr(III) complexes}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
