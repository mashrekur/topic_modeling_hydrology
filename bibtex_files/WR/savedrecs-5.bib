@article{ ISI:A1996UP85300009,
Author = {Gupta, M and Sharma, D and Suidan, MT and Sayles, GD},
Title = {{Biotransformation rates of chloroform under anaerobic conditions .1.
   Methanogenesis}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{6}},
Pages = {{1377-1385}},
Month = {{JUN}},
Abstract = {{Biotransformation of chloroform (CF) was studied in a methanogenic
   environment utilizing acetic acid as the primary substrate. CF removal
   efficiency of more than 99\% was achieved in a chemostat, fed constant
   acetic acid concentration (2510 mg/l) and different CF concentrations,
   uh to 16.74 mu M. Biological methane potential tests were conducted in
   serum bottles using the culture from the chemostat with an average
   biomass concentration of 53 mg/l. The culture exhibited a maximum rate
   of CF transformation of 0.26 mu M/h corresponding to an initial CF
   concentration of 1.25 mu M. At initial CF concentrations higher than
   2.76 mu M, the rate of CF transformation became constant at 0.06 mu M/h.
   CF present at any concentration inhibited the utilization of acetic
   acid, and at CF concentrations equal to or exceeding 2.7 mu M, the
   culture was completely inhibited and no acetic acid; was utilized by the
   culture even after the CF was completely degraded. The culture
   transformed CF without the addition of acetic acid, but the addition of
   acetic acid considerably increased the rate of biotransformation.
   However, increase in acetic acid concentration beyond 50 mg/l did not
   increase the rate. The culture exhibited higher rates of CF
   biotransformation when it was acclimated with higher influent
   concentration of CF in a chemostat. Reductive dehalogenation was one of
   the pathways for the transformation of CF leading to the formation of
   dichloromethane which was also transformed by the culture. Copyright (C)
   1996 Elsevier Science Ltd}},
