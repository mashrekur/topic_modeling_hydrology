@article{ ISI:A1996TU58000029,
Author = {Csepai, L and Kabelka, H},
Title = {{Practical testing of automatically controlled overflow weirs}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{3}},
Pages = {{749-752}},
Month = {{MAR}},
Abstract = {{The advantages of installing motor-regulated overflow weirs at the
   sewage works of Modling show, reflecting the practical use of the
   electronic control of overflow weirs during 3 years, the necessity and
   effectivity of such a system. At this sewage plant electric control of
   the overflow weirs is combined with control of the immersion depth of
   the surface aerators in the aeration tanks. The motor-regulation
   implements two functions: equal flow to each of the three secondary
   settling tanks and optimal position of the surface aerators in the
   aerating tanks}},
