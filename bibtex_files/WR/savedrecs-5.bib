@article{ ISI:A1996UU42300007,
Author = {Tyagi, RD and Du, YG and Bhamidimarri, R},
Title = {{Dynamic behavior of the activated sludge process under shock loading:
   Application of the floc model}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{7}},
Pages = {{1605-1616}},
Month = {{JUL}},
Abstract = {{An integrated system model is developed for an activated sludge system
   which not only couples the hoc behavior to changes in the bulk liquid
   characteristics but also takes into account the hoc size distribution in
   terms of mass in the reacting system. Intensive simulation is then
   carried out based on the comprehensive model developed to assess the
   impact of shock loading on the plant dynamic behavior and to build an
   understanding of the importance of the flee effects. Simulation results
   show that more complex dynamic behavior due to the floc effects is
   likely to occur under severe shock loads which must be considered in the
   development of the control policy for a safer and better operating
   performance. Copyright (C) 1996 Elsevier Science Ltd}},
