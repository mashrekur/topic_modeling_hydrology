@article{ ISI:A1996VM11900003,
Author = {Benson, RL and Truong, YB and McKelvie, ID and Hart, BT},
Title = {{Monitoring of dissolved reactive phosphorus in wastewaters by flow
   injection analysis .1. Method development and validation}},
