@article{ ISI:A1996VT98000010,
Author = {Prahacs, SM and Hall, KJ and Duncan, W},
Title = {{Leeches as in situ biomonitors of chlorinated phenolic compounds .2.
   Pulp mill investigations}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{10}},
Pages = {{2301-2308}},
Month = {{OCT}},
Abstract = {{Leeches (Nephelopsis obscura Verrill) were evaluated as ill situ
   biomonitors of chlorinated phenolic compounds in the Fraser River al
   Prince George, BC, downstream from three bleached kraft pulp mills
   practising chlorine dioxide (ClO2) substitution. Five biomonitoring
   periods, of 7 days duration, were selected to cover a full range of
   seasonal river conditions.
   Leeches bioconcentrated tri- and tetrachlorinated phenolics
   (bioconcentration factors: 465-6000) in proportions similar to those
   present in both mill effluent and river water. Leeches showed
   3,4,5-trichloroguaiacol (3,4,5-TCG) to be the most consistent tracer of
   bleached kraft mill discharge. Leeches provided direct evidence that
   increasing ClO2 substitution reduces amounts of chlorinated phenolics
   accumulated by aquatic organisms, with sharp decreases observed at ClO2
