@article{ ISI:A1996TG77200009,
Author = {Leborans, GF and Novillo, A},
Title = {{Toxicity and bioaccumulation of cadmium in Olisthodiscus luteus
   (Raphidophyceae)}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{1}},
Pages = {{57-62}},
Month = {{JAN}},
Abstract = {{The toxicity and uptake of cadmium has been studied using a culture of a
   marine phytoflagellate (Olisthodiscus luteus). The concentrations tested
   were 10, 50 and 500 mu g l(-1) of cadmium. Olisthodiscus luteus is able
   to bioaccumulate 0.57-56.42 fg cell(-1). The cadmium uptake is dependent
   on the time of exposure and on the internal concentration of the metal,
   the toxic effects resulting in a reduction of this process. Cadmium
   determines a decrease of the cellular volume, of the growth rate and of
   the levels of photosynthetic pigments. The species used shows a
   remarkable adaptation to cadmium.}},
