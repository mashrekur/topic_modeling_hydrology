@article{ ISI:A1996VB04500020,
Author = {Kock, G and Noggler, M and Hofer, R},
Title = {{Pb in otoliths and opercula of Arctic char (Salvelinus alpinus) from
   oligotrophic lakes}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{8}},
Pages = {{1919-1923}},
Month = {{AUG}},
Abstract = {{We investigated the concentrations of Pb ({[}Pb]) in calcified tissues
   (opercula and otoliths) of Arctic char (Salvelinus alpinus) from
   oligotrophic lakes. Fish were collected from 5 Alpine lakes in Northern
   Tyrol, Austria, spanning wide ranges in altitude and physicochemical
   water characteristics. In both opercula and otoliths {[}Pb] correlated
   significantly positive with the age of fish. In the opercula {[}Pb]
   correlated negatively with alkalinity, Ca concentration and pH of the
   lakes, thereby reflecting an increased uptake of the metal in
   low-alkalinity waters. However, {[}Ph] in otoliths did not correlate
   with any of these water characteristics. The tissue-specific differences
