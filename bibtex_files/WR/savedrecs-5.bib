@article{ ISI:A1996VT98000012,
Author = {Siantar, DP and Schreier, CG and Chou, CS and Reinhard, M},
Title = {{Treatment of 1,2-dibromo-3-chloropropane and nitrate-contaminated water
   with zero-valent iron or hydrogen/palladium catalysts}},
Journal = {{WATER RESEARCH}},
