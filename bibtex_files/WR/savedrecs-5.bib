@article{ ISI:A1996VM11900022,
Author = {Mahne, I and Princic, A and Megusar, F},
Title = {{Nitrification/denitrification in nitrogen high-strength liquid wastes}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{9}},
Pages = {{2107-2111}},
Month = {{SEP}},
Abstract = {{To evaluate the possibility of nitrogen removal from nitrogen
   high-strength wastewaters, nitrification rates, denitrification and
   related energy problems, emerging in nondiluted wastewater from a pig
   production plant, are studied on a laboratory level. Excess nitrogen can
   be removed by utilising a nitrification process first and a
   denitrification process afterwards, and so reduce the content of
   residual mineral nitrogen to less than 100 mg l(-1). Oxidation of
   ammonium, the most critical phase, is carried out with an inoculum of
   nitrifying bacteria adapted to the wastewater type under adequate
   aeration, and carefully controlled pH. The dilution of wastewater is not
   needed, nor external organic carbon to reduce nitrate. Copyright (C)
   1996 Elsevier Science Ltd}},
