@article{ ISI:A1996VT98000028,
Author = {Ruzicka, M},
Title = {{An extension of the Mosey model}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{10}},
Pages = {{2440-2446}},
Month = {{OCT}},
Abstract = {{A model of the formation of linear higher fatty acids from glucose in
   the acidogenic phase of anaerobic digestion process is presented. It is
   an extension of the original Mosey model. It can be extended for
   even-carbon acids generally, while for odd-carbon acids it can be done
   with some restriction. This is due to the coupling between pathways for
   odd- and even-carbon acid formation. Higher acid formation occurs in the
   middle range of hydrogen concentrations. The changes in hydrogen
   concentrations cause a shift in the acid proportions. This is due to
   kinetic and not energetic reasons. The phenomenon of nonsaccharolytical
   acidogenesis is highlighted. The validity of the model presumptions is
   discussed. Copyright (C) 1996 Elsevier Science Ltd}},
