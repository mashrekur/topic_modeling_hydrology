@article{ ISI:A1996VB04500015,
Author = {Amaral, OC and Otero, R and Grimalt, JO and Albaiges, J},
Title = {{Volatile and semi-volatile organochlorine compounds in tap and riverine
   waters in the area of influence of a chlorinated organic solvent factory}},
