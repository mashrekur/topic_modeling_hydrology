@article{ ISI:A1996VM11900032,
Author = {Chen, YW and Buffle, J},
Title = {{Physicochemical and microbial preservation of colloid characteristics of
   natural water samples .2. Physicochemical and microbial evolution}},
Journal = {{WATER RESEARCH}},
Year = {{1996}},
Volume = {{30}},
Number = {{9}},
Pages = {{2185-2192}},
Month = {{SEP}},
Abstract = {{In this paper systematic studies were carried out by means of light
   scattering, photon correlation spectroscopy, transmission electron
   microscopy and bacterial analysis to investigate the evolution of
   colloidal samples during storage, in various conditions. The significant
   influence of temperature during short-term sedimentation fractionation
   has been shown. The time scales of coagulation of colloids and bacteria
   proliferation are reported as well as the significant influence of
   stirring in both cases. The stability of various size fractions of water
   samples with respect to bacterial growth has also been studied. General
   recommendations for performing analysis of aquatic colloids are
   proposed. Copyright (C) 1996 Elsevier Science Ltd}},
