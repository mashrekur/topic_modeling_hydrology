@article{ ISI:000273066300032,
Author = {Haest, P. J. and Springael, D. and Smolders, E.},
Title = {{Dechlorination kinetics of TCE at toxic TCE concentrations: Assessment
   of different models}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
