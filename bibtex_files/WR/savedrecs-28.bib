@article{ ISI:000275551300014,
Author = {Johnson, William P. and Pazmino, Eddy and Ma, Huilian},
Title = {{Direct observations of colloid retention in granular media in the
   presence of energy barriers, and implications for inferred mechanisms
   from indirect observations}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
Volume = {{44}},
Number = {{4, SI}},
Pages = {{1158-1169}},
Month = {{FEB}},
Abstract = {{In this paper we present direct observations of retention of colloids in
   granular porous media over a large size range (0.21-9.0 mu m) and
   generalize the significance of attachment in grain to grain contacts and
   attachment on the open surface as a function of colloid:collector ratio.
   We examine reversibility of attachment via these mechanisms with respect
   to ionic strength reduction and fluid velocity increase. We relate these
   direct observations to existing literature, and in some cases offer
   alternative interpretations of mechanisms of retention drawn from
   indirect observations (e.g. via column effluent and retained
   concentrations). (C) 2009 Elsevier Ltd. All rights reserved.}},
