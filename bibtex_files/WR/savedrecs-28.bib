@article{ ISI:000273066300031,
Author = {Knowles, P. R. and Griffin, P. and Davies, P. A.},
Title = {{Complementary methods to investigate the development of clogging within
   a horizontal sub-surface flow tertiary treatment wetland}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
