@article{ ISI:000172510600027,
Author = {Takagi, K and Inamura, T and Kawajiri, M and Noya, K and Hagiwara, Y and
   Suketa, Y},
Title = {{Estimation of endotoxin-like substances in deep seawater by using
   bioassay}},
Journal = {{WATER RESEARCH}},
