@article{ ISI:000344823500036,
Author = {Mestankova, Hana and Schirmer, Kristin and Canonica, Silvio and von
   Gunten, Urs},
Title = {{Development of mutagenicity during degradation of N-nitrosamines by
   advanced oxidation processes}},
