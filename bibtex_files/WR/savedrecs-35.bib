@article{ ISI:000342881300002,
Author = {Lu, Xixin and Huangfu, Xiaoliu and Zhang, Xiang and Wang, Yaan and Ma,
   Jun},
Title = {{Strong enhancement of trace mercury removal from aqueous solution with
   sodium thiosulfate by in situ formed Mn-(hydr)oxides}},
Journal = {{WATER RESEARCH}},
Year = {{2014}},
Volume = {{65}},
Pages = {{22-31}},
Month = {{NOV 15}},
Abstract = {{The effect of sodium thiosulfate (Na2S2O3) on trace mercury removal from
   aqueous solution by in situ MnOx was investigated. Removal efficiency
   was studied at different molar ratios of Na2S2O3/Mn (0, 0.264, 0.593 and
   1.582) and under changes in Mn dosage, reaction time and pH conditions.
   Additionally, the ionic strength and the mercury removal amount were
   examined to evaluate the efficiency of trace mercury removal. The
   results indicated that the presence of thiosulfate clearly improved
   removal of mercury from solution, and that increases in the ionic
   strength enhanced removal in a certain range of thiosulfate
   concentration. At neutral conditions, the mercury removal amount reached
   to maximum of 64 mu g/mg. It is proposed that the ability of thiosulfate
   to reduce some MnOx to Mn2+ as well as transfer the uncharged mercury
   species to a negatively charged species ({[}Hg(S2O3)(2)](2-)) improved
   trace mercury removal. The mechanism analysis revealed that ternary
   complexes or large aggregations may be formed because of surface
   complexation or electrostatic attraction. (C) 2014 Elsevier Ltd. All
   rights reserved.}},
