@article{ ISI:000332808600009,
Author = {Chen, Zhi-Feng and Ying, Guang-Guo and Jiang, Yu-Xia and Yang, Bin and
   Lai, Hua-Jie and Liu, You-Sheng and Pan, Chang-Gui and Peng, Fu-Qiang},
Title = {{Photodegradation of the azole fungicide fluconazole in aqueous solution
   under UV-254: Kinetics, mechanistic investigations and toxicity
   evaluation}},
Journal = {{WATER RESEARCH}},
Year = {{2014}},
Volume = {{52}},
Pages = {{83-91}},
Month = {{APR 1}},
Abstract = {{The azole fungicide fluconazole has been reported to be persistent in
   conventional wastewater treatment plants. This study investigated the
   photodegradation of fluconazole under UV-254 in aqueous solutions. The
   results revealed that the photodegradation of fluconazole was
   pH-dependent (2.0-12.0) following the pseudo-first-order kinetics with
   quantum yield values ranging from 0.023 to 0.090 mol einstein(-1), and
   it underwent a direct and self-sensitized mechanism involving O-1(2).
   The main photodegradation by-products were identified and
   semi-quantitated. The proposed photodegradation pathway included
   hydroxylative defluorination reaction. The 72 h-NOEC and 72 h-LOEC
   values for fluconazole using a freshwater unicellular green alga
   Pseudokirchneriella subcapitata were 10 mu M and 15 mu M. Overall, the
   photodegradation of fluconazole produced a significant decrease in algal
   toxicity. It also proved that the photodegradation by-products will not
   present extra toxicity to this alga than fluconazole itself. (C) 2014
   Elsevier Ltd. All rights reserved.}},
