@article{ ISI:000332808600015,
Author = {Schmidt, Marie and Lege, Sascha and Nijenhuis, Ivonne},
Title = {{Comparison of 1,2-dichloroethane, dichloroethene and vinyl chloride
   carbon stable isotope fractionation during dechlorination by two
   Dehalococcoides strains}},
Journal = {{WATER RESEARCH}},
Year = {{2014}},
Volume = {{52}},
Pages = {{146-154}},
Month = {{APR 1}},
Abstract = {{Carbon stable isotope fractionation during 1,2-dichloroethane (1,2-DCA),
   dichloroethene (DCE) and vinyl chloride (VC) dechlorination was analysed
   for two Dehalococcoides strains, Dehalococcoides mccartyi strain 195
   (formerly Dehalococcoides ethenogenes strain 195) and D. mccartyi strain
   BTF08, and used to characterize the reaction. The isotope enrichment
   factors (epsilon C) determined for 1,2-DCA were -30.8 +/- 1.3 parts per
   thousand and -29.0 +/- 3.0 parts per thousand for D. mccartyi strain
   BTF08 and D. mccartyi strain 195, respectively. Enrichment factors
   (epsilon C) determined for chlorinated ethenes with strain BTF08 were
   -28.8 +/- 1.5 parts per thousand (VC), -30.5 +/- 1.5 parts per thousand
   (cis-DCE) and -12.4 +/- 1.1 parts per thousand (1,1-DCE). Product,
   ethene, related enrichment factors (epsilon C-1,C-2-DCA-ethene)
   calculated for 1,2-DCA (-34.1 and -32.3 parts per thousand for strain
   BTF08 and strain 195, respectively) were similar to substrate based
   enrichment factors (epsilon C-1,C-2-DCA), supporting the hypothesis that
   ethene is the direct product of 1,2-DCA dichloroelimination but that VC
   was a side product as result of branching in the reaction. (C) 2014
   Elsevier Ltd. All rights reserved.}},
