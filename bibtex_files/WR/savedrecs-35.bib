Author = {Sang, Ziye and Jiang, Yanan and Tsoi, Yeuk-Ki and Leung, Kelvin Sze-Yin},
Title = {{Evaluating the environmental impact of artificial sweeteners: A study of
   their distributions, photodegradation and toxicities}},
Journal = {{WATER RESEARCH}},
