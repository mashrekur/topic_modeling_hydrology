@article{ ISI:000381169400046,
Author = {Roman, Pawel and Lipinska, Joanna and Bijrnans, Martijn F. M. and
   Sorokin, Dimitry Y. and Keesman, Karel J. and Janssen, Albert J. H.},
Title = {{Inhibition of a biological sulfide oxidation under haloalkaline
   conditions by thiols and diorgano polysulfanes}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{101}},
Pages = {{448-456}},
Month = {{SEP 15}},
Abstract = {{A novel approach has been developed for the simultaneous description of
   reaction kinetics to describe the formation of polysulfide and sulfate
   anions from the biological oxidation of hydrogen sulfide (H2S) using a
   quick, sulfide-dependent respiration test. Next to H2S, thiols are
   commonly present in sour gas streams. We investigated the inhibition
   mode and the corresponding inhibition constants of six thiols and the
   corresponding diorgano polysulfanes on the biological oxidation of H2S.
   A linear relationship was found between the calculated IC50 values and
   the lipophilicity of the inhibitors. Moreover, a mathematical model was
   proposed to estimate the biomass activity in the absence and presence of
   sulfurous inhibitors. The biomass used in the respiration tests
   originated from a full-scale biodesulfurization reactor. A microbial
   community analysis of this biomass revealed that two groups of
   microorganism are abundant, viz. Ectothiorhodospiraceae and
   Piscirickettsiaceae. (C) 2016 Elsevier Ltd. All rights reserved.}},
