@article{ ISI:000381950400014,
Author = {Okubo, Tsutomu and Kubota, Kengo and Yamaguchi, Takashi and Uemura,
   Shigeki and Harada, Hideki},
Title = {{Development of a new non-aeration-based sewage treatment technology:
   Performance evaluation of a full-scale down-flow hanging sponge reactor
   employing third-generation sponge carriers}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{102}},
Pages = {{138-146}},
Month = {{OCT 1}},
Abstract = {{A practical-scale down-flow hanging sponge (DHS) reactor using
   third-generation (G3) sponge was applied for treatment of the effluent
   from an up-flow anaerobic sludge blanket (UASB) treating municipal
   sewage. The process performance of the DHS reactor filled with G3 sponge
   (DHS-G3) was evaluated by conducting an on-site experiment in India over
   one year. The performance the DHS-G3 for removal of organic matter and
   ammonium-nitrogen at a relatively short retention time (HRT) of only
   0.66 h satisfied the Indian effluent quality standards except for fecal
   form. The removal rate constants for total biochemical oxygen demand
   (BOD) and fecal determined based on the water quality profiles along the
   DHS-G3 almost reached equilibrium imately four months after the start of
   operation, i.e., 2.45 h(-1) for BOD and 2.30 h(-1) for fecal
   respectively. The oxygen utilization activity of retained sludge was
   determined to assess the of heterotrophic and autotrophic bacteria along
   the DHS-G3. Nitrification was promoted in the portion of the DHS-G3
   reactor in the duration with low organic load, while it decreased when
   the load was increased, probably due to proliferation of heterotrophic
   bacteria. (C) 2016 Elsevier Ltd. All rights reserved.}},
