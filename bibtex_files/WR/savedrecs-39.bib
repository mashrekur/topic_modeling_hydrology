@article{ ISI:000388047500035,
Author = {Zhao, Zisheng and Zhang, Yaobin and Li, Yang and Zhao, Huimin and Quan,
   Xie},
Title = {{Electrochemical reduction of carbon dioxide to formate with Fe-C
   electrodes in anaerobic sludge digestion process}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{106}},
Pages = {{339-343}},
Month = {{DEC 1}},
Abstract = {{Electrochemical reduction of carbon dioxide (CO2) to useful chemicals is
   an attractive strategy to cut its emission in atmosphere. However, high
   overpotential and energy consumption required in the electrochemical
   reduction are the major barriers of this process. In this study, a new
   CO2 reduction technique for production of formic acid was proposed from
   waste activated sludge digestion in a microbial electrosynthesis system
   (MES) with iron plate and carbon pillar as the electrodes. Compared with
   other reactors, methane production of the Fe-C MES reactor was slightly
   lower and CO2 was undetectable. Instead, considerable formate (672.3
   mg/L) and H-2 (45.8 mL) were produced in this Fe-C MES reactor, but not
   found in the other reactors. It should be ascribed to the reduction of
   CO2 and H+ at cathode. The reduction of H+ resulted in a weak alkaline
   pH (9.3), which made the methanogenesis slightly lower in Fe-C MES. (C)
   2016 Elsevier Ltd. All rights reserved.}},
