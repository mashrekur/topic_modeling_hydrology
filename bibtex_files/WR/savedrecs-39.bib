@article{ ISI:000383292100026,
Author = {Zermeno-Motante, Maria I. and Nieto-Delgado, Cesar and Cannon, Fred S.
   and Cash, Colin C. and Wunz, Christopher C.},
Title = {{Chemical modeling for precipitation from hypersaline hydrofracturing
   brines}},
