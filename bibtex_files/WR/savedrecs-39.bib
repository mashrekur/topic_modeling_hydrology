@article{ ISI:000383292100015,
Author = {Rajasarkka, Johanna and Pernica, Marek and Kuta, Jan and Lasnak, Jonas
   and Simek, Zdenek and Blaha, Ludek},
Title = {{Drinking water contaminants from epoxy resin-coated pipes: A field study}},
Journal = {{WATER RESEARCH}},
