@article{ ISI:000376805500002,
Author = {Li, Yang and Niu, Junfeng and Shang, Enxiang and Crittenden, John
   Charles},
Title = {{Influence of dissolved organic matter on photogenerated reactive oxygen
   species and metal-oxide nanoparticle toxicity}},
Journal = {{WATER RESEARCH}},
Year = {{2016}},
Volume = {{98}},
Pages = {{9-18}},
Month = {{JUL 1}},
Abstract = {{The effect of humic acid (HA) or fulvic acid (FA) on reactive oxygen
   species (ROS) generation by six metal-oxide nanoparticles (NPs) and
   their toxicities toward Escherichia coli was investigated under UV
   irradiation. Dissolved organic matter (DOM) decreased (OH)-O-center dot
   generation by TiO2, ZnO, and Fe2O3, with FA inhibiting (OH)-O-center dot
   generation more than HA. The generated (OH)-O-center dot in NPs/DOM
   mixtures was higher than the measured concentrations because DOM
   consumes (OH)-O-center dot faster than its molecular probe. None of
   NPs/FA mixtures produced O-2(center dot-). The generated O-2(center
   dot-) concentrations in NPs/HA mixtures (except Fe2O3/HA) were higher
   than the sum of O-2(center dot-) concentrations that produced as NPs and
   HA were presented by themselves. Synergistic O-2(center dot-) generation
   in NPs/HA mixtures resulted from O-2 reduction by electron transferred
   from photoionized HA to NPs. DOM increased O-1(2) generation by TiO2,
   CuO, CeO2, and SiO2, and FA promoted O-1(2) generation more than HA.
   Enhanced O-1(2) generation resulted from DOM sensitization of NPs. HA
   did not increase O-1(2) generation by ZnO and Fe2O3 primarily because
   released ions quenched O-1(2) precursor ((3)HA{*}). Linear correlation
   was developed between total ROS concentrations generated by NPs/DOM
   mixtures and bacterial survival rates (R-2 >= 0.80). The results implied
   the necessity of considering DOM when investigating the photoreactivity
   of NPs. (C) 2016 Elsevier Ltd. All rights reserved.}},
