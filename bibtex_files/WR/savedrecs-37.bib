@article{ ISI:000353735800002,
Author = {Huang, Xianfeng and Li, Xuchun and Pan, Bingcai and Li, Hongchao and
   Zhang, Yanyang and Xie, Bihuang},
Title = {{Self-enhanced ozonation of benzoic acid at acidic pHs}},
Journal = {{WATER RESEARCH}},
Year = {{2015}},
Volume = {{73}},
Pages = {{9-16}},
Month = {{APR 15}},
Abstract = {{Ozonation of recalcitrant contaminants under acidic conditions is
   inefficient due to the lack of initiator (e.g., OH-) for ozone to
   produce hydroxyl radicals (HO center dot). In this study, we reported
   that benzoic acid (BA), which is inert to ozone attack, underwent
   efficient degradation by ozone at acidic pH (2.3). The kinetics of BA
   degradation and ozone decomposition were both enhanced by increasing BA
   concentrations. Essentially, it is a HO center dot-mediated reaction.
   Based on the exclusion of possible contributions of H2O2 and phenol-like
   intermediates for HO center dot production, the reaction mechanism
   involved the formation of ozone ion (O-3(center dot-)), which is an
   effective precursor of HO center dot, was thus proposed. The
   hydroxycyclohexadienyl-type radicals generated during the attack of BA
   by HO center dot may lead to the formation of O-3(center dot-).
   Meanwhile, O-3(center dot-) could also be possibly formed from the
   reaction between ozone and organic (e.g., ROO center dot) or inorganic
   peroxyl radicals (e.g., HO2 center dot). In addition, the hydroxylated
   products like phenol-like intermediates also played a positive role in
   HO center dot production. Consequently, HO center dot was produced
   efficiently under acidic conditions, resulting in rapid degradation of
   BA. This study provides a new approach for ozone activation even at
   acidic pHs, and broadens the knowledge of ozonation in removal of
   micropollutants from water. (C) 2015 Elsevier Ltd. All rights reserved.}},
