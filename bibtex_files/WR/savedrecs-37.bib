@article{ ISI:000363355700041,
Author = {Butkovskyi, A. and Leal, L. Hernandez and Rijnaarts, H. H. M. and
   Zeeman, G.},
Title = {{Fate of pharmaceuticals in full-scale source separated sanitation system}},
