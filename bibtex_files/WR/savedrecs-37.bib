@article{ ISI:000353735800016,
Author = {Hwang, Taewoon and Kotte, Madhusudhana Rao and Han, Jong-In and Oh,
   You-Kwan and Diallo, Mamadou S.},
Title = {{Microalgae recovery by ultrafiltration using novel fouling-resistant
   PVDF membranes with in situ PEGylated polyethyleneimine particles}},
Journal = {{WATER RESEARCH}},
