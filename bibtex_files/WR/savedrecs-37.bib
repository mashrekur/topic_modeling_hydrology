@article{ ISI:000353605000014,
Author = {Gaffney, Vanessa de Jesus and Almeida, Cristina M. M. and Rodrigues,
   Alexandre and Ferreira, Elisabete and Benoliel, Maria Joao and Cardoso,
   Vitor Vale},
Title = {{Occurrence of pharmaceuticals in a water supply system and related human
   health risk assessment}},
Journal = {{WATER RESEARCH}},
Year = {{2015}},
Volume = {{72}},
Number = {{SI}},
Pages = {{199-208}},
Month = {{APR 1}},
Abstract = {{A monitoring study of 31 pharmaceuticals along Lisbon's drinking Water
   supply system was implemented, which comprised the analysis of 250
   samples including raw water (surface water and groundwater), and
   drinking water. Of the 31 pharmaceutical compounds, only sixteen were
   quantified in the analyzed samples, with levels ranging from 0.005 to 46
   ng/L in raw water samples and 0.09-46 ng/L in drinking water samples.
   The human health risk assessment performed showed that appreciable risks
   to the consumer's health arising from exposure to trace levels of
   pharmaceuticals in drinking water are extremely unlikely, as RQs values
   were all below 0.001. Also, pharmaceuticals were selected as indicators
   to be used as a tool to control the quality of raw water and the
   treatment efficiency in the drinking water treatment plants. (C) 2014
   Elsevier Ltd. All rights reserved.}},
