@article{ ISI:000367413000049,
Author = {Fernandez, Y. Bajon and Green, K. and Schuler, K. and Soares, A. and
   Vale, P. and Alibardi, L. and Cartmell, E.},
Title = {{Biological carbon dioxide utilisation in food waste anaerobic digesters}},
Journal = {{WATER RESEARCH}},
Year = {{2015}},
Volume = {{87}},
Pages = {{467-475}},
Month = {{DEC 15}},
Abstract = {{Carbon dioxide (CO2) enrichment of anaerobic digesters (AD) was
   previously identified as a potential on-site carbon revalorisation
   strategy. This study addresses the lack of studies investigating this
   concept in up-scaled units and the need to understand the mechanisms of
   exogenous CO2 utilisation. Two pilot-scale ADs treating food waste were
   monitored for 225 days, with the test unit being periodically injected
   with CO2 using a bubble column. The test AD maintained a CH4 production
   rate of 0.56 +/- 0.13 m(3) CH4.(kg VSfed d)(-1) and a CH4 concentration
   in biogas of 68\% even when dissolved CO2 levels were increased by a 3
   fold over the control unit. An additional uptake of 0.55 kg of exogenous
   CO2 was achieved in the test AD during the trial period. A 2.5 fold
   increase in hydrogen (H-2) concentration was observed and attributed to
   CO2 dissolution and to an alteration of the acidogenesis and
   acetogenesis pathways. A hypothesis for conversion of exogenous CO2 has
   been proposed, which requires validation by microbial community
   analysis. (C) 2015 The Authors. Published by Elsevier Ltd.}},
