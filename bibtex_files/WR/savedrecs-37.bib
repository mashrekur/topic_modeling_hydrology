@article{ ISI:000363355700045,
Author = {Zhang, Lili and Zhang, Chao and Hu, Chengzhi and Liu, Huijuan and Bai,
   Yaohui and Qu, Jiuhui},
Title = {{Sulfur-based mixotrophic denitrification corresponding to different
   electron donors and microbial profiling in anoxic fluidized-bed membrane
   bioreactors}},
