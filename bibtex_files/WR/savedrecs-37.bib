@article{ ISI:000363355700005,
Author = {Doherty, Liam and Zhao, Yaqian and Zhao, Xiaohong and Hu, Yuansheng and
   Hao, Xiaodi and Xu, Lei and Liu, Ranbin},
Title = {{A review of a recently emerged technology: Constructed wetland -
   Microbial fuel cells}},
Journal = {{WATER RESEARCH}},
Year = {{2015}},
Volume = {{85}},
Pages = {{38-45}},
Month = {{NOV 15}},
Abstract = {{Constructed wetlands (CWs) and microbial fuel cells (MFCs) are
   compatible technologies since both are reliant on the actions of
   bacteria to remove contaminants from wastewater. MFCs require the anode
   to remain anaerobic with the cathode exposed to oxygen while these redox
   conditions can develop naturally in CWs. For this reason, research into
   combining the two technologies (termed as CW-MFC) has emerged in recent
   years with the aim of improving the wastewater treatment capacity of
   wetlands while simultaneously producing electrical power. Based on the
   published work (although limited), this review aims to provide a timely,
   current state-of-the-art in CW-MFC while exploring future challenges and
   research directions. (C) 2015 Elsevier Ltd. All rights reserved.}},
