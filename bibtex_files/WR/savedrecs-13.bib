@article{ ISI:000179091500003,
Author = {Jun, M and Wei, L},
Title = {{Effectiveness of ferrate (VI) preoxidation in enhancing the coagulation
   of surface waters}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{20}},
Pages = {{4959-4962}},
Month = {{DEC}},
Abstract = {{Standard jar tests were conducted to evaluate the effectiveness of
   ferrate preoxidation in enhancing the coagulation of surface waters. A
   substantial reduction of residual turbidity after sedimentation and
   filtration was achieved by ferrate preoxidation in all cases of the
   investigation of various water qualities at low ferrate dosage (0.5-1.0
   mg/l). The enhancement of the coagulation was more obvious when the
   organic content in the waters were relatively high. (C) 2002 Published
   by Elsevier Science Ltd.}},
