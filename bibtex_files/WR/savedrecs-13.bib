@article{ ISI:000178394500010,
Author = {Brion, GM and Neelakantan, TR and Lingireddy, S},
Title = {{A neural-network-based classification scheme for sorting sources and
   ages of fecal contamination in water}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{15}},
Pages = {{3765-3774}},
Month = {{SEP}},
Abstract = {{Artificial neural networks (ANNs) were successfully applied to data
   observations from a small watershed consisting of commonly measured
   indicator bacteria, weather conditions, and turbidity to distinguish
   between human sewage and animal-impacted runoff, fresh runoff from aged,
   and agricultural land-use-associated fresh runoff from that of suburban
   land-use-associated-fresh runoff. The ANNs were applied in a cascading,
   or hierarchical scheme. ANN performance was measured in two ways: (1)
   training and (2) testing. An ANN was able to sort sewage from runoff
   with <1\% error. Turbidity was found to be relatively unimportant for
   sorting sewage from runoff, while gross measurements of gram-negative
   and gram-positive bacteria were required. Predictions clustered tightly
   around the known values. ANN classification of aged suburban runoff from
   fresh, and agricultural runoff from suburban was accomplished with >90\%
   accuracy. (C) 2002 Elsevier Science Ltd. All rights reserved.}},
