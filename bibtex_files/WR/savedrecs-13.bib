@article{ ISI:000173680300012,
Author = {Gutierrez, M and Etxebarria, J and de las Fuentes, L},
Title = {{Evaluation of wastewater toxicity: comparative study between Microtox
   (R) and activated sludge oxygen uptake inhibition}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{4}},
Pages = {{919-924}},
Month = {{FEB}},
Abstract = {{Microtox(R) is a frequent toxicity tool for the screening of wastewaters
   discharged into wastewater treatment plants. There is currently an
   increasing controversy between this test and others using activated
   sludge. A Microtox(R) and electrolytic respirometry comparative study
   for toxicity determination has been performed. Seven organic and five
   inorganic toxic compounds have been assessed for comparing both methods.
   Microtox(R) proved to have a higher sensitivity to toxicants but was
   less representative of effects on activated sludge compared to
   respirometry. For instance, assays accomplished with LAS, a
   biodegradable reference surfactant. showed a toxic effect by Microtox(R)
   but good biodegradability and no toxicity in respirometry. This could be
   explained by the different nature of the biological material used, as
   Microtox(R) utilises the seawater Vibrio fischeri, whereas respirometry
   uses the bacterial consortium in activated sludge. For the evaluation of
   the potential toxicity of a compound on a WWTP, the preferred biological
   material be used should be activated sludge itself. Results obtained
   with any other biological material would be just an approach to reality.
   (C) 2002 Elsevier Science Ltd. All rights reserved.}},
