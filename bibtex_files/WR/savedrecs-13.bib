@article{ ISI:000177109900010,
Author = {Lin, SS and Chen, CL and Chang, DJ and Chen, CC},
Title = {{Catalytic wet air oxidation of phenol by various CeO2 catalysts}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{12}},
Pages = {{3009-3014}},
Month = {{JUL}},
Abstract = {{Investigations of various CeO2 catalysts using XRD, O-2-TPD, H-2-TPR and
   catalytic wet air oxidation of phenol showed that CeO2 calcined under
   higher thermal impact (designated as type A) displayed excellent
   catalytic ability for the redox reaction of organics. Results of the
   investigations mentioned above showed that type A CeO2 has a less stable
   structure, more structure oxygen exchangeable and more oxidizing ability
   for the consumption of H-2 and conversion of phenol. At phenol
   concentrations between 400 and 2500 mg/L, oxygen pressure in the range
   of 0.5-1.0 MPa, and temperatures above 160degreesC phenol conversion
   stood at greater than or equal to90\% after 4 It. Total organic carbon
   removal increased with the reaction temperature. Under the reaction
   conditions, CO2 selectivity was about 80\% or more after 4 It reaction.
   The reduced catalytic ability of regenerated CeO2 was caused either by
   stabilization of CeO2 during the process of regeneration or by the
   deposition of some reaction residues. (C) 2002 Elsevier Science Ltd. All
   rights reserved.}},
