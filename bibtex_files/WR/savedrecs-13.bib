@article{ ISI:000174067800004,
Author = {Bhatti, ZI and Toda, H and Furukawa, K},
Title = {{p-Nitrophenol degradation by activated sludge attached on nonwovens}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{5}},
Pages = {{1135-1142}},
Month = {{MAR}},
Abstract = {{p-Nitrophenol (PNP) is a toxic compound that enters the environment
   during, manufacturing and processing of a variety of industrial
   products. This study demonstrates the use of inexpensive and durable
   nonwovens as a biomass retainer for the biological degradation of
   p-nitrophenol. An essential aspect of p-nitrophenol degradation was the
   cultivation of p-nitrophenol degrading biomass prior to its attachment
   on the nonwovens. Results of continuous flow experiments demonstrated
