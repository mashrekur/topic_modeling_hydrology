@article{ ISI:000175982800019,
Author = {Ruel, SM and Comeau, Y and Heduit, A and Deronzier, G and Ginestet, P
   and Audic, JM},
Title = {{Operating conditions for the determination of the biochemical acidogenic
   potential of wastewater}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{9}},
Pages = {{2337-2341}},
Month = {{MAY}},
Abstract = {{The aim of this work was to study the test conditions for the
   determination of the biochemical acidogenic potential (BAP) of
   wastewater, which should be useful to predict the performance of
   enhanced biological phosphorus removal (EBPR). Proposed operating
   conditions for a simple and reproducible BAP test in 250 ml serum
   bottles (equipped with black butyl stoppers and magnetic bars) are: use
   of either frozen or fresh water, no inoculum addition, fermentation
   carried out in the dark during 15 days, addition of 1 mM bromo-ethane
   sulfortate (BES) and 2 mM barium chloride (BaCl(2)) stirring speed
   strong enough to maintain vortex conditions, no pH control and
