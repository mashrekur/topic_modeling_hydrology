@article{ ISI:000174067800017,
Author = {Moilleron, R and Gonzalez, A and Chebbo, G and Thevenot, DR},
Title = {{Determination of aliphatic hydrocarbons in urban runoff samples from the
   ``Le Marais{''} experimental catchment in Paris centre}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
