@article{ ISI:000178600700019,
Author = {Chen, H and Jin, XL and Zhu, K and Yang, RQ},
Title = {{Photocatalytic oxidative degradation of acridine orange in aqueous
   solution with polymeric metalloporphyrins}},
Journal = {{WATER RESEARCH}},
