@article{ ISI:000178600700012,
Author = {Graca, MAS and Rodrigues-Capitulo, A and Ocon, C and Gomez, N},
Title = {{In situ tests for water quality assessment: a case study in Pampean
   rivers}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{16}},
Pages = {{4033-4040}},
Month = {{SEP}},
Abstract = {{Two invertebrate species (Hyalella curvispina and Palaemonetes
   argentinus) and one macrophyte (Egeria densa) from a naturally high
   nutrient content system (Pampean rivers of La Plata, Argentina) were
   evaluated for their potential use in situ assays aiming to assess
   changes in water quality, Invertebrates were individually placed in
   cylindrical chambers in polluted sections of rivers and in reference
   upstream sites. Mortality after 48 h was high in polluted and reduced in
   control sites. Mortality was also higher in situ assays than in
   laboratory static tests, Standard sections of the macrophyte were also
   deployed at the reference and control sites. Growth (7 days) in terms of
   mass increment (but not in length) was consistently reduced in polluted
   sites. Results of benthic invertebrate and periphitic algae survey's
   were consistent with the in situ tests: pollution resulted in a decrease
   in the number of taxa, taxa replacement and in changes in the value of
   the biotic indices Indice Biotico PAMPeano and Indice de Diatomeas
   Pampeano, indicating deterioration of water quality. In situ assays have
   a high potential as environmental tools in integrated approaches of
   bioassessment programs. (C) 2002 Elsevier Science Ltd. All rights
   reserved.}},
