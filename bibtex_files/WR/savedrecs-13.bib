@article{ ISI:000177441400023,
Author = {Jung, J and Lee, MJ},
Title = {{EPR investigation on the efficiency of hydroxyl radical production of
   gamma-irradiated anatase and bentonite}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{13}},
Pages = {{3359-3363}},
Month = {{JUL}},
Abstract = {{Anatase and bentonite were treated by gamma rays in various conditions,
   and the change of the catalysts was characterized by electron
   paramagnetic resonance spectroscopy. Anatase gave four peaks with
   g(parallel to) = 1.951 g(perpendicular to) = 1.973, g = 1.992 and 2.005.
   The height of the four peaks was directly proportional to the efficiency
   of hydroxyl radical production, and anatase treated by gamma rays in
   alkaline condition showed the most efficient production. Bentonite gave
   a peak with g = 2.005. In contrast to anatase, the peak height was
   inversely proportional to the efficiency of hydroxyl radical production,
   and non-treated bentonite was the most efficient catalyst. Furthermore,
   the efficiency of hydroxyl radical production of the catalysts
   significantly influenced the decomposition of trichloroethylene and
   perchloroethylene by gamma rays. (C) 2002 Elsevier Science Ltd. All
   rights reserved.}},
