@article{ ISI:000178600700015,
Author = {Jiang, JQ and Graham, N and Andre, C and Kelsall, GH and Brandon, N},
Title = {{Laboratory study of electro-coagulation-flotation for water treatment}},
Journal = {{WATER RESEARCH}},
