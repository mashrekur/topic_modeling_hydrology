@article{ ISI:000174067800027,
Author = {Zona, R and Solar, S and Gehringer, P},
Title = {{Degradation of 2,4-dichlorophenoxyacetic acid by ionizing radiation:
   influence of oxygen concentration}},
Journal = {{WATER RESEARCH}},
Year = {{2002}},
Volume = {{36}},
Number = {{5}},
Pages = {{1369-1374}},
Month = {{MAR}},
Abstract = {{Ionizing radiation has been proved as a promising method for the
   degradation of the herbicide 2,4-dichlorophenoxyacetic acid (2,4-D). The
   gamma-radiolytic decomposition and chloride formation follows an
   apparent first order kinetic, k(de)/k(Cl) = 3. For complete removal of
   500 muM 2,4-D, a dose of 4 kGy is required. Phenolic intermediates
   (2,4-dichlorophenol, the isomers 2-chloro-4-hydroxy- and
   4-chloro-2-hydroxy phenoxyacetic acid and three hydroxylation products
   of 2,4-D) are formed, Using oxygen saturation during irradiation, they
   are removed again with a dose of 4kGy. For the formation of the main
   organic product acetic acid a reaction mechanism is discussed. Oxygen
   concentration enhances strongly fragmentation and mineralization. A
   reduction of 36\% TOC could be achieved. (C) 2002 Elsevier Science Ltd.
   All rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
