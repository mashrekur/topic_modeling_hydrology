@article{ ISI:000238014600013,
Author = {Ngomsik, Audrey-Flore and Bee, Agnes and Siaugue, Jean-Michel and
   Cabuil, Valerie and Cote, Gerard},
Title = {{Nickel adsorption by magnetic alginate microcapsules containing an
   extractant}},
Journal = {{WATER RESEARCH}},
Year = {{2006}},
Volume = {{40}},
Number = {{9}},
Pages = {{1848-1856}},
Month = {{MAY}},
Abstract = {{The adsorption of heavy metals on biomaterials was investigated by
   studying the potential of alginate microcapsules containing an
   extractant (Cyanex 272) and magnetic nanoparticles (gamma-Fe2O3) for the
   adsorption of nickel (II) from aqueous solutions. A two-stage kinetics
   behaviour was observed with 70\% of the maximum sorption capacity
   achieved within 8h. An increase in nickel removal with increase in pH
   occurred, the maximum uptake capacity being around 0.42 mmol g(-1) at pH
   8. The adsorption isotherm (pH about 5.3) was obtained in a wide range
   of initial nickel concentrations; the experimental data were fitted by a
