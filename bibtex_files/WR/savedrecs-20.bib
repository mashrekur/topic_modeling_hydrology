@article{ ISI:000240305800008,
Author = {Boyer, Treavor H. and Singer, Philip C.},
Title = {{A pilot-scale evaluation of magnetic ion exchange treatment for removal
   of natural organic material and inorganic anions}},
Journal = {{WATER RESEARCH}},
Year = {{2006}},
Volume = {{40}},
Number = {{15}},
Pages = {{2865-2876}},
Month = {{AUG}},
Abstract = {{The objective of this research was to evaluate a magnetic ion exchange
   process (MIEX) for the removal of natural organic material (NOM) and
   bromide on a continuous-flow pilot-scale basis under different operating
   conditions and raw water characteristics. The most important operating
   variable was the effective resin dose (ERD), which is the product of the
   steady-state resin concentration in the contactor and the regeneration
   ratio. The raw water employed in this study had a moderate concentration
   of ultraviolet (UV)-absorbing substances and dissolved organic carbon
   (DOC), and a low turbidity, alkalinity, and concentration of competing
   anionic species. Experiments were conducted using the ambient raw water
   and raw water spiked with bromide, chloride, and sulfate. Substantial
   removal of UV-absorbing substances and DOC was achieved at ERDs as low
   as 0.16 mL/L. Moderate bromide removal was achieved, depending on the
   ERD. Increasing the sulfate concentration resulted in decreased removal
   of UV-absorbing substances, DOC, and bromide. Consistent results were
   observed between the continuous-flow pilot plant tests and batch
   equilibrium studies. (c) 2006 Elsevier Ltd. All rights reserved.}},
