@article{ ISI:000240052200006,
Author = {Zhang, Daijun and Li, Zhenliang and Lu, Peili and Zhang, Tian and Xua,
   Danyu},
Title = {{A method for characterizing the complete settling process of activated
   sludge}},
Journal = {{WATER RESEARCH}},
Year = {{2006}},
Volume = {{40}},
