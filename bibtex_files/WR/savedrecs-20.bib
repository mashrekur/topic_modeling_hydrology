@article{ ISI:000242988600005,
Author = {Ahlgren, Joakim and Reitzel, Kasper and Danielsson, Rolf and Gogoll,
   Adolf and Rydin, Emil},
Title = {{Biogenic phosphorus in oligotrophic mountain lake sediments: Differences
   in composition measured with NMR spectroscopy}},
Journal = {{WATER RESEARCH}},
Year = {{2006}},
Volume = {{40}},
Number = {{20}},
Pages = {{3705-3712}},
Month = {{DEC}},
Abstract = {{Phosphorus (P) composition in alkaline sediment extracts from three
   Swedish oligotrophic mountain lakes was investigated using P-31-NMR
   spectroscopy. Surface sediments from one natural lake and two mature
   reservoirs, one of which has received nutrient additions over the last 3
   years, were compared with respect to biogenic P composition. The results
   show significant differences in the occurrence of labile and biogenic P
   species in the sediments of the different systems. The P compound groups
   that varied most between these three systems were pyrophosphate and
   polyphosphates, compound groups known to play an important role in
   sediment P recycling. The content of these compound groups was lowest in
   the reservoirs and may indicate a coupling between anthropogenic
   disturbances (i.e., impoundment) to a water system and the availability
   of labile P species in the sediment. A statistical study was also
   conducted to determine the accuracy and reliability of using P-31-NMR
   spectroscopy for quantification of sediment P forms. (c) 2006 Elsevier
   Ltd. All rights reserved.}},
