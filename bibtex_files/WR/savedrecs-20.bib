@article{ ISI:000237853400023,
Author = {Gogate, Parag R. and Pandit, Aniruddha B. and Wilhelm, AnneMarie and
   Ratsimba, Berthe and Delmas, Henri},
Title = {{Destruction of formic acid using high frequency cup horn reactor}},
Journal = {{WATER RESEARCH}},
