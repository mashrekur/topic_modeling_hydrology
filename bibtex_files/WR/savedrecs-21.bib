@article{ ISI:000248634600024,
Author = {Zhou, Feng and Huang, Gordon H. and Guo, Huaicheng and Zhang, Wei and
   Hao, Zejia},
Title = {{Spatio-temporal patterns and source apportionment of coastal water
   pollution in eastern Hong Kong}},
Journal = {{WATER RESEARCH}},
Year = {{2007}},
Volume = {{41}},
