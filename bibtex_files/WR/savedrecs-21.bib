@article{ ISI:000244410700001,
Author = {Kumar, Gupta Sunil and Gupta, S. K. and Singh, Gurdeep},
Title = {{Biodegradation of distillery spent wash in anaerobic hybrid reactor}},
Journal = {{WATER RESEARCH}},
