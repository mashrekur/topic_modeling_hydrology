@article{ ISI:000247585800005,
Author = {Dantas, Renato F. and Canterino, Marisa and Marotta, Raffaele and Sansa,
   Carme and Esplugas, Santiago and Andreozzi, Roberto},
Title = {{Bezafibrate removal by means of ozonation: Primary intermediates,
   kinetics, and toxicity assessment}},
