@article{ ISI:000248157600007,
Author = {Ren, Yong-Xiang and Nakano, Kazunori and Nomura, Munehiro and Chiba,
   Nobuo and Nishimura, Osamu},
Title = {{Effects of bacterial activity on estrogen removal in nitrifying
   activated sludge}},
Journal = {{WATER RESEARCH}},
Year = {{2007}},
Volume = {{41}},
Number = {{14}},
Pages = {{3089-3096}},
Month = {{JUL}},
Abstract = {{The effects of bacterial activity on the degradation of estrone (El),
   estradiol (E2), estriol (E3) and ethinylestradiol (EE2) in nitrifying
   activated sludge (NAS) were studied with different substrates and
   organic loading rates (OLRs) and low temperature conditions. Heterotroph
   was shown to have utilized glucose prior to El for metabolism. The
   co-metabolism of ammonia oxidizing bacteria (AOB) dominated the
   degradation of El, E2 and EE2 in NAS. The higher the organic loading,
   the higher the rate of organic matter transformation, with less ammonia
   oxidation and less degradation of El, E2 and EE2. The degradation of E3
   in NAS was shown to be largely due to heterotroph metabolism. On the
   basis of the difference of apparent activity between heterotroph and AOB
   at 4 degrees C, the process of estrogen degradation via the
   co-metabolism of AOB was able to be identified. (C) 2007 Published by
   Elsevier Ltd.}},
