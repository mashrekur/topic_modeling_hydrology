@article{ ISI:000246465400011,
Author = {Germain, E. and Nelles, F. and Drews, A. and Pearce, R. and Kraume, M.
   and Reid, E. and Judd, S. J. and Stephenson, T.},
Title = {{Biomass effects on oxygen transfer in membrane bioreactors}},
