@article{ ISI:000283910600004,
Author = {Pereira, V. J. and Fernandes, D. and Carvalho, G. and Benoliel, M. J.
   and San Romao, M. V. and Barreto Crespo, M. T.},
Title = {{Assessment of the presence and dynamics of fungi in drinking water
   sources using cultural and molecular methods}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
Volume = {{44}},
Number = {{17, SI}},
Pages = {{4850-4859}},
Month = {{SEP}},
Abstract = {{A comparison of different isolation techniques and culture media for
   detection of filamentous fungi and yeasts in the aquatic environment
   revealed that the use of membrane filtration with the media dichloran
   rose bengal chloramphenicol (DRBC) optimized fungi detection in terms of
   abundance and variety in three untreated water sources with very
   different characteristics (surface water, spring water, and
   groundwater). The diversity of the fungi population captured by direct
   DNA extraction of fungi collected by membrane filtration was compared
   with the isolates obtained after selective growth using different
   culture media through amplification of the internal transcribed spacer
   gene and denaturing gradient gel electrophoresis (DGGE). The Czapek-Dox
   agar, Sabouraud dextrose agar, and DRBC media showed closer similarities
   to those obtained by the uncultured biomass for the different water
   sources. Based on these data and the best enumeration results, DRBC is
   recommended for the assessment of fungi in water sources using
   culture-based methods. DGGE was also used to monitor temporal variations
   in the fungal population structure and showed that each water matrix
   possessed a distinct population profile as well as that changes in the
   fungal community can be expected in the different matrices throughout
   the year. (C) 2010 Elsevier Ltd. All rights reserved.}},
