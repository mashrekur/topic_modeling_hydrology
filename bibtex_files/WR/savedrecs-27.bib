@article{ ISI:000285443700021,
Author = {Vione, Davide and Khanra, Swapan and Das, Radharani and Minero, Claudio
   and Maurino, Valter and Brigante, Marcello and Mailhot, Gilles},
Title = {{Effect of dissolved organic compounds on the photodegradation of the
   herbicide MCPA in aqueous solution}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
Volume = {{44}},
Number = {{20}},
Pages = {{6053-6062}},
Month = {{DEC}},
Abstract = {{This work shows that the addition of phenol and 2-propanol as model
   organic compounds significantly decreases the direct photolysis quantum
   yield of 4-chloro-2-methylphenoxy-acetic acid (MCPA) upon UVB
   irradiation in aqueous solution. Laser flash photolysis data suggest
   that 2-propanol is able to decrease the formation of the MCPA excited
   states under irradiation. A decrease from 0.54 to 0.34 of the photolysis
   quantum yield of the anionic form of MCPA (which prevails over the
   undissociated one in surface waters) could have a considerable impact on
   the MCPA lifetime in ecosystems where the direct photolysis is the main
   phototransformation pathway. In surface water bodies where the direct
   photolysis has comparable kinetics as the reaction with center dot OH, a
   decrease of the quantum yield would enhance the relative importance of
   the center dot OH pathway, which yields considerably less toxic
   intermediates than the direct photolysis. (C) 2010 Elsevier Ltd. All
   rights reserved.}},
