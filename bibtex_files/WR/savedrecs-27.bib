@article{ ISI:000278985500015,
Author = {Magureanu, Monica and Piroi, Daniela and Mandache, Nicolae Bogdan and
   David, Victor and Medvedovici, Andrei and Parvulescu, Vasile I.},
Title = {{Degradation of pharmaceutical compound pentoxifylline in water by
   non-thermal plasma treatment}},
Journal = {{WATER RESEARCH}},
