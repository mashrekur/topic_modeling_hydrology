@article{ ISI:000283910600003,
Author = {Shin, Seung Gu and Han, Gyuseong and Lim, Juntaek and Lee, Changsoo and
   Hwang, Seokhwan},
Title = {{A comprehensive microbial insight into two-stage anaerobic digestion of
   food waste-recycling wastewater}},
Journal = {{WATER RESEARCH}},
