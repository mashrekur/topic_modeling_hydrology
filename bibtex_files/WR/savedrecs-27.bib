@article{ ISI:000285443700027,
Author = {Dominguez-Chicas, Angelina and Scrimshaw, Mark D.},
Title = {{Hazard and risk assessment for indirect potable reuse schemes: An
   approach for use in developing Water Safety Plans}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
