@article{ ISI:000277556300005,
Author = {Seo, Dong Cheol and DeLaune, R. D.},
Title = {{Fungal and bacterial mediated denitrification in wetlands: Influence of
   sediment redox condition}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
Volume = {{44}},
