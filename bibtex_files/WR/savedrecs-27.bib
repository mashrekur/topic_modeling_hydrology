@article{ ISI:000275962200013,
Author = {Sanches, Sandra and Barreto Crespo, Maria T. and Pereira, Vanessa J.},
Title = {{Drinking water treatment of priority pesticides using low pressure UV
   photolysis and advanced oxidation processes}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
