@article{ ISI:000276668700010,
Author = {Ramdani, Abdellah and Dold, Peter and Deleris, Stephane and Lamarre,
   Daniel and Gadbois, Alain and Comeau, Yves},
Title = {{Biodegradation of the endogenous residue of activated sludge}},
Journal = {{WATER RESEARCH}},
Year = {{2010}},
