@article{ ISI:000071045900033,
Author = {Morgenroth, E and Sherden, T and van Loosdrecht, MCM and Heijnen, JJ and
   Wilderer, PA},
Title = {{Aerobic granular sludge in a sequencing batch reactor}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{12}},
Pages = {{3191-3194}},
Month = {{DEC}},
Abstract = {{In a laboratory scale sequencing batch reactor (SBR) granules were
   cultured under aerobic conditions. To enhance the growth of granular
   sludge the SBR was operated with very short sedimentation and draw
   phases resulting in the washout of slow settling biomass. Fast settling
   granules were retained in the reactor and thus had an advantage over
   flocs with a slower settling velocity. After 40 days of operation
   granules were the dominant form of microbial aggregates in the reactor,
   even though some pin-point flocs remained in the system. Granules taken
   from the reactor were stored for weeks without disintegrating. After
   about 130 days of operation the granule quality and GOD-removal
   worsened. The reasons for that are yet to be investigated. (C) 1997
   Elsevier Science Ltd.}},
