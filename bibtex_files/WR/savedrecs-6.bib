@article{ ISI:A1997WZ85200033,
Author = {Schramm, KW and Winkler, R and Casper, P and Kettrup, A},
Title = {{PCDD/F in recent and historical sediment layers of Lake Stechlin,
   Germany}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{6}},
Pages = {{1525-1531}},
Month = {{JUN}},
Abstract = {{Lake Stechlin sediment layers were dated and analysed for PCDD/F. The
   concentrations of PCDD/F in the investigated sediment have increased by
   a factor of about 100 since the last century and have decreased by a
   factor of two since the 1970s. The latter finding can be confirmed by
   recent temporal trends in human samples and herring-gull eggs. The
   homologue patterns especially for PCDF are subject to temporal
   variations and can be mainly related to municipal waste incineration and
   the impact of the precursor PCB. The overall time trend corresponds
   roughly with the anthropogenic world-wide production of chlorine. (C)
   1997 Elsevier Science Ltd.}},
