@article{ ISI:A1997WV97000036,
Author = {Munoz, MA and Sanchez, JM and RodriguezMaroto, JM and Borrego, JJ and
   Morinigo, MA},
Title = {{Methane production in anaerobic sludges supplemented with two support
   materials and different levels of acetate and sulphate}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{5}},
Pages = {{1236-1242}},
Month = {{MAY}},
Abstract = {{The influence of two support materials, diabase and polyvinyl chloride
   (PVC), on the methanogenesis from anaerobic sludges supplemented with
   different concentrations of acetate and sulphate was evaluated by
   applying a mathematical model according to the response surface
   technique. The results obtained suggest that the addition of both
   support materials did not exert significant effects on the
   methanogenesis From domestic sludges. The best conditions to produce
   methane were obtained with the lowest concentration of sulphate (1 mM)
   and with the highest concentrations of acetate (75-100 mM). (C) 1997
   Elsevier Science Ltd.}},
