@article{ ISI:A1997WM12100006,
Author = {Janic, J and Conkic, L and Kiurski, J and Benak, J},
Title = {{A method for arsenic level determination and a device for arsenic
   reduction in drinking water}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{3}},
Pages = {{419-428}},
Month = {{MAR}},
Abstract = {{A method of non-flame atomic absorption spectrometry for total arsenic
   concentration at trace level in liquid samples by the hydrides
   generation technique was developed. Separation of arsenic from liquid
   samples was done by reduction of samples using stanochloride. Argon was
   used as supporting gas. The detection limit in this method was 1 ppb.
   The method was checked on water samples taken from water-supply wells by
   determination of total arsenic level. The interfering influence of Au,
   Ag, Cu, Se and Fe on total arsenic concentration determination was also
   examined. Laboratory and semi-industrial devices for quick filtration of
   dissolved arsenic from well water were constructed. Single- and
   double-layer filters filled with quartz sand and anthracite with
   filtration speeds of 20 and 12 m(3)/h, respectively, were used. The
   average reduction of total arsenic concentration level of 42\% was
   attained. (C) 1997 Elsevier Science Ltd.}},
