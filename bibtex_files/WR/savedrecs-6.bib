@article{ ISI:A1997WV97000035,
Author = {Zhou, YY and Zhou, XY},
Title = {{Seasonal variation in kinetic parameters of alkaline phosphatase
   activity in a shallow Chinese freshwater lake (Donghu Lake)}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{5}},
Pages = {{1232-1235}},
Month = {{MAY}},
Abstract = {{Seasonal variation of the kinetic parameters of total alkaline
   phosphatase activity (APA) was studied in a shallow Chinese freshwater
   lake (Donghu Lake). At the three experimental stations the values of
   V-max of APA were higher and the negative correlation between
   orthophosphate and the total APA specific activity (V-max/Chl.) was
   stronger during summer (from June to September) P depletion. At the same
   time, the values of Michaelis constant (K-m) of APA at the three
   stations decreased. Phytoplankton seem to compensate for their
   phosphorus deficiency not only by an increase in enzyme production but
   also by an improved ability to use low substrate concentrations. (C)
   1997 Elsevier Science Ltd.}},
