@article{ ISI:A1997WD07300009,
Author = {Cloot, A and LeRoux, G},
Title = {{Modelling algal blooms in the middle Vaal River: A site specific
   approach}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{2}},
Pages = {{271-279}},
Month = {{FEB}},
Abstract = {{In this article a multi-algal species mathematical model is presented
   that is aimed at assisting the South African authorities, by providing
   them with a tool that can both lead to a better understanding of a
   standing bloom situation and to also enable meaningful predictions of
   water quality in the short and medium time. The model is site specific
   and takes into account that variability of six environmental parameters
   (light, temperature, turbidity, dissolved silicon concentration,
   dissolved nitrogen and phosphorus concentrations), which are known to
   have a major effect on algal growth in the Vaal River. Using data
   relative to the Stilfontein site, the model was double-validated and a
   set of 14 algal categories were designed to represent the total algal
