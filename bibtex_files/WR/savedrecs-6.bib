@article{ ISI:A1997WV97000033,
Author = {Eggins, BR and Palmer, FL and Byrne, JA},
Title = {{Photocatalytic treatment of humic substances in drinking water}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{5}},
Pages = {{1223-1226}},
Month = {{MAY}},
Note = {{World Environmental Congress / 2nd International Conference on Advanced
   Oxidation Technologies (AOTs-2) for Water and Air Treatment, LONDON,
   CANADA, SEP 12-16, 1995}},
Abstract = {{Currently drinking water from the Mourne Mountains in Northern Ireland
   is treated by the traditional process of coagulation and filtration.
   Semiconductor photocatalysis was investigated as an alternative. Aldrich
   humic acid solutions (100 mg dm(-3), filtered through 0.22 mu m
   Millipore filters) were photolysed in a 0.1\% suspension of Degussa P25
   titanium dioxide using a mercury lamp. Degradation of humic acid was
   followed over time by measuring colour in Hazen units, 254/400 nm
   absorbance, fluorescence (excitation 237 nm; emission 400 nm) and size
   exclusion high performance chromatography. The extent of complete
   oxidation was determined by measuring dissolved organic carbon, chemical
   oxygen demand and carbon dioxide released. It took approximately 12 min
   to reduce the humic acid concentration by half; however 50\% complete
   mineralization took 60 min. Some of the intermediates of reaction were
   highly fluorescent. In the absence of oxygen, light or titanium dioxide
   no significant degradation occurred. (C) 1997 Elsevier Science Ltd.}},
