@article{ ISI:A1997WZ85200020,
Author = {Chanson, H},
Title = {{Measuring air-water interface area in supercritical open channel flow}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{6}},
Pages = {{1414-1420}},
Month = {{JUN}},
Abstract = {{In storm waterways and at dam outlets, high-velocity supercritical flows
   are characterized by substantial air bubble entrainment. The entrainment
   of fine air bubbles and the strong turbulent mixing contribute both to
   the air-water transfer of volatile gases (e.g. oxygen, nitrogen, VOC).
   The paper describes new experimental data obtained in a 25 m long
   channel with a 4 degrees slope. The analysis of the data provides new
   information on the air-water how properties and on the distributions
   air-water interface area. Although the amount of entrained air is small
   (i.e. typically C-mean < 0.12), the specific air-water interface area
   can reach over 100 m(2) per unit volume of air and water. The results
   are compared with an earlier prediction (Chanson, 1994) and confirm the
   significant contribution of air entrainment to air-water gas transfer in
   supercritical chute flows. (C) 1997 Elsevier Science Ltd.}},
