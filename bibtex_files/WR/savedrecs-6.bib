@article{ ISI:A1997XJ93200012,
Author = {Duenas, C and Fernandez, MC and Liger, E and Carretero, J},
Title = {{Natural radioactivity levels in bottled water in Spain}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{8}},
Pages = {{1919-1924}},
Month = {{AUG}},
Abstract = {{Spain is a country rich in mineral springs. A nationwide study on the
   natural radioactivity has been made to determine the gross-alpha and
   gross-beta activities in bottled water. These measurements are important
   for extracting radiological information of the activity present in a
   sample. Of all samples collected, only 26.2\% have an alpha-activity
   higher than 100 mBq/l, and none of them have a beta activity higher than
   1000 mBq/l. An explanation for the different alpha and beta activities
   in bottled water that relates to the geological composition of the
   source is suggested. (C) 1997 Elsevier Science Ltd.}},
