@article{ ISI:A1997WV97000038,
Author = {Adachi, A and Ogawa, K and Tsushi, Y and Nagao, N and Kobayashi, T},
Title = {{Determination of vanadium in environmental samples by atomic absorption
   spectrophotometry}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{5}},
Pages = {{1247-1250}},
Month = {{MAY}},
Abstract = {{A simple and sensitive method for the determination of vanadium in water
   and air samples was established by using atomic absorption
   spectrophotometry with graphite Furnace atomizer. The proposed method
   includes formation of a chelate complex by reacting vanadium with
   ammonium 1-pyrrolidine carbodithioate (APDC), extracting the chelate
   with xylene and measurement of the extract using atomic absorption
   spectrometry. The recoveries of added vanadium in various environmental
