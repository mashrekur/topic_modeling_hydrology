@article{ ISI:A1997WV97000006,
Author = {Michalakos, GD and Nieva, JM and Vayenas, DV and Lyberatos, G},
Title = {{Removal of iron from potable water using a trickling filter}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{5}},
Pages = {{991-996}},
Month = {{MAY}},
Abstract = {{A trickling filter for iron removal from potable water has been
   constructed and tested. Iron removal was found to be caused by both
   biological and physico-chemical iron oxidation. The extent of each
   oxidation type was assessed. in addition, iron concentration profiles
   were taken along the filter depth, for both biological and
   physico-chemical iron oxidation, under the same operating conditions.
   Significant quantities of iron precipitated, mandating a periodic filter
   backwash. The possible duration of the filter operating cycles was
   studied as a function of the inlet iron concentration and. the
   volumetric Bow-rate. The limits of feed iron concentration and hydraulic
   loadings on the filter were determined and appropriate operating
   diagrams were constructed. (C) 1997 Elsevier Science Ltd.}},
