@article{ ISI:000071045900021,
Author = {Maugans, CB and Akgerman, A},
Title = {{Catalytic wet oxidation of phenol over a Pt/TiO2 catalyst}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{12}},
Pages = {{3116-3124}},
Month = {{DEC}},
Abstract = {{Wet oxidation of phenol by air or oxygen over a Pt/TiO2 catalyst is
   studied in a batch reactor in the temperature range 150-200 degrees C,
   pressure range 34-82 atm, and a catalyst loading range of 0-4 g catalyst
   L-1. The catalyst was powdered 4.45\% Pt/TiO2 with a maximum particle
   size of 105 mu m. Results show complete oxidation of phenol and almost
   complete total organic carbon (TOC) removal. Small amounts of stable
   organic acids are formed in side reactions of the phenol degradation
   pathway and are not readily degraded. Experimental results show that the
   reaction rate decreases by increased oxygen concentration. Theoretical
   rate expressions are derived, based on postulated oxidation and TOC
   reduction mechanisms. (C) 1997 Elsevier Science Ltd.}},
