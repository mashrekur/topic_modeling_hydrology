@article{ ISI:A1997XE04200037,
Author = {Sjosten, A and Blomqvist, S},
Title = {{Influence of phosphate concentration and reaction temperature when using
   the molybdenum blue method for determination of phosphate in water}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{7}},
Pages = {{1818-1823}},
Month = {{JUL}},
Abstract = {{The formation rate in water of the blue phosphoantimonylmolybdenum
   complex declines both with decreasing phosphate concentration (studied
   range 5-500 mu g PO4-P l(-1)) and decreasing reaction temperature
   (studied range + I to + 21 degrees C). At +15 degrees C, only solutions
   containing greater than or equal to 50 mu g POL-P l(-1) reached full
   colour expression within 5 min, and at +10 degrees C all tested
   concentrations needed a longer reaction time. At temperatures below +15
   degrees C, not even a reaction time of 10 min is always sufficient for
   complete colour development. For instance, at +7 degrees C, maximum
   absorbance level is reached in 10 min only at concentrations greater
   than or equal to 250 mu g PO4-P l(-1), whereas a solution of 5 mu g
   PO4-P l(-1) requires about 30 min. Slow formation rate of the coloured
   complex is a possible, insidious source of under-estimation,
   particularly when determining low concentrations of phosphate. (C) 1997
   Elsevier Science Ltd.}},
