@article{ ISI:000071045900006,
Author = {Sholji, I and Kazi, NM},
Title = {{Kinetics of pneumatic flocculation}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{12}},
Pages = {{2979-2988}},
Month = {{DEC}},
Abstract = {{A mathematical model has been established to describe the kinetics of
   pneumatic flocculation in terms of the process variables: energy of
   agitation, volume of turbid water, temperature, water viscosity, amount
   of synthetic turbidity, flocculating time, amount of air flow, depth of
   water column and size of orifice. The mathematical model tested for both
   normal and taper types of flocculation. The experimental data vindicated
   the model relationship between the concentration ratio and the rate of
   air flow for the air how rate above 50 cm(3) min(-1). Accordingly, the
   developed mathematical model was then linearised. (C) 1997 Elsevier
   Science Ltd.}},
