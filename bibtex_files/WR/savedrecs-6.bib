@article{ ISI:A1997WV97000005,
Author = {Haarhoff, J and Steinbach, S},
Title = {{A comprehensive method for measuring the air transfer efficiency of
   pressure saturators}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{5}},
Pages = {{981-990}},
Month = {{MAY}},
Abstract = {{This paper proposes an explicit and comprehensive method for measuring
   the efficiency of pressure saturators used in dissolved air flotation.
   It is based on a review of methods suggested by other researchers in
   this field, but several improvements to existing methods are made. The
   degree of nitrogen enrichment in the saturator air is accounted for, and
   the actual air composition in the measuring unit and the degree of
   supersaturation of air in the water leaving the measuring unit are taken
   into consideration. A complete mathematical description for determining
   the saturator efficiency from the measured data Is given based on
   Henry's law and molar or mass balances across the measuring bottle. The
   calculation procedure provides a clear picture of how the concentrations
   of oxygen and nitrogen in air and water change through the whole
   saturation and measuring system. (C) 1997 Elsevier Science Ltd.}},
