@article{ ISI:000071045900012,
Author = {Karimi-Jashni, A and Narbaitz, RM},
Title = {{Impact of pH on the adsorption and desorption kinetics of 2-nitrophenol
   on activated carbons}},
Journal = {{WATER RESEARCH}},
Year = {{1997}},
Volume = {{31}},
Number = {{12}},
Pages = {{3039-3044}},
Month = {{DEC}},
Abstract = {{The adsorption and desorption kinetics of 2-nitrophenol (2NP) from
   aqueous solutions using F-400 and WV-B granular activated carbons (GAG)
   have been studied at pHs 1, 4.6 and 13, using batch tests. Adsorption
   and desorption kinetics of 2NP on to both carbons were adequately
   described by the homogeneous solid surface diffusion model (HSSD). The
   adsorption and desorption kinetics can be predicted at different pHs by
   using adsorption kinetics parameters of the pH 4.6 and the isotherm
   parameters for the corresponding pH. Thus, the differences in the rates
   of adsorption are primarily attributable to the differences in the
   equilibrium loadings at the various pHs. (C) 1997 Elsevier Science Ltd.}},
